  if (karel_mutex == KAREL_USED) {
    return;
  }

  karel_mutex = KAREL_USED;
  $.ajax({
    type: "GET",
    url: "/KARELCMD/CRXAPDT?s_func_type=" + GET_CRX_COND,
    dataType: "json",
    cache: false,
  }).always(function() {
    karel_mutex = KAREL_UNUSED;
  }).done(function(json_data) {
    if (json_data.crx_installer_cond & target_cond) {
      resolve(0);
    } else {
      reject();
    }
  }).fail(
    error
  );
}
