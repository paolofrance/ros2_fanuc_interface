  var num_item = parseInt(response_xml.getElementsByTagName("dev_num")[0].textContent);
  devproperty_list = response_xml.getElementsByTagName("property");
  if (num_item == 0) {
    return;
  }

  for (var i = 0; i < num_item; i++) {
    var new_list_item = document.createElement("div");
    new_list_item.addEventListener("click", select_applst, false);
    new_list_item.setAttribute("id", "applist_item_" + i);
    new_list_item.setAttribute("class", "applist_item");
    new_list_item.setAttribute("value", i);

