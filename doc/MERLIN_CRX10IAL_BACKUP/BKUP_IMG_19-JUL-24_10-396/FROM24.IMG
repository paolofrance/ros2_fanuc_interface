    call_back_func();
  });
}

function get_xml_menu_add(call_back_func) {
  var tag_name;
  var menu_add;
  var xml_eg;
  var xml_kn;
  var filepath = new Array();
  var lang_data;
  var pass_count;
  var lang_count;
  var langpath = new Array();
  call_buff = call_back_func;

  xml_ajax = $.ajax({
    type: "GET",
    url: "/TD/RESPONSE.XML?" + Math.random(),
    dataType: "xml",
    cache: false,
  }).done(function(xml_data) {
    tag_name = 'add_menu';
    menu_add = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
    if(menu_add == MENU_EDIT_FLAG){
      tag_name = 'add_lang';
      lang_data = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
      lang_data = parseInt(lang_data, 10);
      pass_count = 0
      lang_count = 0
      
      tag_name = 'add_menu_eg';
      xml_eg = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
      filepath[pass_count] = add_menu_pass + xml_eg;
      pass_count++;
      if((lang_data & LANG_BIT_EG) == LANG_BIT_EG){
        langpath[lang_count] = 'eg';
        lang_count++;
      }
      if((lang_data & LANG_BIT_KN) == LANG_BIT_KN){
        tag_name = 'add_menu_kn';
        xml_kn = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
        filepath[pass_count] = add_menu_pass + xml_kn;
        langpath[lang_count] = 'kn';
        pass_count++;
        lang_count++;
      }

      if((lang_data & LANG_BIT_CH) == LANG_BIT_CH){
        langpath[lang_count] = 'ch';
      }

      top.treemod.mninst.addMenuData(filepath, set_install_result, langpath);

    }else{
      set_install_result(0);
    }
  }).fail(function() {
    xml_ajax.abort();
    get_xml_menu_add(call_back_func);
  });
}

function set_install_result(res) {

  document.getElementById("detail_message").style.display = "block";

  if(res == top.treemod.mninst.SUCCESS_UPD){
    top.treemod.mninst.updateTreeMenu();
    cancel_wait = 0;
    document.getElementById("list").style.display = "none";
    document.getElementById("description").style.display = "none";
    document.getElementById("install_result").style.display = "block";
    if(call_buff != undefined){
      call_buff();
    }
  }else{
    uninstall_app(res);
  }

}

