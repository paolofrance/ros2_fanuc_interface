function xml_install(call_back_func){
  call_buff = call_back_func;
  top.jQuery.varlis.bind('VarEvent', null, check_runtask);
  top.rpcmc_startVarMonitor("crxapdt", "i_ins_stat", 100);
  // Program, Variable, Interval(msec)
}

function check_runtask(event, progName, varName, typeCode, valStr){
  if((progName == "CRXAPDT") && (varName == "I_INS_STAT")){
    if(valStr == INS_SUCCESS){
      top.rpcmc_stopVarMonitor(progName, varName, 100);
      top.jQuery.varlis.unbind('VarEvent', check_runtask);
      check_menu_add(call_buff);
    }else if(valStr == INS_ERROR){
      top.rpcmc_stopVarMonitor(progName, varName, 100);
      top.jQuery.varlis.unbind('VarEvent', check_runtask);
      call_buff();
      top.rpcmc_getVar('crxmulti','i_status', get_xml_ipl_err);
    }else if(valStr == INS_CANCEL){
      top.rpcmc_stopVarMonitor(progName, varName, 100);
      top.jQuery.varlis.unbind('VarEvent', check_runtask);
      cancel_wait = 0;
      call_buff();
    }else if(valStr == DUPLI_INS){
      top.rpcmc_stopVarMonitor(progName, varName, 100);
      top.jQuery.varlis.unbind('VarEvent', check_runtask);
      if(cancel_wait == 0){
        chk_dupmenu(call_buff);
      }else{
        cancel_wait = 0;
        call_buff();
      }
    }
  }
}

