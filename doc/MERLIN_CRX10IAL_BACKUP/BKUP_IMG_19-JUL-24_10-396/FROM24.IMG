    case top.treemod.mninst.ERR_PRS_MENU:
      code_elem.textContent = top.treemod.mninst.ERR_PRS_MENU;
      cause_elem.textContent = CRX_APINS_DICT.CRXCTA_CAUSE2;
      break;
    case top.treemod.mninst.ERR_GET_SCRN:
      code_elem.textContent = top.treemod.mninst.ERR_GET_SCRN;
      cause_elem.textContent = CRX_APINS_DICT.CRXCTA_CAUSE3;
