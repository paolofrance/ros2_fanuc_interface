function chk_license_txt(sub_dir,use_chk_box_args,readme_file){
  var file_exe = getExt(readme_file);
  file_exe = file_exe.toUpperCase();
  document.getElementById("license_box").style.visibility = "hidden";

  if(readme_file != "" && (file_exe == 'HTM' || file_exe == 'TXT')){
    var descript_path = "\\" + sub_dir +  readme_file;
    descript_path += "?val=" + Math.random();
    var app_license_area_elm = document.getElementById("licensetext_frame");
    
    var font_family = window.getComputedStyle(document.body).fontFamily;
    app_license_area_elm.setAttribute("src", descript_path);
    document.getElementById("license_box").style.display = "";
    $("#licensetext_frame").on("load", function() {
      $('#licensetext_frame').contents().find("*").each(function(index, value) {
        var style = $(value).attr("style");
        if (style === undefined) {
          style = "";
        }
        style += "background-color: transparent!important;";
        style += "font-size: 20px;";
        style += "font-family: " + font_family;
        $(value).css('cssText', style);
      });
      var iframe_body_elm = $('#licensetext_frame').contents().find("body");
      iframe_body_elm.append('<link rel="stylesheet" type="text/css" href="/frh/irprog/crx/css/crxstyle.css">');
      iframe_body_elm.addClass("y_scroll");
    });
    if(use_chk_box_args == CHK_BOX_ON){
      document.getElementById("license_check_area").style.display = "";
      document.getElementById("license_check.input").checked = false;
      document.getElementById("install_message").style.display = "none";
      document.getElementById("license_check_message").style.display = "";
      document.getElementById("license_check_message").style.visibility = "hidden";
      document.getElementById("install_btn").refresh(null, null, null, null, true);
    }else{
      document.getElementById("license_check_area").style.display = "none";
      document.getElementById("install_message").style.display = "";
      document.getElementById("install_message").style.visibility = "visible";
      document.getElementById("license_check_message").style.display = "none";
      document.getElementById("install_btn").refresh(null, null, null, null, false);
    }
    document.getElementById("license_box").style.visibility = "visible";
  }else{
    document.getElementById("license_check_area").style.display = "none";
    document.getElementById("license_check_message").style.display = "none";
    document.getElementById("install_message").style.display = "";
    document.getElementById("install_message").style.visibility = "visible";
    document.getElementById("install_btn").refresh(null, null, null, null, false);
    use_chk_box = CHK_BOX_OFF;
  }
}

