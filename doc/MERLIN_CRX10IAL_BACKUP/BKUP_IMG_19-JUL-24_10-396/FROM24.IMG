function set_err_message(error_flag) {

  var list_area = document.getElementById("file_list_area");
  while (list_area.firstChild) {
    list_area.removeChild(list_area.firstChild);
  }

  document.getElementById("install_area").style.display = "none";
  document.getElementById("errer_area").style.display = "";
  document.getElementById("err_message_body").style.display = "";
  document.getElementById("detail_message").style.display = "none";

  var ttl_elem = document.getElementById("err_message_ttl");
  var body_elem = document.getElementById("err_message_body");
  if((error_flag ^ INSTALL_STATUS.OK_INSTALL) == 0){
    // ok
  } else if((error_flag & INSTALL_STATUS.NO_MEDIA) != 0){
    // media error
    ttl_elem.textContent = CRX_APINS_DICT.CRXCTA_MDAERR;
    body_elem.textContent = CRX_APINS_DICT.CRXCTA_MDAMSG;
  } else if((error_flag & INSTALL_STATUS.NO_TP) != 0){
    // tp off error
    ttl_elem.textContent = CRX_APINS_DICT.CRXCTA_TPERR;
    body_elem.textContent = CRX_APINS_DICT.CRXCTA_TPMSG;
  }
  document.getElementById("retry_btn").style.display = "";
}

