    item_content.textContent = devproperty_list[i].getElementsByTagName("dev_name")[0].textContent;
    if (item_content.textContent == "") {
      item_content.textContent = "\u00a0";
    }
    new_list_item.appendChild(item_content);

    item_content = document.createElement("div");
    item_content.setAttribute("id", "applist_provid_" + i);
    item_content.setAttribute("class", "applist_provid");
