function do_install_btn(id, operation) {
  var pop_up_initialize;

  inst_btn_cnt++;
  if (inst_btn_cnt == 1) {
    pop_up_initialize = function(frame) {
      var cancel_btn_elem;
      var ok_btn_elem;
      var pop_up_close;
      var push_ok_btn;
      var push_cancel_btn;

      pop_up_close = function(operation) {
        appendArg.popupFrm.removePopupFrame();
      };
      push_ok_btn = function(id, operation) {
        // check tp
        top.rpcmc_chkkey(top.tpin_type_c, top.tp_enbl_c, null, null,
          function(sys, num, enable, argv, status) {
            if (status != 0) {
              // get status failure
              show_tp_off();
              pop_up_close();
            } else if (enable == TP_KEY_ON) {
              if ((use_chk_box == CHK_BOX_OFF) || (lic_chk_on == true)) {
                frame.document.getElementById('message').textContent = CRX_APINS_DICT.CRXCTA_RUNINST;
                frame.document.getElementById("message").style.height = "50px";
                frame.document.getElementById('ok_btn').style.display = "none";
                frame.document.getElementById('action_area').style.display = "block";
                top.rpcmc_setVar('CRXLIB', 'bOpCancel', 0);
                // change callback to cancel button
                cancel_btn_elem.setCallback(push_cancel_btn);
                install_app(pop_up_close);
              } else {
                // chk_box_off status
                show_chk_box_off();
                pop_up_close();
              }
            } else if (enable == TP_KEY_OFF) {
              // tp off status
              show_tp_off();
              pop_up_close();
            }
          }
        );
      };

      push_cancel_btn = function(id, operation){
        // Cancel function
        frame.document.getElementById('message').textContent = CRX_APINS_DICT.CRXCTA_CXLINST;
        cancel_btn_elem.style.display = "none";
        cancel_install_app(pop_up_close);
      }

      cancel_btn_elem = frame.document.getElementById('cancel_btn');
      cancel_btn_elem.setCallback(pop_up_close);
      cancel_btn_elem.style.display = "inline";
      ok_btn_elem = frame.document.getElementById("ok_btn");
      ok_btn_elem.setCallback(push_ok_btn);
      ok_btn_elem.style.display = "inline";

      var message = CRX_APINS_DICT.CRXCTA_CFMINST;
      message = message.replace('[[XXXXXX]]', document.getElementById("app_name").textContent);
      frame.document.getElementById('message').textContent = message;

    };

    if (num_plug_item >= PLUGIN_MAX_NUM) {
      document.getElementById("errer_area").style.display = "none";
      set_err_message_scrn(INSTALL_STATUS.NO_PLUGIN);
      return;
    }

    if (scrn_id_status == 1) {
      document.getElementById("errer_area").style.display = "none";
      set_err_message_scrn(INSTALL_STATUS.NO_PLUGIN);
      return;
    }

    var ipl_list_json = get_ipl_file_status();

    check_install(
      ipl_list_json,
      function(check_result_code) {
        document.getElementById("install_area").style.display = "";
        document.getElementById("errer_area").style.display = "none";

        var customizeObj = {};
        customizeObj.hideCloseBtn = true;
        customizeObj.callerWindowObj = window;
        customizeObj.refreshFunc = pop_up_initialize;
        customizeObj.appendArg = appendArg;

        top.IHMIComponents.cf.appendPopupFrm("/frh/irprog/crx/html/crxpop.stm", null, window, customizeObj);
        inst_btn_cnt = 0;
      },
      function(check_result_code) {
        set_err_message(check_result_code);
        inst_btn_cnt = 0;
      }
    );
  }
}


