function check_install(ipl_list_json, resolve, reject) {

  //check file
  if (ipl_list_json.ipl_status == false) {
    reject(INSTALL_STATUS.NO_MEDIA)
    return;
  }

  // check tp
  top.rpcmc_chkkey(top.tpin_type_c, top.tp_enbl_c, null, null,
    function(sys, num, enable, argv, status) {
      if (status != 0) {
        // get status failure
        reject(INSTALL_STATUS.NO_TP);
      } else if (enable == TP_KEY_ON) {
        // tp on status
        resolve(INSTALL_STATUS.OK_INSTALL);
      } else if (enable == TP_KEY_OFF) {
        // tp off status
        reject(INSTALL_STATUS.NO_TP);
      }
    }
  );

}

function get_ipl_file_status(){
  var json_data = {
    ipl_status : false,
    ipl_file_num : NaN
  };

  if(karel_mutex == KAREL_UNUSED){
    karel_mutex = KAREL_USED;

    prg_req = new XMLHttpRequest();
    prg_req.open("GET", "/KARELCMD/CRXAPDT?s_func_type=" + GET_IPL_FILE, false);
    prg_req.send(null);
    if (prg_req.readyState == XHR_DONE) {
      if (((prg_req.status >= HTTP_OK_LANGE_MIN) && (prg_req.status <= HTTP_OK_LANGE_MAX)) || (prg_req.status == HTTP_NOT_MODIFIED)) {
        json_data = JSON.parse(prg_req.responseText.replace(", ]", "]"));
      }
    }
    karel_mutex = KAREL_UNUSED;
    prg_req.abort();
  }

  return json_data;
}

