function uninstall_app(call_back_func) {

  karel_mutex = KAREL_USED;

  xml_ajax = $.ajax({
    type: "GET",
    url: "/KARELCMD/CRXAPDT?s_func_type=" + DO_UNINSTALL + "&s_device_id=" + select_device,
    cache: false,
  }).always(function() {
    karel_mutex = KAREL_UNUSED;
  }).done(function() {
    xml_ajax.abort();
    xml_uninstall(call_back_func);
  }).fail(function() {
    document.getElementById("install_area").style.display = "none";
    document.getElementById("errer_area").style.display = "";
    set_err_message(INSTALL_STATUS.NO_MEDIA);
    call_back_func();
  });
}

function xml_uninstall(call_back_func){
  call_buff = call_back_func;
  top.jQuery.varlis.bind('VarEvent', null, check_runtask);
  top.rpcmc_startVarMonitor("crxapdt", "i_del_stat", 100);
  // Program, Variable, Interval(msec)
}

function check_runtask(event, progName, varName, typeCode, valStr){
  if((progName == "CRXAPDT") && (varName == "I_DEL_STAT")){
    if(valStr == DEL_SUCCESS){
      top.rpcmc_stopVarMonitor(progName, varName, 100);
      top.jQuery.varlis.unbind('VarEvent', check_runtask);
      set_uninstall_result();
      call_buff();
    }else if(valStr == DEL_ERROR){
      top.rpcmc_stopVarMonitor(progName, varName, 100);
      top.jQuery.varlis.unbind('VarEvent', check_runtask);
      top.treemod.mninst.updateTreeMenu();
      call_buff();
    }
  }
}

function set_uninstall_result() {
  list_elem = document.getElementById("list");
  descript_elem = document.getElementById("description");

  list_elem.style.display = "none";
  descript_elem.style.display = "none";

  result_elem = document.getElementById("uninstall_result");
  result_elem.style.display = "";

}

function check_menu_xml(call_back_func) {

  $.ajax({
    type: "GET",
    url: "/KARELCMD/CRXAPDT?s_func_type=" + DO_UNINSTMNU + "&s_select_list=" + select_list,
    cache: false,
  }).always(function() {
    karel_mutex = KAREL_UNUSED;
  }).done(function() {
    get_xml_menu_del(call_back_func)
  }).fail(function() {
    call_back_func();
  });
}

function get_xml_menu_del(call_back_func) {

  var tag_name;
  var menu_del;
  var xml_eg;
  var xml_kn;
  var filepath = new Array();
  var pass_count;
  var lang_data;
  var lang_count;
  var langpath = new Array();

  xml_ajax = $.ajax({
    type: "GET",
    url: "/TD/RESPONSE.XML?" + Math.random(),
    dataType: "xml",
    cache: false,
  }).done(function(xml_data) {
    tag_name = 'delfile_list';
    menu_del = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
    if(menu_del == MENU_EDIT_FLAG){
      call_buff = call_back_func
      tag_name = 'add_lang';
      lang_data = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
      lang_data = parseInt(lang_data, 10);
      pass_count = 0;
      lang_count = 0;

      tag_name = 'delfile_list_eg';
      xml_eg = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
      filepath[pass_count] = add_menu_pass + xml_eg;
      pass_count++;

      if((lang_data & LANG_BIT_EG) == LANG_BIT_EG){
        langpath[lang_count] = 'eg';
        lang_count++;
      }

      if((lang_data & LANG_BIT_KN) == LANG_BIT_KN){
        tag_name = 'delfile_list_kn';
        xml_kn = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
        filepath[pass_count] = add_menu_pass + xml_kn;
        langpath[lang_count] = 'kn';
        pass_count++;
        lang_count++;
      }
      if((lang_data & LANG_BIT_CH) == LANG_BIT_CH){
        langpath[lang_count] = 'ch';
      }
      xml_ajax.abort();
      top.treemod.mninst.delMenuData(filepath, del_menu_result, langpath);
    }else{
      xml_ajax.abort();
      uninstall_app(call_back_func);
    }
  }).fail(function() {
  });
}

function del_menu_result(result){

  uninstall_app(call_buff);
}

