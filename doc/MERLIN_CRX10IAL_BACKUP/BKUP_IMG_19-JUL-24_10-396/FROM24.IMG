function del_xml_menu_dup(call_back_func) {

  var tag_name;
  var menu_del;
  var xml_eg;
  var xml_kn;
  var filepath = new Array();
  var lang_data;
  var pass_count;
  var lang_count;
  var langpath = new Array();
  call_buff = call_back_func;
  
  xml_ajax = $.ajax({
    type: "GET",
    url: "/TD/RESPONSE.XML?" + Math.random(),
    dataType: "xml",
    cache: false,
  }).done(function(xml_data) {
    tag_name = 'delfile_list';
    menu_del = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
    if(menu_del == MENU_EDIT_FLAG){
      tag_name = 'add_lang';
      lang_data = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
      lang_data = parseInt(lang_data, 10);
      pass_count = 0;
      lang_count = 0;

      tag_name = 'delfile_list_eg';
      xml_eg = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
      filepath[pass_count] = add_menu_pass + xml_eg;
      pass_count++;

      if((lang_data & LANG_BIT_EG) == LANG_BIT_EG){
        langpath[lang_count] = 'eg';
        lang_count++;
      }
      if((lang_data & LANG_BIT_KN) == LANG_BIT_KN){
        tag_name = 'delfile_list_kn';
        xml_kn = xml_data.getElementsByTagName(tag_name)[0].firstChild.nodeValue;
        filepath[pass_count] = add_menu_pass + xml_kn;
        langpath[lang_count] = 'kn';
        pass_count++;
        lang_count++;
      }
      if((lang_data & LANG_BIT_CH) == LANG_BIT_CH){
        lang_count = lang_count + 1;
        langpath[lang_count] = 'ch';
      }
      xml_ajax.abort();
      top.treemod.mninst.delMenuData(filepath, unins_dupmeu, langpath);
    }else{
      xml_ajax.abort();
      unins_dupmeu();
    }
  }).fail(function() {
  });
}

function unins_dupmeu() {
  if(cancel_wait == 1){
    cancel_wait = 0;
    call_buff();
