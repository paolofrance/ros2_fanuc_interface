	}
];

/**
 * Search target id's popup definition from table.
 *  id : Target popup id.
 */
function searchPopupTable(id) {
	for (var i = 0; i < popupDefTable.length; i++) {
		var def = popupDefTable[i];
		if (def.id == id) {
			return def;
		}
	}
	return null;
}

function calcStrWidth(str) {
	var rulerTag = $('<span>', { id: 'strruler' });
	rulerTag.css({ 'visibility': 'hidden', 'position': 'absolute', 'white-space': 'nowrap' });
	rulerTag.text(str);
	$('body').append(rulerTag);
	var strWidth = rulerTag.get(0).offsetWidth;
	setTimeout(function () {
		$('#strruler').remove();
	}, 0);
	return strWidth;
}

/**
 * Popup dialog function.
 * Generate the desired pop-up when needed.
 */
var popupDialog = (function () {
	// Open Dialog
	function open(targetId, popupBox) {
		$(targetId).append(popupBox);
	}
	// Close Dialog
	function close(popupId) {
		var idStr = '#' + popupId;
		$(idStr).remove();
	}
	return {
		// Confirm dialog (Yes/No)
		selection: function (targetId) {
			var defTable = searchPopupTable(targetId);
			var idStr = '#' + defTable.id;
			var $popupSelecter = $(idStr);
			var $ddnbtn = $(defTable.selectId + 'ddn');	// dropdown button id
			if ($popupSelecter.length == 0) {
				$ddnbtn.removeClass('ui-icon-triangledn-white');
				$ddnbtn.addClass('ui-icon-triangleup-white');
				var popuparrow = (defTable.arrow == 'left') ? 'popupleftdlg' :
					(defTable.arrow == 'right') ? 'popuprightdlg' : 'popupmiddledlg';
				var popupBox = $('<div>',
					{ id: defTable.id, 'class': popuparrow });
				var popupSelect = '';
				var maxStrWidth = 0;
				// Inner data of popup
				for (var i = 0; i < defTable.list.length; i++) {
					if ((defTable.list[i].chkValid != undefined) && (typeof defTable.list[i].chkValid === 'function')) {
						// Is this a valid menu?
						if (!defTable.list[i].chkValid()) {	// If invalid, skip append menu.
							continue;
						}
					}
					var pupsel = $('<div>', {
						'class': defTable.list[i].btncls,
						id: defTable.list[i].btnid
					});
					var popupStr = resources[defTable.list[i].resId];
					if (defTable.list[i].img.length > 0) {
						pupsel.html('<img src="' + defTable.list[i].img + '" class="'
							+ defTable.list[i].imgcls + '"><span class="selplayitem">'
							+ popupStr + '</span>');
					} else {
						pupsel.html(popupStr);
					}
					if (defTable.calcWidth) {
						var strWidth = calcStrWidth(popupStr);
						if (maxStrWidth < strWidth) {
							maxStrWidth = strWidth;
						}
					}
					popupBox.append(pupsel);
				}
				var $targetBtn = $(defTable.targetId);
				//var targetX = $targetBtn.offset().left + $targetBtn[0].clientWidth;
				var targetX = $ddnbtn[0].offsetLeft - ($ddnbtn[0].clientWidth / 2);
				var targetY = $targetBtn.offset().top + $targetBtn[0].clientHeight;
				popupBox.css({ 'top': targetY, 'left': targetX });
				if (defTable.calcWidth) {
					popupBox.css({ 'width': (maxStrWidth + 10 + 12) });	// add (margin+border+padding)+padding
				}
				// to modal
				$('#pceditor').append('<div id="popmenuoverlay" class="popupoverlay"></div>');
				$('#popmenuoverlay').show();
				open('body', popupBox);
			} else {
				close(defTable.id);
				$('#popmenuoverlay').hide();
				$('#popmenuoverlay').remove();
				$ddnbtn.removeClass('ui-icon-triangleup-white');
				$ddnbtn.addClass('ui-icon-triangledn-white');
			}
			$('.btnlaunch').on('click touchend', function (event) {
				if(event.type.indexOf('touch') >= 0){
					event.preventDefault();
				}
				close(defTable.id);
				$('#popmenuoverlay').hide();
				$('#popmenuoverlay').remove();
				$ddnbtn.removeClass('ui-icon-triangleup-white');
				$ddnbtn.addClass('ui-icon-triangledn-white');
				var launchId = event.currentTarget.id;
				// search select menu
				for (var i = 0; i < defTable.list.length; i++) {
					if (launchId == defTable.list[i].btnid) {	// Match!
						debug(DBGINFO, 'launch program:(' + defTable.list[i].url + ')');
						window.open(defTable.list[i].url, '_blank');
						break;
					}
				}
			});
			$('.btnselposedit').on('click touchend', function (event) {
				if(event.type.indexOf('touch') >= 0){
					event.preventDefault();
				}
				close(defTable.id);
				$('#popmenuoverlay').hide();
				$('#popmenuoverlay').remove();
				$ddnbtn.removeClass('ui-icon-triangleup-white');
				$ddnbtn.addClass('ui-icon-triangledn-white');
				return false;
			});
			// Click (Pos)Delete button.
			$('#regposctrldel').on('click touchend', function (event) {
				if(event.type.indexOf('touch') >= 0){
					event.preventDefault();
				}
				selectRegDeleteMenu();
			});
			// Click (Pos)Copy button.
			$('#regposctrlcopy').on('click touchend', function (event) {
				if(event.type.indexOf('touch') >= 0){
					event.preventDefault();
				}
				selectRegCopyMenu();
			});
			// Click (Pos)Paste button.
			$('#regposctrlpaste').on('click touchend', function (event) {
				if(event.type.indexOf('touch') >= 0){
					event.preventDefault();
				}
				selectRegPasteMenu();
			});
			// Click (Pos)Repre button.
			$('#regposctrlrepre').on('click touchend', function (event) {
				if(event.type.indexOf('touch') >= 0){
					event.preventDefault();
				}
				selectRegRepreMenu();
			});
		}
	}
})();

function callbackStepStatus(iotype, index, stepstatus, callbackarg1, callbackarg2) {
	// Step mode exchange
	if ((isProgramStepMode() && (stepstatus == '0')) || 	// prgwin current=step set=continuous send command
		(!isProgramStepMode() && (stepstatus == '1'))) {	//        current=continuous set=step send command
		exchangeStepMode();	// api
	}
}

/**
 * Catch window click event ... Turn off popup.
 **/
function turnoffPopup() {
	if ($('#getoverride').length > 0) {
		$('#getoverride').remove();
		var defTable = searchPopupTable('getoverride');
		var $ddnbtn = $('#' + defTable.buttonId + 'ddn');
		$ddnbtn.removeClass('ui-icon-triangleup-white');
		$ddnbtn.addClass('ui-icon-triangledn-white');
	} else if ($('#selectfile').length > 0) {
		$('#selectfile').remove();
		$('#filemenuddn').removeClass('ui-icon-triangleup-white');
		$('#filemenuddn').addClass('ui-icon-triangledn-white');
	} else if ($('#regselposedit').length > 0) {
		$('#regselposedit').remove();
		$('#editbtnddn').removeClass('ui-icon-triangleup-white');
		$('#editbtnddn').addClass('ui-icon-triangledn-white');
	} else if ($('#toggleexec').length > 0) {
		$('#toggleexec').remove();
		$('#selexecddn').removeClass('ui-icon-triangleup-white');
		$('#selexecddn').addClass('ui-icon-triangledn-white');
	} else if ($('#launchext').length > 0) {
		$('#launchext').remove();
		$('#othermenuddn').removeClass('ui-icon-triangleup-white');
		$('#othermenuddn').addClass('ui-icon-triangledn-white');
	}
}
