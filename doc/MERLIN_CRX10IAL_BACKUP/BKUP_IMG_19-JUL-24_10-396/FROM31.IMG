/*
 * Windows OnLoad Event
 */
$(window).on('load', function () {
  g_win_x = window.innerWidth;
  g_win_y = window.innerHeight;
  debug(DBGINFO, 'onLoad: g_win_x=' + g_win_x + '/g_win_y=' + g_win_y);
  if (g_win_x < MIN_WINDOWWIDTH) {
    g_win_x = MIN_WINDOWWIDTH;
    $(window).width(g_win_x);
  }
  if (g_win_y < MIN_WINDOWHEIGHT) {
    g_win_y = MIN_WINDOWHEIGHT;
    $(window).width(g_win_y);
  }
  $(document).width(g_win_x);
  $(document).height(g_win_y);
  var navUA = navigator.userAgent.toLowerCase();
  var isIE = ((navUA.indexOf('msie') != -1) || (navUA.indexOf('trident') != -1)) ? true : false;
  if (isIE) $('#refposarea').css({"position": "relative"});
  initializeLayout();
  var init_layout_h = g_win_y - (DEFAULTBODYMARGIN + $('#refposbtnarea').outerHeight() + $('#tabarea').outerHeight() + $('#pagearea').outerHeight());
  var padding_h = parseInt($('#refposarea').css('padding-top'), 10) + parseInt($('#refposarea').css('padding-bottom'), 10);
  $('#refposarea').height(init_layout_h - padding_h - DEFAULTBODYMARGIN - GRIDBTMMARGIN);
  setFileName("refpos");
  refPosTableDef.gridAreaWidth = $('#refposarea').width() - DEFAULT_GRID_MARGIN;
  refPosTableDef.tableWidth = $('#refposarea').width() - DEFAULT_GRID_MARGIN;
  refPosTableDef.tableHeight = $('#refposarea').height() - DEFAULT_GRID_MARGIN;
  setCurrentLanguage(initLayoutAreaCallBack);
});

/*
 * Windows Resize Event
 */
$(window).on('resize', function () {
  g_win_x = window.innerWidth;
  g_win_y = window.innerHeight;
  if (g_win_x < MIN_WINDOWWIDTH) {
    g_win_x = MIN_WINDOWWIDTH;
    $(window).width(g_win_x);
  }
  if (g_win_y < MIN_WINDOWHEIGHT) {
    g_win_y = MIN_WINDOWHEIGHT;
    $(window).width(g_win_y);
  }
  $(document).width(g_win_x);
  $(document).height(g_win_y);
  document.getElementById('tabarea').equalizeTabs();
  var resize_layout_h = g_win_y - (DEFAULTBODYMARGIN + $('#refposbtnarea').outerHeight() + $('#tabarea').outerHeight() + $('#pagearea').outerHeight());
  var padding_h = parseInt($('#refposarea').css('padding-top'), 10) + parseInt($('#refposarea').css('padding-bottom'), 10);
  var grid_area_h = resize_layout_h - padding_h - DEFAULTBODYMARGIN - GRIDBTMMARGIN;
  $('#refposarea').height(grid_area_h);
  if (refPosGrid.getRecord(1) != null) {
    var gridSize = {
      gridAreaWidth : $('#refposarea').width() - DEFAULT_GRID_MARGIN,
      tableWidth : $('#refposarea').width() - DEFAULT_GRID_MARGIN,
      tableHeight : grid_area_h - DEFAULT_GRID_MARGIN
    }
    refPosGrid.resizeGrid(gridSize);
  }
});
