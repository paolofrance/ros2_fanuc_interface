  if (copySelType == crntSelType) {
    if (copySelType == SELGRID_ROW) {
      copyRowsData(copyButtonText);
    } else {
      w2ui['posregarea'].paste(copyButtonText);
    }
  }
  w2ui['posregarea'].refresh();
  multiRowFlag = false;
}

// chack recid of select data for crntSelType == SELGRID_CELL
// if data has already exist recid in exchangeRecList, skip pushing it to list.
var exchangeRecList = [];	// Record List.
var exchangeRecIndex = 0;	// Record index.
function isDuplicateRecord(recId) {
  for (var k = 0; k < exchangeRecList.length; k++) {
    if (recId == exchangeRecList[k].recid) {
      return true;
    }
  }
  return false;
}

// called to popupmenu [Convert (Cart./Joint)]
function selectRegRepreMenu() {
  if (isUneditable()) {
    top.IHMIComponents.cf.toast.show(resources['ERREditPosregTpOff']);
    return;
  }
  if (g_select_grid_row == null) {
    top.openCmpAlert(window, 'noselect3Dlg', resources["dlgMsgNoSelect3"]);
    return;
  }
  exchangeRecList = [];
  exchangeRecIndex = 0;
  // Make Select Record List.
  var selRecord = w2ui['posregarea'].getSelection();
  var selRecId = 0;
  for (i = 0; i < selRecord.length; i++) {
    if (crntSelType == SELGRID_CELL) {
      selRecId = selRecord[i].recid
    } else if (crntSelType == SELGRID_ROW) {
      selRecId = selRecord[i];
    }
    if (selRecId >= 0) {
      var selRec = w2ui['posregarea'].get(selRecId);
      if (selRec.rep != POS_REP_NONE) {
        if (!isDuplicateRecord(selRecId)) {	// Record ID is not duplicated.
          exchangeRecList[exchangeRecIndex] = selRec;
          exchangeRecIndex++;
        }
      }
    }
  }
  var btnObj = {
    btnData: top.irpDlgBtnOC,
    btn1Cb:mnchgrepRelay
  };
  top.openCmpConfirm(webPage, window, 'repPosDlg', resources["dlgMsgRepPos"], btnObj);
}

// preparation for copyRowsData and call execExchangePosFrame
function mnchgrepRelay() {
  clearGTimer();
  exchangeRecIndex = 0;
  if (exchangeRecList.length == 0) {
    return;	// if no list, no operation.
  }
  if (crntSelType == SELGRID_CELL) {
    var rowCnt = g_select_grid_row[g_select_grid_row.length - 1].recid - g_select_grid_row[0].recid + 1;
    var startRecid = g_select_grid_row[0].recid;
    g_select_grid_row.length = 0;
    for (i = 0; i < rowCnt; i++) {
      g_select_grid_row[i] = startRecid + i;
    }
  }
  execExchangePosFrame(exchangeRecList[exchangeRecIndex], mnchgrepCallback);
}

// prepare the data for exchange(exchangePosFrame, in common_robotapi)
// exchgInfo is position data before conversion
// newRec is outer frame for position data after conversion
function execExchangePosFrame(srcRecInfo, callback, orgData) {
  // Create Arguments
  var exchgInfo = {};
  exchgInfo.orgData = orgData;
  exchgInfo.srcRec = beforeExchangePosText(createWriteData(srcRecInfo));
  exchgInfo.cb = callback;
  exchgInfo.grp = g_select_group;
  exchgInfo.robot = g_axes_count[g_select_group - 1];
  exchgInfo.ext = g_ext_count[g_select_group - 1];
  exchgInfo.unit = g_axis_unit;
  exchgInfo.srcRec.UF = srcRecInfo.uf;
  exchgInfo.srcRec.UT = srcRecInfo.ut;
  exchgInfo.tbl = POSTBLKIND_REGS;
  if ((srcRecInfo.rep == POS_REP_CART) || (srcRecInfo.rep == POS_REP_BOTH)) {
    exchgInfo.srcRec.frame = COORDINATECART;
    exchgInfo.exchgTo = COORDINATEJOINT;
    exchgInfo.axes = g_axes_count[g_select_group - 1] + g_ext_count[g_select_group - 1];
  } else if (srcRecInfo.rep == POS_REP_JOINT) {
    exchgInfo.srcRec.frame = COORDINATEJOINT;
    exchgInfo.exchgTo = COORDINATECART;
    exchgInfo.axes = g_ext_count[g_select_group - 1];
  } else {
    exchgInfo.srcRec.frame = COORDINATEUNKOWN;
    exchgInfo.exchgTo = COORDINATEUNKOWN;
    exchgInfo.axes = g_ext_count[g_select_group - 1];
  }
  // Set(Initialize) target new record
  var newRec = createrow();
  newRec.rep = srcRecInfo.rep;
  newRec = createWriteData(newRec);
  newRec.recid = srcRecInfo.recid;
  newRec.Comment = srcRecInfo.comment;
  newRec.id = srcRecInfo.id;
  newRec.force = true;
  newRec.config_flag = false;
  newRec.GP = g_select_group;
  newRec.axes = exchgInfo.axes;
  exchgInfo.newRec = newRec;
  exchgInfo.UFVal = POS_REG_UF_VAL;
  exchgInfo.UTVal = POS_REG_UT_VAL;
  // Call exchange process
  exchangePosFrame(exchgInfo);	// api
}

function execExchangeCartToJoint(cartData, orgJointData) {
  execExchangePosFrame(cartData, execExchangeCartToJointCb, orgJointData);
}

function execExchangeCartToJointCb(status, cartToJointData, newRecTbl, orgJointData) {
  if (status != IO_SUCCESS || cartToJointData == null) return;
  var convJointData = createConvData(cartToJointData);

  if (!isSamePosition(convJointData, orgJointData)) {
    pasteJointRepData(orgJointData, cartToJointData.recid);
  }
}

function pasteJointRepData(jointData, recid) {
  var pasteRecData = [];
  top.openCmpAlert(window, 'convcarterrDlg',(resources["msg_recposcarterr_c"] + resources["msg_recposjoint_c"]));
  jointData.w2ui.style = w2ui['posregarea'].get(recid).w2ui.style;
  pasteRecData.push(jointData);
  pasteRepData(pasteRecData);
}

function createConvJointData(jointData, id, recid) {
  var write_data = createrow();
  write_data.force = true;
  write_data.rep = POS_REP_JOINT;
  write_data.axes = g_axes_count[g_select_group - 1] + g_ext_count[g_select_group - 1];
  write_data.config = "";
  write_data.id = id;
  write_data.recid = recid;
  for (var i = POS_REG_CLM_ARR.indexOf('j1'); i <= POS_REG_CLM_ARR.indexOf('ext3'); i++) {
    var up_str = POS_REG_CLM_ARR[i].toUpperCase();
    write_data[POS_REG_CLM_ARR[i]].data = jointData[up_str];
    write_data[POS_REG_CLM_ARR[i]].unit = g_axis_unit[i - POS_REG_CLM_ARR.indexOf('j1')];
  }
  return write_data;
}

// keep position data after conversion for copyRowsData
// prevent continuous transmission of data to control device
var multiRowExchgError = false;
var IO_SUCCESS = '0x0';
function mnchgrepCallback(status, chgRecData) {
  if (exchangeRecIndex == 0) {
    repreButtonText = [];
  }
  if ((chgRecData != null) && (status == IO_SUCCESS)) {	// Data exchange is succeed.
    var pushData = createConvData(chgRecData);
    pushData.w2ui.style = w2ui['posregarea'].get(pushData.recid).w2ui.style;
    repreButtonText.push(pushData);
  } else {
    repreButtonText.push(w2ui['posregarea'].get(g_select_grid_row[0] + exchangeRecIndex));
  }
  // Index to next record
  exchangeRecIndex++;
  // Data exists in the list.
  if (exchangeRecIndex < exchangeRecList.length) {
    execExchangePosFrame(exchangeRecList[exchangeRecIndex], mnchgrepCallback);
    return;
  }
  for (var i = 0; i < exchangeRecIndex; i++) {
    if (exchangeRecList[i].rep == POS_REP_CART ||
      (exchangeRecList[i].rep == POS_REP_BOTH && exchangeRecList[i].x != "")) {
      var pasteData = [];
      pasteData[0] = repreButtonText[i];
      pasteRepData(pasteData);
    } else if (exchangeRecList[i].rep == POS_REP_JOINT || 
      (exchangeRecList[i].rep == POS_REP_BOTH && exchangeRecList[i].x == "")) {
      var orgData = {
      	cartData : repreButtonText[i],
      	jointData : exchangeRecList[i],
      	index : i
      };
      execExchangePosFrame(repreButtonText[i], execExChgCartToJoint, orgData);
    }
  }
}
function execExChgCartToJoint(status, chgJoinData, newRecTbl, orgData) {
  var convJoinData = [];
  if ((chgJoinData != null) && (status == IO_SUCCESS)) {	// Data exchange is succeed.
    var pushData = createConvData(chgJoinData);
    pushData.w2ui.style = w2ui['posregarea'].get(pushData.recid).w2ui.style;
    convJoinData.push(pushData);
  } else {
    convJoinData.push(w2ui['posregarea'].get(g_select_grid_row[0] + orgData.index));
  }
  if (isSamePosition(orgData.jointData, convJoinData[0])) {
    var pasteData = [];
    pasteData[0] = orgData.cartData;
    pasteRepData(pasteData);
  } else {
    top.openCmpAlert(window, 'convcarterrDlg',resources["msg_convcarterr_c"]); 
  }
}

// Compare each axis value before conversion with each axis value after.
function isSamePosition(pos1, pos2) {
  for (var i = 1; i <= 6; i++) {
    if (pos1['j' + i].data === "") break;
    if (Math.abs((pos1['j' + i].data * 1000000) - (pos2['j' + i].data * 1000000)) / 1000000 > 0.1) {
      return false;
    }
  }
  return true;
}

// Function of after position control button process.
function setFocusAfterPosCtrl(btnId) {
  if (btnId.length > 0) {
    $(btnId).blur();
  }
  // Set focus to current selected position table.
  $("#posregarea").w2grid().focus();
}

