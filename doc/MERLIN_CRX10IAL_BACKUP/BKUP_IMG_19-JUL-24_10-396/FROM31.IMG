function initEachWindowResource() {
    for (var i = 0; i < resourceEachPosRegTitle.length; i++) {
        var rmt = resourceEachPosRegTitle[i];
        var idStr = '#' + rmt.id;
        var resourceText = resources[rmt.resId];
        if (rmt.append == 'before') { // Message append to already existed text.
            resourceText = resourceText + $(idStr).html(); //	append text
        } else if (rmt.append == 'after') {
            resourceText = $(idStr).html() + resourceText; //	append text
        }
        $(idStr).html(resourceText);
    }

    for (var i = 0; i < resourceEachPosRegButtonArea.length; i++) {
        var btn = resourceEachPosRegButtonArea[i];
        var btnStr = '#' + btn.id;
        var resourceBtnText = resources[btn.resId];
        if (btn.append == 'before') { // Message append to already existed text.
            resourceBtnText = resourceBtnText + $(btnStr).html(); //	append text
        } else if (btn.append == 'after') {
            resourceBtnText = $(BtnStr).html() + resourceBtnText; //	append text
        }
        $(btnStr).html(resourceBtnText);
    }

    var coldata = w2ui['posregarea'].columns;
    for (var i = 0; i < coldata.length; i++) {
        var rmt = resourceEachPosRegTable[i];
        coldata[i].caption = resources[rmt.resId];
    }

    if (w2ui['posregarea'].records.length > 0) {
        for (var i = 0; i < w2ui['posregarea'].records.length; i++) {
            w2ui['posregarea'].records[i].no = resources["PosRegName"] + "[" + (i + 1) + "]";
        }
    }

    w2ui['posregarea'].refresh();
