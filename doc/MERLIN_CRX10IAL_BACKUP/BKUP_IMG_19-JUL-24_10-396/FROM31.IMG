function getSpeechMsg(msgNum) {
  switch (Number(micLangElem.getValue())) {
    case VOICE_LANG.JP:
      return speechLangResource['jp'][msgNum];
    default:
      return speechLangResource['eg'][msgNum];
  }
}

/**
 @function name : window.onload
 @argument      : none
 @description   : Processing when loading HTML.
                  Upper menu icon display, program screen display
 @return        : none
*/

$(window).on('load', function (evt) {
  // close panel
  top.pnlarea.setPnlMode(top.pnlarea.PLY_PNL, false);
  top.pnlarea.setPnlMode(top.pnlarea.JOG_PNL, false);
  // close Keysheet
  if (top.is_key_open) {
    top.tpkeys.document.getElementById("tpkey_btn_parent").click();
  }
  top.pnlarea.swichActiveMode(false, top.pnlarea.PLY_PNL); //'play_btn_parent'
  top.pnlarea.swichActiveMode(false, top.pnlarea.JOG_PNL); //'jog_btn_parent'
  top.pnlarea.swichActiveMode(false, top.pnlarea.KEY_PNL); //'tpkey_btn_parent'
  try {
    var selectOption = VOICE_LANG.EG;
    if (top.irprogapi.getCurrentLanguage() == 'jp') selectOption = VOICE_LANG.JP;
    micLangElem.refresh(selectOptionList, selectOption, null, false);
    document.getElementById('slctOprtBtn').refresh(null, null, null, null, true, slctPrgOprt);
    top.irprogapi.getProgWithCmtList(getProgListCompCB);
    if (top.irprogapi.global.tabAppVersion >= top.voiceApi.APP_VER) {
      setTimeout(function() {
        android.ApiSetSpeechLang(selectOption);
        android.ApiStartVoiceRecogn();
      }, 0);
    } else {
      micTgl.refresh(false, true);
    }
  } catch (e) {}
  document.getElementById('slctOprtBtn').setCallback(slctPrgOprt);
  document.getElementById('startOprtBtn').setCallback(startPrgOprt);
  document.getElementById('stopOprtBtn').setCallback(stopPrgOprt);
  document.getElementById('abortOprtBtn').setCallback(abortPrgOprt);
  document.getElementById('resetOprtBtn').setCallback(resetPrgOprt);
  micTgl.setCallback(switchMic);
  micLangElem.setCallback(setOprtLang);
  // 4D graphic screen
  top.rpcmc_setVar(top.SYSNAME_C, '$DCS_SHOW.$DISPLAY', 0);
  var viewArea = $('<iframe>', {id:'webgl', class: 'webgl', scrolling: 'auto', name: 'webgl', src: '/frh/irprog/progoprt/progoprt_tpgl1.stm', frameborder: '0'});
  if (isIE) {
    $('#' + upperImages[0].id).prepend(viewArea[0]); // prepend is not supported in IE.
  } else { // Leave the proven code as is.
    upperImages[0].prepend(viewArea[0]); // upperImages exists in divided.js
  }
  var graphic_frm = document.getElementById("webgl").contentWindow;
  graphic_frm.onload = function () {
    $('#webgl').css({ 'height': $('#' + upperImages[0].id).outerHeight() });
    $('#webgl').css({ 'width': $('#' + upperImages[0].id).outerWidth() });
  };
});

$(window).on('resize', function (evt) {
  $('#webgl').css({ 'height': $('#' + upperImages[0].id).outerHeight() });
  $('#webgl').css({ 'width': $('#' + upperImages[0].id).outerWidth() });
});

$(window).on('unload', function (evt) {
  top.pnlarea.swichActiveMode(true, top.pnlarea.PLY_PNL); //'play_btn_parent'
  top.pnlarea.swichActiveMode(true, top.pnlarea.JOG_PNL); //'jog_btn_parent'
  top.pnlarea.swichActiveMode(true, top.pnlarea.KEY_PNL); //'tpkey_btn_parent'
  if (window.frameElement.id === "prim") {
    try {
      if (top.irprogapi.global.tabAppVersion >= top.voiceApi.APP_VER) {
        android.ApiStopVoiceRecogn();
      }
    } catch (e) {}
  }
});

var progWithCmtList = [];
function getProgListCompCB(progList) {
  progWithCmtList = progList;
  document.getElementById('slctOprtBtn').refresh(null, null, null, null, false, slctPrgOprt);
}

function slctPrgOprt() {
  // Disable button-hitting. Don't forget to deactivate 'pointer-events'.
  $('#slctOprtBtn').css('pointer-events', 'none');
  // Conditions for outputting a dialog notifying that there is no selectable program.
  if (progWithCmtList.length == 0) {
    top.openCmpPupAppendFrm(top.window, VOICE_POPUP_URL, { appendArg: 'emptyprglistdlg' }, undefined, false, null, popupRfFunc);
    top.voiceApi.reqSpeech(getSpeechMsg(MSG.SLCT_NO_PRG_POP));
  } else {
    top.rpcmc_getVar(top.SYSNAME_C, '$ALM_IF.$TPEDIT_PROG', openSelPopCmtMode, null, null);
    function openSelPopCmtMode(progName, varName, typeCode, valStr, callbackProg, callbackarg) {
      var progWithCmtMode = 1;
      top.openCmpPupAppendFrm(top.window, '/frh/jcgtp/selectprogdlg.stm', { dispProgList: progWithCmtList, valStr: valStr, mode: progWithCmtMode }, top.irpDlgHead["selectprogdlgDlg"], false, { height: '400px' }, popupRfFunc);
    }
    top.voiceApi.reqSpeech(getSpeechMsg(MSG.SLCT_POP));
  }
  function popupRfFunc() {
    $('#slctOprtBtn').css('pointer-events', '');
  }
}

function startPrgOprt(btnId) {
