// Language Resource Initialize.
function initEachWindowResource() {
  for (var i = 0; i < resourceEachRefPosTitle.length; i++) {
    var rmt = resourceEachRefPosTitle[i];
    var idStr = '#' + rmt.id;
    var resourceText = resources[rmt.resId];
    if (rmt.append == 'before') { // Message append to already existed text.
      resourceText = resourceText + $(idStr).html(); //	append text
    } else if (rmt.append == 'after') {
      resourceText = $(idStr).html() + resourceText; //	append text
    }
    $(idStr).html(resourceText);
  }

  for (var i = 0; i < refPosTableDef.headerDefine.length; i++) {
    if (i >= 6) {
      var jogId = i - 5;
      refPosTableDef.headerDefine[i].caption = resources['Jog'] + jogId;
      for (var j = 0; j < refPosTableDef.headerDefine[i].subHeader.length; j++) {
        var rmt = resourceEachRefPosColumn[j + 6];
        refPosTableDef.headerDefine[i].subHeader[j].caption = resources[rmt.resId];
      }
    } else if (i <= 5) {
      var rmt = resourceEachRefPosColumn[i];
      refPosTableDef.headerDefine[i].caption = resources[rmt.resId];
    }
  }
  for (var i = 0; i < g_ref_pos_enable.length; i++) {
    var rmt = resourceEachRefPosEnable[i];
    g_ref_pos_enable[i].value = resources[rmt.resId];
  }
  for (var i = 0; i < refPosTableDef.columnDefine.length; i++) {
    if (refPosTableDef.columnDefine[i].fieldId == 'enable'
      || refPosTableDef.columnDefine[i].fieldId == 'home') {
      refPosTableDef.columnDefine[i].fieldType.itemList = g_ref_pos_enable;
    }
  }
  debug(DBGDUMP, 'Finish windows resource.');
}
