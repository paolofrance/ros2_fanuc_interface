/*
 * PosReg Color Setting(Cartesian, Joint)
 */
function setPosRegColor(record) {
  switch (record.rep) {
    case POS_REP_NONE:
      for (var i = 0; i < g_columns_cart.length; i++) {
        record.w2ui.style[g_columns_cart[i]] = STYLE_GRID_DEFAULT;
      }
      for (var i = 0; i < g_columns_joint.length; i++) {
        record.w2ui.style[g_columns_joint[i]] = STYLE_GRID_DEFAULT;
      }
      break;
    case POS_REP_CART:
      for (var i = 0; i < g_columns_cart.length; i++) {
        record.w2ui.style[g_columns_cart[i]] = STYLE_GRID_DEFAULT;
      }
      for (var i = 0; i < g_columns_joint.length; i++) {
        record.w2ui.style[g_columns_joint[i]] = STYLE_GRID_INVALID;
      }
      break;
    case POS_REP_BOTH:
      if (g_select_mode == POS_REP_CART) {
        for (var i = 0; i < g_columns_joint.length; i++) {
          record.w2ui.style[g_columns_joint[i]] = STYLE_GRID_INVALID;
        }
        for (var i = 0; i < g_columns_cart.length; i++) {
          record.w2ui.style[g_columns_cart[i]] = STYLE_GRID_DEFAULT;
        }
      } else {
        for (var i = 0; i < g_columns_cart.length; i++) {
          record.w2ui.style[g_columns_cart[i]] = STYLE_GRID_DEFAULT;
        }
        for (var i = 0; i < g_columns_joint.length; i++) {
          record.w2ui.style[g_columns_joint[i]] = STYLE_GRID_INVALID;
        }
      }
      break;
    case POS_REP_JOINT:
      if (g_select_mode == POS_REP_JOINT) {
        for (var i = 0; i < g_columns_cart.length; i++) {
          record.w2ui.style[g_columns_cart[i]] = STYLE_GRID_INVALID;
        }
        for (var i = 0; i < g_columns_joint.length; i++) {
          record.w2ui.style[g_columns_joint[i]] = STYLE_GRID_DEFAULT;
        }
      } else {
        for (var i = 0; i < g_columns_joint.length; i++) {
          record.w2ui.style[g_columns_joint[i]] = STYLE_GRID_DEFAULT;
        }
        for (var i = 0; i < g_columns_cart.length; i++) {
          record.w2ui.style[g_columns_cart[i]] = STYLE_GRID_INVALID;
        }
      }
      break;
    default:
      for (var i = 0; i < g_columns_cart.length; i++) {
        record.w2ui.style[g_columns_cart[i]] = STYLE_GRID_INVALID;
      }
      for (var i = 0; i < g_columns_joint.length; i++) {
        record.w2ui.style[g_columns_joint[i]] = STYLE_GRID_INVALID;
      }
      break;
  }
  w2ui['posregarea'].refreshRow(record.recid);
}

/*
