    g_win_x = MIN_WINDOWWIDTH;
    $(window).width(g_win_x);
  }
  if (g_win_y < MIN_WINDOWHEIGHT) {
    g_win_y = MIN_WINDOWHEIGHT;
    $(window).width(g_win_y);
