/** SIP keyboard manager */
var SIPManager = {
  init: function() {
    if (isiPendant()) {
      var doc = document,
      div = doc.createElement("div");
      appendActiveXControl(div, "ECC1F43C-6B1E-4E74-AAED-62395D3597C1", "SIPManager", 1, 1);
