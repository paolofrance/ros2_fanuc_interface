/*
 * Position Register Comment Callback
 */
function getPosRegCommentCallback(data_arr) {
  g_get_comment_flag = false;
  if ((getXmlErrorData().flg) && (getXmlErrorData().init)) {
    setXmlErrorData();
  }
  if (data_arr == null
    || data_arr.length == 0
    || g_no_send_flag) {
    return;
  }
  for (key in data_arr) {
    var id = key.toString().slice(2);
    var idNum = Number(id);
    var recid = w2ui['posregarea'].find({ id: id });
    var record = w2ui['posregarea'].records[recid];
    var data = data_arr[key];

    if (idNum < g_current_item_no_start
      || idNum > g_current_item_no_end) {
      return;
    }
    record.comment = data.comment;
    setPosRegColor(record);
  }
  setFileName("posregvalue");
  getPosRegValue(g_select_group, g_current_item_no_start, g_current_item_no_end - g_current_item_no_start + 1, getPosRegValueCallback);
}

/*
 * Position Register Value Callback
 */
function getPosRegValueCallback(data_arr) {
  if ((getXmlErrorData().flg) && (getXmlErrorData().init)) {
    setXmlErrorData();
  }
  if (data_arr == null
    || data_arr.length == 0
    || g_no_send_flag) {
    return;
  }
  var label = Object.keys(data_arr)[0].toString();
  var grp = Number(label.slice(0, 1));
  if (grp != g_select_group) {
    return;
  }
  var chg_data = w2ui['posregarea'].getChanges();
  if (chg_data.length != 0) {
    w2ui['posregarea'].save();
  }
  for (key in data_arr) {
    var id = key.toString().slice(2);
    var recid = w2ui['posregarea'].find({ id: id });
    var record = w2ui['posregarea'].records[recid];
    var data = data_arr[key];

    if (id < g_current_item_no_start
      || id > g_current_item_no_end) {
      return;
    }
    if (data.rep) {
      switch (Number(data.rep)) {
        case POS_REP_CART:
        case POS_REP_BOTH:
          record.rep = Number(data.rep);
          record.config = data.Config;
          record.x = Number(data.X);
          record.y = Number(data.Y);
          record.z = Number(data.Z);
          record.w = Number(data.W);
          record.p = Number(data.P);
          record.r = Number(data.R);
          record.ut = POS_REG_UT_VAL;
          record.uf = POS_REG_UF_VAL;
          record.axes = data.axes;
          for (var i = POS_REG_CLM_ARR.indexOf('ext1'); i <= POS_REG_CLM_ARR.indexOf('ext3'); i++) {
            var up_str = POS_REG_CLM_ARR[i].toUpperCase();
            record[POS_REG_CLM_ARR[i]] = {};
            if (data[up_str]) {
              record[POS_REG_CLM_ARR[i]].data = Number(data[up_str].data);
              record[POS_REG_CLM_ARR[i]].unit = data[up_str].unit;
            } else {
              record[POS_REG_CLM_ARR[i]].data = "";
              record[POS_REG_CLM_ARR[i]].unit = "";
            }
          }
          break;
        case POS_REP_JOINT:
          record.rep = POS_REP_JOINT;
          record.config = data.Config;
          for (var i = POS_REG_CLM_ARR.indexOf('j1'); i <= POS_REG_CLM_ARR.indexOf('ext3'); i++) {
            record[POS_REG_CLM_ARR[i]] = {};
            var up_str = POS_REG_CLM_ARR[i].toUpperCase();
            if (data[up_str]) {
              record[POS_REG_CLM_ARR[i]].data = Number(data[up_str].data);
              record[POS_REG_CLM_ARR[i]].unit = data[up_str].unit;
            } else {
              record[POS_REG_CLM_ARR[i]].data = "";
              record[POS_REG_CLM_ARR[i]].unit = "";
            }
          }
          record.ut = POS_REG_UT_VAL;
          record.uf = POS_REG_UF_VAL;
          record.axes = data.axes;
          break;
      }
    } else {
      record.rep = POS_REP_NONE;
      record.config = "";
      record.axes = "";
      for (var i = POS_REG_CLM_ARR.indexOf('x'); i <= POS_REG_CLM_ARR.indexOf('ut'); i++) {
        if (i >= POS_REG_CLM_ARR.indexOf('j1') && i <= POS_REG_CLM_ARR.indexOf('ext3')) {
          record[POS_REG_CLM_ARR[i]] = {};
          record[POS_REG_CLM_ARR[i]].data = "";
          record[POS_REG_CLM_ARR[i]].unit = "";
        } else {
          record[POS_REG_CLM_ARR[i]] = "";
        }
      }
    }
    setPosRegColor(record);
  }
  clearTimeout(g_timer);
  g_timer = setTimeout("onTimer()", POSREG_INTERVAL_TIME);
}

