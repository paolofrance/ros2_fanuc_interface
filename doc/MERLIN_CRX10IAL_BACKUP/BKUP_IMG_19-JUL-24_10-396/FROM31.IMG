var resources = [];

function initResource() {
  var lang = getCurrentLanguage();
  $.getJSON('lang/reg_' + lang + '.jsn')
    .done(function (data) {
      resources = data;
      initEachWindowResource();
      initProc();
      debug(DBGINFO, 'Finish language resource.');
    })
    .fail(function () {
      if (lang != LANG_EG) {
        currentLang = LANG_EG;
        initResource();
        console.log("change Language :" + lang + " > " + currentLang);
      }
    });
}

