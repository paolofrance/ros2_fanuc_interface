    initSelParts(bPart);
  }
}

function initSelParts(value) {
  var selPartsSpd = [];
  var i = 0;
  for (key in BODY_PARTS) {
    var elemId = key.toLowerCase();
    if ((value & BODY_PARTS[key]) > 0) {
      selPartsSpd.push(partsMaxSpd[i]);
      $("#" + elemId).removeClass("unselectparts");
      $("#" + elemId).addClass("selectparts");
      $("#" + elemId + "_txt").addClass("selectpartstext");
    } else {
      $("#" + elemId).addClass("unselectparts");
      $("#" + elemId).removeClass("selectparts");
    }
    i++;
  }
  if (oldSelBodyVal == null) {
    oldSelBodyVal = value;
  }
  currentSelBodyVal = value;
  calcMaxSpd(selPartsSpd);
}

function selBodyPart(e) {
  var dangerParts = ["head_face", "neck", "leg"];
  var selBodyVal = 0;
  var bodyPart = e.target.id;
  if (bodyPart == "") {
    bodyPart = e.currentTarget.id;
  }
  // Convert 'div' id to 'path' id
  if (bodyPart.indexOf('Hand') != -1) {
    bodyPart = 'hand';
  }
  // caution dialog
  if (dangerParts.indexOf(bodyPart) != -1) {
    top.openCmpConfirm(webPage, window, 'spdlmtDlg', spdlmtRes['nonoptional'], { btnData :{btn1:top.irpDlgBtnOC.btn1}}, true);
    return
  }
  if ($("#" + bodyPart).hasClass("selectparts")) {
    $("#" + bodyPart).removeClass("selectparts");
    $("#" + bodyPart).addClass("unselectparts");
    $("#" + bodyPart + "_txt").removeClass("selectpartstext");
  } else {
    $("#" + bodyPart).removeClass("unselectparts");
    $("#" + bodyPart).addClass("selectparts");
    $("#" + bodyPart + "_txt").addClass("selectpartstext");
  }
  var selPartsSpd = [];
  var i = 0;
  for (key in BODY_PARTS) {
    var elemId = key.toLowerCase();
    if ($("#" + elemId).hasClass("selectparts")) {
      selPartsSpd.push(partsMaxSpd[i]);
      selBodyVal += BODY_PARTS[key];
    }
    i++;
  }
  currentSelBodyVal = selBodyVal;
  calcMaxSpd(selPartsSpd);
}
// (IN) param : 0 - 127
function calcMaxSpd(param) {
  var calcSpd;
  if (param.length == 0) {
    calcSpd = partsMaxSpd[0];
  } else {
    calcSpd = Math.min.apply(null, param);
  }
  $("#spdNum").html(calcSpd);
  if (oldSpd == null) {
    oldSpd = calcSpd;
  }
  currentSpd = calcSpd;
}

function applySpd() {
  $('#spdApply').css('pointer-events', 'none');
  setTimeout(function () {
    $('#spdApply').css('pointer-events', '');
  }, 1000);
  top.setVarWithCallback(top.SYSNAME_C, '$TABUI_SCRN.$CLLB_BPARTS', currentSelBodyVal, setSysBpartsCB);
}

function setSysBpartsCB(status, prog_name, var_name, cbArg1, cbArg2){
  top.setVarWithCallback(top.SYSNAME_C, '$DCSS_CLLB[1].$SPD_LIM', currentSpd, setSysSpdLimCB, status);
}

function setSysSpdLimCB(status, prog_name, var_name, cbArg1, cbArg2){
  top.setVarWithCallback(top.SYSNAME_C, '$CF_LIM_GRP[1].$LIM_PARAM[13]', currentSpd, setSysLimParamCB, status & cbArg1);
}

function setSysLimParamCB(status, prog_name, var_name, cbArg1, cbArg2){
  top.setVarWithCallback(top.SYSNAME_C, '$CF_LIM_GRP[1].$SPEEDLIM', currentSpd, setSysCfSpeedLimCB, status & cbArg1);
}

function setSysCfSpeedLimCB(status, prog_name, var_name, cbArg1, cbArg2){
  top.setVarWithCallback(top.SYSNAME_C, '$CP_LIM_GRP[1].$SPEEDLIM', currentSpd, setSysCpSpeedLimCB, status & cbArg1);
}

function setSysCpSpeedLimCB(status, prog_name, var_name, cbArg1, cbArg2) {
  if(status & cbArg1){
    if(oldSpd != currentSpd){
      top.irprogapi.applyConfirmDCS(null, applyDCSCB);
    } else {
      oldSelBodyVal = currentSelBodyVal;
    }
  }
}

function applyDCSCB(status, procCode, cbArgs){
  if (procCode == 0 || procCode == 101){
    oldSpd = currentSpd;
    oldSelBodyVal = currentSelBodyVal;
  }
