 */
function setButtonEvent() {
  function handleRecPos () {
    if (isUneditable()) {
      top.IHMIComponents.cf.toast.show(resources['ERREditPosregTpOff']);
    } else if ((g_select_grid_row == null) || (g_select_grid_row.length > 1)) {
      top.openCmpAlert(window, 'noSelect1Dlg', resources["dlgMsgNoSelect1"]);
    } else {
      top.openCmpConfirm(webPage, window, 'recPosDlg', resources["dlgMsgRecPos"], {btnData : top.irpDlgBtnYN, btn1Cb : recordPosition});
    }
  }
  $('#edit_btn').on('mouseup touchend', function (event) {
  	if(event.type.indexOf('touch') >= 0){
      event.preventDefault();
    }
    var pupLeng = $('#regselposedit').length;
    $('input').attr('readonly', 'readonly');
    // setFocusAfterPosCtrl('');	// poswin
    turnoffPopup();
    if (pupLeng == 0) {	// If it does not already exist, display popup.
      popupDialog.selection('regselposedit');
    }
    // Prevent keyboard appearance
    setTimeout(function () {
      $('input').removeAttr('readonly');
    }, 50);
    return false;
  });
  $("#cartesian_btn").on('click touchend', function (event) {
    if(event.type.indexOf('touch') >= 0){
      event.preventDefault();
    }
    $('#cartesian_btn').css('background-color', '#0e6eb8');
    $('#joint_btn').css('background-color', '#696969');
    if (g_select_mode != POS_REP_CART) {
      g_select_mode = POS_REP_CART;
      if (g_no_send_flag == true && g_keep_data_flag == true) {
        g_no_send_flag = false;
        setAllPosReg();
      }
      setGridColumn();
      initPosRegData();
    }
  });
  $("#joint_btn").on('click touchend', function (event) {
    if(event.type.indexOf('touch') >= 0){
      event.preventDefault();
    }
    $('#cartesian_btn').css('background-color', '#696969');
    $('#joint_btn').css('background-color', '#0e6eb8');
    if (g_select_mode != POS_REP_JOINT) {
      g_select_mode = POS_REP_JOINT;
      if (g_no_send_flag == true && g_keep_data_flag == true) {
        g_no_send_flag = false;
        setAllPosReg();
      }
      setGridColumn();
      initPosRegData();
    }
  });

  // If there is any change data, send it all before switching.
  $("#page_prevbtn").on('click touchend', function (event) {
    if(event.type.indexOf('touch') >= 0){
      event.preventDefault();
    }
    if (g_keep_data_flag) {
      setAllPosReg();
    }
    if (g_current_page_no > 1) {
      var pageNum = Math.ceil(g_current_item_no_start / GRID_ROW_MAX_COUNT) - 1;
      setPosRegSelData(pageNum);
    }
  });
  $("#page_nextbtn").on('click touchend', function (event) {
    if(event.type.indexOf('touch') >= 0){
      event.preventDefault();
    }
    if (g_keep_data_flag) {
      setAllPosReg();
    }
    if (g_current_page_no < g_current_page_max) {
      var pageNum = Math.ceil(g_current_item_no_start / GRID_ROW_MAX_COUNT) + 1;
      setPosRegSelData(pageNum);
    }
  });
  $("#page_num").on('click', function (event) {
    var target = $(event.target);
    if (g_keep_data_flag && target.hasClass('page_sel')) {
      setAllPosReg();
    }
    var sel_menu = $('.page_frame');
    if (sel_menu.length) {
      if (target.hasClass('page_sel')) {
        var pageNum = Number(event.target.outerText);
        setPosRegSelData(pageNum);
      }
    }
  });

  document.getElementById("recpos_btn").setCallback(handleRecPos);

  //copy event check
  w2ui['posregarea'].on('copy', function (event) {
    debug(DBGDUMP, 'copy data is \n' + event.text);
    copyEventText.text = event.text;
    copyEventText.type = g_select_mode;
    copySelType = crntSelType; // memory of copy mode
    if (copyEventText.type == POS_REP_CART && w2ui['posregarea'].getColumn('ext1').hidden == false) {
      copyEventText.type = POS_REP_BOTH;
    }
    copyEventText.key = '';
  });

  //paste event check
  w2ui['posregarea'].on('paste', function (event) {
    if (isUneditable()) {
      top.IHMIComponents.cf.toast.show(resources['ERREditPosregTpOff']);
      event.preventDefault();
      return;
    }
    if(copySelType != crntSelType) {
      event.preventDefault();
      return;
    }
    if (copySelType == SELGRID_ROW) {
      if (typeof copyButtonText !== 'undefined') {
        copyRowsData(copyButtonText);
      }
      event.preventDefault();
      return false;
    }
    if (typeof copyEventText.text === 'undefined') {
      return;
    }
    clearGTimer();
    multiRowFlag = true;

    var pasteRowPos = event.index;
    var pastecolumnpos = event.column;
    var copydata = copyEventText.text.split(CODELF);
    var lastPasteRow = -1;
    var noSendFlag = true;
    var pageDataMode = g_select_mode;

    if ((pageDataMode == POS_REP_CART) && !(w2ui['posregarea'].getColumn('ext1').hidden)) {
      pageDataMode = POS_REP_BOTH; // if data mode is BOTH(6), change pageDataMode from cart to both
    }
    if (copySelType != crntSelType || copyEventText.type !== pageDataMode) {
      event.preventDefault();
      return false;
    }
    for (var x = 0; x < countPasteRow(copydata); x++) {
      var checkRecordData = w2ui['posregarea'].get(pasteRowPos + x);
      if (checkRecordData.rep == pageDataMode) {
          lastPasteRow = x;  // record the last row go through recPosReg
      }
    }
    for (var i = 0; i < countPasteRow(copydata); i++) {
      var targetIndex = pastecolumnpos - 1;
      var cellData = copydata[i].split(CODETAB);
      var recordData = w2ui['posregarea'].get(pasteRowPos + i);
      var newRec = createrow();
      newRec = $.extend({}, recordData);
      if (newRec.rep != POS_REP_NONE) {
        if ((copyEventText.type !== pageDataMode) || (newRec.rep != pageDataMode)) {
          if (event.column != GRID_FIELD_COMMENT) {
            continue; // if try to paste for other than comments cell and other mode row, skip it
          }
        }
      }
      noSendFlag = false;
      for (var j = 0; j < cellData.length; j++) {
        targetIndex++;
        if (targetIndex > GRID_FIELD_UT) {
          continue;
        }
        for (; w2ui['posregarea'].getColumn(POS_REG_CLM_ARR[targetIndex]).hidden;) {
          // If the cell it is trying to select is hidden, select next cell.
          targetIndex += 1;
        }
        if ((copyEventText.type != recordData.rep) && (captionTable[targetIndex] != POS_REG_CLM_ARR[GRID_FIELD_COMMENT]) &&
        (recordData.rep != POS_REP_NONE)) {
          // if try to paste for other than comments cell and other mode row, skip it
        } else {
          if (targetIndex == GRID_FIELD_COMMENT) {
            newRec[captionTable[targetIndex]] = cellData[j];
          }
          else if (cellData[j] !== "" && targetIndex <= GRID_FIELD_EXT3) {
            if (targetIndex >= GRID_FIELD_J1 && targetIndex <= GRID_FIELD_EXT3) {
              newRec[captionTable[targetIndex]].data = cellData[j];
            } else {
              newRec[captionTable[targetIndex]] = cellData[j];
            }
          }
        }
      }
      if (recordData.rep != newRec.rep) {
        newRec.force = true;
      }
      if (countPasteRow(copydata) == i + 1 || i == lastPasteRow) {
        multiRowFlag = false; // if this row is the last go through recPosReg, make it pass through setPosRegValue
      }
      var recData = { clm: event.column, num: cellData.length };
      recPosReg(newRec, recData, false);
      w2ui['posregarea'].set(newRec.recid, newRec);
      w2ui['posregarea'].save();
    }
    event.isCancelled = true;
    if (noSendFlag && g_keep_data_flag == false) {  // if do not go through recPosReg, set timeout
      multiRowFlag = false;
      clearGTimer();
      g_timer = setTimeout("onTimer()", POSREG_INTERVAL_TIME);
    }
  });
}

/*
 * Tab Event Setting
 */
function setTabEvent() {
  var argObj = {showTabObj : {'g1' : true, 'g2' : false, 'g3' : false, 'g4' : false, 'g5' : false, 'g6' : false, 'g7' : false, 'g8' : false}, width : null};
  for (var i = 1; i <= g_group_max; i++) {
    argObj.showTabObj['g' + String(i)] = true;
  }

  // init set tab
  var selTabData = currentTabPage >>> PAGE_BIT_NUM;
  if (selTabData <= g_group_max && selTabData > 0) {
    g_select_tab = 'g' + selTabData;
  } else {
    g_select_tab = 'g1';
  }
  g_select_group = parseInt(g_select_tab.substr(1));
  document.getElementById('tabarea').refresh(null, g_group_max, g_select_tab, null, argObj);

  // tab click event
  document.getElementById('tabarea').setCallback(tabClick);
  function tabClick(id, operation, value) {
    if (g_select_tab != value) {
      // If there is any change data, send it all before switching.
      if (g_keep_data_flag) {
        setAllPosReg();
      }
      g_select_tab = value;
      g_select_group = parseInt(value.substr(1));

      w2ui['posregarea'].clear();
      initPageArea(Number(getRegMaxNum(REG_TYPE_POS)));
      setGridColumn();
      initPosRegData();
      top.rpcmc_getVar(top.SYSNAME_C, '$SCR_GRP[' + g_select_group + '].$ARM_TYPE', getPosRegArmtypeCallback);
      top.irprogapi.getXVRPosAxisUnit(g_select_group, getAxisUnitCallback);
      existPosCopyRec = false; // hide paste menu
      var tabNum = g_select_tab.substring(1);
      currentTabPage = 1 + (tabNum << PAGE_BIT_NUM);
      sessionStorage.setItem('posRegSelData', currentTabPage);
    }
  }
}

/*
 * Grid Event Setting
 */
function setGridEvent() {
  if ((w2utils.isIOS) || (w2utils.isAndroid)) {
    w2ui['posregarea'].on('blur', function (event) {
      // To take the focus off the grid.
      $("#page_num").focus();
      $("#page_num").blur();
    });
    w2ui['posregarea'].on('focus', function (event) {
      // To prevent the display of the software keyboard.
      event.preventDefault();
      $("#posregarea").w2grid().blur();
    });
  }
  // click event
  w2ui['posregarea'].on('click', function (event) {
    var grid = this;
    event.onComplete = function () {
      if (event.originalEvent.type == 'keydown') {
        return;
      }
      if (event.column >= GRID_FIELD_COMMENT
        && event.column <= GRID_FIELD_EXT3) {
        w2ui['posregarea'].editField(event.recid, event.column);
      }
    }
  });
  // Double Click Event
  w2ui['posregarea'].on('dblClick', function (event) {
    event.preventDefault();
  });
  // change event
  w2ui['posregarea'].on('change', function (event) {
    var grid = this;
    var recid = Number(event.recid);
    var record = w2ui['posregarea'].records[recid];
    var colid = Number(event.column);
    var recData = { clm: event.column, num: 1 };
    
    if (((entrykeysCart.indexOf(captionTable[colid]) >= 0)
      || (entrykeysJoint.indexOf(captionTable[colid]) >= 0)
      || (entrykeysExt.indexOf(captionTable[colid]) >= 0))
      && (event.value_new !== "")) {
      event.value_new = Number(event.value_new.toFixed(3));
    }

    var next_val = event.value_new;
    g_select_grid_row = grid.getSelection();
    if (event.value_original == next_val) {
      return;
    }
    switch (event.column) {
      case GRID_FIELD_COMMENT:
        if (checkLangAndCharBytes(next_val)) {
          event.value_new = event.value_previous;
          record[POS_REG_CLM_ARR[event.column]] = event.value_previous;
          top.openCmpAlert(window, 'err2ByteCharsDlg', resources['ERR2ByteChars']);
          break;
        }
        if (countChar(next_val) > POS_REG_COMMENT_MAX) {
          event.value_new = multByteStringSlice(next_val, POS_REG_COMMENT_MAX);
          event.value_previous = multByteStringSlice(next_val, POS_REG_COMMENT_MAX);
          record[POS_REG_CLM_ARR[GRID_FIELD_COMMENT]] = event.value_new;
          w2ui['posregarea'].refreshCell(recid, POS_REG_CLM_ARR[GRID_FIELD_COMMENT]);
          top.openCmpAlert(window, 'errTooManyCharsDlg', resources['ERRTooManyChars']);
        } else {
          record[POS_REG_CLM_ARR[GRID_FIELD_COMMENT]] = event.value_new;
        }
        if (next_val.match(/[&']/)){
          event.value_new = event.value_previous;
          record[POS_REG_CLM_ARR[event.column]] = event.value_previous;
          top.openCmpAlert(window, 'msgInvalidCharsDlg', resources['msgInvalidChars']);
          break;
        }
        if (event.value_new === "") {
          record.force = true;
        }
        recPosReg(record, recData, false);
        break;
      case GRID_FIELD_CONFIG:
        var configData = confirmConfigAxisNum(event.value_new);
        if (configData == false) { break; }
        record[POS_REG_CLM_ARR[GRID_FIELD_CONFIG]] = configData;
        recPosReg(record, recData, false);
        break;
      case GRID_FIELD_X:
      case GRID_FIELD_Y:
      case GRID_FIELD_Z:
      case GRID_FIELD_W:
      case GRID_FIELD_P:
      case GRID_FIELD_R:
        if (next_val !== "") {
          record[POS_REG_CLM_ARR[event.column]] = event.value_new;
          recPosReg(record, recData, false);
        }
        break;
      case GRID_FIELD_J1:
      case GRID_FIELD_J2:
      case GRID_FIELD_J3:
      case GRID_FIELD_J4:
      case GRID_FIELD_J5:
      case GRID_FIELD_J6:
      case GRID_FIELD_EXT1:
      case GRID_FIELD_EXT2:
      case GRID_FIELD_EXT3:
        if (next_val !== "") {
          event.value_new = {};
          event.value_new.data = next_val;
          event.value_new.unit = g_axis_unit[event.column - GRID_FIELD_J1];
          record[POS_REG_CLM_ARR[event.column]] = event.value_new;
          recPosReg(record, recData, false);
        }
        break;
    }
    event.preventDefault();
  });
  // unselect event
  w2ui['posregarea'].on('unselect', function (event) {
    event.onComplete = function () {
      g_select_grid_row = this.getSelection();
    }
  });
  // select event
  w2ui['posregarea'].on('select', function (event) {

    // paint select cell or column
    var grid = w2ui['posregarea'];
    crntSelType = grid.selectType;
    posSaveRec = w2ui['posregarea'].get(event.recid);

    if (crntSelType == SELGRID_CELL) {
      if (event.column == 0) {	// Click Number column. Add check undefined
        grid.selectNone();
        grid.selectType = SELGRID_ROW;	// to row select
        crntSelType = grid.selectType;	//memory of current mode
        grid.refresh();
      }
    }
    else if (crntSelType == SELGRID_ROW) {
      if (!(event.column == 0) && !(event.column == undefined)) {	// valid column
        grid.selectNone();
        grid.selectType = SELGRID_CELL;	// to cell select
        crntSelType = grid.selectType;	//memory of current mode
        grid.refresh();
      }
    }
    event.onComplete = function () {
      g_select_grid_row = grid.getSelection();
    }

  });

  var backUpPosRegData = null;
  // cell input control
  w2ui['posregarea'].on('editField', function (event) {
    if (isUneditable()) {
      top.IHMIComponents.cf.toast.show(resources['ERREditPosregTpOff']);
      event.preventDefault();
      return;
    }
    var recid = Number(event.recid);
    var record = w2ui['posregarea'].records[recid];
    var colid = Number(event.column);
    if (record.w2ui.style[colid] == STYLE_GRID_INVALID) {
      event.preventDefault();
    }
    if (entrykeysCart.indexOf(captionTable[colid]) >= 0
      || entrykeysJoint.indexOf(captionTable[colid]) >= 0
      || entrykeysExt.indexOf(captionTable[colid]) >= 0) {
      backUpPosRegData = record[captionTable[colid]];
      if (backUpPosRegData.data !== "" && g_grid_joint.indexOf(captionTable[colid]) >= 0) {
        record[captionTable[colid]] = Number(backUpPosRegData.data.toFixed(3));
      }
      else if (backUpPosRegData !== "" && entrykeysCart.indexOf(captionTable[colid]) >= 0) {
        record[captionTable[colid]] = Number(backUpPosRegData.toFixed(3));
      }
    }
  });

  w2ui['posregarea'].on('restore', function (event) {
    var recid = Number(event.recid);
    var record = w2ui['posregarea'].records[recid];
    var colid = Number(event.column);
    if (entrykeysCart.indexOf(captionTable[colid]) >= 0
      || entrykeysJoint.indexOf(captionTable[colid]) >= 0
      || entrykeysExt.indexOf(captionTable[colid]) >= 0) {
      record[captionTable[colid]] = backUpPosRegData;
      backUpPosRegData = null;
    }
  });
  // keydown event
  w2ui['posregarea'].on('keydown', function (event) {
    var kdnKeyCode = event.originalEvent.keyCode;
    var curSelGridRow = $.extend({}, g_select_grid_row);
    if ((kdnKeyCode == KEYCODE_RTARW) && (crntSelType == SELGRID_ROW)) {
      w2ui['posregarea'].selectNone(); // If selected number cell, select comment cell (and change crntSelType).
      w2ui['posregarea'].select({ recid: curSelGridRow[0], column: GRID_FIELD_COMMENT });
      event.preventDefault();
      event.stopPropagation();
    }
    else if ((event.originalEvent.ctrlKey) && (kdnKeyCode == KEYCODE_X)) { // if input key code of cut process
      if (isUneditable()) {
        top.IHMIComponents.cf.toast.show(resources['ERREditPosregTpOff']);
      } else {
        // copy process
        selectRegCopyMenu();
        if (selectOnlyCmt()) {
          deleteComment();
        } else {
          selectRegDeleteMenu();
        }
      }
      event.preventDefault();
      event.stopPropagation();
    }
    else if ((event.originalEvent.ctrlKey) && (kdnKeyCode == KEYCODE_C)) { // if input key code of copy process
      selectRegCopyMenu();
    }
    else if ((kdnKeyCode == KEYCODE_DEL)) { // input delete key
      if (isUneditable()) {
        top.IHMIComponents.cf.toast.show(resources['ERREditPosregTpOff']);
      } else if (selectOnlyCmt()) {
        deleteComment();
      } else {
        selectRegDeleteMenu();
      }
      event.preventDefault();
      event.stopPropagation();
    }
    else if (kdnKeyCode == KEYCODE_TAB) {
      $("#posregarea").w2grid().focus();
      if ((crntSelType == SELGRID_ROW) && !(event.originalEvent.shiftKey)) {
        w2ui['posregarea'].selectNone(); // If selected number cell, select comment cell (and change crntSelType).
        w2ui['posregarea'].select({ recid: curSelGridRow[0], column: GRID_FIELD_COMMENT });
      } else if (crntSelType == SELGRID_CELL) {
        var selGrid = w2ui['posregarea'].getSelection();
        var nextCol = ((event.originalEvent.shiftKey) ? selGrid[0].column - 1 : selGrid[0].column + 1);
        if ((nextCol <= GRID_FIELD_UT) && (nextCol >= GRID_FIELD_INDEX)) {
          for (; w2ui['posregarea'].getColumn(POS_REG_CLM_ARR[nextCol]).hidden;) {
            // If the cell it is trying to select is hidden, select next cell.
            nextCol = ((event.originalEvent.shiftKey) ? nextCol - 1 : nextCol + 1);
          }
          w2ui['posregarea'].selectNone();
          w2ui['posregarea'].select({ recid: curSelGridRow[0].recid, column: nextCol });
          w2ui['posregarea'].scrollIntoView();
        }
      }
      event.preventDefault();
      event.stopPropagation();
    }
  });
}

/*
 * Grid Column Setting
 */
function setGridColumn() {
  w2ui['posregarea'].hideColumn('config', 'x', 'y', 'z', 'w', 'p', 'r', 'j1', 'j2', 'j3', 'j4', 'j5', 'j6', 'ext1', 'ext2', 'ext3');
  if (g_select_mode == POS_REP_CART) {
      w2ui['posregarea'].showColumn('config', 'x', 'y', 'z', 'w', 'p', 'r');
  } else {
    switch (g_axes_count[g_select_group - 1]) {
      case 1:
        w2ui['posregarea'].showColumn('j1');
        break;
      case 2:
        w2ui['posregarea'].showColumn('j1', 'j2');
        break;
      case 3:
        w2ui['posregarea'].showColumn('j1', 'j2', 'j3');
        break;
      case 4:
        w2ui['posregarea'].showColumn('j1', 'j2', 'j3', 'j4');
        break;
      case 5:
        w2ui['posregarea'].showColumn('j1', 'j2', 'j3', 'j4', 'j5');
        break;
      default:
        w2ui['posregarea'].showColumn('j1', 'j2', 'j3', 'j4', 'j5', 'j6');
        break;
    }
  }
  switch (g_ext_count[g_select_group - 1]) {
    case 1:
      w2ui['posregarea'].showColumn('ext1');
      break;
    case 2:
      w2ui['posregarea'].showColumn('ext1', 'ext2');
      break;
    case 3:
      w2ui['posregarea'].showColumn('ext1', 'ext2', 'ext3');
      break;
    default:
      break;
  }
  w2ui['posregarea'].refresh();
}

/*
 * Initialize Position Register Data
 */
function initPosRegData() {
  clearGTimer();
  var linebuff = [];
  w2ui['posregarea'].clear();
  for (var i = g_current_item_no_start; i <= g_current_item_no_end; i++) {
    var newline = createrow();
    newline.recid = linebuff.length;
    newline.label = resources["PosRegName"] + "[" + i + "]";
    newline.id = i;
    newline.rep = POS_REP_NONE;
    linebuff.push(newline);
  }
  if (linebuff.length != 0) {
    w2ui['posregarea'].add(linebuff);
  }
  w2ui['posregarea'].refresh();
  if (g_select_grid_row != null) {
    w2ui['posregarea'].select(g_select_grid_row[0]);
  }
  g_get_comment_flag = true;
  if (g_no_send_flag) {
    // Since continuous updating is in progress, do not acquire data.
  } else {
    setFileName("posregcomment");
    getPosRegValue(1, g_current_item_no_start, g_current_item_no_end - g_current_item_no_start + 1, getPosRegCommentCallback);
  }
  // Function key and TP key had be hidden, readjust the screen size.
  $(window).trigger('resize');
}

