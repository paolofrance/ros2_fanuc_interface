/*
 * Record Position Register Value
 */
var cmtUpdateFlg = {};
function recPosReg(record, recData, delete_flag) {
  clearGTimer();
  var write_data = [];
  switch (record.rep) {
    case POS_REP_CART:
    case POS_REP_BOTH:
    case POS_REP_JOINT:
      break;
    case POS_REP_NONE:
      if (((recData.clm == GRID_FIELD_COMMENT) && (recData.num == 1))
        || (delete_flag)) {
      } else {
        switch (g_select_mode) {
          case POS_REP_CART:
            if (w2ui['posregarea'].getColumn('ext1').hidden) {
              record.rep = POS_REP_CART;
            } else {
              record.rep = POS_REP_BOTH;
            }
            record.axes = g_ext_count[g_select_group - 1];
            record.ut = POS_REG_UT_VAL;
            record.uf = POS_REG_UF_VAL;
            // When config and other than comments are entered.
            for (var i = POS_REG_CLM_ARR.indexOf('x'); i <= POS_REG_CLM_ARR.indexOf('r'); i++) {
              if (record[POS_REG_CLM_ARR[i]] === "") {
                record[POS_REG_CLM_ARR[i]] = 0;
              }
            }
            for (var i = POS_REG_CLM_ARR.indexOf('ext1'); i <= POS_REG_CLM_ARR.indexOf('ext3'); i++) {
              if (w2ui['posregarea'].getColumn(POS_REG_CLM_ARR[i]).hidden) {
              } else {
                if (record[POS_REG_CLM_ARR[i]].data === "") {
                  record[POS_REG_CLM_ARR[i]].data = 0;
                  record[POS_REG_CLM_ARR[i]].unit = g_axis_unit[i - POS_REG_CLM_ARR.indexOf('j1')];
                }
              }
            }
            // If anything other than config is entered in the initial data, we get the config of the current position.
            if (recData.clm > GRID_FIELD_CONFIG) {
              getCurrentPosition(g_select_group, getCurrentConfigCallback, record.id);
              return;
              // When config is entered.
            } else {
              // No action.
            }
            break;
          case POS_REP_JOINT:
            record.force = true;
            record.rep = POS_REP_JOINT;
            record.axes = g_axes_count[g_select_group - 1] + g_ext_count[g_select_group - 1];
            record.ut = POS_REG_UT_VAL;
            record.uf = POS_REG_UF_VAL;
            for (var i = POS_REG_CLM_ARR.indexOf('j1'); i <= POS_REG_CLM_ARR.indexOf('ext3'); i++) {
              if (w2ui['posregarea'].getColumn(POS_REG_CLM_ARR[i]).hidden) {
              } else {
                if (record[POS_REG_CLM_ARR[i]].data === "") {
                  record[POS_REG_CLM_ARR[i]].data = 0;
                  record[POS_REG_CLM_ARR[i]].unit = g_axis_unit[i - POS_REG_CLM_ARR.indexOf('j1')];
                } else if (record[POS_REG_CLM_ARR[i]].unit == "") {
                  record[POS_REG_CLM_ARR[i]].unit = g_axis_unit[i - POS_REG_CLM_ARR.indexOf('j1')];
                }
              }
            }
            break;
        }
      }
      break;
  }
  setPosRegColor(record);
  if (recData.clm == GRID_FIELD_COMMENT) {
    cmtUpdateFlg[record.id] = true;
  } else if (cmtUpdateFlg[record.id] != true) {
    cmtUpdateFlg[record.id] = false;
  }
  if (g_no_send_flag) { // If continuous input is in progress.
    g_keep_data_flag = true;
    clearTimeout(g_timer_send_keep_data);
    g_timer_send_keep_data = null;
    g_timer_send_keep_data = setTimeout("onTimerAfterSend()", POSREG_INTERVAL_TIME);

  } else {// If it was a single input.
    write_data.push(createWriteData(record));
    if (multiRowFlag == false) {
      setPosRegValue(g_select_group, write_data, setPosRegCallback);
      cmtUpdateFlg = {};
    }
    g_keep_data_flag = false;
    g_no_send_flag = true;
    g_timer_send_keep_data = setTimeout("onTimerAfterSend()", POSREG_INTERVAL_TIME);
    record.force = false;
  }
}

/*
