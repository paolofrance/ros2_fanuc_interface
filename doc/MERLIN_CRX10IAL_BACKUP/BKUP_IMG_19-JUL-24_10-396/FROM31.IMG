  var padding_h = parseInt($('#posregarea').css('padding-top'), 10) + parseInt($('#posregarea').css('padding-bottom'), 10);
  $('#posregarea').height(init_layout_h - padding_h - DEFAULTBODYMARGIN - GRIDBTMMARGIN);
  if (getCurrentLanguage() === null) {
    setCurrentLanguage(initLayoutAreaCallBack);
  }
});

/*
 * Windows Resize Event
 */
$(window).on('resize', function (evt) {
  g_win_x = window.innerWidth;
  g_win_y = window.innerHeight;
  if (g_win_x < MIN_WINDOWWIDTH) {
    g_win_x = MIN_WINDOWWIDTH;
    $(window).width(g_win_x);
  }
  if (g_win_y < MIN_WINDOWHEIGHT) {
    g_win_y = MIN_WINDOWHEIGHT;
    $(window).width(g_win_y);
  }
  $(document).width(g_win_x);
  $(document).height(g_win_y);
  document.getElementById('tabarea').equalizeTabs();
  var resize_layout_h = g_win_y - (DEFAULTBODYMARGIN + $('#pagearea').outerHeight() + $('#tabarea').outerHeight() + $('#buttonarea').outerHeight());
  var padding_h = parseInt($('#posregarea').css('padding-top'), 10) + parseInt($('#posregarea').css('padding-bottom'), 10);
  $('#posregarea').height(resize_layout_h - padding_h - DEFAULTBODYMARGIN - GRIDBTMMARGIN);
  if (window.innerWidth != g_win_x_org) {
    w2ui['posregarea'].refresh();
    g_win_x_org = window.innerWidth;
  }
});

/*
