function setRenderFlt(recVal) {
  var returnVal = "";
  if (recVal !== "" && !isNaN(recVal)) {
    returnVal = Number(recVal).toFixed(3);
  }
  return returnVal;
}

/*
 * Set render for in range
 */
function setRefPosInRange(recVal) {
  var returnVal = '';
  for (var s in g_ref_pos_enable) {
    if ((getCurrentLanguage() == 'eg')) {
      if (g_ref_pos_bool[s].id == recVal) {
        returnVal = g_ref_pos_bool[s].value;
      }
    } else {
      if (g_ref_pos_enable[s].id == recVal) {
        returnVal = g_ref_pos_enable[s].value;
      }
    }
  }
  return returnVal;
}

