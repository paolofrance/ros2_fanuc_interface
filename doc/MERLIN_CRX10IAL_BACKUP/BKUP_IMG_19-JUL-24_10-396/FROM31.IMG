function setRenderJoint(record, index, col_index) {
    var return_val = "";
    if (record[POS_REG_CLM_ARR[col_index]].data !== "") {
        return_val = Number(record[POS_REG_CLM_ARR[col_index]].data).toFixed(3);
    }
    if (record.w2ui.style[col_index] == STYLE_GRID_INVALID) {
        return_val = "";
