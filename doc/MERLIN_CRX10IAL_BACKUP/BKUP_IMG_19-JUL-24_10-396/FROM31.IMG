  background-image: url("vstree.png?id=20160909191445");
  background-repeat: no-repeat;
  background-color: transparent;
  cursor: pointer;
}
.vstree .vstree-open > .vstree-mark {
  background-position: -30px 0;
}
.vstree .vstree-closed > .vstree-mark {
  background-position: 0 0;
}
.vstree .vstree-open > ul {
  display: block;
}
.vstree .vstree-closed > ul {
  display: none;
}
.vstree ul,
.vstree li {
  padding: 0;
  list-style-type: none;
  margin: 0;
  display: block;
}
.vstree li {
  line-height: 34px;
  min-height: 34px;
  display: block;
  white-space: nowrap;
  margin-left: 32px;
}
.vstree > ul > li {
  margin-left: 0;
}
.vstree .vstree-icon, .vstree .vstree-mark {
  padding: 0;
  margin: 0;
  display: inline-block;
  text-decoration: none;
}
.vstree .vstree-mark {
  width: 30px;
  height: 32px;
}
.vstree a {
  margin: 0;
  padding: 0px 6px 0 2px;
  line-height: 34px;
  height: 34px;
  min-width: 160px;
  display: inline-block;
  white-space: nowrap;
  color: black;
  text-decoration: none;
  outline-style: none; /* this style does not work on IE7 */
}
.vstree a.vstree-selected {
  background: #0e6eb8;
  color: white;
}
.vstree a.vstree-disabled {
  background: #e1e7ec;
  color: #808080;
}
.vstree .vstree-icon {
  position: relative;
  top: 2px;
  width: 30px;
  height: 30px;
  background-color: #67727e;
  margin-right: 5px;
  cursor: pointer;
}
li.vstree-open > ul {
  display: block;
}
li.vstree-closed > ul {
  display: none;
}
.vstree a.vstree-trained {
  background: #e1e7ec;
  color: #00993e;
}
.vstree a.vstree-not-trained {
  background: #e1e7ec;
  color: #b31e23;
}
.vstree a.vstree-trained.vstree-selected {
  background: #00993e;
  color: white;
}
.vstree a.vstree-not-trained.vstree-selected {
  background: #b31e23;
  color: white;
}

/* treeSelect */
.tree-select {
  /* overflow: auto; */
  width: auto;
}
.tree-select .tree-select-open > ul {
  display: block;
}
.tree-select .tree-select-closed > ul {
  display: none;
}
.tree-select ul,
.tree-select li {
  padding: 0;
  list-style-type: none;
  margin: 0;
  display: block;
}
.tree-select li {
  line-height: 34px;
  min-height: 34px;
  display: block;
  white-space: nowrap;
  margin-left: 32px;
  margin-bottom: 3px;
}
.tree-select li:first-child {
  margin-top: 3px;
}
.tree-select > ul:first-child >  li:first-child {
  margin-top: 0;
}
.tree-select li.tree-select-last {
  margin-bottom: 0;
}
.tree-select > ul > li {
  margin-left: 0;
}
.tree-select .tree-select-icon {
  display: inline-block;
  text-decoration: none;
  width: 30px;
  height: 30px;
  vertical-align: middle;
  margin-right: 5px;
  margin-left: 5px;
  cursor: pointer;
}
.tree-select .tree-select-node {
  margin: 0;
  margin-right: 3px;
  line-height: 34px;
  height: 34px;
  vertical-align: middle;
  min-width: 160px;
  display: block;
  white-space: nowrap;
  color: white;
  text-decoration: none;
  outline-style: none; /* this style does not work on IE7 */
}
.tree-select .tree-select-open > .tree-select-node,
.tree-select .tree-select-closed > .tree-select-node {
  background-color: #97a1ae;
}
.tree-select .tree-select-leaf > .tree-select-node {
  background-color: #67727e;
  padding-left: 10px;
}
.tree-select .tree-select-leaf > .tree-select-node.tree-select-selected {
  background-color: #0e6eb8;
  color: white;
}
.tree-select .tree-select-leaf > .tree-select-node.tree-select-disabled {
  background-color: #e1e7ec;
  color: #808080;
}
.tree-select .tree-select-leaf > .tree-select-node:hover {
  filter: alpha(opacity=70);
}
.tree-select .tree-select-leaf > .tree-select-node.tree-select-selected:hover,
.tree-select .tree-select-leaf > .tree-select-node.tree-select-disabled:hover {
  filter: alpha(opacity=100);
}
.tree-select .tree-select-leaf > .tree-select-node > .tree-select-arrow {
  display: none;
}
.tree-select .tree-select-closed > .tree-select-node {
  line-height: 30px;
  height: 30px;
  border: solid 2px #97a1ae;
}
.tree-select .tree-select-closed-selected > .tree-select-node {
  border-color: #6fc7d5;
}
.tree-select .tree-select-open > .tree-select-node > .tree-select-arrow,
.tree-select .tree-select-closed > .tree-select-node > .tree-select-arrow {
  display: inline-block;
  vertical-align: middle;
  margin-right: 10px;
  margin-left: 10px;
  width: 0;
  height: 0;
}
.tree-select .tree-select-open > .tree-select-node:hover > .tree-select-arrow,
.tree-select .tree-select-closed > .tree-select-node:hover > .tree-select-arrow {
  filter: alpha(opacity=70);
}
.tree-select .tree-select-closed > .tree-select-node > .tree-select-arrow {
  border-top: 7px solid transparent;
  border-right: 0px solid transparent;
  border-bottom: 7px solid transparent;
  border-left: 13px solid white;
}
.tree-select .tree-select-open > .tree-select-node > .tree-select-arrow {
  border-top: 13px solid white;
  border-right: 7px solid transparent;
  border-bottom: 0px solid transparent;
  border-left: 7px solid transparent;
}
li.tree-select-open > ul {
  display: block;
}
li.tree-select-closed > ul {
  display: none;
}
