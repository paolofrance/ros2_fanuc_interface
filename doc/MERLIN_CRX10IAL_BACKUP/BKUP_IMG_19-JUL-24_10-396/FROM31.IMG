      top.openCmpPupAppendFrm(top.window, VOICE_POPUP_URL, { appendArg: 'checkStartPrg' }, undefined, false, null, popupRfFunc);
      top.voiceApi.reqSpeech(getSpeechMsg(MSG.STRAT_PRG_CHECK_POP));
    } else {
      startPrgOprtCB();
    }
  } else { //Program status running
    if (stratBtnId == 'startOprtBtn') {
      popupRfFunc();
    } else {
      top.voiceApi.reqSpeech('');
