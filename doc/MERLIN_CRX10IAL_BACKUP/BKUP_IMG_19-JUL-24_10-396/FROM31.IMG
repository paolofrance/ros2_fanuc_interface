// Set current position data to Record.
function getCurrentPositionCallback(cartpos, jointpos, id) {
  var recid = w2ui['posregarea'].find({ id: id });
  var record = w2ui['posregarea'].records[recid];
  var grp = jointpos.Group;
  if (grp != g_select_group
    || id < g_current_item_no_start
    || id > g_current_item_no_end) {
    return;
  }
  switch (g_select_mode) {
    case POS_REP_CART:
      switch (record.rep) {
        case POS_REP_CART:
        case POS_REP_BOTH:
          break;
        case POS_REP_JOINT:
          record.force = true;
          if (w2ui['posregarea'].getColumn('ext1').hidden) {
            record.rep = POS_REP_CART;
          } else {
            record.rep = POS_REP_BOTH;
          }
          record.axes = g_ext_count[g_select_group - 1];
          break;
        case POS_REP_NONE:
          if (w2ui['posregarea'].getColumn('ext1').hidden) {
            record.rep = POS_REP_CART;
          } else {
            record.rep = POS_REP_BOTH;
          }
          record.axes = g_ext_count[g_select_group - 1];
          break;
      }
      record.config = cartpos.Config;
      record.x = cartpos.X;
      record.y = cartpos.Y;
      record.z = cartpos.Z;
      record.w = cartpos.W;
      record.p = cartpos.P;
      record.r = cartpos.R;
      for (var i = POS_REG_CLM_ARR.indexOf('ext1'); i <= POS_REG_CLM_ARR.indexOf('ext3'); i++) {
        if (w2ui['posregarea'].getColumn(POS_REG_CLM_ARR[i]).hidden) {
        } else {
          var up_str = POS_REG_CLM_ARR[i].toUpperCase();
          record[POS_REG_CLM_ARR[i]].data = jointpos[up_str];
          record[POS_REG_CLM_ARR[i]].unit = g_axis_unit[i - POS_REG_CLM_ARR.indexOf('j1')];
        }
      }
      var recData = { clm: GRID_FIELD_X, num: 2};
      var orgJointData = [];
      orgJointData = createConvJointData(jointpos, record.id, recid[0]);
      if (g_jposrec_enb) {
      	pasteJointRepData(orgJointData, recid[0]);
      } else {
      	execExchangeCartToJoint(record, orgJointData);
      }
      break;
    case POS_REP_JOINT:
      switch (record.rep) {
        case POS_REP_CART:
        case POS_REP_BOTH:
        case POS_REP_NONE:
          record.force = true;
          record.rep = POS_REP_JOINT;
          record.axes = g_axes_count[g_select_group - 1] + g_ext_count[g_select_group - 1];
          break;
        case POS_REP_JOINT:
          break;
      }
      record.config = "";
      for (var i = POS_REG_CLM_ARR.indexOf('j1'); i <= POS_REG_CLM_ARR.indexOf('ext3'); i++) {
        if (w2ui['posregarea'].getColumn(POS_REG_CLM_ARR[i]).hidden) {
        } else {
          var up_str = POS_REG_CLM_ARR[i].toUpperCase();
          record[POS_REG_CLM_ARR[i]].data = jointpos[up_str];
          record[POS_REG_CLM_ARR[i]].unit = g_axis_unit[i - POS_REG_CLM_ARR.indexOf('j1')];
        }
      }
      var recData = { clm: GRID_FIELD_J1, num: 2};
      break;
  }
  recPosReg(record, recData, false);
}

// Callback for acquire the current location's config when you put values in the initial data.
function getCurrentConfigCallback(cartpos, jointpos, id) {
  var recid = w2ui['posregarea'].find({ id: id });
  var record = w2ui['posregarea'].records[recid];
  var grp = jointpos.Group;
  if (grp != g_select_group
    || id < g_current_item_no_start
    || id > g_current_item_no_end
    || g_select_mode != POS_REP_CART
    || record.rep == POS_REP_JOINT
