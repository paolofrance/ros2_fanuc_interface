  g_no_send_flag = true;
  g_timer_send_keep_data = setTimeout("onTimerAfterSend()", POSREG_INTERVAL_TIME);
}

/*
 * Create write data
 */
function createWriteData(record) {
  var write_data = {};
  write_data.id = record.id;
  write_data.gnum = g_select_group;
  write_data.comment = record.comment;
  write_data.force = record.force;
  write_data.only_cmt_flag = false;
  if (typeof cmtUpdateFlg[record.id] != 'undefined') {
    write_data.only_pos_flag = ((cmtUpdateFlg[record.id]) ? false : true);
   } else {
    write_data.only_pos_flag = true;
  }
  switch (record.rep) {
    case POS_REP_CART:
    case POS_REP_BOTH:
      write_data.rep = record.rep;
      write_data.axes = record.axes;
      write_data.Config = record.config;
      write_data.X = record.x;
      write_data.Y = record.y;
      write_data.Z = record.z;
      write_data.W = record.w;
      write_data.P = record.p;
      write_data.R = record.r;
      for (var i = POS_REG_CLM_ARR.indexOf('ext1'); i <= POS_REG_CLM_ARR.indexOf('ext3'); i++) {
        if (w2ui['posregarea'].getColumn(POS_REG_CLM_ARR[i]).hidden) {
        } else {
          var up_str = POS_REG_CLM_ARR[i].toUpperCase();
          if (record[POS_REG_CLM_ARR[i]] !== "") {
            write_data[up_str] = {};
            write_data[up_str].data = record[POS_REG_CLM_ARR[i]].data;
            write_data[up_str].unit = record[POS_REG_CLM_ARR[i]].unit;
          }
        }
      }
      break;
    case POS_REP_JOINT:
      write_data.rep = record.rep;
      write_data.axes = record.axes;
      write_data.rob_axes = g_axes_count[g_select_group - 1];
      write_data.Config = record.config;
      for (var i = POS_REG_CLM_ARR.indexOf('j1'); i <= POS_REG_CLM_ARR.indexOf('ext3'); i++) {
        var up_str = POS_REG_CLM_ARR[i].toUpperCase();
        if (record[POS_REG_CLM_ARR[i]] !== ""
          && record[POS_REG_CLM_ARR[i]].data !== "") {
          write_data[up_str] = {};
          write_data[up_str].data = record[POS_REG_CLM_ARR[i]].data;
          write_data[up_str].unit = record[POS_REG_CLM_ARR[i]].unit;
        }
      }
      break;
    case POS_REP_NONE:
      write_data.only_cmt_flag = true;
      break;
  }
  return write_data;
}

