function storePmcFile(filename, file_data) {
  var xmlreq = new XMLHttpRequest();
  xmlreq.onreadystatechange = function () {
    if ((xmlreq.readyState === 4) && (xmlreq.status === 200)) {
      document.getElementById('overlay_all').style.display = "block";
      document.getElementById("msgpopup").style.display = "block";
      document.getElementById("poptitle").innerHTML = langResource.webfiler_TitleSendFin_c;
      document.getElementById("popcomment").innerHTML = langResource.webfiler_ComSendFin_c;
    } else {
      document.getElementById('overlay_all').style.display = "block";
      document.getElementById("msgpopup").style.display = "block";
      document.getElementById("poptitle").innerHTML = langResource.webfiler_TitleFailure_c;
      document.getElementById("popcomment").innerHTML = langResource.webfiler_ComFailure_c;
    }
  };
  xmlreq.open('POST', "/td?file=" + filename);
  xmlreq.send(file_data);
};

