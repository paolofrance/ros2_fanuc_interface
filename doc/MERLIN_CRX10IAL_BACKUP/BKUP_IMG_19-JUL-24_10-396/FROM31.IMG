function countPasteRow(copyData) {
  var selRecidMin = (crntSelType == SELGRID_ROW) ? g_select_grid_row[0] : g_select_grid_row[0].recid;
  var selRecidMax = selRecidMin + copyData.length - 1;
  var pageRecidMax = (g_current_item_no_end - 1) % GRID_ROW_MAX_COUNT;
  return ((pageRecidMax <= selRecidMax) ? pageRecidMax : selRecidMax) - selRecidMin + 1;
}

// for row data copy & paste
var pasteComment = "";
function copyRowsData(copiedText) {
  var pasteCount = countPasteRow(copiedText);
  if (pasteCount > 1) {
    multiRowFlag = true;
  }
  for (i = 0; i < pasteCount; i++) {
    clearGTimer();
    var pasterow = $.extend({}, copiedText[i]);
    var selectrow = w2ui['posregarea'].records[g_select_grid_row[0] + i];
    if (pasterow.rep === POS_REP_NONE) {
      pasteComment = pasterow.comment;
      pasteClearPos(1, selectrow.recid);
    } else {
      pasterow.recid = selectrow.recid;
      pasterow.id = selectrow.id;
      pasterow.label = resources["PosRegName"] + "[" + (selectrow.id) + "]";
      if (selectrow.rep != pasterow.rep) {
        pasterow.force = true;
      }
      w2ui['posregarea'].set(pasterow.recid, pasterow);
      if (pasteCount == i + 1) {
        multiRowFlag = false;
      }
      var recData = { clm: GRID_FIELD_COMMENT, num: 2 };
      recPosReg(pasterow, recData, false);
    }
  }
  w2ui['posregarea'].save();
}

// for paste repre data
function pasteRepData(repText) {
  if (repText.length > 1) { // If paste multi Row, set save data flag.
    multiRowFlag = true;
  }
  for (i = 0; i < repText.length; i++) {
    clearGTimer();
    var pasterow = $.extend({}, repText[i]);
    var recData = { clm: GRID_FIELD_X, num: 2 };

    pasterow.label = resources["PosRegName"] + "[" + (repText[i].id) + "]";
    w2ui['posregarea'].set(pasterow.recid, pasterow);
    
    if (repText.length == i + 1) {
      multiRowFlag = false;
    }
    recPosReg(pasterow, recData, false);
  }
  w2ui['posregarea'].save();
}

