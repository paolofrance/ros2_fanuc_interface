function recRefPos(writeData) {
  stopTimer();
  setRefPosValue(g_select_group, writeData, setRefPosCallback);
  // Data visible to the user round to three decimal places.
  for (var i = 0; i < writeData.length; i++) {
    var roundRecord = writeData[i];
    for (var j = 1; j <= REF_POS_AXIS.length; j++) {
      roundRecord.record[REF_POS_CLM_ARR.indexOf("axis" + j)].recVal = Math.round(parseFloat(roundRecord.record[REF_POS_CLM_ARR.indexOf("axis" + j)].recVal).toFixed(7) * 1000) / 1000;
      roundRecord.record[REF_POS_CLM_ARR.indexOf("range" + j)].recVal = Math.round(parseFloat(roundRecord.record[REF_POS_CLM_ARR.indexOf("range" + j)].recVal).toFixed(7) * 1000) / 1000;
    }
    var tgtRow = refPosGrid.getColumn(REF_POS_CLM_ARR[COLUMN_INDEX]).indexOf(String(roundRecord.record[COLUMN_INDEX].recVal)) + 1
    refPosGrid.updateRecord(tgtRow, roundRecord);
  }
}

