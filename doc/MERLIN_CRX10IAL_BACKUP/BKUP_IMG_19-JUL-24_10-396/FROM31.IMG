/*
 * Grid Event Setting
 */
function setGridEvent(event, type, info) {
  if (refPosGrid.getSelectType() == SELGRID_NONE) {
    return;
  }
  switch(type) {
    case 'change':
      changeEvent(info);
      break;
    case 'copy':
      copyEvent();
      break;
    case 'paste':
      pasteEvent();
      break;
    case 'cut':
      copyEvent();
      deleteEvent();
      break;
    case 'delete':
      deleteEvent();
      break;
  }
}
/*
 * Grid change event
 */
function changeEvent(info) {
  var chgRec = refPosGrid.getRecord(info.row);
  var nextCol = REF_POS_CLM_ARR.indexOf(info.fieldId);
  var checkNum = /^[-]?([1-9]\d*|0)(\.\d+)?$/;
  var nextVal = info.val;
  if (info.prev === nextVal) {
    return;
  }
  chgRec.force = false;
  switch (nextCol) {
    case COLUMN_ENABLE:
    case COLUMN_HOME:
      if (nextVal == g_ref_pos_enable[0].value) {
        chgRec.record[nextCol].recVal = g_ref_pos_enable[0].id;
      } else if (nextVal == g_ref_pos_enable[1].value) {
        chgRec.record[nextCol].recVal = g_ref_pos_enable[1].id;
      }
      recRefPos([chgRec]);
      break;
    case COLUMN_COMMENT:
      if (checkLangAndCharBytes(nextVal)) {
        nextVal = info.prev;
        top.openCmpAlert(window, 'err2ByteCharsDlg', resources['ERR2ByteChars']);
      }
      if (countChar(nextVal) > REF_POS_COMMENT_MAX) {
        nextVal = multByteStringSlice(nextVal, REF_POS_COMMENT_MAX);
        top.openCmpAlert(window, 'errTooManyCharsDlg', resources['ERRTooManyChars']);
      }
      chgRec.record[nextCol].recVal = nextVal;
      if (nextVal === "") {
        chgRec.force = true;
        recRefPos([chgRec]);
      } else {
        recRefPos([chgRec]);
      }
      break;
    case COLUMN_SIGNAL_IO:
      if (nextVal == g_ref_pos_io[0].value) {
        chgRec.record[nextCol].recVal = g_ref_pos_io[0].id;
      } else if (nextVal == g_ref_pos_io[1].value) {
        chgRec.record[nextCol].recVal = g_ref_pos_io[1].id;
      }
      recRefPos([chgRec]);
      break;
    case COLUMN_SIGNAL_IO_NUM:
      // Block string pasting and Limit the range of Value
      if (checkNum.test(nextVal)) {
        nextVal = Number(nextVal).toFixed(0);
        if (Number(nextVal) < refPosTableDef.columnDefine[COLUMN_SIGNAL_IO_NUM].fieldType.min){
          nextVal = refPosTableDef.columnDefine[COLUMN_SIGNAL_IO_NUM].fieldType.min;
        } else if (Number(nextVal) > refPosTableDef.columnDefine[COLUMN_SIGNAL_IO_NUM].fieldType.max) {
          nextVal = refPosTableDef.columnDefine[COLUMN_SIGNAL_IO_NUM].fieldType.max;
        }
        chgRec.record[nextCol].recVal = nextVal;
        recRefPos([chgRec]);
      } else {
        getRefPosVar(g_select_group, chgRec.record[COLUMN_INDEX].recVal, getRefPosVarCallback);
      }
      break;
    case COLUMN_RANGE1:
    case COLUMN_RANGE2:
    case COLUMN_RANGE3:
    case COLUMN_RANGE4:
    case COLUMN_RANGE5:
    case COLUMN_RANGE6:
    case COLUMN_RANGE7:
    case COLUMN_RANGE8:
    case COLUMN_RANGE9:
      if (checkNum.test(nextVal)) {
        nextVal = Math.abs(nextVal);
      }
      // falls through
    case COLUMN_AXIS1:
    case COLUMN_AXIS2:
    case COLUMN_AXIS3:
    case COLUMN_AXIS4:
    case COLUMN_AXIS5:
    case COLUMN_AXIS6:
    case COLUMN_AXIS7:
    case COLUMN_AXIS8:
    case COLUMN_AXIS9:
      if (checkNum.test(nextVal)) {
        chgRec.record[nextCol].recVal = nextVal;
        recRefPos([chgRec]);
      } else {
        getRefPosVar(g_select_group, chgRec.record[COLUMN_INDEX].recVal, getRefPosVarCallback);
      }
      break;
  }
}

/*
 * Grid copy event
 */
function copyEvent() {
  var copyData = [];
  var copyRowArry = [];
  if (refPosGrid.getSelectType() == SELGRID_ROW) {
    startCol = COLUMN_ENABLE;
    endCol = COLUMN_RANGE9;
  } else {
    startCol = refPosGrid.getSelectRange().start.col;
    endCol = refPosGrid.getSelectRange().end.col;
  }
  var selRow = refPosGrid.getSelectRow();
  for (var i = selRow.start; i <= selRow.end; i++) {
    for (var j = startCol; j <= endCol; j++) {
      copyRowArry.push(refPosGrid.getCellValue(i, REF_POS_CLM_ARR[j]));
    }
    copyData.push(copyRowArry);
    copyRowArry = [];
  }
  copyEventText = copyData;
}

/*
 * Grid paste event
 */
function pasteEvent() {
  if (typeof copyEventText !== 'undefined') {
    if (!checkRefPosData()) {
      top.openCmpAlert(window, 'errPaste', resources['ERRPaste']);
    }
  }
}

/*
 * Grid delete event.
 */
function deleteEvent() {
  if (refPosGrid.getSelectType() == SELGRID_CELL) {
    var slctRng = refPosGrid.getSelectRange();
    if (slctRng.start.col > COLUMN_COMMENT ||
      slctRng.end.col < COLUMN_COMMENT) {
      return;
    }
  }
  var slctRow = refPosGrid.getSelectRow();
  var setRec;
  var setArry = [];
  for (var i = slctRow.start; i <= slctRow.end; i++) {
    refPosGrid.setCellValue(i, REF_POS_CLM_ARR[COLUMN_COMMENT], "");
    setRec = refPosGrid.getRecord(i);
    setRec.force = true;
    setArry.push(setRec);
  }
  recRefPos(setArry);
}

/*
 * Initialize Reference positions Data
 */
function initRefPosData() {
  var selType = refPosGrid.getSelectType();
  var selRng;
  switch (selType) {
    case SELGRID_ROW:
      selRng = refPosGrid.getSelectRow();
    break;
    case SELGRID_CELL:
      selRng = refPosGrid.getSelectRange();
    break;
  }
  var linebuff = [];
  for (var i = g_current_item_no_start; i <= g_current_item_no_end; i++) {
    var newline = createRecord();
    newline.record[COLUMN_INDEX].recVal = i;
    linebuff.push(newline);
  }
  if (refPosGrid.getRecord(1) != null) {
    refPosGrid.clearGrid();
    if (linebuff.length != 0) {
      refPosGrid.drawRecord(linebuff);
    }
  } else {
    refPosGrid.refresh(document, 'refposarea', refPosTableDef, linebuff);
  }
  switch (selType){
    case SELGRID_ROW:
      selRng.end = selRng.start;
      refPosGrid.setSelectRow(selRng);
      break;
    case SELGRID_CELL:
      refPosGrid.setSelectCell(selRng.start.row, REF_POS_CLM_ARR[selRng.start.col])
      break;
  }
  getRefPosAxisUnit(g_select_group, getRefPosAxisUnitCallback);
  // Function key and TP key had be hidden, readjust the screen size.
  $(window).trigger('resize');
}

/*
 * Check paste Ref Pos Data
 */
function checkRefPosData() {
  var recid = Number(refPosGrid.getSelectRow().start);
  var column = (refPosGrid.getSelectType() == SELGRID_ROW) ? COLUMN_ENABLE : refPosGrid.getSelectCol().start;
  var copydata = copyEventText;
  var checkNum = /^[-]?([1-9]\d*|0)(\.\d+)?$/;
  var dataList = [];

  for (var i = 0; i < copydata.length; i++) {
    var cellData = copydata[i];
    var pstRec = refPosGrid.getRecord(recid + i);
    if (pstRec == undefined) break;
    for (var j = 0; j < cellData.length; j++) {
      var nextCol = column + j;
      var nextVal = cellData[j];
      if ((nextVal == "") && (nextCol != COLUMN_COMMENT)) {
        return false;
      }

      switch (nextCol) {
        case COLUMN_ENABLE:
        case COLUMN_HOME:
          if (nextVal == g_ref_pos_enable[0].value) {
            dataList.push(createValArr(nextCol, pstRec.record[COLUMN_INDEX].recVal, g_ref_pos_enable[0].id));
          } else if (nextVal == g_ref_pos_enable[1].value) {
            dataList.push(createValArr(nextCol, pstRec.record[COLUMN_INDEX].recVal, g_ref_pos_enable[1].id));
          } else {
            return false;
          }
          break;
        case COLUMN_SIGNAL_IO:
          if (nextVal == g_ref_pos_io[0].value) {
            dataList.push(createValArr(nextCol, pstRec.record[COLUMN_INDEX].recVal, g_ref_pos_io[0].id));
          } else if (nextVal == g_ref_pos_io[1].value) {
            dataList.push(createValArr(nextCol, pstRec.record[COLUMN_INDEX].recVal, g_ref_pos_io[1].id));
          } else {
            return false;
          }
          break;
        case COLUMN_COMMENT:
          if (countChar(nextVal) <= REF_POS_COMMENT_MAX) {
            dataList.push(createValArr(nextCol, pstRec.record[COLUMN_INDEX].recVal, nextVal));
          } else {
            return false;
          }
          break;
        case COLUMN_SIGNAL_IO_NUM:
          if (checkNum.test(nextVal)) {
            nextVal = Number(nextVal).toFixed(0);
            if (Number(nextVal) < refPosTableDef.columnDefine[COLUMN_SIGNAL_IO_NUM].fieldType.min){
              nextVal = refPosTableDef.columnDefine[COLUMN_SIGNAL_IO_NUM].fieldType.min;
            } else if (Number(nextVal) > refPosTableDef.columnDefine[COLUMN_SIGNAL_IO_NUM].fieldType.max) {
              nextVal = refPosTableDef.columnDefine[COLUMN_SIGNAL_IO_NUM].fieldType.max;
            }
            dataList.push(createValArr(nextCol, pstRec.record[COLUMN_INDEX].recVal, nextVal));
          } else {
            return false;
          }
          break;
        case COLUMN_RANGE1:
        case COLUMN_RANGE2:
        case COLUMN_RANGE3:
        case COLUMN_RANGE4:
        case COLUMN_RANGE5:
        case COLUMN_RANGE6:
        case COLUMN_RANGE7:
        case COLUMN_RANGE8:
        case COLUMN_RANGE9:
          if (checkNum.test(nextVal)) {
            nextVal = Math.abs(nextVal);
          }
          // falls through
        case COLUMN_AXIS1:
        case COLUMN_AXIS2:
        case COLUMN_AXIS3:
        case COLUMN_AXIS4:
        case COLUMN_AXIS5:
        case COLUMN_AXIS6:
        case COLUMN_AXIS7:
        case COLUMN_AXIS8:
        case COLUMN_AXIS9:
          if (checkNum.test(nextVal)) {
            nextVal = Number(nextVal);
            dataList.push(createValArr(nextCol, pstRec.record[COLUMN_INDEX].recVal, nextVal));
          } else {
            return false;
          }
          break;
        default:
          return false;
      }
    }
  }
  pasteRefPosData(dataList);
  return true;
}

/*
 * Paste Ref Pos Data
 */
function pasteRefPosData(dataList) {
  var rows = [];
  for (var i = 0; i < dataList.length; i++) {
    refPosGrid.setCellValue(dataList[i].row - g_current_item_no_start + 1, REF_POS_CLM_ARR[dataList[i].col], dataList[i].val);
    rows.push(dataList[i].row - g_current_item_no_start + 1);
  }
  var rowArr = rows.filter(function (x, i, self) {
    return self.indexOf(x) === i;
  });
  var setRec;
  var setArry = [];
  for (var i = 0; i < rowArr.length; i++) {
    setRec = refPosGrid.getRecord([rowArr[i]]);
    setRec.force = false;
    setArry.push(setRec); 
  }
  recRefPos(setArry);
}

function createValArr(nextCol, nextRow, nextVal) {
  var setArr = {};
  setArr.col = nextCol;
  setArr.row = nextRow;
  setArr.val = nextVal;
  return setArr;
}

