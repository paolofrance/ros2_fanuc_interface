/*
 * Grid Event Setting
 */
function setGridEvent(event, type, info) {
  if (regGrid.getSelectType() == SELGRID_NONE) {
    return;
  }
  if ((type == 'paste' || type == 'cut' || type == 'delete') && isUneditable()) {
    top.IHMIComponents.cf.toast.show(resources['ERREditRegTpOff']);
    return;
  }
  switch(type) {
    case 'selectCell':
      if (isUneditable() && event.keyCode == undefined) {
        regGrid.clearSelect();
        if (info.fieldId == REG_CLM_ARR[COLUMN_LABEL]) {
          regGrid.setSelectRow({start: info.row, end: info.row}); // Select Row
        } else {
          regGrid.setSelectCell(info.row, info.fieldId); // Select Cell
          top.IHMIComponents.cf.toast.show(resources['ERREditRegTpOff']);
        }
      }
      break;
    case 'change':
      changeEvent(info);
      break;
    case 'copy':
      copyEvent();
      break;
    case 'paste':
      if (typeof copyEventText !== 'undefined') pasteRegData();
      break;
    case 'cut':
      copyEvent();
      clearRegData();
      break;
    case 'delete':
      clearRegData();
      break;
  }
}

// change event
function changeEvent(info) {
  var index = regGrid.getRecord(info.row).record[COLUMN_NO].recVal;
  if ('value' == info.fieldId) {
    if (!isNaN(info.val)) {
      setNumRegValue(index, convertRegValType(info.val));
    }
  }
  if ('comment' == info.fieldId) {
    if (checkLangAndCharBytes(info.val, 0)) {
      // No action
    } else {
      setNumRegComment(index, info.val);
    }
  }
}

// Convert register value to int or float type
function convertRegValType(val) {
  var regVal = '';
  if (Number(val) % 1 == 0 && val !== "") {
    if (Number(val) > UPPR_INT_VAL || Number(val) < LOWR_INT_VAL) {
      regVal = parseFloat(val).toExponential(16); // float type
    } else {
      regVal = String(parseFloat(val)); // int type
    }
  } else {
    regVal = parseFloat(val).toExponential(16); // float type
  }
  return regVal;
}

// copy event
function copyEvent() {
  var copyData = [];
  var copyRowArry = [];
  var slctCol = getSlctCol();
  var slctRow = regGrid.getSelectRow();
  for (var i = slctRow.start; i <= slctRow.end; i++) {
    for (var j = slctCol.start; j <= slctCol.end; j++) {
      copyRowArry.push(regGrid.getCellValue(i, REG_CLM_ARR[j]));
    }
    copyData.push(copyRowArry);
    copyRowArry = [];
  }
  copyEventText = copyData;
}

// get selected column
function getSlctCol() {
  var slctCol = {
    start : COLUMN_COMMENT,
    end : COLUMN_VALUE
  };
  if (regGrid.getSelectType() == SELGRID_CELL) {
    slctCol.start = regGrid.getSelectCol().start;
    slctCol.end = regGrid.getSelectCol().end;
  }
  return slctCol;
}

/*
 * Initialize Register Data
 */
function initRegData(current_item_start, current_item_end) {
  clearTimeout(g_timer_val);

  var slctType = regGrid.getSelectType();
  var slctRng;
  switch (slctType) {
    case SELGRID_ROW:
      slctRng = regGrid.getSelectRow();
      break;
    case SELGRID_CELL:
      slctRng = regGrid.getSelectRange();
      break;
  }

  var linebuff = [];
  for (var i = current_item_start; i <= current_item_end; i++) {
    var newline = createrow();
    newline.record[COLUMN_LABEL].recVal = resources["RegName"] + "[" + i + "]";
    newline.record[COLUMN_NO].recVal = i;
    linebuff.push(newline);
  }
  if (regGrid.getRecord(1) != null) {
    regGrid.clearGrid();
    if (linebuff.length != 0) {
      regGrid.drawRecord(linebuff);
    }
  } else {
    regGrid.refresh(document, 'regarea', regTableDef, linebuff);
  }
  
  switch (slctType){
    case SELGRID_ROW:
      slctRng.end = slctRng.start;
      regGrid.setSelectRow(slctRng);
      break;
    case SELGRID_CELL:
      regGrid.setSelectCell(slctRng.start.row, REG_CLM_ARR[slctRng.start.col]);
      break;
  }

  g_timer_val = setTimeout("onTimer()", 10);
  // Function key and TP key had be hidden, readjust the screen size.
  $(window).trigger('resize');
}

/*
 * Grid Row Create
 */
function createrow() {
  var empty = {
    record:[]
  };
  empty.record[COLUMN_LABEL] = {recVal : ""};
  empty.record[COLUMN_NO] = {recVal : ""};
  empty.record[COLUMN_COMMENT] = {recVal : ""};
  empty.record[COLUMN_VALUE] = {recVal : ""};
  return empty;
}

/*
 * SetTimeOut Function
 */
function onTimer() {
  var num = getCurrentItemEnd()-getCurrentItemStart()+1;
  getRegValueXVR(getCurrentItemStart(), num, getRegValueCallback);
  g_timer_val = setTimeout("onTimer()", g_interval_time);
}

function getRegValueCallback(regdata_arr) {
  for (var i = 0; i < Object.keys(regdata_arr).length; i++) {
    var recRow = i + 1;
    var recData = regGrid.getRecord(recRow);
    if (recData != null) {
      var regData = regdata_arr[Object.keys(regdata_arr)[i]];
      var floatVal = parseFloat(regData.value);
      if ((recData.record[COLUMN_VALUE].recVal !== floatVal) || (recData.record[COLUMN_VALUE].recVal === '')) {
        regGrid.setCellValue(recRow, REG_CLM_ARR[COLUMN_VALUE], floatVal);
      }
      if (recData.record[COLUMN_COMMENT].recVal != regData.comment) {
        regGrid.setCellValue(recRow, REG_CLM_ARR[COLUMN_COMMENT], regData.comment);
      }
    } else {
      // Data to throw away(before page value)
    }
  }
}

function setButtonEvent() {
  $("#page_prevbtn").on('click touchend', function (event) {
    if(event.type.indexOf('touch') >= 0){
      event.preventDefault();
    }
    if (g_current_page_no <= 1) {
      return;
    }
    currentTabPage = Math.ceil(g_current_item_no_start / GRID_ROW_MAX_COUNT) - 1;
    sessionStorage.setItem('regSelData', currentTabPage);
  });

  $("#page_nextbtn").on('click touchend', function (event) {
    if(event.type.indexOf('touch') >= 0){
      event.preventDefault();
    }
    if (g_current_page_no >= g_current_page_max) {
      return;
    }
    currentTabPage = Math.ceil(g_current_item_no_start / GRID_ROW_MAX_COUNT) + 1;
    sessionStorage.setItem('regSelData', currentTabPage);
  });

  $("#page_num").on('click', function (event) {
    var target = $(event.target);
    if (g_current_page_max == 1) {
      return;
    }
    var sel_menu = $('.page_frame');
    if (sel_menu.length) {
      if (target.hasClass('page_sel')) {
        currentTabPage = Number(event.target.outerText);
        sessionStorage.setItem('regSelData', currentTabPage);
      }
    }
  });
}

/*
 * Set Register Max Num Callback
 */
function setRegMaxNumCallback() {
  initPageArea(getRegMaxNum(REG_TYPE_NUM), currentTabPage);
  initRegData(getCurrentItemStart(), getCurrentItemEnd());
}

function initLayoutAreaCallBack() {
  initResource();
}

function initProc() {
  setButtonEvent();
  correctPageLayout();
  regGrid.setCallback(setGridEvent);
  setRegMaxNum(setRegMaxNumCallback);
  setPageAreaEvent(initRegData);
  getIOvalue(top.tpin_type_c, top.tp_enbl_c, chgTpEnbForReg);
}

/*
 * Clear Register Data (Delete Event)
 */
function clearRegData() {
  var slctCol = getSlctCol();
  if (slctCol.start == COLUMN_VALUE) return;
  var slctRow = regGrid.getSelectRow();
  for (var i = slctRow.start; i <= slctRow.end; i++) {
    setNumRegComment(regGrid.getRecord(i).record[COLUMN_NO].recVal, DEF_COMMENT);
  }
}

/*
 * Check paste Register Data
 */
function checkPasteData(pasteRow, pasteCol) {
  var copydata = copyEventText;
  var dataList = [];
  var checkNum = /^[-]?([1-9]\d*|0)(\.\d+)?$/;

  for (var i = 0; i < copydata.length; i++) {
    var cellData = copydata[i];

    for (var j = 0; j < cellData.length; j++) {
      var nextCol = pasteCol + j;
      var nextVal = cellData[j];
      switch (nextCol) {
        case COLUMN_COMMENT:
          if (checkLangAndCharBytes(nextVal, 0)) {
            return false;
          } else {
            dataList.push(createValArr(nextCol, pasteRow + i, nextVal));
          }
          break;
        case COLUMN_VALUE:
          if (checkNum.test(nextVal)) {
            dataList.push(createValArr(nextCol, pasteRow + i, convertRegValType(nextVal)));
          } else {
            return false;
          }
          break;
        default:
          break;
      }
    }
  }
  return dataList;
}

/*
 * Set reg data after paste
 */
function pasteRegData() {
  var pasteRow = regGrid.getRecord(regGrid.getSelectRow().start).record[COLUMN_NO].recVal;
  var pasteCol = (regGrid.getSelectType() == SELGRID_ROW) ? COLUMN_COMMENT : regGrid.getSelectCol().start;
  var checkedList = checkPasteData(pasteRow, pasteCol); // checkedList -> dataList or false
  if (checkedList == false) {
    top.openCmpAlert(window, 'errPasteDlg', resources['ERRPaste']);
    return;
  }

  for (var i = 0; i < checkedList.length; i++) {
    if (checkedList[i].row > g_current_item_no_end) break;
    if (checkedList[i].col == COLUMN_COMMENT) {
      setNumRegComment(checkedList[i].row, checkedList[i].val);
    } else {
      setNumRegValue(checkedList[i].row, checkedList[i].val);
    }
  }
}

function createValArr(nextCol, nextRow, nextVal) {
  var setArr = {};
  setArr.col = nextCol;
  setArr.row = nextRow;
  setArr.val = nextVal;
  return setArr;
}

// Check that the iframe is other than prim, dual, or third, and that TP is enabled or disabled.
function isUneditable() {
  return isMonitorDisp() && !tpEnb;
}

// The monitor screen of ihmieditr or DCS.
function isMonitorDisp() {
  // true: Monitor, false: the iframe is other than prim, dual, third.
  return window.frameElement.id !== "prim" && window.frameElement.id !== "dual" && window.frameElement.id !== "third";
}

// TP Enable status event.
function chgTpEnbForReg(event, type, index, value) {
  if (type == top.tpin_type_c && index == top.tp_enbl_c) {
    tpEnb = !(Number(value) == 0);
    chgGridLockForReg();
  }
}

function chgGridLockForReg() {
  if (isMonitorDisp()) {
    if (tpEnb) {
      // TP ON
      regGrid.unlock();
    } else {
      // TP OFF
      regGrid.lock();
    }
  }
}

