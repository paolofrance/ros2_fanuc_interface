/*
 * Get Reference positions Sysvar Value Callback.
 */
function getRefPosVarCallback(prog, sysvar, type, val, arg, clm) {
  if ((getXmlErrorData().flg) && (getXmlErrorData().init)) {
    setXmlErrorData();
  }
    var recid = refPosGrid.getColumn(REF_POS_CLM_ARR[COLUMN_INDEX]).indexOf(String(arg.id)) + 1;
    if (arg.grp != g_select_group
      || arg.id < g_current_item_no_start
      || arg.id > g_current_item_no_end) {
      return;
    }
    switch (clm) {
      case COLUMN_ENABLE:
      case COLUMN_HOME:
      case COLUMN_SIGNAL_IO:
      case COLUMN_COMMENT:
      case COLUMN_SIGNAL_IO_NUM:
        refPosGrid.setCellValue(recid, REF_POS_CLM_ARR[clm], val);
        break;
      case COLUMN_INRANGE:
        var updRec = refPosGrid.getRecord(recid);
        if (val == 'TRUE') {
          updRec.record[COLUMN_INRANGE].recColor = GRIDCOLOR_NORMAL;
        } else {
          updRec.record[COLUMN_INRANGE].recColor = GRIDCOLOR_ERROR;
        }
        updRec.record[COLUMN_INRANGE].recVal = val;
        refPosGrid.updateRecord(recid, updRec);
        break;
      case COLUMN_AXIS1:
      case COLUMN_AXIS2:
      case COLUMN_AXIS3:
      case COLUMN_AXIS4:
      case COLUMN_AXIS5:
      case COLUMN_AXIS6:
      case COLUMN_AXIS7:
      case COLUMN_AXIS8:
      case COLUMN_AXIS9:
      case COLUMN_RANGE1:
      case COLUMN_RANGE2:
      case COLUMN_RANGE3:
      case COLUMN_RANGE4:
      case COLUMN_RANGE5:
      case COLUMN_RANGE6:
      case COLUMN_RANGE7:
      case COLUMN_RANGE8:
      case COLUMN_RANGE9:
        var unitName = 'unit' + REF_POS_CLM_ARR[clm].replace(/[^0-9^\.]/g, "");
        var dispVal;
        var updRec = refPosGrid.getRecord(recid);
        if (updRec[unitName] == 'TRUE') {
          dispVal = Math.round(parseFloat(val) * (180 / Math.PI) * 1000) / 1000;
        } else {
          dispVal = Math.round(parseFloat(val) * 1000) / 1000;
        }
        refPosGrid.setCellValue(recid, REF_POS_CLM_ARR[clm], dispVal);
        break;
    }
}

/*
 * Get Reference positions Sysvar Value Callback(XVR).
 */
function getRefPosValueCallback(data_arr) {
  if ((getXmlErrorData().flg) && (getXmlErrorData().init)) {
    top.openCmpAlert(window, 'errXmlComDlg', resources['ERRXmlComment']);
    setXmlErrorData();
  }
  if (data_arr == null ||
    data_arr.length == 0) {
    return;
  }
  var label = Object.keys(data_arr)[0].toString();
  var grp = Number(label.slice(0, 1));
  if (grp != g_select_group) {
    return;
  }
  stopTimer();
  for (var j = g_current_item_no_start; j < Object.keys(data_arr).length + 1; j++) {
    var tgtRow = refPosGrid.getColumn(REF_POS_CLM_ARR[COLUMN_INDEX]).indexOf(String(j)) + 1;
    var chgRec = refPosGrid.getRecord(tgtRow);
    var data = data_arr[grp + "," + j];

    if ((j < g_current_item_no_start) ||
      (j > g_current_item_no_end)) {
      startTimer();
      return;
    }
    chgRec.record[COLUMN_INRANGE].recVal = data["$ATPERCH"];
    if (data["$ATPERCH"] == 'TRUE') {
      chgRec.record[COLUMN_INRANGE].recColor = GRIDCOLOR_NORMAL;
    } else {
      chgRec.record[COLUMN_INRANGE].recColor = GRIDCOLOR_ERROR;
    }
    if (data["$COMMENT"] != "********") {
      chgRec.record[COLUMN_COMMENT].recVal = data["$COMMENT"];
    }
    chgRec.record[COLUMN_ENABLE].recVal = data["$ENABLED"];
    chgRec.record[COLUMN_HOME].recVal = data["$HOMEPOS"];
    chgRec.record[COLUMN_SIGNAL_IO].recVal = data["$DOUT_TYPE"];
    chgRec.record[COLUMN_SIGNAL_IO_NUM].recVal = data["$DOUT_INDX"];

    for (var i = 1; i <= REF_POS_AXIS.length; i++) {
      var unitName = 'unit' + i;
      var dispValPos;
      var dispValTol;
      if (chgRec[unitName] == 'TRUE') {
        dispValPos = Math.round(parseFloat(data["$PERCHPOS"]["J" + i]).toFixed(7) * (180 / Math.PI) * 1000) / 1000;
        dispValTol = Math.round(parseFloat(data["$PERCHTOL"]["J" + i]).toFixed(7) * (180 / Math.PI) * 1000) / 1000;
      } else {
        dispValPos = Math.round(parseFloat(data["$PERCHPOS"]["J" + i]).toFixed(7) * 1000) / 1000;
        dispValTol = Math.round(parseFloat(data["$PERCHTOL"]["J" + i]).toFixed(7) * 1000) / 1000;
      }
      chgRec.record[REF_POS_CLM_ARR.indexOf('axis' + i)].recVal = dispValPos;
      chgRec.record[REF_POS_CLM_ARR.indexOf('range' + i)].recVal = dispValTol;
    }
    refPosGrid.updateRecord(tgtRow, chgRec);
  }
  startTimer();
}

/*
 * Get Reference positions Sysvar Value Callback(XVR).
 */
function getInrangeCallback(max, data_arr) {
  if (data_arr == null ||
    data_arr.length == 0) {
    return;
  }
  var label = Object.keys(data_arr)[0].toString();
  var grp = Number(label.slice(0, 1));
  stopTimer();
  if (grp != g_select_group) {
    onTimerInrange();
  } else {
    if (data_arr[g_select_group + ",1"] != undefined) {
      setRefXVRData(data_arr);
      for (var j = g_current_item_no_start; j < Object.keys(data_arr).length + 1; j++) {
        var tgtRow = refPosGrid.getColumn(REF_POS_CLM_ARR[COLUMN_INDEX]).indexOf(String(j)) + 1;
        var chgRec = refPosGrid.getRecord(tgtRow);
        var data = data_arr[grp + "," + j];

        if ((j < g_current_item_no_start) ||
          (j > g_current_item_no_end) ||
          (typeof chgRec === "undefined")) {
          startTimer();
          return;
        }
        if (chgRec.record[COLUMN_INRANGE].recVal != data["$ATPERCH"]) {
          chgRec.record[COLUMN_INRANGE].recVal = data["$ATPERCH"];
          if (data["$ATPERCH"] == 'TRUE') {
            chgRec.record[COLUMN_INRANGE].recColor = GRIDCOLOR_NORMAL;
          } else {
            chgRec.record[COLUMN_INRANGE].recColor = GRIDCOLOR_ERROR;
          }
          refPosGrid.updateRecord(tgtRow, chgRec);
        }
      }
    }
    startTimer();
  }
}

/*
 * Get Reference positions Axis Unit Callback.
 */
function getRefPosAxisUnitCallback(prog, sysvar, type, val, grp, unit_name) {
  if (grp != g_select_group) {
    return;
  }
  for (var i = 1; i <= g_current_item_no_end - g_current_item_no_start + 1; i++) {
    var tgtRec = refPosGrid.getRecord(i);
    tgtRec[unit_name] = val;
    refPosGrid.updateRecord(i, tgtRec);
  }
  var chkRec = refPosGrid.getRecord(1);
  for (var i = 1; i <= REF_POS_AXIS.length; i++) {
    if (chkRec['unit' + i] == "") {
      return;
    }
  }
  getRefPosValueCallback(getRefXVRData());
}

/*
 * Get Current Angle Callback
 */
function getCurAngCallback(grp, str, id) {
  var updRec = refPosGrid.getRecord(refPosGrid.getColumn(REF_POS_CLM_ARR[COLUMN_INDEX]).indexOf(id) + 1);
  if (grp != g_select_group
    || Number(id) < g_current_item_no_start
    || Number(id) > g_current_item_no_end) {
    return;
  }
  var ang = getPosRegJointData(str);
  for (var i = 1; i <= REF_POS_AXIS.length; i++) {
    eval('var clm = ' + 'COLUMN_AXIS' + i + ';');
    var axisNum = REF_POS_CLM_ARR[clm].replace(/[^0-9^\.]/g, "");
    var unitName = 'unit' + axisNum;
    if (ang[REF_POS_AXIS[i - 1]] !== "") {
      if (updRec[unitName] == 'TRUE') {
        updRec.record[clm].recVal = ang[REF_POS_AXIS[i - 1]];
      }
    } else {
      updRec.record[clm].recVal = 0;
    }
  }
  updRec.force = false;
  recRefPos([updRec]);
}

