      'posReg_val2': {'min':parent.g_regIOlimit_val[parent.ORDER_REGI].min, 'max':parent.g_regIOlimit_val[parent.ORDER_REGI].max},
      'posReg_val3': {'min': 1, 'max': 16383},
    };
    function textboxCallback(id, type, value){
      parent.detailFix(id, type, value);
    }
    function selectCallback(id, type, value) {
      switch (id) {
        case 'uframe_val':
          callBackUframeSelect(id, type, value);
          break;
        case 'posReg_val':
          callBackPosRegSelect(id, type, value);
          break;
        default:
          break;
      }
    }
    //select uframeArea value for show&hide
    function callBackUframeSelect(id, type, value){
      top.IHMIComponents.cf.turnOnOffClass(document.getElementById('uframeTextArea1'), "hide", !(value === selectList['disp'][0][1]));
      top.IHMIComponents.cf.turnOnOffClass(document.getElementById('uframeTextArea2'), "hide", !(value === selectList['disp'][1][1]));
      top.IHMIComponents.cf.turnOnOffClass(document.getElementById('uframeTextArea3'), "hide", !(value === selectList['disp'][2][1]));
      parent.detailFix(id, type, value);
    }
    //select posRegArea value for show&hide
    function callBackPosRegSelect(id, type, value){
      top.IHMIComponents.cf.turnOnOffClass(document.getElementById('posRegTextArea1'), "hide", !(value === selectList['disp'][0][1]));
      top.IHMIComponents.cf.turnOnOffClass(document.getElementById('posRegTextArea2'), "hide", !(value === selectList['disp'][1][1]));
      top.IHMIComponents.cf.turnOnOffClass(document.getElementById('posRegTextArea3'), "hide", !(value === selectList['disp'][2][1]));
      parent.detailFix(id, type, value);
    }
    window.onload = function(){
      for (var key in componentList) {
        if ('data' in componentList[key]) {  // select
          document.getElementById(key).setCallback(selectCallback);
        } else {  // textbox
          document.getElementById(key).setCallback(textboxCallback);
        }
      }
      top.IHMIComponents.cf.KeyEvent.attachHandlers(window);
      document.getElementById('group_val').refresh(null, "20", true);
      document.getElementById('group_val').setCallback(textboxCallback);
      var headerTextBox = document.getElementsByClassName("headerTextbox");
      var headerTextBoxLen = headerTextBox.length
      var indexNo = 0;
      if (parent.gcurrentLang == 'eg') {
        for (var i =0; i < headerTextBoxLen; i++) {
          document.getElementsByClassName("headerTextbox")[indexNo].className = "headerTextboxEg";
        }
      }
    }
