    function callbackSelect(id, type, value){
      if (id == (ELEM_PREFIX + '.variable_select')) {
        switchValueSelectDisplay(value);
      } else if (id == (ELEM_PREFIX + '.value_select1')) {
        switchValueDisplayPattern1(value);
