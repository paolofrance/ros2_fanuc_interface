    function callbackRadioSelect(id, type, value){
      var disables = {'seconds': true, 'condition1': true, 'condition2' : true};
      var waitValue = document.getElementById('wait_sec').getValue();
      var condValue = document.getElementById('conditionValue').getValue();
      var timeOutVal = document.getElementById('time_out').getValue();
      var select_time_val = document.getElementById('time_out_option').getValue();
      if (value == '1') {   // select wait_sec
        disables['seconds'] = false;
      } else if (value == '2'){ // select conditions1(select+textbox)
        disables['condition1'] = false;
      } else {              // select conditions2(free textbox)
        disables['condition2'] = false;
      }
      document.getElementById('wait_sec').refresh(waitValue, "0.0", "327.67", disables['seconds']);
      document.getElementById('conditionValue').refresh(condValue, 255, disables['condition2']);
      document.getElementById('time_out').refresh(timeOutVal, "0.0", "327.67", disables['condition1']);
      document.getElementById('time_out_option').refresh(null, select_time_val, disables['condition1']);
      var str, key, elm, val, range, selSwicth;
      for(var cnt in paramSetList) {
        key = Object.keys(paramSetList[cnt])[0];
        elm = document.getElementById(ELEM_PREFIX + key);
        val = elm.getValue();
        if('data' in paramSetList[cnt][key]) {  // select
          elm.refresh(paramSetList[cnt][key].data, val, null, disables['condition1']);
          if(key.indexOf('.operator_select') == -1) {
            if(key == '.variable_select') {
              selSwicth = Number(paramSetList[cnt][key].data[val][1]);
              str = paramSetList[cnt][key].data[val][0];
            }
            else {
              if((key == '.value_select1' && selSwicth >= 0 && selSwicth <= 4) ||
                 (key == '.value_select2' && selSwicth >= 5)) {
                str = paramSetList[cnt][key].data[val][0];
              }
            }
          }
        }
        else {
          range = [];
          if(key == '.value_const') {
            range = paramSetList[cnt][key];
          }
          else {
            if(str && str != parent.DTL_CONSTANT && str != 'ON' && str != 'OFF') {
              range = parent.g_regIOlimit_val[str];
            }
          }
          if (range.length != 0) {
            if (str == parent.ORDER_REGI) {
              parent.editorAPI.funcs.refreshLocalRTBComp(elm, val, disables['condition1'], true);
            } else {
              elm.refresh(val, range.min, range.max, disables['condition1']);
            }
          }
        }
      }
      parent.detailFix(id, type, value);
    }
