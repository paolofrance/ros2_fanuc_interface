          help_width = "560px";
        }else{
          help_width = "640px";
        }
      }else{
        help_title = "Not Suppoted";
        help_width = "300px";
        help_height = "100px";
      }
      var customizeObj = {
        callerWindowObj: window,
        width: help_width,
