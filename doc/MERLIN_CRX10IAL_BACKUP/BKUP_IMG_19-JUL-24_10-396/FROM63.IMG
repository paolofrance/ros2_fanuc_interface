      }
    }
    top.IHMIComponents.cf.KeyEvent.attachHandlers(window);
    document.getElementById('group_val').refresh(null, "20", true);
    document.getElementById('group_val').setCallback(textboxCallback);
    var headerTextBox = document.getElementsByClassName("headerTextbox");
    var headerTextBoxLen = headerTextBox.length
    var indexNo = 0;
