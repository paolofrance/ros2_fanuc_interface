    function popupBtnCallback(id, operation) {
      switch(id){
        case 'btn1':
          var value = document.getElementById('time_out').getValue();
          var setValue = parseFloat(value).toFixed(2);
          setValue = setValue.replace(".","");
          top.rpcmc_setVar(top.SYSNAME_C, '$WAITTMOUT', setValue );
        break;
        case 'btn2':
        case'closeBtn':
          document.getElementById('time_out').refresh(parent.g_timeOutVal, "0.0", "327.67", false);
        break;
        default:
        break;
      }
    }
