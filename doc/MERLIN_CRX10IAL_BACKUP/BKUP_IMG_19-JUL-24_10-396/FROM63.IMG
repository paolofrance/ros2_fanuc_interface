    function textboxCallback(id, type, value) {
      parent.detailFix(id, type, value);
    }
    function selectSpeedMethod(id, type, value) {
      var taxtArea = document.getElementsByClassName("speedTextBoxAreas");
      for(var cnt = 0; cnt < taxtArea.length; cnt++) {
        top.IHMIComponents.cf.addClass(taxtArea[cnt], "hide");
        if (value === selectOptionListSpeedMethod[cnt][1]) {
          top.IHMIComponents.cf.removeClass(taxtArea[cnt], "hide");
        }
      }
      var unitArea = document.getElementById('unit_l');
      if(value === '3') {
        top.IHMIComponents.cf.addClass(unitArea, "hide");
      } else {
        top.IHMIComponents.cf.removeClass(unitArea, "hide");
      }
    }
    function selectSpeedUnit(id, type, value) {
      var targetForm;
      var formRoot = document.getElementById("speedConstTextAreas");
      for (var i = 0, targetForm = top.IHMIComponents.cf.firstElementNode(formRoot); i < formRoot.childElementCount && targetForm !== null; i++, targetForm = top.IHMIComponents.cf.nextElementNode(targetForm)) {
        top.IHMIComponents.cf.addClass(targetForm, "hide");  
        if (value === selectOptionListSpeedUnit[i][1]) {
          top.IHMIComponents.cf.removeClass(targetForm, "hide");
        }
      }
    }
    function selectStopUnit(id, type, value) {
      var targetForm;
      var formRoot = document.getElementById("stopTextBoxAreas");
      for (var i = 0, targetForm = top.IHMIComponents.cf.firstElementNode(formRoot); i < formRoot.childElementCount && targetForm !== null; i++, targetForm = top.IHMIComponents.cf.nextElementNode(targetForm)) {
        top.IHMIComponents.cf.addClass(targetForm, "hide"); 
        if (value != '0' && value === selectOptionListStop[i + 1][1]) {
          top.IHMIComponents.cf.removeClass(targetForm, "hide");
        }
      }
    }
    function callbackSelect(id, type, value) {
      switch(id) {
        case "speed_method":
          selectSpeedMethod(id, type, value);
          break;
        case "unit_l":
          selectSpeedUnit(id, type, value);
          break;
        case "stop_l":
          selectStopUnit(id, type, value);
          break;
        case "nameraka_l":
        default:
          break;
      }
      parent.detailFix(id, type, value);
    }
    function callBackPosition(id, type, value) {
      parent.detailFix(id, type, value);
    }
    function callbackFormList(id, operation, arg) {
      var addFormArray = arg.addFormArray;
      var addElem;
      if (operation === "addRow") {
        for (var i=0; i < addFormArray.length; i++) {
          addElem = document.getElementById(addFormArray[i].id);
          if (addFormArray[i].type === "label") {
            addElem.refresh("Additional motion" + arg.numOfRow + ":", "null", false, false);
          }
          else if (addFormArray[i].type === "textboxString") {
            addElem.refresh("", 255, false);
            addElem.setCallback(textboxCallback);
          }
          else if(addFormArray[i].type === "select") {
            document.getElementById(addFormArray[i].id).refresh(parent.addMotionCondVarSel, 0, null, false);
            document.getElementById(addFormArray[i].id).setCallback(callbackSelectAddMotion);
            document.getElementById('addmotion_select_' + (arg.numOfRow-1)).style.display='none'
            if (parent.gcurrentLang == 'eg') {
              addElem.className = "select touch input-form-list-selectEg";
            } else {
              addElem.className = "select touch input-form-list-select";
            }
          }
        }
        if (arg.isUserOpe) {
          document.getElementById("option_formlist").scrollToLastLine(document.getElementsByClassName("content")[0]);
        }
        parent.inputfocusEvt_Reregister();
      } else if (operation === "removeRow") {
        parent.detailFix(id, operation, arg);
        document.getElementById("addmotion_select_" + arg.numOfRow).refresh(parent.addMotionCondVarSel, 0, null, false);
        document.getElementById("addmotion_select_" + arg.numOfRow).setCallback(callbackSelectAddMotion);
        document.getElementById('addmotion_select_' + arg.numOfRow).style.display='flex';
      } else if (operation === "removeAllRows") {
        document.getElementById("addmotion_select_" + arg.numOfRow).refresh(parent.addMotionCondVarSel, 0, null, false);
        document.getElementById("addmotion_select_" + arg.numOfRow).setCallback(callbackSelectAddMotion);
        document.getElementById('addmotion_select_' + arg.numOfRow).style.display='flex';
      }
    }
    function callbackSelectAddMotion(id, type, value) {
      init(doc, 'label', id+'.label', {'className':'input-form-list-select select-label'});
      var ids = id.match(/[0-9]*$/)[0];
      var targetName = document.getElementById('addmotion_'+ids).id;
      if (value === '0') {
        document.getElementById(targetName).refresh('', 255, false);
      }
      else {
        var motion = parent.addMotionCondVarSel.filter(function(data) {return value === data[1]});
        if(motion.length) {
          document.getElementById(targetName).refresh(motion[0][0], 255, false);
        }
      }
      parent.detailFix(id, type, value);
    }
    function callBackSelectTypeReg(id, type, value) {
      var vreg = "1";
      for(var cnt in VRIndirectOrder) {
        VRIndirectOrder[cnt][4] = '0';
      }
      getRegisterInfo(vreg);
      top.IHMIComponents.cf.turnOnOffClass(document.getElementById("savedNo"), 'hide', value === POS_VREG_UNDEFINE);
      parent.voffsetArg.vreg = vreg;
      parent.detailFix(id, type, value);
    }
    function callBackSelectVisReg(id, type, value) {
      parent.voffsetArg.vreg = value;
      top.IHMIComponents.cf.turnOnOffClass(document.getElementById('indexarea'), 'hide', VRList[Number(value) - 1][0] != selectVRListIndirect[0][0] && VRList[Number(value) - 1][0] != selectVRListIndirect[1][0]);
      if(VRList[Number(value) - 1][0] === selectVRListIndirect[0][0]) {
        parent.editorAPI.funcs.refreshLocalRTBComp(document.getElementById('index_val'), String(VRIndirectOrder[0][4]), false, true);
      }
      else if(VRList[Number(value) - 1][0] === selectVRListIndirect[1][0]) {
        document.getElementById('index_val').refresh(String(VRIndirectOrder[1][4]), String(VRIndirectOrder[1][2]), String(VRIndirectOrder[1][3]), false);
      }
      parent.detailFix(id, type, value);
    }
    function accordion(changeId, hiddenId) {
      var hasClassDesc = top.IHMIComponents.cf.hasClass(document.getElementById(changeId), 'position-folding-bg-desc');
      top.IHMIComponents.cf.turnOnOffClass(document.getElementById(changeId + 'Open'), 'hide', hasClassDesc);
      top.IHMIComponents.cf.replaceClass(document.getElementById(changeId),
      hasClassDesc ? 'position-folding-bg-desc' : 'position-folding-bg-asc',
      hasClassDesc ? 'position-folding-bg-asc' : 'position-folding-bg-desc');
      if (hiddenId !== '') top.IHMIComponents.cf.turnOnOffClass(document.getElementById(hiddenId), 'hide', !hasClassDesc);
    }
    function getRegisterInfo(vreg, indirect) {
      top.IHMIComponents.cf.IUIFRequest(window, { request: "get_reginfo" }).send(function (response) {
        var registerInfo = getResponseJson(response);
        if (registerInfo.status != "") {
          top.IHMIComponents.cf.alertMessage(registerInfo.status, "Error", null, window.parent, null);
        }
        else {
          // eslint-disable-next-line no-inner-declarations
          function createRegArray(comments) {
            return comments.map((comment, index) => {
            return [top.IHMIComponents.cf.format("%s: %s", index + 1, comment), index + 1];
            });
          }
          var vregs = createRegArray(registerInfo.vregs);
          for(var cnt = 0; cnt < selectVRListIndirect.length; cnt++) {
            vregs.push(selectVRListIndirect[cnt]);
            vregs[vregs.length - 1][1] = vregs.length;
            VRIndirectOrder[cnt][1] = vregs.length;
          }
          top.IHMIComponents.cf.turnOnOffClass(document.getElementById('indexarea'), 'hide', vreg != POS_VREG_INDIRECT_R && vreg != POS_VREG_INDIRECT_AR);
          var idxInitParam = {setVal: '0', min: '1', max: '16383'};
          indirect = indirect === '...' ? '0' : indirect;
          if(vreg === POS_VREG_INDIRECT_R) {
            vreg = String(VRIndirectOrder[0][1]);
            idxInitParam = {setVal: indirect, min: String(VRIndirectOrder[0][2]), max: String(VRIndirectOrder[0][3])};
            VRIndirectOrder[0][4] = indirect;
            parent.editorAPI.funcs.refreshLocalRTBComp(document.getElementById('index_val'), idxInitParam['setVal'], false, true);
          }
          else if(vreg === POS_VREG_INDIRECT_AR) {
            vreg = String(VRIndirectOrder[1][1]);
            idxInitParam['setVal'] = indirect;
            VRIndirectOrder[1][4] = indirect;
            document.getElementById('index_val').refresh(idxInitParam['setVal'], idxInitParam['min'], idxInitParam['max'], false);
          }
          else if(vreg === POS_VREG_UNDEFINE) {
            vreg = "1";
            document.getElementById('index_val').refresh(idxInitParam['setVal'], idxInitParam['min'], idxInitParam['max'], false);
          }
          document.getElementById('selVRIndex').refresh(vregs, vreg, null, false);
          VRList = [];
          VRList = vregs;
        }
      });
    }
    function getResponseJson(response) {
      eval("var data =" + response.firstChild.textContent);
      // eslint-disable-next-line no-undef
      return data;
    }
    function initPosLeft() {
      if(top.home.g_insOption.arcTool && !selectOptionListSpeedMethod[3]) {
        selectOptionListSpeedMethod.push([ 'Weld speed', "3" ]);
      }
      document.getElementById("speed_method").refresh(selectOptionListSpeedMethod, 0, null, false);
      document.getElementById("unit_l").refresh(selectOptionListSpeedUnit, 1, null, false);
      var taxtArea = document.getElementsByClassName("speedTextBoxAreas");
      for(var cnt = 0; cnt < taxtArea.length; cnt++) {
        if(taxtArea[cnt].id != 'speedConstTextAreas') {
          parent.editorAPI.funcs.refreshLocalRTBComp(taxtArea[cnt].firstElementChild, '1', false, true);
        }
        top.IHMIComponents.cf.addClass(taxtArea[cnt], "hide");
        if (selectOptionListSpeedMethod[cnt][1] === '0') {
          top.IHMIComponents.cf.removeClass(taxtArea[cnt], "hide");
        }
      }
      var targetForm;
      var formRoot = document.getElementById("speedConstTextAreas");
      for (var i = 0, targetForm = top.IHMIComponents.cf.firstElementNode(formRoot); i < formRoot.childElementCount && targetForm !== null; i++, targetForm = top.IHMIComponents.cf.nextElementNode(targetForm)) {
        if(targetForm.classList.contains('speedTextBoxAreas') == false) {
          targetForm.firstElementChild.refresh(parent.spdValMinMax[selectOptionListSpeedUnit[i][0]][1], parent.spdValMinMax[selectOptionListSpeedUnit[i][0]][0], parent.spdValMinMax[selectOptionListSpeedUnit[i][0]][1], false);
