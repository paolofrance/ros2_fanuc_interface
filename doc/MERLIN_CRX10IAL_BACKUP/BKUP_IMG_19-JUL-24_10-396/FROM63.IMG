    function textboxCallback(id, type, value) {
      parent.detailFix(id, type, value);
    }
    function callbackSelect(id, type, value) {
      parent.detailFix(id, type, value);
    }
    function callbackRadioSelect(id, type, value) {
      var index = (value - 1);
      var currentValue;
      var disable;
      for (var i=0; i<outputComponent.length; i++) {
        disable = (index == i) ? false: true;
        for (var key in outputComponent[i]) {
          currentValue = document.getElementById(key).getValue();
          if ('data' in outputComponent[i][key]) {  // select
            document.getElementById(key).refresh(outputComponent[i][key].data, currentValue, null, disable);
          } else if ('length' in outputComponent[i][key]) {  // textboxString
            document.getElementById(key).refresh(currentValue, outputComponent[i][key].length, disable);
          } else {        // textBoxInteger
            document.getElementById(key).refresh(currentValue, outputComponent[i][key].min, outputComponent[i][key].max, disable);
          }
        }
      }
      parent.detailFix(id, type, value);
    }
    window.onload = function(){
      document.getElementById('output_list').setCallback(callbackRadioSelect);
      for (var i=0; i<outputComponent.length; i++) {
        for (var key in outputComponent[i]) {
          if ('data' in outputComponent[i][key]) {  // select
            document.getElementById(key).setCallback(callbackSelect);
            document.getElementById(key).refresh(outputComponent[i][key].data, 0, null, false);
          } else {  // textbox
            document.getElementById(key).setCallback(textboxCallback);
            if ('length' in outputComponent[i][key]) {  // textboxString
              document.getElementById(key).refresh('', outputComponent[i][key].length, false);
