    function init_selectIndex(prefix, disabled){
      for (var key in componentList[IDX_SELECT]){ // setting for select box
        document.getElementById(prefix + key).refresh(componentList[IDX_SELECT][key], 0, null, disabled);
      }
      for (var key in componentList[IDX_TEXTBOX]){ // setting for textbox
        var initVal = (key == '.variable_index') ? '0' : '1';
        if (key == '.variable_index' || key == '.value_index') {
          parent.editorAPI.funcs.refreshLocalRTBComp(document.getElementById(prefix + key), initVal, disabled, true);
