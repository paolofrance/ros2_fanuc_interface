      return text;
    }
    function selectValset(selectVal) {
      if(selectVal != SEL_VAL_IDX_0) {
        var key = selectRegiValList[selectVal][0];
        if (key == parent.ORDER_REGI) {
          parent.editorAPI.funcs.refreshLocalRTBComp(document.getElementById('regi_val_index'), inputVal[key].val, false, true);
        } else {
          document.getElementById('regi_val_index').refresh(inputVal[key].val, parent.g_regIOlimit_val[key].min, parent.g_regIOlimit_val[key].max, false);
        }
      }
    }
