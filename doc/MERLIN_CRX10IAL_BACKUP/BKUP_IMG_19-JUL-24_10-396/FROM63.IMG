      if (value == VALUE_CONST_PATTERN) {
        document.getElementById('indexarea').style.display = 'none';
        document.getElementById('constarea').style.display = 'block';
      } else{
        document.getElementById('indexarea').style.display = 'block';
        document.getElementById('constarea').style.display = 'none';
      }
      selectValset(value);
      parent.detailFix(id, type, value);
    }
    function callbackRadioSelect(id, type, value){
      var currentValue;
      var selectVal;
      var disable = (value == '1') ? false : true;
      for (key in regComponent) {
        currentValue = document.getElementById(key).getValue();
        if ('data' in regComponent[key]) {  // select
          selectVal = (currentValue == '0') ? regComponent[key].data[1][0] : regComponent[key].data[currentValue][0];
          document.getElementById(key).refresh(regComponent[key].data, currentValue, null, disable);
        } else {  // textbox
          if (key == 'regi_index') {
            parent.editorAPI.funcs.refreshLocalRTBComp(document.getElementById(key), currentValue, disable, true);
          } else if (key == 'regi_val_index') {
            if (selectVal == parent.ORDER_REGI) {
              parent.editorAPI.funcs.refreshLocalRTBComp(document.getElementById(key), currentValue, disable, true);
            } else {
              document.getElementById(key).refresh(currentValue, parent.g_regIOlimit_val[selectVal].min, parent.g_regIOlimit_val[selectVal].max, disable);
            }
          } else {
            document.getElementById(key).refresh(currentValue, regComponent[key].min, regComponent[key].max, disable);
          }
        }
      }
      currentValue = document.getElementById('regi').getValue();
      document.getElementById('regi').refresh(currentValue, 255, !disable);
