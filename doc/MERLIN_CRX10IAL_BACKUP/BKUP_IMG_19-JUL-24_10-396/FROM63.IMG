        parent.ORDER_ARGUMENT + '['
      ]
    };
    var componentList = {
      'uframe_val': {data: selectList['disp']},
      'uframe_val1': {'min': parent.g_regIOlimit_val['UFRAME'].min, 'max': parent.g_regIOlimit_val['UFRAME'].max},
      'uframe_val2': {'min':parent.g_regIOlimit_val[parent.ORDER_REGI].min, 'max':parent.g_regIOlimit_val[parent.ORDER_REGI].max},
      'uframe_val3': {'min': 1, 'max': 16383},
      'posReg_val': {data: selectList['disp']},
