<script type="text/javascript">
  var g_popupFrm;
  var appendArg = {};
  var g_checkbox_input_id = 'checkbox_speed_clamp.input';
  var g_checkbox_id = 'checkbox_speed_clamp';
  var g_yesbtn_id = 'yes_btn1';
  var g_yesbtn_id_confirm = 'yes_btn2';
  var g_yesbtn_id_favorite = 'favoupdate_btn';
  var g_btn_id_remark = 'remark_ok';
  var g_btn_id_remarking = 'remarking';
  var g_btn_id_unremarking = 'unremarking';
  var g_yesbtn_id_notDisplayable = 'yes_btn3';
  //pop-up screen types
  var DEF_SCRN_TYP = {
    SPEED_CLAMP: 1,//speed clamp
    NOT_SUPPORT: 2,//not support
    ALERT      : 3,//alert_message
    CONFIRM    : 4,//confirmation
    REPLACE_MTN: 5,//replace
    FAVO_UPDATE: 6,//favorite update
    REMARK     : 7,//remark
    NOT_DISPLAYABLE: 8 //not displayable
  };
  function clickBtnCallback(id, operation) {
    var popup_screen_type = appendArg.popup_screen_type;
    switch(id) {
      case g_yesbtn_id://button for speed_clamp pop-up screen
        var checkbox_value = document.getElementById(g_checkbox_input_id).checked;
        top.home.cb_sub_close_popup_speed_clamp(checkbox_value);
        break;
      case g_yesbtn_id_confirm://button for conformation pop-up screen
        var func = appendArg.close_cb_function;
        if (typeof func === 'function') {
          func(id, operation, popup_screen_type);
          return;
        }
        break;
      case g_yesbtn_id_favorite://favorite update
        var func = appendArg.close_cb_function;
        if (typeof func === 'function') {
          func(id, operation, popup_screen_type);
          return;
        }
        break;
      case g_btn_id_remark://reamrk or unremark
      case g_btn_id_remarking://mixed all reamrking
      case g_btn_id_unremarking://mixed all unreamrkking
        var func = appendArg.close_cb_function;
        if (typeof func === 'function') {
          func(id, appendArg.opekind);
        }
        break;
      case g_yesbtn_id_notDisplayable: //not displayable
        var func = appendArg.close_cb_function;
        if (typeof func === 'function') {
          func(id, appendArg.opekind);
        }
        break;
      default:
        break;
    }
    top.home.set_popup_screen_frame(popup_screen_type, null);
    g_popupFrm.removePopupFrame();
  }
  function clickReplaceBtnCallback(id) {
    var popup_screen_type = appendArg.popup_screen_type;
    var weldOrder = /^(weldstartml|weldstartca|weldstopml|weldstopca)$/
    var cArgs = {};
    cArgs.afterMtn = id.replace("replace_", "");
    cArgs.orderType = '';
    if (cArgs.afterMtn.match(weldOrder)) cArgs.orderType = 'weld';
    g_popupFrm.setRemoveCallbackArg(cArgs);
    top.home.set_popup_screen_frame(popup_screen_type, null);
    g_popupFrm.removePopupFrame();
  }
  function checkEvtProc(id, operation, checked) {
  }
  function onload_remark(appendArg) {
    document.getElementById("note_remark").innerHTML = appendArg.confirmation_message;
    document.getElementById("remark_cancel").setCallback(clickBtnCallback);
    if(appendArg.opekind == top.home.REMARK_OPE_REMARK) {
      document.getElementById(g_btn_id_remark).setCallback(clickBtnCallback);
      document.getElementById("unity_remark").style.display = 'block';
      document.getElementById("mixed_remark").style.display = 'none';
    }
    else if(appendArg.opekind == top.home.REMARK_OPE_UNREMARK) {
      document.getElementById(g_btn_id_remark).setCallback(clickBtnCallback);
      document.getElementById("unity_remark").style.display = 'block';
      document.getElementById("mixed_remark").style.display = 'none';
    }
    else {
      document.getElementById(g_btn_id_remarking).setCallback(clickBtnCallback);
      document.getElementById(g_btn_id_unremarking).setCallback(clickBtnCallback);
      document.getElementById('note_remark').classList.add('mixed');
      document.getElementById("unity_remark").style.display = 'none';
      document.getElementById("mixed_remark").style.display = 'block';
    }
    document.getElementById("remark").style.display = 'block';
  }
  function onload_not_diplayable(appendArg) {
    var message = appendArg.confirmation_message;
    document.getElementById(g_yesbtn_id_notDisplayable).setCallback(clickBtnCallback);
    document.getElementById("label_message3").refresh(message, "null", false, true);
    document.getElementById("not_displayable").style.display = 'block';
  }
  window.onload = function(){
    g_popupFrm = parent.ihcpPopupFrm;
    g_popupFrm.setCallback(clickBtnCallback);
    appendArg = g_popupFrm.getAppendArg();
    var popup_screen_type = appendArg.popup_screen_type;
    /* Request is canceled. */
    if(top.home.g_popup_frame[popup_screen_type - 1].disp_sts == top.home.DEF_POPUP_SCRN_STS.REQ_CANCEL) {
      /* Delete popup and exit */
      top.home.set_popup_screen_frame(appendArg.popup_screen_type, null);
      g_popupFrm.removePopupFrame();
    }
    switch(popup_screen_type) {
      case DEF_SCRN_TYP.SPEED_CLAMP://speed_clamp
        document.getElementById(g_yesbtn_id).setCallback(clickBtnCallback);
        document.getElementById(g_checkbox_id).setCallback(checkEvtProc);
        document.getElementById(g_checkbox_id).refresh(false, false);
        document.getElementById("speed_clamp").style.display = 'block';
        break;
      case DEF_SCRN_TYP.NOT_SUPPORT://not_support
      case DEF_SCRN_TYP.ALERT://alert_message
        var message = appendArg.confirmation_message;
        document.getElementById("label_message1").refresh(message, "null", false, true);
        document.getElementById("alert_message").style.display = 'block';
        break;
      case DEF_SCRN_TYP.CONFIRM://confirmation
        var message = appendArg.confirmation_message;
        var btnText = appendArg.buttonText;
        document.getElementById(g_yesbtn_id_confirm).refresh(btnText, null, null, null, false, null);
        document.getElementById(g_yesbtn_id_confirm).setCallback(clickBtnCallback);
        document.getElementById("label_message2").refresh(message, "null", false, true);
        document.getElementById("confirmation").style.display = 'block';
        break;
      case DEF_SCRN_TYP.REPLACE_MTN://replace
        var chkTbl =[
          { chkStr: /^(vsnline|vsnjoint)$/, displayAreaId: "replace_vsn_button_area"},
          { chkStr: /^(weldstartml|weldstartca)$/, displayAreaId: "replace_weldstart_button_area", displayWeldMsg: true},
          { chkStr: /^(weldstopml|weldstopca)$/, displayAreaId: "replace_weldstop_button_area", displayWeldMsg: true}
        ];
        var tblSearch = chkTbl.find(function(data) {
          return appendArg.beforeMtn.match(/^[a-z]*/)[0].match(data.chkStr);
        });
        document.getElementById("replace_" + appendArg.beforeMtn).parentElement.remove();
        if(tblSearch) {
          document.getElementById(tblSearch.displayAreaId).style.display = 'inline-flex';
          if (tblSearch.displayWeldMsg === true) {
            document.getElementById("message_weld_replace_mtn").style.display = 'block';
          }
        } else {
          document.getElementById("replace_button_area").style.display = 'inline-flex';
          if (top.home.g_order_option.indexOf("R904") == -1) {
            document.getElementById("replace_spline").parentElement.remove();
          }
        }
        document.getElementById("replace_mtn").style.display = 'block';
        break;
      case DEF_SCRN_TYP.FAVO_UPDATE://favorite update
        var message = appendArg.confirmation_message;
        document.getElementById(g_yesbtn_id_favorite).setCallback(clickBtnCallback);
        document.getElementById("update_message").refresh(message, "null", false, true);
        document.getElementById("favorite_update").style.display = 'block';
        break;
      case DEF_SCRN_TYP.REMARK://remark
        onload_remark(appendArg);
        break;
      case DEF_SCRN_TYP.NOT_DISPLAYABLE:// not displayable
        onload_not_diplayable(appendArg);
        break;
      default :
        break;
    }
    top.home.set_popup_screen_frame(popup_screen_type, g_popupFrm);
  };
</script>
