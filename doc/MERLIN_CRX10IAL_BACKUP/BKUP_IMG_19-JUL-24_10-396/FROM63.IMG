    function request_lof_status(callback, argObj){
      var lang = top.irprogapi.getCurrentLanguage();
      var frameXml = "";
      frameXml += '<?xml version="1.0" encoding="' + top.irprogapi.global.langEncoding[lang] + '"?>\r';
      frameXml += '<XMLCFG>\r<PROG name="*SYSTEM*">\r';
      frameXml += '<VAR name="$LIFSET.$HD[1].$MAKER" />\r'; // add system value name
      frameXml += '<VAR name="$LIFSET.$HD[1].$MODEL" />\r'; // add system value name
      frameXml += '</PROG>\r</XMLCFG>';
      // Create Blob frame CVR
      var fileCvr = new Blob([frameXml], { type: "text/plain" });
      var cbArgs = {};
      cbArgs.sysIndex = 1;
      cbArgs.cbFunc = callback;
      cbArgs.cbObj = argObj;
      top.getXHR('crxeditorinit.cvr', fileCvr, set_lof_state, cbArgs);
    }
