      document.getElementById('wait_sec').setCallback(textboxCallback);
      document.getElementById('conditionValue').setCallback(textboxCallback);
      document.getElementById('jump_select').setCallback(callbackSelect);
      document.getElementById('time_out').setCallback(timeOutCallback);
      document.getElementById('time_out_option').setCallback(listSelectCallback);
      if (parent.gcurrentLang == 'eg') {
        var radiolist = document.getElementsByClassName("radioList");
        radiolist[0].className = "radioListEg";
      } else if (parent.gcurrentLang == 'gr') {
        var radiolist = document.getElementsByClassName("radioList");
        radiolist[0].className = "radioListGr";
      } else if (parent.gcurrentLang == 'fr') {
        var radiolist = document.getElementsByClassName("radioList");
        radiolist[0].className = "radioListFr";
      }
      switchValueSelectDisplay(0);
    };
