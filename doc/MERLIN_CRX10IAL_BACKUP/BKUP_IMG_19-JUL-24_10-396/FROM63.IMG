      for (var cnt = 0; cnt < inputVal.length; cnt++){
        for(var key in inputVal[cnt]) {
          if(cnt == IDX_LEFT) {
            inputVal[cnt][key].val = 0;
          }
          else if(cnt == IDX_RIGHT) {
            if(key != 'ON' && key != 'OFF') {
              inputVal[cnt][key].val = 1;
            }
          }
          else {}
        }
      }
