    function input_val_type(id, operation, arg) {
      var select_type
      select_type = document.getElementById(id).getValue();
      switch(id){
        case 'at_pos':
          if(select_type == 1){
            document.getElementById("gus_out").refresh(0, parent.le_ini[3], parent.le_ini[4], false);
          }else{
            document.getElementById("gus_out").refresh(1, "1", "16383", false);
          }
          parent.detailFix("gus_out", "changed", "1");
          break;
        case 'st_pos':
          if(select_type == 1){
            document.getElementById("shu_off").refresh(0, parent.le_ini[5], parent.le_ini[6]);
          }else{
            document.getElementById("shu_off").refresh(1, "1", "16383", false);
          }
          parent.detailFix("shu_off", "changed", "1");
          break;
        case 'sof_pos':
          if(select_type == 1){
            document.getElementById("sof_chk").style.display = "block";
            document.getElementById("sof_reg").style.display = "none";
            document.getElementById("stand_off").refresh(false, false);
            parent.detailFix("stand_off", "changed", "1");
          }else{
            document.getElementById("sof_chk").style.display = "none";
            document.getElementById("sof_reg").style.display = "block";
            document.getElementById("stand_off_reg").refresh(1, "1", "16383", false);
            parent.detailFix("stand_off_reg", "changed", "1");
          }
          break;
        case 'lof_pos':
          if(select_type == 1){
            document.getElementById("lof_chk").style.display = "block";
            document.getElementById("lof_reg").style.display = "none";
            document.getElementById("laser_off").refresh(false, false);
            parent.detailFix("laser_off", "changed", "1");
          }else{
            document.getElementById("lof_chk").style.display = "none";
            document.getElementById("lof_reg").style.display = "block";
            document.getElementById("laser_off_reg").refresh(1, "1", "16383", false);
            parent.detailFix("laser_off_reg", "changed", "1");
          }
          break;
        default:
          break;
      }
    }
    function select_slop_down(id, operation, arg) {
      var select_type
      select_type = document.getElementById(id).getValue();
      if(select_type == 1){
        document.getElementById("slpd_val").refresh("1", 1, 10, false);
      }else{
        document.getElementById("slpd_val").refresh("1", "1", "16383", false);
      }
      parent.detailFix("slpd_val", "changed", "1");
    }
    function request_lof_status(callback, argObj){
      var lang = top.irprogapi.getCurrentLanguage();
      var frameXml = "";
      frameXml += '<?xml version="1.0" encoding="' + top.irprogapi.global.langEncoding[lang] + '"?>\r';
      frameXml += '<XMLCFG>\r<PROG name="*SYSTEM*">\r';
      frameXml += '<VAR name="$LIFSET.$HD[1].$MAKER" />\r'; // add system value name
      frameXml += '<VAR name="$LIFSET.$HD[1].$MODEL" />\r'; // add system value name
      frameXml += '</PROG>\r</XMLCFG>';
      // Create Blob frame CVR
      var fileCvr = new Blob([frameXml], { type: "text/plain" });
      var cbArgs = {};
      cbArgs.sysIndex = 1;
      cbArgs.cbFunc = callback;
      cbArgs.cbObj = argObj;
      top.getXHR('crxeditorinit.cvr', fileCvr, set_lof_state, cbArgs);
    }
    function set_lof_state(status, getText, cbArgs){
      if(status != 0){ return;}
      var XmlObj = getSysValXmlData(getText, 'VAR');
      var help_title = "";
      var help_width = "";
      var help_height = "200px";
      var lang = top.irprogapi.getCurrentLanguage();
      maker_val = parseInt(XmlObj.getElementsByTagName("VAR")[0].textContent, 10);
      model_val = parseInt(XmlObj.getElementsByTagName("VAR")[1].textContent, 10);
      if((maker_val == 3) && (model_val == 1)){
        help_title = "Laser ON output";
        if(lang == 'jp'){
          help_width = "580px";
        }else{
          help_width = "560px";
        }
      }else if((maker_val == 3) && (model_val == 2)){
        help_title = "Interlock Ch output";
        if(lang == 'jp'){
          help_width = "560px";
        }else{
          help_width = "640px";
        }
      }else{
        help_title = "Not Suppoted";
        help_width = "300px";
        help_height = "100px";
      }
      var customizeObj = {
        callerWindowObj: window,
        width: help_width,
        height: help_height,
        designpattern: "mouse",
        appendArg: { showHelpTitle: help_title },
      }
      openHelp(customizeObj);
    }
    window.onload = function(){      
      initPosLeft();
      initPosRight();
      for(var i = 0; i < inputList_left.length; i++) {
        for(var cnt = 0; cnt < inputList_left[i].length; cnt++) {
          if(i === 0) {
            document.getElementById(inputList_left[i][cnt]).setCallback(textboxCallback);
          }
          else {
            document.getElementById(inputList_left[i][cnt]).setCallback(callbackSelect);
          }
        }
      }
      for(var i = 0; i < inputList_right.length; i++) {
        for(var cnt = 0; cnt < inputList_right[i].length; cnt++) {
          if(i === 0) {
            document.getElementById(inputList_right[i][cnt]).setCallback(textboxCallback);
          }
          else {
            document.getElementById(inputList_right[i][cnt]).setCallback(callbackSelect);
          }
        }
      }
      document.getElementById("option_formlist").setCallback(callbackFormList);
      if (parent.gcurrentLang == 'eg') {
        document.getElementById("option_formlist").refresh(optionFormList, null, parent.DETAIL_ADD_MOTION_MAX, "660px");
        document.getElementById("addmotion_select_1").className = "select touch input-form-list-selectEg";
      } else if (parent.gcurrentLang == 'gr') {
        var headerTextBox = document.getElementsByClassName("headerTextbox");
        var headerTextBoxLen = headerTextBox.length
        var indexNo = 0;
        for (var i =0; i < headerTextBoxLen; i++) {
          document.getElementsByClassName("headerTextbox")[indexNo].className = "headerTextboxGr";
        }
        document.getElementById("option_formlist").refresh(optionFormList, null, parent.DETAIL_ADD_MOTION_MAX, "678px");
        document.getElementById("addmotion_select_1").className = "select touch input-form-list-select";
      } else if (parent.gcurrentLang == 'fr') {
        document.getElementById("tlsp_label").style.width = "200px";
        var headerTextBox = document.getElementsByClassName("headerTextbox");
        var headerTextBoxLen = headerTextBox.length
        var indexNo = 0;
        for (var i =0; i < headerTextBoxLen; i++) {
          document.getElementsByClassName("headerTextbox")[indexNo].className = "headerTextboxFr";
        }
        document.getElementById("option_formlist").refresh(optionFormList, null, parent.DETAIL_ADD_MOTION_MAX, "740px");
        document.getElementById("addmotion_select_1").className = "select touch input-form-list-select";
      } else {
        document.getElementById("option_formlist").refresh(optionFormList, null, parent.DETAIL_ADD_MOTION_MAX, "560px");
        document.getElementById("addmotion_select_1").className = "select touch input-form-list-select";
      }
      document.getElementById("gus_out").setCallback(textboxCallback);
      document.getElementById("shu_off").setCallback(textboxCallback);
      document.getElementById("stand_off").setCallback(textboxCallback);
      document.getElementById("stand_off_reg").setCallback(textboxCallback);
      document.getElementById("laser_off").setCallback(textboxCallback);
      document.getElementById("laser_off_reg").setCallback(textboxCallback);
      document.getElementById("slope_down").setCallback(textboxCallback);
      document.getElementById("slpd_val").setCallback(textboxCallback);
      document.getElementById("at_pos").setCallback(input_val_type);
      document.getElementById("st_pos").setCallback(input_val_type);
      document.getElementById("sof_pos").setCallback(input_val_type);
      document.getElementById("lof_pos").setCallback(input_val_type);
      document.getElementById("slpd_edit").setCallback(select_slop_down);
      if(parent.pre_ls_ini[0] == 14){
        document.getElementById("shu_off_area").style.display = "none";
        document.getElementById("stand_off_area").style.display = "none";
        document.getElementById("laser_off_area").style.display = "flex";
      }else{
        document.getElementById("shu_off_area").style.display = "flex";
        document.getElementById("stand_off_area").style.display = "flex";
        document.getElementById("laser_off_area").style.display = "none";
      }
      var initArgs = parent.loadParam_posCmp_initArg(parent.g_program_data[parent.activeTarget].position[0].number,
                                              parent.g_program_data[parent.activeTarget].position[0].kind,
                                              document.getElementById("robotPos"));
      if(!parent.unredo_flg || parent.g_RO_flg || !parent.g_TPenbl_flg) {
        initArgs.fold = false;
      }
      document.getElementById("robotPos").refresh(initArgs);
      select_tab(1);
    };
