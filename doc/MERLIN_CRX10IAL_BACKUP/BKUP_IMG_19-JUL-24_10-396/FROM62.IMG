            help_width = "580px";
          }else{
            help_width = "700px";
          }
          break;
        case 3:
          request_lof_status();
          return;
        case 4:
          help_title = "Stand-by OFF";
          if(lang == 'jp'){
            help_width = "600px";
          }else{
            help_width = "640px";
          }
          break;
        default:
          break;
      }
      var customizeObj = {
        callerWindowObj: window,
        width: help_width,
        height: "200px",
        designpattern: "mouse",
        appendArg: { showHelpTitle: help_title },
      }
      openHelp(customizeObj);
    }
    function openHelp(customizeObj) {
      top.IHMIComponents.cf.appendPopupFrm(URL_HELP_POPUP, null, window.parent, customizeObj);
    }
