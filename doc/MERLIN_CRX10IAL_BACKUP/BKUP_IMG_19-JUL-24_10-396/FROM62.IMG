        var initArgs = parent.loadParam_posCmp_initArg(parent.g_program_data[parent.activeTarget].position[0].number,
                                                parent.g_program_data[parent.activeTarget].position[0].kind,
                                                document.getElementById("robotPos"));
        if(!parent.unredo_flg || parent.g_RO_flg || !parent.g_TPenbl_flg) {
          initArgs.fold = false;
        }
        document.getElementById("robotPos").refresh(initArgs);
        document.getElementById("positon_view").style.display = "block";
        break;
