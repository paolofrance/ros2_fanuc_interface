      parent.detailFix(id, type, value);
    }
    function callBackPosition(id, type, value) {
      parent.detailFix(id, type, value);
    }
    function getSysValXmlData(getText, tagName) {
      var xmlObj = null;
      var dom = new DOMParser();
      try {
        xmlObj = dom.parseFromString(getText, 'text/xml');
      } catch (e) {
        parent.debug(parent.DBGERROR, e);
        alert('getXML err');
      }
      if (xmlObj != null) {
        if(xmlObj.getElementsByTagName('parsererror').length > 0) {
          parent.debug(parent.DBGERROR, 'xml parse error');
        }
        // success get xml data.
        if(xmlObj.getElementsByTagName(tagName).length == 0) {
          xmlObj = null; // Illegal XML data.
        }
      }
      return xmlObj;
    }
    function select_tab(id_val){
      document.getElementById("le_tab").style.background = "#E6E6E6";
      document.getElementById("le_tab").style.color = "#000000";
      document.getElementById("le_area").style.display = "none";
      switch(id_val){
      case 1:
        document.getElementById("le_tab").style.background = "#0E6EB8";
        document.getElementById("le_tab").style.color = "#FFFFFF";
        document.getElementById("le_area").style.display = "block";
        break;
      default:
        break;
      }
    }
    function input_val_type(id, operation, arg) {
      var select_type
      select_type = document.getElementById(id).getValue();
      switch(id){
        case 'at_pos':
          if(select_type == 1){
            document.getElementById("gus_out").refresh(0, parent.le_ini[3], parent.le_ini[4], false);
          }else{
            document.getElementById("gus_out").refresh(1, "1", "16383", false);
          }
          parent.detailFix("gus_out", "changed", "1");
          break;
        case 'st_pos':
          if(select_type == 1){
            document.getElementById("shu_off").refresh(0, parent.le_ini[5], parent.le_ini[6]);
          }else{
            document.getElementById("shu_off").refresh(1, "1", "16383", false);
          }
          parent.detailFix("shu_off", "changed", "1");
          break;
        case 'sof_pos':
          if(select_type == 1){
            document.getElementById("sof_chk").style.display = "block";
            document.getElementById("sof_reg").style.display = "none";
            document.getElementById("stand_off").refresh(false, false);
            parent.detailFix("stand_off", "changed", "1");
          }else{
            document.getElementById("sof_chk").style.display = "none";
            document.getElementById("sof_reg").style.display = "block";
            document.getElementById("stand_off_reg").refresh(1, "1", "16383", false);
