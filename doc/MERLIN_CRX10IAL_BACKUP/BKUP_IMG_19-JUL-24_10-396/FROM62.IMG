        document.getElementById("laser_off_area").style.display = "none";
      }
      select_tab(1);
    };
    function pushBtnHelpIcon(val) {
      var help_title = "";
      var help_width = "640px";
      var lang = top.irprogapi.getCurrentLanguage();
      switch (val) {
        case 1:
          help_title = "Afterflow time";
          if(lang == 'jp'){
            help_width = "580px";
          }else{
            help_width = "620px";
          }
          break;
        case 2:
          help_title = "Shutter off";
