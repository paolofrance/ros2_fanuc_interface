/* === Dialog moving === */
var dragCmdDialog = false;	// Dragging the header of the dialog.
function isDragCmdDialog() {
	return dragCmdDialog;
}
var prevCmdDialogPos = { x: 0, y: 0 };	// Previous Dialog position.
var crntCmdDialogPos = { x: 0, y: 0 };	// Current Dialog position.
// Mouse down on Dialog header.
function downCmdDialogBox(evt) {
	var tempClient = getEventClient(evt);
	prevDialogPos.x = tempClient.x;
	prevDialogPos.y = tempClient.y;
	dragCmdDialog = true;
}
// Mouse up on Dialog header.
function upCmdDialogBox(evt) {
	dragCmdDialog = false;
}
// Mouse move on Dialog header.
function moveCmdDialogBox(evt) {
	var tempClient = getEventClient(evt);
	crntCmdDialogPos.x = tempClient.x;
	crntCmdDialogPos.y = tempClient.y;
	var diffX = crntCmdDialogPos.x - prevCmdDialogPos.x;
	var diffY = crntCmdDialogPos.y - prevCmdDialogPos.y;
	prevCmdDialogPos.x = tempClient.x;
	prevCmdDialogPos.y = tempClient.y;
	debug(DBGDUMP, "Move Command DialogBox diff(" + diffX + ":" + diffY + ")");
	var crntT = evt.target.parentElement.offsetTop;
	var crntL = evt.target.parentElement.offsetLeft;
	var crntH = evt.target.parentElement.offsetHeight;
	var crntW = evt.target.parentElement.offsetWidth;
	var dlgIdStr = '#' + evt.target.parentElement.id;
	var maxT = $('#pceditor').height();
	var maxL = $('#pceditor').width();
	crntT += diffY;
	if (crntT < 0) {
		crntT = 0;
	} else if ((crntT + crntH) > maxT) {
		crntT = (maxT - crntH);
	}
	crntL += diffX;
	if (crntL < 0) {
		crntL = 0;
	} else if ((crntL + crntW) > maxL) {
		crntL = (maxL - crntW);
	}
	$(dlgIdStr).css({ "left": crntL + "px", "top": crntT + "px" });
}

// Close Dialog
function closeCmdDialog(addId, dlgId) {
	$(dlgId).hide();
	$(dlgId).remove();
	removeOverlayScreen(addId);
}

/**
 * Command Dialog TAB key control
 * @param event evnet object
 * @param element dialog control object
 */
function cmddlgTabControl(event, element) {
	var tableng = element.length - 1; // omit empty column
	var nxttabidx = event.target.tabIndex;
	var searchNode = element[nxttabidx];
	var maxTabIdx = nxttabidx;	// get max tab index
	for (i = 0; i < element.length; i++) {
		if (element[i].tabIndex > maxTabIdx) {
			maxTabIdx = element[i].tabIndex;
		}
	}
	var isVisible = false;
	while (!isVisible) {
		nxttabidx = (event.shiftKey) ? nxttabidx - 1 : nxttabidx + 1;	// plus 1 or minus 1
		if (nxttabidx > maxTabIdx) {
			nxttabidx = 1;
		}
		else if (nxttabidx <= 0) {
			nxttabidx = maxTabIdx;
		}
		for (var j = 0; j <= tableng; j++) {
			if (element[j].tabIndex == nxttabidx) {
				searchNode = element[j];
				break;
			}
		}
		var pId = searchNode.id;
		while (pId == '') {
			searchNode = searchNode.parentElement;
			pId = searchNode.parentElement.id;
		}
		if ($('#' + pId).is(':visible')) {
			isVisible = true;
		}
	}
	searchNode.focus();
	return false;
}

// Specified string append to instruction statement for build Mixed Logic Instruction
function appendInstructionStr(appendStr) {
	var csrPos = $('#instructstr').get(0).selectionStart;	// append(insert) to cursor position.
	var instStr = $('#instructstr').val().substr(0, csrPos)
		+ appendStr + $('#instructstr').val().substr(csrPos);
	$('#instructstr').val(instStr).focus();
	$('#instructstr').get(0).selectionStart = csrPos + appendStr.length;
	$('#instructstr').get(0).selectionEnd = csrPos + appendStr.length;
}

// Find "[]" and move the cursor.
function countCommandCursorPos(cmd) {
	var cusMove = cmd.length;
	if (cmd.indexOf("[]") >= 0) {
		cusMove = cmd.indexOf("[]") + 1;
	} else if (cmd.indexOf("[...]") >= 0) {
		cusMove = cmd.indexOf("[...]") + 4;
	} else if (cmd.indexOf("\'...\'") >= 0) {
		cusMove = cmd.indexOf("\'...\'") + 5;
	}
	return cusMove;
}

