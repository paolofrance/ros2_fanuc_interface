// Set the size of the scroll bar
function setScrlbarHeight(treeMenu){
  
  if(treeMenu == BASE.treemenutype){

    baseScrollbarSize = setScrlbarSize(BASE);

  } else if(treeMenu == CUSTOM.treemenutype){
    customScrollbarSize = setScrlbarSize(CUSTOM);

  }
}

function setScrlbarSize(tree){
  var scrollBarSize = 0;
  var scrollBarSizePar = 0;
  var treeListID = '#' + tree.treelistid;
  var treeAreaID = '#' + tree.treeareaid;
  var treeScrollbarID = '#' + tree.scrollbarid;

  if($(treeListID).height() > $(treeAreaID).height()){
    /* Calculates the percentage of the scroll area relative to the movable range of the scroll bar. */
    scrollBarSizePar = ($(treeAreaID).height() / $(treeListID).height());
    /* Based on the result of 1, it is possible to calculate the bar size by dividing the scrollable range. */
    scrollBarSize = ($(treeAreaID).height() * scrollBarSizePar );
    scrollBarSize = Math.ceil(scrollBarSize);
  }
  $(treeScrollbarID).height(scrollBarSize);
  return scrollBarSize;
}

// scrollbar event
function initScrollbar() {
  var scrollelem = document.getElementsByClassName('scrollbar');
  var baseTreeAreaId = '#' + BASE.treeareaid;
  var customTreeAreaId = '#' + CUSTOM.treeareaid;

  // Set the height of the scroll bar
  setScrlbarHeight(BASE.treemenutype);
  setScrlbarHeight(CUSTOM.treemenutype);
  
  /* Synchronize scroll bar and tree box operations. */
  // tree scroll
  // for base tree menu
  $(baseTreeAreaId).on('scroll', function() {
    setScrbarPos(BASE);
  });

  // for custom tree menu
  $(customTreeAreaId).on('scroll', function() {
    setScrbarPos(CUSTOM);
  });

  // bar move
  for(var i = 0; i < scrollelem.length; i++) {
    scrollelem[i].addEventListener("mousedown", moveStart, false);
    scrollelem[i].addEventListener("touchstart", moveStart, false);
  }
  activeBottomBtnEvents(true);
}
// tree menu scroll event
function setScrbarPos(tree){
 
  var treeListID = '#' + tree.treelistid;
  var treeAreaID = '#' + tree.treeareaid;
  var treeScrollbarID = '#' + tree.scrollbarid;
  var scrollPosi = 0;
  if($(treeListID).height() != 0){
    /* Calculate the position of the scroll bar when the tree moves. */
    /* Calculate the ratio of the scroll bar area to the scrolling range. */
    var scrollPosiPar = ($(treeListID).height() / $(treeAreaID).height());
    /* Calculate where the scroll bar is relative to the scroll range */
    scrollPosi = ($(treeAreaID).scrollTop() / scrollPosiPar);
    scrollPosi = Math.ceil(scrollPosi);
  }
  $(treeScrollbarID).css("top", scrollPosi);
}


var cursorPosi = 0;
// scrollbar move start event
function moveStart(event){

  this.classList.add("drag");
  if(event.type === "mousedown"){
    var eve = event;
  }else{
    var eve = event.changedTouches[0];
  }
  
  // Get relative coordinates within an element.
  cursorPosi = eve.pageY - this.offsetTop;

  document.body.addEventListener("mousemove", getCurPos, false);
  document.body.addEventListener("touchmove", getCurPos, false);
  document.body.addEventListener("mouseup", moveStopScrollbar, false);
  document.body.addEventListener("touchend", moveStopScrollbar, false);
  document.body.addEventListener("mouseleave", moveStopScrollbar, false);
  document.body.addEventListener("touchleave", moveStopScrollbar, false);
  
}
// scrollbar move event
function getCurPos(event){
  var crtScrlBar = document.getElementsByClassName("drag")[0];
  var scrlPos = 0;
  event.preventDefault();

  if(event.type === "mousemove"){
    var eve = event;
  }else{
    var eve = event.changedTouches[0];
  }
      
  // Move scrollbar.
  scrlPos = eve.pageY - cursorPosi;

  // Scroll through the tree menu.
  if(crtScrlBar.id == BASE.scrollbarid){
    setTreeScrlPos(scrlPos, BASE);
  }else if(crtScrlBar.id == CUSTOM.scrollbarid){
    setTreeScrlPos(scrlPos, CUSTOM);
  }

}

function setTreeScrlPos(scrollbarPosi, tree){
  var treeListID  = '#' + tree.treelistid;
  var treeAreaID  = '#' + tree.treeareaid;

  // Find the scroll position of the tree while moving the scroll bar.
  // Calculate ratio of tree menu to scroll area.
  var treeScrollPosiPar = ($(treeAreaID).height() / $(treeListID).height()); 
  // Find the amount of movement by dividing the scroll bar position by the ratio.
  var treeScrollPosi = (scrollbarPosi / treeScrollPosiPar);
  $(treeAreaID).scrollTop(treeScrollPosi);
  
}

function moveStopScrollbar(event){
  var crtScrlBar = document.getElementsByClassName("drag")[0];

  // move event handler remove
  document.body.removeEventListener("mousemove", getCurPos);
  document.body.removeEventListener("touchmove", getCurPos);
  document.body.removeEventListener("mouseup", moveStopScrollbar);
  document.body.removeEventListener("touchend", moveStopScrollbar);
  document.body.removeEventListener("mouseleave", moveStopScrollbar);
  document.body.removeEventListener("touchleave", moveStopScrollbar);

  //delete classname [.drag]
  if(crtScrlBar){
    crtScrlBar.classList.remove("drag");
  }
}
