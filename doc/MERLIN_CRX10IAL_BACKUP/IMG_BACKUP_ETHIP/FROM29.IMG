function updIOAsgnRow(status, setIOInf) {
  if (asgnIOType == TAB_DI || asgnIOType == TAB_DO) {
    getPMCDevXVR(getIOAsgnUpd);
  } else {
    getIOAsgnUpd();
  }
  function getIOAsgnUpd(PMCDevData) {
    getIOAssignedInf(g_ary_tab_id[asgnIOType].tab, updAsgnRecord, PMCDevData);
  }
  function updAsgnRecord(status, asgnInf, PMCDevData) {
    var startNum = Number(setIOInf.fstLogPort);
    var ioNum = String(startNum);
    if (asgnIOType == TAB_DI || asgnIOType == TAB_DO) {
      var numBits = Math.abs(setIOInf.nLogPorts);
      ioNum = startNum + "-" + (startNum + numBits - 1);
    }
    var updRecord = {record: []};
    updRecord.record[ASGN_CLM_NO] = {recVal: ioNum};
    for (var i = 0; i < asgnInf.length; i++) {
      if (startNum == Number(asgnInf[i].fst_log_port)) {
        updRecord.record[ASGN_CLM_TYPE] = {recVal: getDevName(asgnInf[i].rack_no, asgnInf[i].slot_no)};
        updRecord.record[ASGN_CLM_RACK] = {recVal: asgnInf[i].rack_no};
        updRecord.record[ASGN_CLM_SLOT] = {recVal: asgnInf[i].slot_no};
        updRecord.record[ASGN_CLM_STRTNUM] = {recVal: asgnInf[i].fst_phy_port};
        updRecord.record[ASGN_CLM_CHANNEL] = {recVal: asgnInf[i].fst_phy_port};
        updRecord.record[ASGN_CLM_NUMPTS] = {recVal: asgnInf[i].n_log_ports};
        updRecord.record[ASGN_CLM_STATUS] = setStatus(asgnInf[i].valid, g_ary_tab_id[asgnIOType].tab, asgnInf[i].rack_no, asgnInf[i].slot_no, PMCDevData);
        break;
      }
    }
    if (updRecord.record.length < 2) {
      updRecord.record[ASGN_CLM_TYPE] = {recVal: getDevName(setIOInf.rackNo, setIOInf.slotNo)};
      updRecord.record[ASGN_CLM_RACK] = {recVal: setIOInf.rackNo};
      updRecord.record[ASGN_CLM_SLOT] = {recVal: setIOInf.slotNo};
      updRecord.record[ASGN_CLM_STRTNUM] = {recVal: setIOInf.fstPhyPort};
      updRecord.record[ASGN_CLM_CHANNEL] = {recVal: setIOInf.fstPhyPort};
      updRecord.record[ASGN_CLM_NUMPTS] = {recVal: Math.abs(setIOInf.nLogPorts)};
      updRecord.record[ASGN_CLM_STATUS] = {recVal: ASGN_STAT_VAL_ARR[ASGN_STAT_INVALID], recColor: GRIDCOLOR_ERROR};
    }
    var slctRow = ioAsgnGrid.getSelectRow().end;
    ioAsgnGrid.deleteRecord(slctRow);

    var asgnNoList = ioAsgnGrid.getColumn(ASGN_CLM_ARR[ASGN_CLM_NO]);
    var insRecRow = srchAsgnInsRow(startNum, asgnNoList);
    if (insRecRow > asgnNoList.length) {
      ioAsgnGrid.appendRecord(updRecord);
    } else {
      ioAsgnGrid.insertRecord(insRecRow, updRecord);
    }
    ioAsgnGrid.setSelectRow({start: insRecRow, end: insRecRow});
    updEditInf(insRecRow, true);
    setListSelectable('auto');
  }
}

function srchAsgnInsRow(startNum, asgnNumList) {
  var insRow = 1;
  for (var i = 0; i < asgnNumList.length; i++) {
    var gridStartNum = Number(asgnNumList[i]);
    if (asgnIOType == TAB_DI || asgnIOType == TAB_DO) {
      gridStartNum = Number(asgnNumList[i].split('-')[0]);
    }
    if (gridStartNum <= startNum) {
      insRow++;
    } else {
      break;
    }
  }
  return insRow;
}

function updIOAsgn() {
  var setIOInf = {};
  var memType;
  // "-" is for forced assignment.
  setIOInf.nLogPorts = '-';
  setIOInf.phyPortType = g_ary_tab_id[asgnIOType].tab;
  switch (asgnIOType) {
    case TAB_DI:
    case TAB_DO:
      memType = MEM_BOO_TYPE;
      setIOInf.nLogPorts += document.getElementById('numBitsTb').getValue();
      break;
    case TAB_AI:
    case TAB_AO:
      memType = MEM_NUM_TYPE;
      setIOInf.nLogPorts += '1';
      break;
    case TAB_GI:
    case TAB_GO:
      setIOInf.phyPortType = g_ary_tab_id[(asgnIOType == TAB_GI) ? TAB_DI : TAB_DO].tab;
      memType = MEM_NUM_TYPE;
      setIOInf.nLogPorts += document.getElementById('numberPointsTb').getValue();
      break;
  }
  setIOInf.logPortType = g_ary_tab_id[asgnIOType].tab;
  setIOInf.fstLogPort = document.getElementById('startNumTb').getValue();
  setIOInf.rackNo = document.getElementById('rackTb').getValue();
  setIOInf.slotNo = document.getElementById('slotTb').getValue();
  setIOInf.fstPhyPort = document.getElementById('startPointTb').getValue();
  if (setIOInf.rackNo == '0' && setIOInf.slotNo == '0') {
    if (setIOInf.fstPhyPort == '0') {
      setIOInf.phyPortType = '0';
    } else {
      setIOInf.phyPortType = memType;
    }
  }
  top.callSetIOAssign(setIOInf, oldValue.startNum, updIOAsgnRow); // irprog_io
}

