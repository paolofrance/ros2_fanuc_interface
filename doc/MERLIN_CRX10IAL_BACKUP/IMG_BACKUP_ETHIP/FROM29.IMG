function setBtnDisabled(disableBool, targetBtnList){
  if(targetBtnList != null) {
    for(var i=0; i < targetBtnList.length; i++){
      targetBtnList[i].disabled = disableBool;
    }
  }  
}

function updateDelBtnState() {
  var treeElem = document.getElementById(CUSTOM.treelistid);
  var selectNodes = treeElem.getElementsByClassName('select_node');

  if (selectNodes.length && del_btn.disabled == true) {
    del_btn.disabled = false;
  } else if (!selectNodes.length && del_btn.disabled == false) {
    del_btn.disabled = true;
  }
}

function updateEditBtnState() {
  var treeElem = document.getElementById(CUSTOM.treelistid);
  var selectNodes = treeElem.getElementsByClassName('select_node');
  // -1:not custom_list parent 0:select base_list 1:select custom_list parent
  var selectClass = 0;
  if(selectNodes.length != 0){
    selectClass = selectNodes[0].className.indexOf('parent');
  }

  if (selectClass > 0) {
    edit_btn.disabled = false;
  } else if (selectClass <= 0) {
    edit_btn.disabled = true;
  }
}

function appendEditBox(node, text) {
  setDisabledCstmTree(true);
  var editbox = document.createElement('input');
  editbox.id = 'editbox';
  editbox.className = 'editbox';

  if (text) {
    editbox.value = text;
  } else {
    editbox.value = NEW_SECTION;
  }
  node.appendChild(editbox);

  /*
    select focus and text.
    Focus does not support ipad.
  */
  editbox.focus();
  editbox.select();
  var navUA = navigator.userAgent.toLowerCase();
  if(!$('#editbox').is(':focus') && navUA.indexOf('android') >= 0 ) {
    $('#focus_button').css({ 'display': 'block' });
  } else {
    editbox.addEventListener('blur', confNodeValue, false);
    editbox.addEventListener('keydown', checkEnter, false);
    document.addEventListener('touchend', blurEditbox, false);
    document.addEventListener('click', blurEditbox, false);
  }
}

function clickInputFocusBtn() {
  var editbox = document.getElementById('editbox');
  $('#focus_button').css({'display': 'none'});
  setTimeout(function(){
    $('#editbox').focus();
    $('#editbox').select();
    editbox.addEventListener('blur', confNodeValue, false);
    editbox.addEventListener('keydown', checkEnter, false);
    document.addEventListener('touchend', blurEditbox, false);
    document.addEventListener('click', blurEditbox, false);
  },100);
}

function blurEditbox(event){
  if (event.type.indexOf("touch") == 0) {
    event.preventDefault();
  }
  if (event.target.tagName == EDITBOX_TAGNAME ||
      event.target.id == 'focus_button') {
    $('#editbox').focus();
    $('#editbox').select();
  } else {
    $('#editbox').blur();
  }
}

function checkEnter(event){
  if( event.which == KEYCODE_ENTER ){
    confNodeValue(event);
  }
}

function confNodeValue(event) {
  var editbox = document.getElementById('editbox');
  var value = editbox.value;
  var node = editbox.parentNode;
  var oldText = node.innerText;
  var icon = document.createElement('ins');
  var text = document.createTextNode(value);
  var curIcon = node.getElementsByTagName('ins')[0];
  var tooltip = document.createElement('div');
  
  if(isNgSectionName(value)){
    var editBoxTopPos = $(editbox).offset().top;
    var custmAreaTopPos = $('#custom_tree_area').offset().top;
    var custmAreaH = $('#custom_tree_area').height();
    var topPos = custmAreaTopPos + (custmAreaH / 2);
    // Set tooltip
    if($('#editbox_tooltip').length == 0){
      if(topPos > editBoxTopPos) {
        tooltip.className = 'edit_arrow_lt';
        tooltip.style.top = '48px';
      } else {
        tooltip.className = 'edit_arrow_lb';
        tooltip.style.top = '-48px';
      }
      tooltip.id = 'editbox_tooltip';
      tooltip.innerText = resources["inputErrMsg"];
      node.parentNode.insertBefore(tooltip, node.nextSibling);
      $(node).css({ 'z-index': '10000' });
      $('.menucstm_area').append('<div class="screenoverlayCls"></div>');
      $('.tree_area').css({ 'overflow-y': 'hidden' });
      node.parentNode.style.position = 'relative';
    }
    $('input').focus();
    return;
  } else {
    // Delete tooltip
    if($('#editbox_tooltip').length > 0){
      $('.screenoverlayCls').remove();
      $('#editbox_tooltip').remove();
      $(node).css({ 'z-index': '10' });
      $('.tree_area').css({ 'overflow-y': 'scroll' });
      node.parentNode.style.position = '';
    }
  }


  if (!curIcon) {
    icon.className = 'tree_ico open';
  } else {
    if (curIcon.classList.contains('open')) {
      icon.className = 'tree_ico open';
    } else {
      icon.className = 'tree_ico close';
    }
  }

  if(oldText != text.nodeValue){
    setBtnDisabled(false, [restor_btn, update_btn]);
  }
  setDisabledCstmTree(false);
  editbox.removeEventListener('blur', confNodeValue);
  document.removeEventListener('touchend', blurEditbox);
  document.removeEventListener('click', blurEditbox);
  node.removeChild(editbox);
  node.innerHTML = '';
  node.appendChild(icon);
  node.appendChild(text);
}

function clickSectionBtn(event) {
  if (event.type.indexOf("touch") == 0) {
    event.preventDefault();
  }
 
  if(addNodeFlag){
    return;
  }

  if($('input').length != 0) {
    return;
  }

  var treeElem = document.getElementById(CUSTOM.treelistid);
  var selectNodes = treeElem.getElementsByClassName('select_node');
  var rootSelectNode;
  var appendParent;
  var li;
  var parentNode;
  var ul;

  if(!sectionBtnMoveFlag) {
    addNodeFlag = true;
    // Create a new section
    li = document.createElement('div');
    li.setAttribute('data', 'li');
      
    parentNode = document.createElement('div');
    parentNode.className = 'tree_node parent';

    ul = document.createElement('div');
    ul.className = BASE.treelistclass;
          
    li.appendChild(parentNode);
    li.appendChild(ul);

    // when not select
    if (!selectNodes.length) {
      treeElem.appendChild(li);
    }
    else{
      rootSelectNode = selectNodes[0];

      // for section
      if(rootSelectNode.classList[1] == "parent"){
        appendParent = rootSelectNode.parentNode.parentNode;
      }
      // for menu
      else{
        appendParent = rootSelectNode.parentNode.parentNode.parentNode.children[1];
        if(!appendParent){
          appendParent = rootSelectNode.parentNode.parentNode; 
        }
      }
      appendParent.insertBefore(li, rootSelectNode.parentNode.nextSibling);
    }

    // setting
    bindOpenCloseEvent(CUSTOM.treelistclass);
    bindSelectEvent(CUSTOM.treelistclass);

    setBtnDisabled(false, [restor_btn, update_btn]);
    new Sortable(ul, {
      group: 'sortable',
      animation: 180,
      onAdd: addFunc,
      onChoose: chooseFunc,
      onUnChoose: unChooseFunc,
      onUpdate: moveEndFunc,
      filter: "input",
    });

    // Click event initialize
    bindDbclickEvent(CUSTOM.treelistclass);

    // resize scrollbar
    setScrlbarHeight(CUSTOM.treemenutype);
 
    var arg = {};
    arg.currentTarget = parentNode;
    selectTreeNode(arg);

    setTimeout(function(){
      appendEditBox(parentNode);
    },500);
  }

  // Change sectionBtn move flag
  sectionBtnMoveFlag = false;
  addNodeFlag = false;
}

function downSectionBtn() {
  var btn = document.getElementById('section_button');
  btn.style.backgroundColor = '#0e6eb8';
}

function upSectionBtn() {
  var btn = document.getElementById('section_button');
  btn.style.backgroundColor = '#67727e';
}

// edit button 
function clickEditBtn(event) {
  if ($('input').length != 0){
    event.preventDefault();
    return false;
  }
  var treeElem = document.getElementById(CUSTOM.treelistid);
  var selectNodes = treeElem.getElementsByClassName('select_node'); 
  var rootSelectNode;
  
  if (!selectNodes.length) {
    return false;
  }

  rootSelectNode = selectNodes[0];

  if (!rootSelectNode.classList.contains('parent')) {
    return false;
  }

  setTimeout(function(){
    appendEditBox(rootSelectNode, rootSelectNode.innerText);
  },200);
}

// delete button
function clickDelBtn(event) {
  if ($('input').length != 0){
    event.preventDefault();
    return;
  }
  var treeElem = document.getElementById(CUSTOM.treelistid);
  var selectNodes = treeElem.getElementsByClassName('select_node');
  var rootSelectNode;
  var selectLi;
  var selectUl;

  if (!selectNodes.length) {
    return false;
  }

  rootSelectNode = selectNodes[0];
  selectLi = rootSelectNode.parentNode;
  selectUl = selectLi.parentNode;
  selectUl.removeChild(selectLi);

  // Disable button
  setBtnDisabled(true, [del_btn, edit_btn]);
  setBtnDisabled(false, [restor_btn, update_btn]);

  setScrlbarHeight(CUSTOM.treemenutype);
}

// restore button event
function clickRestoreBtn(event){
  if(hasErrSec()) return;
  activeBottomBtnEvents(false);
  getCustmTreeData();
}

function getCustmTreeData(){
  var customTree = document.getElementById(CUSTOM.treelistid);
  // tree count initial
  // Set 1 to change only the custom tree.
  treeCnt = 1;

  // Disable button
  setBtnDisabled(true, [del_btn, edit_btn, restor_btn, update_btn]);

  // custom menu clear
  while(customTree.firstChild){
    customTree.innerHTML = '';
  }

  // get custom menu
  getTreeMenu(CUSTOM_TREE_TYPE, getTreeMenuCallback, CUSTOM.treemenutype); 
}


function makeFileName(){
  var fileName = "";
  // language check
  var langtype = top.irprogapi.getCurrentLanguage();
  if(langtype == 'jp'){
