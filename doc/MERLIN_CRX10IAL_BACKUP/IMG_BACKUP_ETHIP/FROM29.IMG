function ioAsgnTbEvent(id) {
  // Check row selection.
  var selRowNum = ioAsgnGrid.getSelectRow().end;
  if (selRowNum == undefined || selRowNum == 0) {
    initAsgnElm();
    return;
  }
  var tbId = id.split('.')[0];
  if (isIncorrectIORange(tbId)) {
    loadOldValue();
    setListSelectable('auto');
    return;
  }
  var rackNum = document.getElementById('rackTb').getValue();
  var slotNum = document.getElementById('slotTb').getValue();
  if (isUnavailableIOMod(rackNum, slotNum, tbId)) {
    loadOldValue();
    setListSelectable('auto');
    return;
  }
  document.getElementById('ioModTypeTxt').innerHTML = getDevName(rackNum, slotNum);
  if (isNotAsgnSetting()) {
    setListSelectable('auto');
    return;
  }
  updIOAsgn();
}

function isNotAsgnSetting() {
  var selRecNum = ioAsgnGrid.getSelectRow().end;
  var rtnBool = false;
  if (ioAsgnGrid.getRecord(selRecNum).record[ASGN_CLM_NO].recVal == ASGN_ADD_STR) {
    var asgnObj = {};
    asgnObj.startNum = Number(document.getElementById('startNumTb').getValue());
    asgnObj.rackVal = document.getElementById('rackTb').getValue();
    asgnObj.slotVal = document.getElementById('slotTb').getValue();
    asgnObj.startPt = document.getElementById('startPointTb').getValue();
    switch (asgnIOType) {
      case TAB_DI:
      case TAB_DO:
        asgnObj.numBits = Number(document.getElementById('numBitsTb').getValue());
        var ioRange = asgnObj.startNum + "-" + (asgnObj.startNum + asgnObj.numBits - 1);
        var ioRangeLbl = g_ary_tab_id[asgnIOType].name + "[" + ioRange + "]";
        document.getElementById('ioRangeTxt').innerHTML = ioRangeLbl;
        if (asgnObj.rackVal == "0" 
        && asgnObj.slotVal == "0"
        && asgnObj.startPt == "0") {
          ioAsgnGrid.getRecord(selRecNum).initEditRec.record[ASGN_CLM_NO].recVal = ioRange;
          rtnBool = true;
        }
        break;
      case TAB_AI:
      case TAB_AO:
        if (asgnObj.rackVal == "0" 
        && asgnObj.slotVal == "0"
        && asgnObj.startPt == "0") {
          ioAsgnGrid.getRecord(selRecNum).initEditRec.record[ASGN_CLM_NO].recVal = asgnObj.startNum;
          rtnBool = true;
        }
        break;
      case TAB_GI:
      case TAB_GO:
        asgnObj.numPts = document.getElementById('numberPointsTb').getValue();
        if (asgnObj.numPts == "0"
        || (asgnObj.rackVal == "0" 
        && asgnObj.slotVal == "0"
        && asgnObj.startPt == "0")) {
          var newRec = ioAsgnGrid.getRecord(selRecNum).initEditRec.record;
          newRec[ASGN_CLM_NO].recVal = asgnObj.startNum;
          newRec[ASGN_CLM_RACK].recVal = asgnObj.rackVal;
          newRec[ASGN_CLM_SLOT].recVal = asgnObj.slotVal;
          newRec[ASGN_CLM_STRTNUM].recVal = asgnObj.startPt;
          newRec[ASGN_CLM_NUMPTS].recVal = asgnObj.numPts;
          newRec[ASGN_CLM_STATUS].recVal = document.getElementById('statusTxt').innerHTML;
          newRec[ASGN_CLM_STATUS].recColor = GRIDCOLOR_ERROR;
          rtnBool = true;
        }
        break;
      default:
        break;
    }
  }
  if (rtnBool) {
    saveOldValue(asgnObj);
  }
  return rtnBool;
}

function isUnavailableIOMod(rackNum, slotNum, tgtTbId) {
  switch(asgnIOType) {
    case TAB_DI:
    case TAB_DO:
      if (isPMCMod()) return true;
      break;
    case TAB_AI:
    case TAB_AO:
      break;
    case TAB_GI:
    case TAB_GO:
      if (isPMCMod()) return true;
      break;
  }
  return false;

  function isPMCMod() {
    if (Number(rackNum) == 33 && Number(slotNum) >= 8) {
      top.openCmpAlert(window, 'errSetPMCMod', resources['ERRSetPMCMod'], null, setFocusTb);
      return true;
    }
  }
  function setFocusTb() {
    if (tgtTbId != undefined) {
      document.getElementById(tgtTbId).setFocus();
    }
  }
}

function isIncorrectIORange(tgtTbId) {
  var startNum = Number(document.getElementById('startNumTb').getValue());
  var numBits = Number(document.getElementById('numBitsTb').getValue());
  var endNum = (asgnIOType == TAB_DI || asgnIOType == TAB_DO) ? startNum + numBits - 1 : startNum;
  // Check input upper limit.
  if ((endNum) > getIOMaxNum(g_ary_tab_id[asgnIOType].tab)) {
    top.openCmpAlert(window, 'errAsgnNot', resources["ERRAsgnNot"], null, setFocusTb);
    return true;
  }

  // Check the registered number.
  var asgnIONo = ioAsgnGrid.getColumn(ASGN_CLM_ARR[ASGN_CLM_NO]);
  var selTab = g_ary_tab_id[asgnIOType].name;
  var selRowNum = ioAsgnGrid.getSelectRow().end;
  switch (asgnIOType) {
    case TAB_DI:
    case TAB_DO:
      for (var i = 0; i < asgnIONo.length; i++) {
        var asgnIORng = asgnIONo[i].split('-');
        if (selRowNum == (i + 1)) {
        } else if ((startNum >= Number(asgnIORng[0]) && startNum <= Number(asgnIORng[1])) ||
        (endNum >= Number(asgnIORng[0]) && endNum <= Number(asgnIORng[1])) ||
        (startNum < Number(asgnIORng[0]) && endNum > Number(asgnIORng[1]))) {
          var errMsg = resources["ERRAsgnContainsNum"].replace('{{ioType}}', selTab + "[" + startNum + "-" + (endNum) + "]");
          top.openCmpAlert(window, 'errAsgnContainsNumDlg', errMsg, null, setFocusTb);
          return true;
        }
      }
      break;
    case TAB_AI:
    case TAB_AO:
    case TAB_GI:
    case TAB_GO:
      for (var i = 0; i < asgnIONo.length; i++) {
        if (selRowNum == (i + 1)) {
        } else if (startNum == Number(asgnIONo[i])) {
          var errMsg = resources["ERRAsgnedNum"].replace('{{ioType}}', selTab + "[" + startNum + "]");
          top.openCmpAlert(window, 'errAsgnedNumDlg', errMsg, null, setFocusTb);
          return true;
        }
      }
      break;
    default:
      return true;
  }
  return false;

  function setFocusTb() {
    if (tgtTbId != undefined) {
      document.getElementById(tgtTbId).setFocus();
    }
  }
}

function saveOldValue(saveObj) {
