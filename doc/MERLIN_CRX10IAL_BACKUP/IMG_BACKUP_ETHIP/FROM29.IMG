function updEditInf(tgtRow, toastFlg) {
  var selRec = ioAsgnGrid.getRecord(tgtRow).record;
  if (selRec[ASGN_CLM_NO].recVal == ASGN_ADD_STR) {
    selRec = ioAsgnGrid.getRecord(tgtRow).initEditRec.record;
  }
  var asgnObj = {};
  asgnObj.rackNum = selRec[ASGN_CLM_RACK].recVal;
  asgnObj.slotNum = selRec[ASGN_CLM_SLOT].recVal;
  asgnObj.startPt = selRec[ASGN_CLM_STRTNUM].recVal;
  asgnObj.numPts = selRec[ASGN_CLM_NUMPTS].recVal;

  var ioMaxNum = getIOMaxNum(g_ary_tab_id[asgnIOType].tab);
  var elmDsbl = false;
  var ioStartNum = selRec[ASGN_CLM_NO].recVal;
  var inputMaxNum = getInputMaxNum();
  switch (asgnIOType) {
    case TAB_DI:
    case TAB_DO:
      if (selRec[ASGN_CLM_STATUS].recVal == ASGN_STAT_VAL_ARR[ASGN_STAT_PMC]) {
        elmDsbl = true;
      }
      var ioRangeNum = ioStartNum.split('-');
      var numBits = Number(ioRangeNum[1]) - Number(ioRangeNum[0]) + 1;
      asgnObj.numBits = String(numBits);
      ioStartNum = ioRangeNum[0];
      document.getElementById('numBitsTb').refresh(numBits, 1, ioMaxNum, elmDsbl);
      document.getElementById('ioRangeTxt').innerHTML = g_ary_tab_id[asgnIOType].name + "[" + selRec[ASGN_CLM_NO].recVal + "]";
      document.getElementById('startPointTb').refresh(selRec[ASGN_CLM_STRTNUM].recVal, 0, inputMaxNum.startPt, elmDsbl);
      break;
    case TAB_AI:
    case TAB_AO:
      document.getElementById('startPointTb').refresh(selRec[ASGN_CLM_CHANNEL].recVal, 0, inputMaxNum.startPt, elmDsbl);
      break;
    case TAB_GI:
    case TAB_GO:
      document.getElementById('startPointTb').refresh(selRec[ASGN_CLM_STRTNUM].recVal, 0, inputMaxNum.startPt, elmDsbl);
      document.getElementById('numberPointsTb').refresh(selRec[ASGN_CLM_NUMPTS].recVal, 0, ASGN_GIO_NUMPTS_MAX, elmDsbl);
      break;
    default:
      break;
  }
  asgnObj.startNum = ioStartNum;
  saveOldValue(asgnObj);

  var oldStatus = document.getElementById('statusTxt').innerHTML;
  var addBtnDsbl = (getNewRecLineNum() > 0);
  document.getElementById('startNumTb').refresh(ioStartNum, 1, ioMaxNum, elmDsbl);
  document.getElementById('ioModTypeTxt').innerHTML = selRec[ASGN_CLM_TYPE].recVal;
  document.getElementById('rackTb').refresh(selRec[ASGN_CLM_RACK].recVal, 0, inputMaxNum.rack, elmDsbl);
  document.getElementById('slotTb').refresh(selRec[ASGN_CLM_SLOT].recVal, 0, inputMaxNum.slot, elmDsbl);
  document.getElementById('addBtn').refresh(null, null, null, null, addBtnDsbl, null);
  document.getElementById('delBtn').refresh(null, null, null, null, elmDsbl, null);
  document.getElementById('slctBtn').refresh(null, null, null, null, elmDsbl, null);
  document.getElementById('statusTxt').innerHTML = selRec[ASGN_CLM_STATUS].recVal;

  var statClsName = '';
  switch (selRec[ASGN_CLM_STATUS].recColor) {
    case GRIDCOLOR_ERROR:
      statClsName = "statusError";
      break;
    case GRIDCOLOR_CAUTION:
      statClsName = "statusCaution";
      break;
    case GRIDCOLOR_NORMAL:
      statClsName = "statusNormal";
      break;
    default:
      break;
  }
  document.getElementById('statusTxt').className = statClsName;
  if (toastFlg
  && ASGN_STAT_VAL_ARR[ASGN_STAT_PENDING] == selRec[ASGN_CLM_STATUS].recVal
  && ASGN_STAT_VAL_ARR[ASGN_STAT_PENDING] != oldStatus) {
    top.IHMIComponents.cf.toast.show(resources['AsgnRebootReq']);
  }
}

