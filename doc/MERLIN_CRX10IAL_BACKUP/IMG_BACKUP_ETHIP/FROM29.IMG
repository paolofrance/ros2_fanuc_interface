var resources = [];

function initResource() {
  var lang = getCurrentLanguage();
  $.getJSON('lang/io_' + lang + '.jsn')
    .done(function (data) {
      resources = data;
      if (window.frameElement.id === "dual" || window.frameElement.id === "third") {
        top.openCmpAlert(window, 'ioPrimOpenReq', resources['ioPrimOpenReq'], true);
      } else {
        initEachWindowResource();
      }
      debug(DBGINFO, 'Finish language resource.');
    })
    .fail(function () {
      if (lang != LANG_EG) {
        currentLang = LANG_EG;
        initResource();
        console.log("change Language :" + lang + " > " + currentLang);
      }
    });
}

