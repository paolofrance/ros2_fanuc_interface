// Event when the [Configure I/O] button is pressed.
function openIOAsgn() {
  if (g_select_tab == TAB_DI || g_select_tab == TAB_DO
    || g_select_tab == TAB_AI || g_select_tab == TAB_AO
    || g_select_tab == TAB_GI || g_select_tab == TAB_GO) {
    asgnIOType = g_select_tab;
  } else {
    asgnIOType = TAB_DI;
  }
  setListSelectable('none');
  document.getElementById('ioAsgnWindow').style.display = "";
  document.getElementById('ioAsgnWindow').style.fontFamily = resources["IOAssignedFont"];
  initIOAsgnWindowResource(asgnIOType);
  setTitleAreaTxt();
  saveOldValue({startNum: 0, numBits: 0, rackNum: 0, slotNum: 0, startPt: 0, numPts: 0});

  if (ioAsgnTableDef == null) {
    getIOModuleInf(getIOModuleInfCB);
    initAsgnTable();
    ioAsgnGrid.refresh(document, 'asgnUpperGrid', ioAsgnTableDef, null);
    ioAsgnGrid.setCallback(asgnGridEvent);
    ioAsgnGrid.lock();
  } else {
    settingAsgnWindow();
  }

  function getIOModuleInfCB(status, data) {
    ioDevData[TAB_DI] = [];
    ioDevData[TAB_DO] = [];
    ioDevData[TAB_AI] = [];
    ioDevData[TAB_AO] = [];
    ioDevData[TAB_AI + MOD_ARRY_NAME] = [];
    ioDevData[TAB_AO + MOD_ARRY_NAME] = [];
    for (var i = 0; i < data.length; i++) {
      var ioModType = data[i].pd_port_type.split(',');
      for (var j = 0; j < ioModType.length; j++) {
        if (ioModType[j] == String(IO_TYPE_FLAG) || ioModType[j] == String(IO_TYPE_MARKER) || ioModType[j] == String(IO_TYPE_RELAY)) {
          ioDevData[TAB_DI].push(data[i]);
          ioDevData[TAB_DO].push(data[i]);
          ioDevData[TAB_AI].push(data[i]);
          ioDevData[TAB_AO].push(data[i]);
          break;
        }
        if (data[i].devname != 'Unknown' && (data[i].rack != '0' || data[i].slot != '0')) {
          switch (ioModType[j]) {
            case String(IO_TYPE_DIN):
              ioDevData[TAB_DI].push(data[i]);
              break;
            case String(IO_TYPE_DOUT):
              ioDevData[TAB_DO].push(data[i]);
              break;
            case String(IO_TYPE_AIN):
              ioDevData[TAB_AI].push(data[i]);
              ioDevData[TAB_AI + MOD_ARRY_NAME].push(data[i]);
              break;
            case String(IO_TYPE_AOUT):
              ioDevData[TAB_AO].push(data[i]);
              ioDevData[TAB_AO + MOD_ARRY_NAME].push(data[i]);
              break;
          }
        }
      }
    }
    settingAsgnWindow();
  }

  function settingAsgnWindow() {
    dispEachIOType();
    getIOAsgnData();
    setAsgnTxtBoxEvent();
    setAsgnBtnEvent();
    $(window).trigger('resize');
  }
}

