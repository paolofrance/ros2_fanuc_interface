  font-size: 12px;
}

.pltroutetitle {
  display: inline-block;
  height: 100%;
  border-right: solid 1px #000000;
  text-align: center;
  line-height: 25px;
}

.pltroutetitle.no {
  width: 25px;
}

.pltroutetitle.motion {
  width: 82px;
}

.pltroutetitle.pos {
  width: 72px;
}

.pltroutetitle.speed {
  width: 227px;
}

.pltroutetitle.path {
  width: 210px;
  border: 0;
}

.pltrouteptnmain {
  width: 100%;
}

.pltrouterow {
  display: flex;
  width: 100%;
  height: 40px;
  border-bottom: solid 1px #000000;
}

.pltrouterow:last-child {
  border-bottom: 0;
}

.pltroutecell {
  height: 100%;
  border-right: solid 1px #000000;
  text-align: center;
  line-height: 40px;
}

.pltroutecell.no {
  width: 25px;
}

.pltroutecell.motion {
  width: 82px;
}

.pltroutecell.pos {
  width: 72px;
}

.pltroutecell.speed {
  display: flex;
  width: 227px;
}

.pltroutecell.path {
  display: flex;
  width: 210px;
  border: 0;
}

.pltrouteselbox {
  position: relative;
  height: 28px;
  background-color: #FFFFFF;
  line-height: 30px;
  text-align: left;
  border: 1px solid #97a1ae;
  color: #000000;
  margin: 5px 0 0 5px;
  cursor: pointer;
}

.plttupbtn:focus,
.pltrouteselbox:focus {
