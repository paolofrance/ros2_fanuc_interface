/*
 * Tab Event Setting
 */
function SetTabEvent() {
  g_ary_tab_id[TAB_DI] = { tab: IO_TYPE_DIN, name: "DI"};
  g_ary_tab_id[TAB_DO] = { tab: IO_TYPE_DOUT, name: "DO"};
  g_ary_tab_id[TAB_AI] = { tab: IO_TYPE_AIN, name: "AI"};
  g_ary_tab_id[TAB_AO] = { tab: IO_TYPE_AOUT, name: "AO"};
  g_ary_tab_id[TAB_RI] = { tab: IO_TYPE_RIN, name: "RI"};
  g_ary_tab_id[TAB_RO] = { tab: IO_TYPE_ROUT, name: "RO"};
  g_ary_tab_id[TAB_GI] = { tab: IO_TYPE_GIN, name: "GI"};
  g_ary_tab_id[TAB_GO] = { tab: IO_TYPE_GOUT, name: "GO"};
  g_ary_tab_id[TAB_F] = { tab: IO_TYPE_FLAG, name: "F"};

  // init set tab
  g_select_tab = getIOSelData('tab');
  g_cur_type = g_ary_tab_id[g_select_tab].tab;
  g_str_cur_type = g_ary_tab_id[g_select_tab].name;

  var tabCount = document.getElementById('tabarea').querySelectorAll("li.ihcp-tab-item").length;
  document.getElementById('tabarea').refresh(null, tabCount, g_select_tab);
  SetGridColumn(g_select_tab);

  // tab click event
  document.getElementById('tabarea').setCallback(tabClick);
  function tabClick(id, operation, value) {
    document.getElementById('openIOAsgn').style.display = 'none';
    w2ui['ioarea'].refresh();

    if (g_select_tab != value) {
      if (window.frameElement.id == "prim"
      && (value == TAB_DI || value == TAB_DO
      || value == TAB_AI || value == TAB_AO
      || value == TAB_GI || value == TAB_GO)) {
        document.getElementById('openIOAsgn').style.display = 'block';
      }
      dispOnWaiting('initio', 'forinitio');
      top.rpcmc_stopIOGroupMonitor(g_cur_type, g_current_item_no_start, g_current_item_no_end - g_current_item_no_start + 1);
      g_select_tab = value;
      g_cur_type = g_ary_tab_id[g_select_tab].tab;
      g_str_cur_type = g_ary_tab_id[g_select_tab].name;
      w2ui['ioarea'].clear();
      g_select_grid_row = null;
      SetGridColumn(value);
      initPageArea(getIOMaxNum(g_cur_type), getIOSelData(g_select_tab));
      setIOSelData('tab', g_select_tab);
      initIOPage();
    }
  }
}

function setBtnEvent() {
  document.getElementById('openIOAsgn').refresh(null, null, null, null, false, openIOAsgn);
}

/*
 * Grid Column Setting
 */
function SetGridColumn(g_select_tab) {
    switch (g_select_tab) {
        case TAB_DI:
        case TAB_DO:
        case TAB_RI:
        case TAB_RO:
            w2ui['ioarea'].hideColumn('value');
            w2ui['ioarea'].showColumn('simulate');
            w2ui['ioarea'].showColumn('state');
            break;
        case TAB_AI:
        case TAB_AO:
        case TAB_GI:
        case TAB_GO:
            w2ui['ioarea'].hideColumn('state');
            w2ui['ioarea'].showColumn('simulate');
            w2ui['ioarea'].showColumn('value');
            break;
        case TAB_F:
            w2ui['ioarea'].hideColumn('simulate');
            w2ui['ioarea'].hideColumn('value');
            w2ui['ioarea'].showColumn('state');
            break;
    }
}

/*
 * Grid Event Setting
 */
function SetGridEvent() {
  if ((w2utils.isIOS) || (w2utils.isAndroid)) {
    w2ui['ioarea'].on('blur', function (event) {
      // To take the focus off the grid.
      $("#page_num").focus();
      $("#page_num").blur();
    });
    w2ui['ioarea'].on('focus', function (event) {
      // To prevent the display of the software keyboard.
      event.preventDefault();
      $("#ioarea").w2grid().blur();
    });
  }

    // click event
    w2ui['ioarea'].on('click', function (event) {
        var grid = this;
        event.onComplete = function () {
            //g_select_grid_row = grid.getSelection();
            //console.log(grid.getRecordsHTML());
            if (event.originalEvent.type == 'keydown') {
                return;
          }
          var cellVal = w2ui['ioarea'].getCellValue(event.recid, event.column, false);
            switch (event.column) {
              case COLUMN_SIMULATE:
                if ((cellVal != STR_SIM_NONE) &&
                    (cellVal != STR_SIM_TEST_U) &&
                    (cellVal != STR_SIM_TEST_S)) {
                        w2ui['ioarea'].editField(event.recid, event.column);
                    }
                    break;
                case COLUMN_STATUS:
                case COLUMN_VALUE:
                    if (cellVal != STR_STS_NONE) {
                        w2ui['ioarea'].editField(event.recid, event.column);
                    }
                    break;
                case COLUMN_COMMENT:
                    w2ui['ioarea'].editField(event.recid, event.column);
                    break;
                default:
                    break;
            }
        }
    });

    // w2ui grid Double Click Event
    w2ui['ioarea'].on('dblClick', function (event) {
        event.preventDefault();
    });
    // unselect event
    w2ui['ioarea'].on('unselect', function (event) {
      event.onComplete = function () {
        g_select_grid_row = this.getSelection();
      }
    });
    // select event
    w2ui['ioarea'].on('select', function (event) {
        // paint select cell or column
        var grid = this;
        crntSelType = grid.selectType;
        if (crntSelType == SELGRID_CELL) {
          if (event.column == 0) {	// Click Number column. Add check undefined
            grid.selectNone();
            grid.selectType = SELGRID_ROW;	// to row select
            crntSelType = grid.selectType;	//memory of current mode
            grid.refresh();
          }
        }
        else if (crntSelType == SELGRID_ROW) {
          if (!(event.column == 0) && !(event.column == undefined)) {	// valid column
            grid.selectNone();
            grid.selectType = SELGRID_CELL;	// to cell select
            crntSelType = grid.selectType;	//memory of current mode
            grid.refresh();
          }
        }
        event.onComplete = function () {
            g_select_grid_row = w2ui['ioarea'].getSelection();
        }
    });
    // copy event
    w2ui['ioarea'].on('copy', function (event) {
      if (crntSelType == SELGRID_ROW) {
        var copyRowArr = w2ui['ioarea'].get(w2ui['ioarea'].getSelection());
        var copydata = [];
        for(var i = 0; i < copyRowArr.length;i++) {
          var simData = "";
          var valueData = "";
          var stateData = "";

          if(copyRowArr[i].simulate === '*' ||
             copyRowArr[i].simulate === '2' ||
             copyRowArr[i].simulate === '3') {
            simData = CODETAB;
          } else if(copyRowArr[i].simulate === '0' ||
                    copyRowArr[i].simulate === '1') {
            simData = g_io_simulate[copyRowArr[i].simulate].text + CODETAB;
          }

          if(copyRowArr[i].value === '*') {
            valueData = CODETAB;
          } else if(copyRowArr[i].value !== '') {
            valueData = copyRowArr[i].value + CODETAB;
          }

          if(copyRowArr[i].state === '*') {
            stateData = CODETAB;
          } else if(copyRowArr[i].state !== '') {
            stateData = g_io_status[copyRowArr[i].state].text + CODETAB;
          }
          // for comment
          copydata[i] = simData + valueData + stateData + copyRowArr[i].comment;
        }
        copyEventText.text = copydata;
      } else {
        copyEventText.text = event.text.split(CODELF);
        copyEventText.key = '';
      }
    });
    // Paste Event
  w2ui['ioarea'].on('paste', function (event) {
    if (window.frameElement.id !== 'prim' && !tpEnb) {
      top.IHMIComponents.cf.toast.show(resources['ERREditIoTpOff']);
    } else if (typeof copyEventText.text !== 'undefined') {
      checkIOData();
    }
    event.preventDefault();
    return false;
  });
    // w2ui grid keydown Event
    w2ui['ioarea'].on('keydown', function (event) {
        var kdnKeyCode = event.originalEvent.keyCode;
        var curSelGridRow = $.extend({}, g_select_grid_row);
        if ((kdnKeyCode != KEYCODE_LFARW) && (kdnKeyCode != KEYCODE_UPARW)
              && (kdnKeyCode != KEYCODE_RTARW) && (kdnKeyCode != KEYCODE_DNARW)) {
                var recid = '';
                var column = '';
                if(crntSelType == SELGRID_ROW) {
                  recid = w2ui['ioarea'].getSelection()[0];
                  column = COLUMN_INDEX;
                } else {
                  recid = w2ui['ioarea'].getSelection()[0].recid;
                  column = w2ui['ioarea'].getSelection()[0].column;
                }
                blockGridEvent(event, recid, column);
        }

        if (kdnKeyCode == KEYCODE_RTARW && crntSelType == SELGRID_ROW) {
            w2ui['ioarea'].selectNone(); // If selected number cell, select comment cell (and change crntSelType).
            if(w2ui['ioarea'].getColumn(IO_CLM_ARR[COLUMN_SIMULATE]).hidden) {  // for Ftab
              w2ui['ioarea'].select({ recid: curSelGridRow[0], column: COLUMN_STATUS });
            } else {
              w2ui['ioarea'].select({ recid: curSelGridRow[0], column: COLUMN_SIMULATE });
            }
            event.preventDefault();
            event.stopPropagation();
        }
        else if ((event.originalEvent.ctrlKey) && (kdnKeyCode == KEYCODE_X)) { // if input key code of cut process
            // copy process
            if (window.frameElement.id !== 'prim' && !tpEnb) {
              top.IHMIComponents.cf.toast.show(resources['ERREditIoTpOff']);
            } else {
              w2ui['ioarea'].copy();
              clearIOCmtProc();
            }
            event.preventDefault();
            event.stopPropagation();
        }
        else if ((kdnKeyCode == KEYCODE_DEL)) { // input delete key
            if (window.frameElement.id !== 'prim' && !tpEnb) {
              top.IHMIComponents.cf.toast.show(resources['ERREditIoTpOff']);
            } else {
              clearIOCmtProc();
            }
            event.preventDefault();
            event.stopPropagation();
        }
        else if (kdnKeyCode == KEYCODE_TAB) {
            $("#ioarea").w2grid().focus();
            if ((crntSelType == SELGRID_ROW) && !(event.originalEvent.shiftKey)) {
              w2ui['ioarea'].selectNone(); // If selected number cell, select comment cell (and change crntSelType).
              w2ui['ioarea'].select({ recid: curSelGridRow[0], column: COLUMN_SIMULATE });
            } else if (crntSelType == SELGRID_CELL) {
              var selGrid = w2ui['ioarea'].getSelection();
              var nextCol = ((event.originalEvent.shiftKey) ? selGrid[0].column - 1 : selGrid[0].column + 1);
              if ((nextCol <= COLUMN_COMMENT) && (nextCol >= COLUMN_INDEX)) {
                  for (; w2ui['ioarea'].getColumn(IO_CLM_ARR[nextCol]).hidden;) {
                      // If the cell it is trying to select is hidden, select next cell.
                      nextCol = ((event.originalEvent.shiftKey) ? nextCol - 1 : nextCol + 1);
                  }
                  w2ui['ioarea'].selectNone();
                  w2ui['ioarea'].select({ recid: curSelGridRow[0].recid, column: nextCol });
                  w2ui['ioarea'].scrollIntoView();
              }
            }
            event.preventDefault();
            event.stopPropagation();
        }
    });

    // w2ui grid Change Event
    w2ui['ioarea'].on('change', function (event) {
        var recid = Number(event.recid);
        var index = Number(recid) + g_current_item_no_start;
        var next_val = event.value_new;
        var original_val = event.value_original;
        if ((original_val == next_val) || ((next_val === "") && (event.column != COLUMN_COMMENT))) {
            event.preventDefault();
        }
        event.onComplete = function () {
            if (event.column == COLUMN_COMMENT
                && countChar(next_val) > IO_COMMENT_MAX) {
                event.value_new = multByteStringSlice(next_val, IO_COMMENT_MAX);
                next_val = event.value_new;
                top.openCmpAlert(window, 'errTooManyCharsDlg', resources['ERRTooManyChars']);
            }
            w2ui['ioarea'].mergeChanges();
            w2ui['ioarea'].save();
            //w2ui['ioarea'].getChanges();
            switch (event.column) {
                case COLUMN_SIMULATE:
                    setIOSimColor(recid, next_val);
                    switch (w2ui['ioarea'].get(recid).simulate) {
                        case STR_SIM_OFF:
                            setIOunsim(g_cur_type, index, setIOunsimCallback);
                            break;
                        case STR_SIM_ON:
                            setIOsim(g_cur_type, index, setIOsimCallback);
                            break;
                    }
                    break;
                case COLUMN_STATUS:
                    if (next_val !== "") {
                        setIOStsColor(recid, next_val);
                        setIOvalue(g_cur_type, index, next_val, setIOvalueCallback, original_val);
                    }
                    break;
                case COLUMN_VALUE:
                    if (next_val !== "") {
                        setIOvalue(g_cur_type, index, next_val, setIOvalueCallback, original_val);
                    }
                    break;
                case COLUMN_COMMENT:
                    top.setIOcomment(g_cur_type, index, next_val, setIOcommentCallback);
                    break;
            }
        }
    });

    w2ui['ioarea'].on('editField', function (event) {
      if (window.frameElement.id !== 'prim' && !tpEnb) {
        top.IHMIComponents.cf.toast.show(resources['ERREditIoTpOff']);
        event.preventDefault();
      }
    });
}

/*
 * Get sim, value, comment.
 */
function getIOAllCB(status, iotype, index, count, value, callbackarg1, callbackarg2) {
  for (var i = 0; i < count; i++) {
    getIOsimCallback(status, iotype, value[i].index, (value[i].sim === "") ? STR_STS_NONE : value[i].sim);
    updIOvalue(status, iotype, value[i].index, (value[i].val === "") ? STR_STS_NONE : value[i].val);
    getIOcommentCallback(status, iotype, value[i].index, value[i].comment);
  }
  dispOffWaiting('initio');
  top.rpcmc_startIOGroupMonitor(g_cur_type, g_current_item_no_start, g_current_item_no_end - g_current_item_no_start + 1);
}
/*
 * Initializing IO data when change page
 */
function initIOPage() {
    var linebuff = [];
    w2ui['ioarea'].clear();
    if (getIOMaxNum(g_cur_type) == 0) {
      // Function key and TP key had be hidden, readjust the screen size.
      $(window).trigger('resize');
      dispOffWaiting('initio');
      return;
    }
    for (var i = g_current_item_no_start; i <= g_current_item_no_end; i++) {
        var newline = createrow();
        newline.recid = linebuff.length;
        newline.index = g_str_cur_type + '[' + String(i) + ']';
        linebuff.push(newline)
    }
    if (linebuff.length != 0) {
        w2ui['ioarea'].add(linebuff);
    }
    //w2ui['ioarea'].refresh();
    if (g_select_grid_row != null) {
        w2ui['ioarea'].select(g_select_grid_row[0]);
    }
    // For rpcmc_startIOGroupMonitor. rpcmc_startIOGroupMonitor is called by cmpInitIOPage.
    top.rpcmc_stopIOGroupMonitor(g_cur_type, g_current_item_no_start, g_current_item_no_end - g_current_item_no_start + 1);
    top.get_ioall(g_cur_type, g_current_item_no_start, g_current_item_no_end - g_current_item_no_start + 1, getIOAllCB);
    // Function key and TP key had be hidden, readjust the screen size.
    $(window).trigger('resize');
}

/*
 * Grid Row Create
 */
function createrow() {
    var empty = {};
    empty.recid = "";
    empty.index = "";
    empty.simulate = "";
    empty.state = "";
    empty.value = "";
    empty.comment = "";
    empty.w2ui = {};
    empty.w2ui.style = {
        1: "background-color: #ffffff; color:#000000",
        2: "background-color: #ffffff; color:#000000",
        3: "background-color: #ffffff; color:#000000",
        4: "background-color: #ffffff; color:#000000"
    };
    return empty;
}

/*
 * Update value of I/O on the screen to the latest state.
 */
function updIOvalue(sts, type, index, value) {
  if (index < g_current_item_no_start
    || index > g_current_item_no_end
    || type != g_cur_type) {
    return;
  }
  var recid = index - g_current_item_no_start;
  var cur_val = "";
  var next_val = "";

  if (sts == IO_SUCCESS) {
    next_val = value;
  } else if (sts == IOSTAT_NOT_IO_ALLOC) {
    next_val = STR_STS_NONE;
  } else if (sts == IOSTAT_PRIO_023) {
    next_val = STR_STS_NONE;
  } else {
    // no action
  }
  switch (Number(type)) {
    case IO_TYPE_DIN:
    case IO_TYPE_DOUT:
    case IO_TYPE_RIN:
    case IO_TYPE_ROUT:
    case IO_TYPE_FLAG:
      cur_val = w2ui['ioarea'].get(recid).state;
      if (cur_val != next_val) {
        w2ui['ioarea'].get(recid).state = next_val;
        setIOStsColor(recid, next_val);
      } else if (sts == null) {
        setIOStsColor(recid, next_val);
      }
      break;
    case IO_TYPE_AIN:
    case IO_TYPE_AOUT:
    case IO_TYPE_GIN:
    case IO_TYPE_GOUT:
      cur_val = w2ui['ioarea'].get(recid).value;
      if (cur_val != next_val) {
        w2ui['ioarea'].get(recid).value = next_val;
        setIOValColor(recid, next_val);
      }
      break;
  }
}
function chgIOvalueEvt(event, type, index, value) {
  if (type == top.tpin_type_c && index == top.tp_enbl_c) {
    tpEnb = !(Number(value) == 0);
    return;
  }
  if (index < g_current_item_no_start
    || index > g_current_item_no_end
    || type != g_cur_type) {
    return;
  }
  var recid = index - g_current_item_no_start;
  var cur_val;
  if ((type == IO_TYPE_AIN) || (type == IO_TYPE_AOUT) || (type == IO_TYPE_GIN) || (type == IO_TYPE_GOUT)) {
    cur_val = w2ui['ioarea'].get(recid).value;
  } else {
    cur_val = w2ui['ioarea'].get(recid).state;
  }
  if (cur_val == STR_STS_NONE) {
    updIOvalue(IOSTAT_NOT_IO_ALLOC, type, index, cur_val);
  } else {
    updIOvalue(IO_SUCCESS, type, index, value);
  }
}

/*
 * Block w2ui grid event when not I/O Allocate
 */
function blockGridEvent(event, recid, column) {
    switch (column) {
        case COLUMN_SIMULATE:
            if (w2ui['ioarea'].getCellValue(recid, COLUMN_SIMULATE, false) == STR_SIM_NONE) {
                event.preventDefault();
            }
        case COLUMN_STATUS:
            if (w2ui['ioarea'].getCellValue(recid, COLUMN_STATUS, false) == STR_STS_NONE) {
                event.preventDefault();
            }
            break;
        case COLUMN_VALUE:
            if (w2ui['ioarea'].getCellValue(recid, COLUMN_VALUE, false) == STR_STS_NONE) {
                event.preventDefault();
            }
            break;
    }
}

/*
 * IO Status Color Setting
 */
function setIOStsColor(recid, val) {
    switch (val) {
        case STR_STS_OFF:
            w2ui['ioarea'].get(recid).w2ui.style[COLUMN_STATUS] = STYLE_GRID_ORANGE;
            break;
        case STR_STS_ON:
            w2ui['ioarea'].get(recid).w2ui.style[COLUMN_STATUS] = STYLE_GRID_GREEN;
            break;
        case STR_STS_NONE:
            w2ui['ioarea'].get(recid).w2ui.style[COLUMN_STATUS] = STYLE_GRID_INVALID;
            break;
        default:
            w2ui['ioarea'].get(recid).w2ui.style[COLUMN_STATUS] = STYLE_GRID_DEFAULT;
            break;
    }
    w2ui['ioarea'].refreshCell(recid, 'state');
}

/*
 * IO Value Color Setting
 */
function setIOValColor(recid, val) {
    switch (val) {
        case STR_STS_NONE:
            w2ui['ioarea'].get(recid).w2ui.style[COLUMN_VALUE] = STYLE_GRID_INVALID;
            break;
        default:
            w2ui['ioarea'].get(recid).w2ui.style[COLUMN_VALUE] = STYLE_GRID_DEFAULT;
            break;
    }
    w2ui['ioarea'].refreshCell(recid, 'value');
}

/*
 * IO Simulate Color Setting
 */
function setIOSimColor(recid, val) {
  switch (val) {
    case STR_SIM_ON:
      w2ui['ioarea'].get(recid).w2ui.style[COLUMN_SIMULATE] = STYLE_GRID_YELLOW;
      break;
    case STR_STS_NONE:
      w2ui['ioarea'].get(recid).w2ui.style[COLUMN_SIMULATE] = STYLE_GRID_INVALID;
      break;
    case STR_SIM_TEST_U:
    case STR_SIM_TEST_S:
      w2ui['ioarea'].get(recid).w2ui.style[COLUMN_SIMULATE] = STYLE_GRID_YELLOW;
      break;
    default:
      w2ui['ioarea'].get(recid).w2ui.style[COLUMN_SIMULATE] = STYLE_GRID_DEFAULT;
      break;
  }
  w2ui['ioarea'].refreshCell(recid, 'simulate');
}

/*
 * Set I/O Simulate(for select box)
 */
function setIOSimulate(record, index, col_index) {
  var html = '';
  if (this.getCellValue(index, col_index) == STR_STS_NONE) {
    return STR_STS_NONE;
  }
  for (var s in g_io_status) {
    if (g_io_simulate[s].id == this.getCellValue(index, col_index)) html = g_io_simulate[s].text;
  }
  if ((STR_SIM_TEST_U == this.getCellValue(index, col_index)) ||
      (STR_SIM_TEST_S == this.getCellValue(index, col_index))) {
      html = g_io_simulate[1].text;
  }
  return html;
}

/*
 * Set I/O Status(for select box)
 */
function setIOStatus(record, index, col_index) {
    var html = '';
    if (this.getCellValue(index, col_index) == STR_STS_NONE) {
        return STR_STS_NONE;
    }
    for (var s in g_io_status) {
        if (g_io_status[s].id == this.getCellValue(index, col_index)) html = g_io_status[s].text;
    }
    return html;
}

