// Show tree menu
function getTreeMenuCallback(menuData, menuType){
  var menu = makeTreeMenu(menuType, menuData)
    .replace(/&/g, "&amp;");

  if(menuType == BASE.treemenutype){
    var treeListID ='#' + BASE.treelistid;
    $(treeListID).append(menu);
    treeCnt++;
  }
  else if(menuType == CUSTOM.treemenutype){
    var treeListID ='#' + CUSTOM.treelistid;
    $(treeListID).append(menu);
    treeCnt++;
  }

  if(treeCnt == GET_TREE_CMP){
    initTree();
    // Display messages that cannot be used except in Japanese and English
    if(top.mainfrm.prim.document.getElementsByClassName('popup-frame').length == 0) {
      setAvailableMsg();
    }
  }
}

function hasErrSec() {
  var inputData = document.getElementsByTagName('input');
  if(inputData.length > 0 && isNgSectionName(inputData[0].value)) {
    return true;
  } else {
    return false;
  }
}

// Display messages that cannot be used except in Japanese and English
function setAvailableMsg(){
  var langtype = top.irprogapi.getCurrentLanguage();

  function setMsg(){
    if((langtype != 'jp') && (langtype != 'eg')){
      top.openCmpAlert(window, 'availableDlg', resources["availableMsg"], true);
    }
  }

  if(langtype != null) {
    setMsg();
  } else {
    setTimeout(function(){
      langtype = top.irprogapi.getCurrentLanguage();
      if(langtype != null) {        
        setMsg();
      }
    },500);
  }
}

// Make dom
function makeTreeMenu(menuType, menuData){
  var treeDom = "";
  var list =
  '<div data="li">';
  var indent =
  '<div class="base_list" data="ul">';

  var section = 
  '<div class="tree_node parent" >' +
  '<ins class="tree_ico open"></ins>';

  var item =
  '<div class="tree_node">'+
  '<span>';
  var itemEnd = '</span>';
  
  var divEnd = '</div>';
  
  var urlStart = '<div hidden>';
  var urlEnd = '</div>';

  // change indent tag for custom menu
  if(menuType == CUSTOM.treemenutype){
    indent = '<div class="custom_list" data="ul">';
  }

  for(var i in menuData){
    if(menuData[i].children.length > 0 || menuData[i].data.value == ""){
      var sectionName = menuData[i].data.text.toUpperCase();
      if(!(sectionName.match(ngStr))){ 
        treeDom +=
        list + section + menuData[i].data.text +
        urlStart + menuData[i].data.value + urlEnd +
        divEnd + indent + makeTreeMenu(menuType, menuData[i].children);
      }
    }  
    else if(menuData[i].data.value != "" || menuData[i].children.length == 0){
      treeDom += 
        list + item + menuData[i].data.text + itemEnd +
        urlStart + menuData[i].data.value + urlEnd +
        divEnd + divEnd;   
    }
  }
  treeDom += divEnd + divEnd;

  return treeDom;
}

// Custom tree initialize
function initTree() {
  var sortableTree = document.getElementsByClassName(BASE.treelistclass); 
  var customTree = document.getElementsByClassName(CUSTOM.treelistclass);
  var buttonTree = document.getElementById('button_tree');

  function cloneFunc() {
    cloneFlag = true;
  }

  function endFunc(event) {
    var li = event.clone;
    var childSortable = li.getElementsByClassName(BASE.treelistclass);
    cloneFlag = false;

    // Is section button
    if (li.classList.contains('button_li')) {
      var sectionBtn = document.getElementById('section_button');
      upSectionBtn();

      // button.js
      sectionBtn.removeEventListener('touchmove', setSectionBtnMoveFlag);
      sectionBtn.removeEventListener('touchend', clickSectionBtn);
      sectionBtn.removeEventListener('click', clickSectionBtn);
      sectionBtn.addEventListener('touchmove', setSectionBtnMoveFlag, false);
      sectionBtn.addEventListener('touchend', clickSectionBtn, false);
      sectionBtn.addEventListener('click', clickSectionBtn, false);
    }

    if (addFlag) {
      addFlag = false;

      if (childSortable.length) {
        for (var i = 0; i < childSortable.length; i++) {
          var ul = childSortable[i];
          var obj = new Sortable(ul, {
            group: {
              name: 'sortable',
              pull: 'clone',
              put: false
            },
            animation: 180,
            sort: false,
            onClone: cloneFunc,
            onEnd: endFunc,
            onChoose: chooseFunc,
            onUnchoose: unChooseFunc,
            onUpdate: moveEndFunc,
            filter: "input",
          });
          sortableArray.push(obj);
        }
      }
    }
  }

  // Initialize sortable tree
  for (var i = 0; i < sortableTree.length; i++) {
    var obj = new Sortable(sortableTree[i], {
      group: {
        name: 'sortable',
        pull: 'clone',
        put: false
      },
      animation: 180,
      sort: false,
      onClone: cloneFunc,
      onEnd: endFunc,
      onChoose: chooseFunc,
      onUnchoose: unChooseFunc,
      onUpdate: moveEndFunc,
      filter: "input",
    });
    sortableArray.push(obj);
  }

  // Initialize custom tree
  customArray = [];
  for (var i = 0; i < customTree.length; i++) {
    var obj = new Sortable(customTree[i], {
      group: 'sortable',
      animation: 180,
      onAdd: addFunc,
      onChoose: chooseFunc,
      onUnchoose: unChooseFunc,
      onUpdate: moveEndFunc,
      filter: "input",
    });
    customArray.push(obj);
  }

  // Initialize button tree
  sectionBtnObj = {};
  sectionBtnObj = new Sortable(buttonTree, {
    group: {
      name: 'sortable',
      pull: 'clone',
      put: false
    },
    animation: 180,
    sort: false,
    onClone: cloneFunc,
    onEnd: endFunc,
    onChoose: chooseFunc,
    onUnchoose: unChooseFunc,
    onUpdate: moveEndFunc,
    filter: "input",
  });

  // Click event initialize
  bindDbclickEvent(CUSTOM.treelistclass);
  bindOpenCloseEvent(BASE.treelistclass);
  bindOpenCloseEvent(CUSTOM.treelistclass);
  bindSelectEvent(BASE.treelistclass);
  bindSelectEvent(CUSTOM.treelistclass);
  initScrollbar();
}
