function changeAsstCombo(cmdId, selId, iptId, asstId, selChg) {
	var selidStr = '#' + selId;
	var iptidStr = '#' + iptId;
	var currentCombo = $(selidStr).parent('.cmdcombo');
	var asstStr = $(selidStr, currentCombo).val();
	var indAsstParam = {};
	indAsstParam.cmdId = cmdId;
	indAsstParam.selId = selId;
	indAsstParam.iptId = iptId;
	indAsstParam.selChg = selChg;
	setTimeout(function () {
		$(selidStr + ' option:selected').attr("selected", false);
	}, 0);
	// If specified indirect item, display input text box of element[].
	var hideList = [];
	var showList = [];
	var indElemNum = parseInt(selId.substr((selId.lastIndexOf('-') + 1)));
	if (isIndirectItem(asstStr)) {	// indirect item...
		showList = [indElemNum + 1, indElemNum + 2, indElemNum + 3];
		asstStr = asstStr.substr(0, asstStr.indexOf('['));	// pickup register string.
	} else {
		if ((asstId == ASSIST_STD) || (asstId == ASSIST_REG)) {
			hideList = [indElemNum + 1, indElemNum + 2, indElemNum + 3];
		}
	}
	changeShowHideCellControl((cmdId == 'CMDSUBSTR') ? DLGID_STR : DLGID_CMD, hideList, showList);
	appendIndAsstVal(asstStr, indAsstParam);
