function toggleJog() {
  visJog ^= JOG_BIT;
  if (isNaN(visJog)) {
    visJog = DEF_BIT;
  }
  top.rpcmc_setVar(top.SYSNAME_C, TPGL_VISIBLE_JOG, visJog);
}

function rewriteNodemap(status, prog_name, var_name) {
  FRCCanvas.draw3d_SendPkt(TPGL_REINITDRAW_FC, scene.ctrl_data, undefined);
}

function toggleNode(){
  if (visNode > 0) {
    visNode = 0;
    top.setVarWithCallback(top.SYSNAME_C, TPGL_UPDATE_NODE, visNode, rewriteNodemap);
  }
  else {
    visNode = NODE_BIT;
    top.rpcmc_setVar(top.SYSNAME_C, TPGL_UPDATE_NODE, visNode);
    top.IHMIComponents.cf.toast.show(notice_lmt, null);
  }
}

function setMaxNumGP(progname, varname, typecode, str) {
  max_grp = parseInt(str);
}

function setCurGP(progname, varname, typecode, str) {
  cur_grp = parseInt(str);
}

function setVisJogColor(str) {
  visJog = parseInt(str);
  if (visJog & JOG_BIT) {
    $('#assist_onoff').css({'background-color' : '#0E6EB8'});
  } else {
    $('#assist_onoff').css({'background-color' : 'transparent'});
  }
}

function setVisNodeColor(str) {
  if (str == "TRUE") {
    visNode = NODE_BIT;
  }
  else {
    visNode = 0;
  }
  if (visNode > 0) {
    $('#node_onoff').css({'background-color' : '#0E6EB8'});
  } else {
    $('#node_onoff').css({'background-color' : 'transparent'});
  }
}

function setSingDispColor(str) {
  if (str == STR_VAL_TRUE) {
    singDispVal = SING_DISP_VALID;
    $('#sing_disp_onoff').css({'background-color' : '#0E6EB8'});
  } else {
    singDispVal = 0;
    $('#sing_disp_onoff').css({'background-color' : 'transparent'});
  }
}

function setCurrVisNode(progname, varname, typecode, str) {
  if (str == "TRUE") {
    curr_visNode = NODE_BIT;
  }
  else {
    curr_visNode = 0;
  }
  if (isNaN(curr_visNode)) {
    curr_visNode = NODE_BIT;
  }
}

function setSingDispCurrStat(progname, varname, typecode, str) {
  setSingDispColor(str);
}

function changeView(view_num) {
  switch (view_num) {
    case VIEW_DEFAULT:
      scene.view.x = -50;
      scene.view.y = -300;
      scene.view.z = 0;
      scene.view.w = 0.5;
      scene.view.p = 45;
      scene.view.r = 45;
      break;
    case VIEW_FRONT:
      scene.view.x = 0;
      scene.view.y = 0;
      scene.view.z = 450;
      scene.view.w = 0.5;
      scene.view.p = 0;
      scene.view.r = 0;
      break;
    case VIEW_RIGHT:
      scene.view.x = 90;
      scene.view.y = -150;
      scene.view.z = 450;
      scene.view.w = 0.5;
      scene.view.p = 0;
      scene.view.r = 90;
      break;
    case VIEW_LEFT:
      scene.view.x = 80;
      scene.view.y = 0;
      scene.view.z = 450;
      scene.view.w = 0.5;
      scene.view.p = 0;
      scene.view.r = 270;
      break;
    case VIEW_TOP:
      scene.view.x = 200;
      scene.view.y = 0;
      scene.view.z = 800;
      scene.view.w = 0.5;
      scene.view.p = 90;
      scene.view.r = 0;
      break;
    default:
      break;
  }
  scene.SetView(true);
}

function restartCurposMonitor(newtype) {
    top.rpcmc_stopCurposMonitor(cur_postype);
    top.rpcmc_startCurposMonitor(newtype, CPM_INT);
}

function statVarEvent(event, progName, varName, typeCode, valStr) {
  switch(varName) {
    case TPGL_VISIBLE_JOG:
      setVisJogColor(valStr);
      break;
    case TPGL_UPDATE_NODE:
      setVisNodeColor(valStr);
      break;
    case JCR_GP:
      cur_grp = parseInt(valStr);
      restartCurposMonitor(cur_postype);
      break;
    case TPGL_SING_DISP:
      setSingDispColor(valStr);
      break;
    default:
      break;
  }
}

function selectCurposTab(newtype, oldtype){
  switch (oldtype) {
    case PMON_MON_CURPOS_JOINT_C:
      $('#pos_joint_tab').removeClass('pos_tab_selected');
      break;
    case PMON_MON_CURPOS_WORLD_C:
      $('#pos_world_tab').removeClass('pos_tab_selected');
      break;
    case  PMON_MON_CURPOS_USER_C:
      $('#pos_user_tab').removeClass('pos_tab_selected');
      break;
    default:
      break;
  }

  switch (newtype) {
    case PMON_MON_CURPOS_JOINT_C:
      $('#pos_joint_tab').addClass('pos_tab_selected');
      break;
    case PMON_MON_CURPOS_WORLD_C:
      $('#pos_world_tab').addClass('pos_tab_selected');
      break;
    case PMON_MON_CURPOS_USER_C:
      $('#pos_user_tab').addClass('pos_tab_selected');
      break;
    default:
      break;
  }
}

function changeCurpos(newtype){
  if (newtype == cur_postype) {
    return;
  }
  else {
    restartCurposMonitor(newtype);
  }
}

function statCurposEvent(event, montype, group_num, title, line) {
  var gnum = parseInt(group_num);
  selectCurposTab(montype, cur_postype);
  cur_postype = montype;
  if (gnum == cur_grp) {
    $('#pos_cpos_area').html("<div class='pos_grp'>" + "GP: " + group_num + "</div>");
    $('#pos_cpos_area').append("<div class='pos_title'>" + title + "</div>");
    for (var count = 0; count < line.length; count++) {
      $('#pos_cpos_area').append("<div class='pos_txt'>" + line[count].label + line[count].value + " " + line[count].unit + "</div>");
    }
    $('#pos_cpos_area').append("<div class='pos_bottom'></div>");
  }
}

function togglePosarea() {
  var curpos   = $('#curpos_onoff');
  var pos_area = $('#pos_area');
  var pos_diff_area = $('#pos_diff_area');
  if (curpos.hasClass('menu_icon_selected')) {
    curpos.removeClass('menu_icon_selected');
    pos_area.css({'display' : 'none'});
    top.rpcmc_stopCurposMonitor(cur_postype);
    parent.adin4Dfrm_adjust(false);
  }
  else {
    curpos.addClass('menu_icon_selected');
    pos_area.css({'display' : 'block'});
    restartCurposMonitor(cur_postype);
    parent.adin4Dfrm_adjust(true);
  }
}

function toggleSingDisp() {
  singDispVal = (singDispVal == SING_DISP_VALID) ? 0 : SING_DISP_VALID;
  top.rpcmc_setVar(top.SYSNAME_C, TPGL_SING_DISP, singDispVal);
}

function changeMonitor(kind) {
  var setMoni = 'change_4d';
  switch(kind) {
    case MONI_IO:
      setMoni = 'monitor_io';
      break;
    case MONI_REG:
      setMoni = 'monitor_reg';
      break;
    case MONI_POSREG:
      setMoni = 'monitor_posreg';
      break;
    case MONI_SAFEIO:
      setMoni = 'monitor_safeio';
      break;
    default:
      break;
  }
  parent.upperArea_selectHandling(setMoni);
}

function chgCurposDist(type) {
  var pos_cpos = $('#pos_cpos_tab');
  var pos_dist = $('#pos_dist_tab');
  var pos_cpos_area = $('#pos_cpos_area');
  var pos_diff_area = $('#pos_diff_area');
  if (type == 0) {
    pos_cpos.addClass("pos_tab_selected");
    pos_dist.removeClass("pos_tab_selected");
    pos_cpos_area.css({'display':'block'});
    pos_diff_area.css({'display':'none'});
  }
  else {
    pos_cpos.removeClass("pos_tab_selected");
    pos_dist.addClass("pos_tab_selected");
    pos_cpos_area.css({'display':'none'});
    pos_diff_area.css({'display':'block'});
  }
}

function getCurLinePos(montype) {
  var nextpos;
  var nextinfo = scene.GetObjectIf(NEXT_INFO_ID);

  if (nextinfo != undefined) {
    nextidx = nextinfo.text.split(",")[0];
    if (nextinfo.text.indexOf("GP:") >= 0) { 
      nextgnum = parseInt(nextinfo.text.split("GP:")[1]);
    }
    else {
      nextgnum = null;
    }
  }

  var is_cart = false;
  switch (montype) {
    case PMON_MON_CURPOS_WORLD_C: 
      nextpos = scene.GetObjectIf(NEXT_WPOS_ID);
      is_cart = true;
      break;
    case PMON_MON_CURPOS_USER_C: 
      nextpos = scene.GetObjectIf(NEXT_USER_ID);
      is_cart = true;
      break;
    case PMON_MON_CURPOS_JOINT_C:
      nextpos = scene.GetObjectIf(NEXT_JOINT_ID);
      break;
    default: 
      break;
  }

  if (nextpos != undefined) {
    if (is_cart) {
      pos_uf = parseInt(nextpos.text.split("UF:")[1]);
      pos_ut = parseInt(nextpos.text.split("UT:")[1]);
 
      var pos_x = parseFloat(nextpos.text.split("X:")[1]);
      var pos_y = parseFloat(nextpos.text.split("Y:")[1]);
      var pos_z = parseFloat(nextpos.text.split("Z:")[1]);
      posval = [pos_x, pos_y, pos_z];
    }
    else {
      var axs_num = parseInt(nextpos.text.split("AXS:")[1]);
      var jnt_posval = [];
      var jnt_axs_key;
      var jnt_axs_val;
      if (!isNaN(axs_num)) {
        for (var cnt=1; cnt <= axs_num; cnt++) {
          jnt_axs_key = "J" + cnt + ":";
          jnt_axs_val = parseFloat(nextpos.text.split(jnt_axs_key)[1]);
          jnt_posval.push(jnt_axs_val);
        }
      }
      posval = jnt_posval;
    }
  }
}

function resetDistText() {
  if (scene != undefined) {
    var nextinfo = scene.GetObjectIf(NEXT_INFO_ID);
    if (nextinfo != undefined) {
      nextinfo.text = tpglLangResource.tabtpgl_reset_disttxt_c;
    }
  }
}

function writeJointDist(line){
  var pos_diff_area = $('#pos_diff_area');
  if (posval.length > 0) {
    for (var count=0; count < posval.length; count++) {
      if ((posval[count] != undefined) && !(isNaN(posval[count])) &&
          (line[count] != undefined) && !(isNaN(line[count].value))) {
        var diff_value = parseFloat(line[count].value) - posval[count];
        diff_value = Math.round(diff_value * (10**3)) / (10**3);
        if (Math.abs(diff_value) <= 0.001) {
          diff_value = 0;
        }
        pos_diff_area.append("<div class='pos_txt'>" + line[count].label + " " + diff_value + " " + line[count].unit + "</div>");
      }
    }
  }
  else {
    pos_diff_area.append("<div class='pos_txt'>" +  tpglLangResource.tabtpgl_notchg_jnt_c + "</div>");
  }
}

function updateDistArea(title, line, montype) {
  var pos_diff_area = $('#pos_diff_area');
  getCurLinePos(montype);
  if (nextidx != undefined) {
    pos_diff_area.html("<div class='pos_next'>" + nextidx +  "</div>"); 
    if ((nextgnum != undefined) && (nextgnum != null) && (line != undefined)) {
      if (cur_grp == nextgnum) {
        pos_diff_area.append("<div class='pos_grp'>GP: " + nextgnum + "</div>");
        pos_diff_area.append("<div class='pos_title'>" + title + "</div>");

        if (montype == PMON_MON_CURPOS_JOINT_C) {
          writeJointDist(line);
          old_title = title;
          old_line = line;
          return;
        }

        var cur_uf;
        var cur_ut;
        if (montype == PMON_MON_CURPOS_USER_C) {
          cur_uf = parseInt(title.split(":")[1]);
          cur_ut = parseInt(title.split(":")[2]);
        }
        else if (montype == PMON_MON_CURPOS_WORLD_C) {
          cur_uf = 0;
          cur_ut = parseInt(title.split(":")[1]);
        }
        else {
          return;
        }

        if (isNaN(pos_uf) || isNaN(pos_ut)) {
         pos_diff_area.append("<div class='pos_txt'>" +  tpglLangResource.tabtpgl_notcalc_cart_c + "</div>");
          old_title = title;
          old_line = line;
          return;
        }
        if ((pos_uf != cur_uf)  && (pos_uf != 255)) {
          pos_diff_area.html("<div class='pos_txt'>" +  tpglLangResource.tabtpgl_uframe_diff_c + "</div>");
          old_title = title;
          old_line = line;
          return;
        }
        else if ((pos_ut != cur_ut) && (pos_ut != 255)) {
          pos_diff_area.html("<div class='pos_txt'>" +  tpglLangResource.tabtpgl_utool_diff_c + "</div>");
          old_title = title;
          old_line = line;
          return;
        }

        for (var count = 1; count <= 3; count++) {
          if (posval[count-1] != undefined) {
            var diff_value = parseFloat(line[count].value) - posval[count - 1];
            diff_value = Math.round(diff_value * (10**3)) / (10**3);
            if (Math.abs(diff_value) <= 0.001) {
              diff_value = 0;
            }
            pos_diff_area.append("<div class='pos_txt'>" + line[count].label + " " + diff_value + " " + line[count].unit + "</div>");
          }
          else {
            pos_diff_area.append("<div class='pos_txt'>" +  tpglLangResource.tabtpgl_notcalc_cart_c + "</div>");
            break;
          }
        }
      }
      else {
        pos_diff_area.html("<div class='pos_txt'>" +  tpglLangResource.tabtpgl_grpchg_c + "</div>");
      }
      old_title = title;
      old_line = line;
    }
  }
  else {
    pos_diff_area.html("<div class='pos_next'>" +  tpglLangResource.tabtpgl_calc_dist_c + "</div>");
  }
}

function ihmiedit_handlePipeEvent() {
  if (nextinfo_enb) {
    updateDistArea(old_title, old_line, cur_postype);
  }
}

function statCurposDiffEvent(event, montype, group_num, title, line) { 
  var pos_diff_area = $('#pos_diff_area');
  if (nextinfo_enb) {
    if (cur_grp == group_num) {
      updateDistArea(title, line, montype);
    }
  }
  else {
    pos_diff_area.html("<div class='pos_txt'>" +  tpglLangResource.tabtpgl_nextinf_dsbl_c + "</div>"); 
  }
}

function setVisibleFlag(progname, varname, typecode, str) {
  var visflag = parseInt(str);
  if (visflag != 255) {
    top.rpcmc_setVar(top.SYSNAME_C, TPGL_VISIBLE_NODE, 255);
    top.setVarWithCallback(top.SYSNAME_C, TPGL_UPDATE_NODE, 0, rewriteNodemap);
  }
}

function unloadTpgl(){
  top.rpcmc_stopVarMonitor(top.SYSNAME_C, TPGL_VISIBLE_JOG, 100);
  top.rpcmc_stopVarMonitor(top.SYSNAME_C, TPGL_UPDATE_NODE, 100);
  top.rpcmc_stopVarMonitor(top.SYSNAME_C, TPGL_SING_DISP, 100);
  top.rpcmc_stopCurposMonitor(PMON_MON_CURPOS_WORLD_C);
  top.rpcmc_stopCurposMonitor(PMON_MON_CURPOS_JOINT_C);
  top.rpcmc_stopCurposMonitor(PMON_MON_CURPOS_USER_C);
  top.jQuery.varlis.unbind('VarEvent', statVarEvent);
  top.jQuery.curposlis.unbind('CurposEvent', statCurposEvent);
  top.jQuery.curposlis.unbind('CurposEvent', statCurposDiffEvent);
  top.jQuery.filelis.unbind('PipeEvent', ihmiedit_handlePipeEvent);
  resetDistText();
}

function loadTpgl(){
  top.rpcmc_startVarMonitor(top.SYSNAME_C, TPGL_VISIBLE_JOG, 100);
  top.rpcmc_startVarMonitor(top.SYSNAME_C, TPGL_UPDATE_NODE, 100);
  top.rpcmc_startVarMonitor(top.SYSNAME_C, TPGL_SING_DISP, 100);
  top.jQuery.varlis.bind('VarEvent', null, statVarEvent);
  top.jQuery.curposlis.bind('CurposEvent', statCurposEvent);
  top.jQuery.curposlis.bind('CurposEvent', statCurposDiffEvent);
  top.jQuery.filelis.bind('PipeEvent', null, ihmiedit_handlePipeEvent);

  top.rpcmc_getVar(top.SYSNAME_C, TPGL_UPDATE_NODE, setCurrVisNode);
  top.rpcmc_getVar(top.SYSNAME_C, TPGL_VISIBLE_NODE, setVisibleFlag);
  top.rpcmc_getVar(top.SYSNAME_C, SCR_GP_NUM, setMaxNumGP);
  top.rpcmc_getVar(top.SYSNAME_C, JCR_GP, setCurGP);
  top.rpcmc_getVar(top.SYSNAME_C, TPGL_SING_DISP, setSingDispCurrStat);

  var curpos  = $('#curpos_onoff');
  if (curpos.hasClass('menu_icon_selected')) {
    restartCurposMonitor(cur_postype);
  }
}

function stopTpgl() {
  FRCCanvas.draw3d_SendPkt(TPGL_EXIT_FC, undefined, undefined);
  top.jQuery.filelis.unbind('PipeEvent', draw3d_HandlePipeEvent);
  unloadTpgl();
}

function restartTpgl() {
  if (scene === undefined) {
    setTimeout(function() { restartTpgl(); }, 100);
  }
  else {
    top.jQuery.filelis.unbind('PipeEvent', draw3d_HandlePipeEvent);
    top.jQuery.filelis.bind('PipeEvent', scene.ctrl_data, draw3d_HandlePipeEvent);
    FRCCanvas.draw3d_SendPkt(TPGL_EXIT_FC, undefined, undefined);
    FRCCanvas.draw3d_SendPkt(TPGL_INITWGLDRAW_FC, undefined, undefined);
    unloadTpgl();
    loadTpgl();
  }
}

function menu_open(){
  $('#menu_area').css({'display' : 'inline-block'});
  $('#menu_open_button').css({'display' : 'none'});
  $('#menu_close_button').css({'display' : 'inline-block'});
}

function menu_close(){
  $('#menu_area').css({'display' : ''});
  $('#menu_open_button').css({'display' : ''});
  $('#menu_close_button').css({'display' : ''});
}

function addMenu() {
  $('#j1').append("<div class='content_area' id='content_area'> </div>");
  $('#content_area').append("<div class='menu_area' id='menu_area'>" +
                              "<button class='menu_close_icon' id='menu_close_button' onclick='menu_close();'> </button>" +
                              "<div id='menu_icon_area'></div></div>");
  $('#content_area').append("<button class='menu_open_icon' id='menu_open_button' onclick='menu_open()'> </button>");
  $('#content_area').append("<div class='pos_area' id='pos_area'> </div>");
  $('#menu_icon_area').append("<div class='menu_area_row' id='menu_area_row_1'></div>" +
                              "<div class='menu_area_row' id='menu_area_row_2'></div>" +
                              "<div class='menu_area_row' id='menu_area_row_3'></div>");
  $('#menu_area_row_1').append("<button class='view_icon' id='view_default' onclick='changeView(VIEW_DEFAULT)'> </button>");
  $('#menu_area_row_1').append("<button class='view_icon' id='view_front' onclick='changeView(VIEW_FRONT)'> </button>");
  $('#menu_area_row_1').append("<button class='view_icon' id='view_right' onclick='changeView(VIEW_RIGHT)'> </button>");
  $('#menu_area_row_1').append("<button class='view_icon' id='view_left' onclick='changeView(VIEW_LEFT)'> </button>");
  $('#menu_area_row_1').append("<button class='view_icon' id='view_top' onclick='changeView(VIEW_TOP)'> </button>");
  $('#menu_area_row_2').append("<button class='menu_icon' id='node_onoff' onclick='toggleNode()'> </button>");
  $('#menu_area_row_2').append("<button class='menu_icon' id='assist_onoff' onclick='toggleJog()'> </button>");
  $('#menu_area_row_2').append("<button class='menu_icon' id='curpos_onoff'  onclick='togglePosarea()'> </button>");
  if (top.g_crx) {
    $('#menu_area_row_2').append("<button class='menu_icon' id='sing_disp_onoff'  onclick='toggleSingDisp()'> </button>");
  }
  $('#menu_area_row_3').append("<button class='menu_icon' id='change_io'  onclick='changeMonitor(MONI_IO)'> </button>");
  $('#menu_area_row_3').append("<button class='menu_icon' id='change_reg'  onclick='changeMonitor(MONI_REG)'> </button>");
  $('#menu_area_row_3').append("<button class='menu_icon' id='change_posreg'  onclick='changeMonitor(MONI_POSREG)'> </button>");
  if (top.g_order.j568) {
    $('#menu_area_row_3').append("<button class='menu_icon' id='change_safeio' onclick='changeMonitor(MONI_SAFEIO)'> </button>");
  }
  $('#pos_area').append("<div class='pos_btn_area' id='pos_btn_area'> </div>");
  $('#pos_area').append("<div class='pos_txt_area' id='pos_txt_area'> </div>");
  $('#pos_btn_area').append("<div class='pos_tab' id='pos_joint_tab' onclick='changeCurpos(PMON_MON_CURPOS_JOINT_C)'> <span class='pos_tab_txt'>" + joint_txt + "</span> </div>");
  $('#pos_btn_area').append("<div class='pos_tab' id='pos_world_tab' onclick='changeCurpos(PMON_MON_CURPOS_WORLD_C)'> <span class='pos_tab_txt'>" + world_txt + "</span> </div>");
  $('#pos_btn_area').append("<div class='pos_tab' id='pos_user_tab' onclick='changeCurpos(PMON_MON_CURPOS_USER_C)'> <span class='pos_tab_txt'>" + user_txt + "</span> </div>");
  $('#pos_txt_area').append("<div class='pos_cont_area' id='pos_cont_area'> </div>");
  $('#pos_cont_area').append("<div id='pos_dist_switch'> </div>");
  $('#pos_dist_switch').append("<div class='pos_tab pos_tab_selected' id='pos_cpos_tab' onclick='chgCurposDist(0)'> <span class='pos_tab_txt'>" + tpglLangResource.tabtpgl_position_c + "</span> </div>");
  $('#pos_dist_switch').append("<div class='pos_tab' id='pos_dist_tab' onclick='chgCurposDist(1)'> <span class='pos_tab_txt'>" + tpglLangResource.tabtpgl_distance_c + "</span> </div>");
  $('#pos_cont_area').append("<div class='pos_cpos_area' id='pos_cpos_area'> </div>");
  $('#pos_cont_area').append("<div class='pos_diff_area' id='pos_diff_area'> </div>");
}

function init_tabui4d() {
  scene = FRCCanvas.Scene;
  if (scene === undefined) {
    setTimeout(function(){init_tabui4d();}, 100);
  }
}

$(window).on('load', function() {
  init_tabui4d();
  addMenu();
  unloadTpgl();
  loadTpgl();
});

$(window).on('unload', function() {
  unloadTpgl();
});

