/* = Foundation definition = */
body {
	display: block;
	margin: 1px;
	font-size: 1.2vw;
	/* hide select shadow for iPad*/
	-webkit-tap-highlight-color:rgba(0,0,0,0);
	-webkit-text-size-adjust: none;
}

button, input, select, textarea {
  font-family : inherit;
}

button:disabled {
  pointer-events: none;
}

/* = Not strage Picture = */
/* - To prevent display of selectmenu in iPad -*/
img {
	-webkit-touch-callout: none;
	-moz-touch-callout: none;
	user-select: none;
	-webkit-user-select: none;
	-moz-user-select: none;
	vertical-align: top;
}
.saveselmenu {
	-webkit-touch-callout: none;
	-moz-touch-callout: none;
	user-select: none;
	-webkit-user-select: none;
	-moz-user-select: none;
}

.container {
	width: 100%;
	height: 100%;
	/*margin: 0 auto; A margin appears on the iPad. */
	margin: 0;
	font-size: 1em;
	background-color: #333333;
}

/* = Overall grid rayout = */
/* - for header layout - */
.grid1, .grid12, .grid2, .grid3, .grid35, .grid4, .grid5, .grid53, .grid6, .grid7, .grid8, .grid9 {
	float: left;
	display: block;
	margin-left: 0px;
}
.grid1	{width: 10%;}
.grid12 {width: 12%;}
.grid2	{width: 20%;}
.grid3	{width: 30%;}
.grid35 {width: 35%;}
.grid4	{width: 40%;}
.grid5	{width: 50%;}
.grid53 {width: 53%;}
.grid6	{width: 60%;}
.grid7	{width: 70%;}
.grid8	{width: 80%;}
.grid9	{width: 90%;}
.grid	{width:100.0%;}
.first {margin-left: 0;clear: left;}
/* - for layout area - */
.grid02, .grid15, .grid20, .grid25, .grid30, .grid38, .grid40, .grid48, .grid58, .grid60, .grid70 {
	float: left;
	display: block;
	margin-left: 0px;
}
.grid02	{width: 20px;}
.grid15	{width: 15%;}
.grid20	{width: 20%;}
.grid25 {width: 25%;}
.grid30	{width: 30%;}
.grid38	{width: calc(40% - 20px);}
.grid40	{width: 40%;}
.grid60 {width: calc(60% - 20px);}
.grid48	{width: calc(50% - 20px);}
.grid58	{width: calc(60% - 20px);}
.grid70	{width: 70%;}
.grid00	{width:100%;}

/* clearfix */
.clearfix:after {
	visibility: hidden;
	display: block;
	font-size: 0;
	content: " ";
	clear: both;
	height: 0;
