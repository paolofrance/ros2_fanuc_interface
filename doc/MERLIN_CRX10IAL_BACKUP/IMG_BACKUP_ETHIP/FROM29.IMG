// Add 'NEW' Record
function addBtnEvent() {
  document.getElementById('addBtn').refresh(null, null, null, null, true, null);
  var currentNo = ioAsgnGrid.getColumn(ASGN_CLM_ARR[ASGN_CLM_NO]);
  var newRecStartNum = 1;
  var newRecIONum;

  switch (asgnIOType) {
    case TAB_DI:
    case TAB_DO:
      for (var i = 0; i < currentNo.length; i++) {
        var ioRangeArry = currentNo[i].split('-');
        if (newRecStartNum == parseInt(ioRangeArry[0])) {
          newRecStartNum = parseInt(ioRangeArry[1]) + 1;
        } else {
          break;
        }
      }
      newRecIONum = newRecStartNum + '-' + newRecStartNum;
      break;
    case TAB_AI:
    case TAB_AO:
    case TAB_GI:
    case TAB_GO:
      for (var i = 0; i < currentNo.length; i++) {
        if (newRecStartNum == parseInt(currentNo[i])) {
          newRecStartNum = parseInt(currentNo[i]) + 1;
        } else {
          break;
        }
      }
      newRecIONum = String(newRecStartNum);
      break;
  }

  var currentIOMaxNum = getIOMaxNum(g_ary_tab_id[asgnIOType].tab);
  if (newRecStartNum > currentIOMaxNum) {
    top.openCmpAlert(window, 'errAsgnNewRow', resources['ERRAsgnNewRow'], false, enableAddBtn);
  } else {
    var newRecord = ({
      record: [
        {recVal: ASGN_ADD_STR}, // no
        {recVal: ''}, // type
        {recVal: ''}, // rack
        {recVal: ''}, // slot
        {recVal: ''}, // startNum
        {recVal: ''}, // channel
        {recVal: ''}, // numPoints
        {recVal: ''} // status
      ],
      initEditRec: {
        record: [
          {recVal: newRecIONum}, // no
          {recVal: 'Unknown'}, // type
          {recVal: '0'}, // rack
          {recVal: '0'}, // slot
          {recVal: '0'}, // startNum
          {recVal: '0'}, // channel
          {recVal: '0'}, // numPoints
          {recVal: 'INVALID', recColor: GRIDCOLOR_ERROR} // status
        ]
      }
    });
    ioAsgnGrid.appendRecord(newRecord);
    var rowNum = currentNo.length + 1;
    ioAsgnGrid.setSelectRow({start: rowNum, end: rowNum});
    updEditInf(rowNum, false);
  }

  function enableAddBtn() {
