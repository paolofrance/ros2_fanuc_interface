function createAsgnRec(data, PMCDevData) {
  var asgnData = [];
  var crntTabNum = g_ary_tab_id[asgnIOType].tab;

  for (var i = 0; i < data.length; i ++) {
    var singleRec = {record: []};
    var ioNum = Number(data[i].fst_log_port);
    if (ioNum > getIOMaxNum(crntTabNum)) {
      break;
    }
    if (crntTabNum == IO_TYPE_DIN || crntTabNum == IO_TYPE_DOUT) {
      ioNum += "-" + (ioNum + Number(data[i].n_log_ports) - 1);
    }
    singleRec.record[ASGN_CLM_NO] = {recVal: ioNum};
    singleRec.record[ASGN_CLM_TYPE] = {recVal: getDevName(data[i].rack_no, data[i].slot_no)};
    singleRec.record[ASGN_CLM_RACK] = {recVal: data[i].rack_no};
    singleRec.record[ASGN_CLM_SLOT] = {recVal: data[i].slot_no};
    singleRec.record[ASGN_CLM_STRTNUM] = {recVal: data[i].fst_phy_port};
    singleRec.record[ASGN_CLM_CHANNEL] = {recVal: data[i].fst_phy_port};
    singleRec.record[ASGN_CLM_NUMPTS] = {recVal: data[i].n_log_ports};
    singleRec.record[ASGN_CLM_STATUS] = setStatus(data[i].valid, String(crntTabNum), data[i].rack_no, data[i].slot_no, PMCDevData);
    asgnData.push(singleRec);
  }
  return asgnData;
}

function setStatus(status, ioType, rackNum, slotNum, PMCDevData) {
  //status: 1 = INVALID, 2 = ACTIVE, 3 = PENDING
  var statNum = Number(status);

  if (ioType == IO_TYPE_DIN || ioType == IO_TYPE_DOUT) {
    if ((statNum == ASGN_STAT_ACTIVE || statNum == ASGN_STAT_PENDING) && rackNum == 33 && slotNum > 7) {
      statNum = ASGN_STAT_PMC;
    } else {
      for (var i = 0; i < PMCDevData.length; i++) {
        if (rackNum == PMCDevData[i].rack &&
        slotNum == PMCDevData[i].slot &&
        ioType == PMCDevData[i].ioType) {
          statNum = ASGN_STAT_OCCUPY;
          break;
        }
      }
    }
  }

  var statColor = GRIDCOLOR_NONE;
  switch (statNum) {
    case ASGN_STAT_INVALID:
    case ASGN_STAT_OCCUPY:
      statColor = GRIDCOLOR_ERROR;
      break;
    case ASGN_STAT_PENDING:
      statColor = GRIDCOLOR_CAUTION;
      break;
    case ASGN_STAT_ACTIVE:
      statColor = GRIDCOLOR_NORMAL;
      break;
  }
  return {recVal: ASGN_STAT_VAL_ARR[statNum], recColor: statColor};
}

