function getIframeHeight() {
  var asgnWindowArea = document.getElementById('ioAsgnWindow').offsetHeight;
  var asgnUpLowArea = document.getElementById('asgnUpperArea').offsetHeight + document.getElementById('asgnLowerArea').offsetHeight;
  if (asgnWindowArea < asgnUpLowArea) {
    return asgnUpLowArea;
  }
  return asgnWindowArea;
}

// Setting of "select I/O module top position"
function getTopPosition() {
  var minTop = getIframeHeight() - 400 - POPUP_TOP;
  if (minTop <= $(window).scrollTop()) {
    return minTop + 'px';
  } else {
    return $(window).scrollTop() + POPUP_TOP + 'px';
  }
}

// Setting of "select I/O module popup Height"
function getPopHeight() {
  // maximum pop height
  var popY = parseInt(top.mainfrm.document.getElementById('prim').style.height.replace('px','')) - (POPUP_TOP * 2);
  var ioDevArray = getModuleEachIOtype(true);
  if (ioDevArray.length <= 5 || popY < 400) {
    // minimum pop height
    popY = 400;
  } else if ((Math.floor((ioDevArray.length + 1) * GRID_ROW_HEIGHT) + POPUP_TTL_BTN_AREA) < Math.floor(popY)) {
    // middle pop height
    popY = Math.floor((ioDevArray.length + 1) * GRID_ROW_HEIGHT) + POPUP_TTL_BTN_AREA;
  }
  return popY + 'px';
}

