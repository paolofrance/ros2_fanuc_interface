var g_win_y; // Current window Y size.
var g_select_tab = 'g1';
var g_group_max = MAX_GROUP_NUM;
var g_select_group = 1;

// Variables for copy and paste
var copyEventText = undefined;

// Initial display value of tab and page
var currentTabPage = 4097;
// Maximum number of bits on the page
var pageBitMax = Math.pow(2, PAGE_BIT_NUM) - 1;

var payloadGrid = new top.IHMIGridComponent();
var payloadTableDef;

/*
 * Tab Event Setting
 */
function setTabEvent() {
  var argObj = {showTabObj : {'g1' : true, 'g2' : false, 'g3' : false, 'g4' : false, 'g5' : false, 'g6' : false, 'g7' : false, 'g8' : false}, width : null};
  for (var i = 1; i <= g_group_max; i++) {
    argObj.showTabObj['g' + String(i)] = true;
  }

    // init set tab
    var selTabData = currentTabPage >>> PAGE_BIT_NUM;
    if (selTabData <= g_group_max && selTabData > 0) {
      g_select_tab = 'g' + selTabData;
    } else {
      g_select_tab = 'g1';
    }
    g_select_group = parseInt(g_select_tab.substr(1));
    document.getElementById('tabarea').refresh(null, g_group_max, g_select_tab, null, argObj);
    
    // tab click event
  document.getElementById('tabarea').setCallback(tabClick);
  function tabClick(id, operation, value) {
    if (g_select_tab != value) {
      g_select_tab = value;
      unloadVarMonitor();
      g_select_group = parseInt(g_select_tab.substr(1));
      loadVarMonitor();

      initPageArea(g_payload_max);
      initPayloadPage();
      var tabNum = g_select_tab.substring(1);
      currentTabPage = 1 + (tabNum << PAGE_BIT_NUM);
      sessionStorage.setItem('payloadSelData', currentTabPage);
    }
  }
}

/*
 * Grid Event Setting
 */
function setGridEvent(event, type, info) {
  if (payloadGrid.getSelectType() == SELGRID_NONE) {
    return;
  }
  switch(type) {
    case 'change':
      changeEvent(info);
      break;
    case 'copy':
      copyEvent();
      break;
    case 'paste':
      pasteEvent();
      break;
    case 'cut':
      copyEvent();
      clearPayloadData();
      break;
    case 'delete':
      clearPayloadData();
      break;
  }
}

// Change Event
function changeEvent(info) {
  var recid = Number(info.row);
  var index = Number(payloadGrid.getRecord(info.row).record[COLUMN_INDEX].recVal);
  var columnNum = info.col;
  var column = PAYLOAD_CLM_ARR[columnNum];
  var nextVal = info.val;
  var prevVal = info.prev;

  switch(columnNum) {
    case COLUMN_COMMENT:
      var errmsg = checkDataValue(nextVal, column);
      var sysVal = calcSysvarVal(nextVal, column);
      if(checkLangAndCharBytes(nextVal)){
        cancelPayloadChange();
        top.openCmpAlert(window, 'payloadErrValDlg', resources['ERR2ByteChars']);
      } else if(errmsg != "") {		// If the number of characters is exceeded.
        prevVal = sysVal;
        cancelPayloadChange();
        setPayloadData(g_select_group, index, column, sysVal);
        top.openCmpAlert(window, 'payloadErrValDlg', errmsg);
      } else {
        payloadGrid.setCellValue(recid, column, sysVal);
        setPayloadData(g_select_group, index, column, sysVal);
      }
      break;
    case COLUMN_PAYLOAD:
    case COLUMN_X:
    case COLUMN_Y:
    case COLUMN_Z:
    case COLUMN_IX:
    case COLUMN_IY:
    case COLUMN_IZ:
      var btnObj = {
        btnData:top.irpDlgBtnYN,
        btn1Cb:setPayloadChange,
        btn2Cb:cancelPayloadChange,
        closeBtnCb:cancelPayloadChange
      };
      top.openCmpConfirm(webPage, window, 'defaultNoticeDlg', resources["dlgDefaultNotice"], btnObj );
      break;
  }
  function setPayloadChange() {
    var errmsg = checkDataValue(nextVal, column);
    if (errmsg != "") {
      cancelPayloadChange();
      top.openCmpAlert(window, 'payloadErrValDlg', errmsg);
    } else {
      payloadGrid.setCellValue(recid, column, nextVal);
      var sysVal = calcSysvarVal(nextVal, column);
      var setPldInf = {};
      setPldInf.index = index;
      setPldInf[column] = sysVal;
      var backUpInf = [];
      backUpInf.push(createValArr(column, recid, prevVal));
      setPayloadDataIF(g_select_group, index, PLDCHK, setPayloadDataIFCB, setPldInf, backUpInf);
    }
  }
  function cancelPayloadChange() {
    payloadGrid.setCellValue(recid, column, prevVal);
  }
}

