// create XML with updated custom tree 
function updateCustomtree(customTree){
  var localXml = "";
  var tagEnd = '</FRTreeViewItem>';
  var headerStart = '<FRTreeViewItem Header="';
  var headerEnd = '">';
  var headerEnds = '"/>';
  var returnXml;

  if(customTree.length == 0) {
    return false;
  }

  for(var i = 0; i < customTree.length; i++){
    // section
   if(customTree[i].children.length == 2 || customTree[i].children[0].children[1].innerText == ""){
        
    returnXml = updateCustomtree(customTree[i].children[1].children);
    localXml += headerStart + customTree[i].children[0].innerText + headerEnd + returnXml + tagEnd;
    // section null
    if(returnXml == false){
      return false;
    }

   }// menu
   else if(customTree[i].children[0].children[1].innerText != ""){
      localXml += 
        headerStart + customTree[i].children[0].children[0].innerText + 
        '" url="' + customTree[i].children[0].children[1].innerText + headerEnds ;
   }
 }  
  return localXml;
}

/**
