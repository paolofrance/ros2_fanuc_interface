/*
 * Check paste IO Data
 */
function checkIOData() {
  var recid    = (crntSelType == SELGRID_ROW) ? g_select_grid_row[0] : g_select_grid_row[0].recid;
  var data_list = [];
  var write_count = 0;
  var copydata = copyEventText.text;
  var checkNum = /^[-]?([1-9]\d*|0)(\.\d+)?$/;
  var original_val;

  for (var i = 0; i < copydata.length; i++) {
    var cellData = copydata[i].split(CODETAB);
    var record = w2ui['ioarea'].records[recid + i];
    if (record == undefined) break;
    var nextRow = recid + i;
    var eventCol = (crntSelType == SELGRID_ROW) ? COLUMN_SIMULATE : g_select_grid_row[0].column;
    for (var j = 0; j < cellData.length; j++) {
      var nextCol = eventCol + j;
      var nextVal = cellData[j];
      write_count++;
      if ((nextCol > COLUMN_COMMENT) ||
        (record.w2ui.style[nextCol] == STYLE_GRID_INVALID)) {
        continue;
      }
      for (; w2ui['ioarea'].getColumn(IO_CLM_ARR[nextCol]).hidden;) {
        // If the cell it is trying to paste is hidden, select next cell.
        nextCol += 1;
        eventCol += 1;
      }
      switch (nextCol) {
        case COLUMN_SIMULATE:
          var nextCell = w2ui['ioarea'].getCellValue(nextRow, nextCol, false);
          if ((nextCell == STR_SIM_TEST_U) ||
              (nextCell == STR_SIM_TEST_S)) {
              break;
            }
          if (nextVal == g_io_simulate[0].text) { // Whether this paste data is "U" or "S"
            data_list.push(createValArr(nextCol, nextRow, STR_SIM_OFF));
          } else if (nextVal == g_io_simulate[1].text) {
            data_list.push(createValArr(nextCol, nextRow, STR_SIM_ON));
          }
          break;
        case COLUMN_STATUS:
          original_val = record.state;
          if(g_io_status[original_val].text == nextVal) {
            write_count--;
            break;
          }
          if (nextVal == g_io_status[0].text) { // Whether this paste data is "ON" or "OFF"
            data_list.push(createValArr(nextCol, nextRow, STR_STS_OFF));
          } else if (nextVal == g_io_status[1].text) {
            data_list.push(createValArr(nextCol, nextRow, STR_STS_ON));
          }
          break;
        case COLUMN_VALUE:
          original_val = record.value;
          if(original_val == nextVal) {
            write_count--;
            break;
          }
          if (checkNum.test(nextVal)) {
            nextVal = Number(nextVal);
            data_list.push(createValArr(nextCol, nextRow, nextVal));
          }
          break;
        case COLUMN_COMMENT:
          data_list.push(createValArr(nextCol, nextRow, nextVal));
          break;
        default:
          break;
      }
    }
  }
  if (data_list.length == write_count) {
    pasteIOData(data_list, original_val);
  } else {
    top.openCmpAlert(window, 'errPasteDlg', resources['ERRPaste']);
  }
}

/*
 * Paste IO Data
 */
function pasteIOData(data_list, original_val) {
  for (var i = 0; i < data_list.length; i++) {
    switch (data_list[i].col) {
      case COLUMN_SIMULATE:
        setIOSimColor(data_list[i].row, data_list[i].val);
        w2ui['ioarea'].get(data_list[i].row).simulate = data_list[i].val;
        if (data_list[i].val == STR_SIM_OFF) {
          setIOunsim(g_cur_type, data_list[i].row + g_current_item_no_start, setIOunsimCallback);
        } else if (data_list[i].val == STR_SIM_ON) {
          setIOsim(g_cur_type, data_list[i].row + g_current_item_no_start, setIOsimCallback);
        }
        break;
      case COLUMN_STATUS:
        setIOvalue(g_cur_type, data_list[i].row + g_current_item_no_start, data_list[i].val, setIOvalueCallback, original_val);
        break;
      case COLUMN_VALUE:
        setIOvalue(g_cur_type, data_list[i].row + g_current_item_no_start, data_list[i].val, setIOvalueCallback, original_val);
        break;
      case COLUMN_COMMENT:
        top.setIOcomment(g_cur_type, data_list[i].row + g_current_item_no_start, data_list[i].val, setIOcommentCallback);
        break;
      default:
        break;
    }
  }
  w2ui['ioarea'].save();
}

function createValArr(nextCol, nextRow, nextVal) {
  var setArr = {};
  setArr.col = nextCol;
  setArr.row = nextRow;
  setArr.val = nextVal;
  return setArr;
}

function setButtonEvent() {   
  // If there is any change data, send it all before switching.
  $("#page_prevbtn").on('click touchend', function (event) {
    if(event.type.indexOf('touch') >= 0){
      event.preventDefault();
    }
    if (g_current_page_no <= 1) {
      return;
    }
    dispOnWaiting('initio', 'forinitio');
    var pageNum = Math.ceil(g_current_item_no_start / GRID_ROW_MAX_COUNT) - 1;
    setIOSelData(g_select_tab , pageNum);
    top.rpcmc_stopIOGroupMonitor(g_cur_type, g_current_item_no_start, g_current_item_no_end - g_current_item_no_start + 1);
  });
  $("#page_nextbtn").on('click touchend', function (event) {
    if(event.type.indexOf('touch') >= 0){
      event.preventDefault();
    }
    if (g_current_page_no >= g_current_page_max) {
      return;
    }
    dispOnWaiting('initio', 'forinitio');
    var pageNum = Math.ceil(g_current_item_no_start / GRID_ROW_MAX_COUNT) + 1;
    setIOSelData(g_select_tab, pageNum);
    top.rpcmc_stopIOGroupMonitor(g_cur_type, g_current_item_no_start, g_current_item_no_end - g_current_item_no_start + 1);
  });
  $("#page_num").on('click', function (event) {
    if (g_current_page_max == 1) {
      return;
    }
    var sel_menu = $('.page_frame');
    if (sel_menu.length) {
      var target = $(event.target);
      if (target.hasClass('page_sel')) {
        dispOnWaiting('initio', 'forinitio');
        var pageNum = Number(event.target.outerText);
        setIOSelData(g_select_tab, pageNum);
        top.rpcmc_stopIOGroupMonitor(g_cur_type, g_current_item_no_start, g_current_item_no_end - g_current_item_no_start + 1);
      }
    }
  });
}

