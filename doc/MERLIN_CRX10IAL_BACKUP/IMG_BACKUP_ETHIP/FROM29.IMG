// object to tree menu.
function jsonToTree(array, ul) {
  var section;
  var li;
  var node;
  var text;
  var icon;
  var new_ul;

  for (var i = 0; i < array.length; i++) {
    section = array[i];
    li = document.createElement('div');
    node = document.createElement('div');
    li.setAttribute('data', 'li');
    
    if (section.children.length) { // have child
      icon = document.createElement('ins');
      text = document.createTextNode(section.data.text);
      new_ul = document.createElement('div');

      node.className = 'tree_node parent';
      icon.className = 'tree_ico open';
      new_ul.className = 'base_list';
      new_ul.setAttribute('data', 'ul');

      jsonToTree(section.children, new_ul);

      node.appendChild(icon);
      node.appendChild(text);
      li.appendChild(node);
      li.appendChild(new_ul);
    } else { // have no child
      text = document.createElement('span');

      node.className = 'tree_node';
      text.innerText = section.data.text;

      node.appendChild(text);
      li.appendChild(node);
    }
    ul.appendChild(li);
  }
}

function convertXml(elem) {
  var text = '<FRTreeView Name="myTreeView">' + TreeToXml(elem) + '</FRTreeView>';
  return text;
}

// tree menu to xml
function TreeToXml(ul) {
  var xml_text = '';
  var li_array = ul.children;
  var li;
  var node;
  var header;
  var new_ul;

  for (var i = 0; i < li_array.length; i++) {
    li = li_array[i];
    node = li.children[0];

    if (node.classList.contains("parent")) { // parent tree
      new_ul = li.children[1];
      header = node.innerText.replace(/^\s/, '');
      xml_text += '<FRTreeViewItem Header="' + header + '">' +
        TreeToXml(new_ul) + '</FRTreeViewItem>';
    } else { // child node
      header = node.children[0].innerText;
      xml_text += '<FRTreeViewItem Header="' + header + '" url="' + getXmlUrl(header) + '"/>';
    }
  }

  return xml_text;
}

function setGenerator(array) {
  var section;
  var obj;

  for (var i = 0; i < array.length; i++) {
    section = array[i];

    if (section.children.length) { // have child;
      setGenerator(section.children);
    } else { // have no child;
      obj = {};
      obj.header = section.data.text;
      obj.url = section.data.value;
      xml_generator.push(obj);
    }
  }
}

function getXmlUrl(header) {
  var obj;

  for (var i = 0; i < xml_generator.length; i++) {
    obj = xml_generator[i];

    if (obj.header == header) {
      return obj.url;
    }
  }
  
  return '';
}␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀// Use jQuery
