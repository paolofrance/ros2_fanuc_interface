/*
 * Get Payload Data Callback
 */
function getPayloadDataCallback(status, getText) {
  if (status != top.IO_SUCCESS) {
    console.log("getXHR error");
  } else {
    var strXml = getText;
    var xmlObj = null;
    var domParser = new DOMParser();
    try {
      xmlObj = domParser.parseFromString(strXml, 'text/xml');
    } catch (e) {
      top.irprogapi.inner.apiErrorLog(e);
    }
    if ((xmlObj == null) || (xmlObj.getElementsByTagName('parsererror').length)) {
      // failure Get XML Data.
      top.irprogapi.inner.apiErrorLog('setPlstCheckDlgText: xml error.');
    } else {
      // Success Get XML Data.
      var xmlVarElem = xmlObj.getElementsByTagName("XMLVAR");
      var plstNum = xmlVarElem[0].getElementsByTagName("ARRAY");
      var varElem = xmlVarElem[0].getElementsByTagName("VAR");
      $("#select_number").html(plstNum[0].textContent);
      for(var i = 0; i < varElem.length; i++) {
        var index = Math.floor(i / 8) + 1;
        var column = PAYLOAD_CLM_ARR[(Math.floor(i % 8) + 1)];
        payloadGrid.setCellValue(index, column, calcDispVal(varElem[i].textContent, column));
      }
    }
  }
}

/*
 * Check paste Data
 */
function checkPasteData() {
  var data_list = [];
  var copydata = copyEventText;
  var pasteRowPos = Number(payloadGrid.getSelectRow().start);
  var pasteColPos = (payloadGrid.getSelectType() == SELGRID_ROW) ? COLUMN_PAYLOAD : payloadGrid.getSelectCol().start;

  for (var i = 0; i < copydata.length; i++) {
    var pasteData = copydata[i];
    var index = pasteRowPos + i;
    for (var j = 0; j < pasteData.length; j++) {
      var nextCol = pasteColPos + j;
      var column = PAYLOAD_CLM_ARR[nextCol];
      var nextVal = pasteData[j];
      if(isErrData(nextVal, column)) {
        top.openCmpAlert(window, 'errPasteDlg', resources['ERRPaste']);
        return;
      }
      data_list.push(createValArr(column, index, nextVal));
    }
  }
  pastePayloadData(data_list);
}

function createValArr(nextCol, nextRow, nextVal) {
  var setArr = {};
  setArr.col = nextCol;
  setArr.row = nextRow;
  setArr.val = nextVal;
  return setArr;
}

/*
 * Paste Payload Data
 */
function pastePayloadData(data_list) {
  var setPldInf = {};
  var backUpInf = [];
  var index = Number(payloadGrid.getRecord(data_list[0].row).record[COLUMN_INDEX].recVal);
  for (var i = 0; i < data_list.length; i++) {
    var row = data_list[i].row;
    var col = data_list[i].col;
    var nextVal = data_list[i].val;
    var prevVal = payloadGrid.getCellValue(row, col);
    if (prevVal == undefined) break;
    backUpInf.push(createValArr(col, row, prevVal));
    payloadGrid.setCellValue(row, col, nextVal);

    var sysVal = calcSysvarVal(nextVal, col);
    switch (col) {
      case 'comment':
        setPayloadData(g_select_group, index, col, sysVal);
        break;
      case 'payload':
      case 'ix':
      case 'iy':
      case 'iz':
      case 'x':
      case 'y':
      case 'z':
        setPldInf[col] = sysVal;
        break;
      default:
        break;
    }
  }
  setPldInf.index = index;
  if (!pasteOnlyCmt()) setPayloadDataIF(g_select_group, setPldInf.index, PLDCHK, setPayloadDataIFCB, setPldInf, backUpInf);
}

function pasteOnlyCmt() {
  if (payloadGrid.getSelectType() == SELGRID_ROW) return false;
  if ((copyEventText[0].length == 1) && (payloadGrid.getSelectCol().start == COLUMN_COMMENT)) {
    return true; // If select comment cell and have only only one row copydata, return true.
  }
  return false;
}

/*
 * clear Payload Data (Delete Event)
 */
function clearPayloadData() {
  var slctCol = getSlctCol();
  if (slctCol.end < COLUMN_COMMENT || COLUMN_COMMENT < slctCol.start) return;
  var selRow = payloadGrid.getSelectRow();
  for (var i = selRow.start; i <= selRow.end; i++) {
    payloadGrid.setCellValue(i, PAYLOAD_CLM_ARR[COLUMN_COMMENT], '');
    setPayloadData(g_select_group, Number(payloadGrid.getRecord(i).record[COLUMN_INDEX].recVal), PAYLOAD_CLM_ARR[COLUMN_COMMENT], '');
  }
}

/*
 * Grid Row Create
 */
function createrow() {
  var empty = {
    record:[]
  };
  empty.record[COLUMN_INDEX] = {recVal : ""};
  empty.record[COLUMN_PAYLOAD] = {recVal : 0};
  empty.record[COLUMN_COMMENT] = {recVal : ""};
  empty.record[COLUMN_X] = {recVal : 0};
  empty.record[COLUMN_Y] = {recVal : 0};
  empty.record[COLUMN_Z] = {recVal : 0};
  empty.record[COLUMN_IX] = {recVal : 0};
  empty.record[COLUMN_IY] = {recVal : 0};
  empty.record[COLUMN_IZ] = {recVal : 0};
  return empty;
}

