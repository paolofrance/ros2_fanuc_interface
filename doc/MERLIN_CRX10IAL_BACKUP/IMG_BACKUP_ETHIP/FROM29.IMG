/*
 * Select change process...
 */
function changeShowHideCellControl(dlgId, hideList, showList) {
	var idPrefix = '#' + ((dlgId == DLGID_STR) ? 'strdlgtbl' : 'cmddlgtbl');
	$(hideList).each(function (i) {
		$(idPrefix + 'th-' + hideList[i]).hide();
		$(idPrefix + 'td-2-' + hideList[i]).hide();
		$(idPrefix + 'td-3-' + hideList[i]).hide();
	});
