 // Auto Complete Text Table
 var autoCompTextTable = {};
function getAutoCompTextTable() {
	return autoCompTextTable;
}
function getAutoCompTextList(ch) {
	return autoCompTextTable[ch];
}

/**
 *  List initialyze.
 */
var isCompleteCmdList = false;
function initCommandList() {
	// Get command list
	isCompleteCmdList = false;
	var treetype = 'instruct_tree';
	$.getJSON('/softpart/xmljson?file=/frh/irprog/pcedtdef_cmdlist.xml&id=' + treetype + '&connect_id=' + top.g_connect_id, function(commandTree) {
		// Use treeview
		/* set to treeview */
		UIFComponents.treeSelect.init(document, 'jstree_div', { "sameSelect": true });
		document.getElementById('jstree_div').refresh(commandTree, '', false);
		document.getElementById('jstree_div').config.onclick = nodeOnClick;
		$('.tree-select-disabled').remove();
		isCompleteCmdList = true;
	}).fail(function(xhr, textStatus, errorThrown) {
		debug(DBGFATAL, 'Get data failed.('+ textStatus + ':' + errorThrown.message + ')');
	});
	// Get Autocomplete xml data
	$.getJSON('/softpart/xmljson?file=/frh/irprog/pcedtdef_atcomp.xml&id=' + treetype + '&connect_id=' + top.g_connect_id, function(autoCompList) {
		// Construct Auto Complete Command Table
		var autoCompItem = autoCompList[0].children;
		$(autoCompItem).each(function(i, leaf) {
			var leafVal = leaf.data.value;
			var fstCh = leafVal.charAt(0);
			var validCh = /^[A-Z\uFF71-\uFF9D]/;	// Is 1st char A to Z or Hankaku-kana(A to NN)?
			// Check the valid first letters of autocomplete table
			if (!validCh.test(fstCh)) {
				return;
			}
			var leafList = autoCompTextTable[fstCh];
			if (leafList == undefined) {
				leafList = [];
			}
			leafVal = $('<div />').html(leafVal).text();	// decode quote (&#39 --> ')
			leafList.push(leafVal);
			leafList.sort(function (one, two) {	// Sort object.
				return (one < two) ? -1 : 1;
			});
			autoCompTextTable[fstCh] = leafList;
		});
	}).fail(function(xhr, textStatus, errorThrown) {
		debug(DBGFATAL, 'Get Autocomplete data failed.('+ textStatus + ':' + errorThrown.message + ')');
	});
}
// Click New design tree
function nodeOnClick(root, props) {
	if (typeof props.value === 'string') {
		var addParam = JSON.parse(props.addParam);
		var cmdParent = addParam.parent;
		var cmdId = addParam.cmdId;
		var tblId = addParam.tblId;
		var cmdStr = props.value;
		var prgInfoObj = ProgramInfoManager.getCurrentProgInfo();
		if (prgInfoObj.getEditAttrValue().MTNGRP == 0) {	// Motion group nothing...
			if ((cmdParent == 'GRPMOTION') || (cmdParent == 'GRPADDMOTION')) {	// Motion instruction
				defineDialog.open('#jstree_div', '#confalertdlg', 'dlgMsgMotionGrp');	// Alert on
				return;
			}
		}
		var dlgResult = false;
		if ((tblId != 'TBLNONDEF') &&
			(tblId != 'TBLRUNFIND') &&
			(tblId != 'TBLPALLET') &&
      (tblId != 'TBLGETOFF') &&
      (tblId != 'TBLINSTPLT')) {
			dlgResult = defineCmdDialog.open(tblId, cmdId, cmdStr);
		} else if (tblId == 'TBLPALLET') {
			if (typeof pltDialogMainProcess === 'function') {
        dlgResult = pltDialogMainProcess(addParam.cmdId);
        if (dlgResult == "err") { return;}
			}
		}
		if (!dlgResult) {	// Command is table-non-define or not defined dialog table.
			selectedCommand.str = cmdStr;
			selectedCommand.parent = cmdParent;
			popupPosition.x = root.getSelectedNode().offsetLeft;
			popupPosition.y = (isScrModeEditorTab()) ?	// adjust popup top for tablet mode.
				root.getSelectedNode().offsetTop - root.scrollTop -root.offsetTop - (root.getSelectedNode().clientHeight / 2) :
				root.getSelectedNode().offsetTop - root.scrollTop + root.getSelectedNode().clientHeight;
			popupDialog.confirm('cmdinsconf', popupPosition, insertYes, insertNo);
		}
	}
}

// Watch dog of build command list complete.
function waitCmdListComplete() {
  if (!isCompleteCmdList) {
    setTimeout(function () {
	    waitCmdListComplete();
    }, 50);
    return;
  }
  initMovetoMenu();	// layout
}

/**
 * Select Command list item.
 */
var selectedCommand = {};
var popupPosition = { x: 0, y: 0 };
var selectElement;	// selected tree element data
var cmdwinOpen = true;

// Insert? Yes process.
function insertYes() {
	var editor = getPcEditor();
	var csrLine = editor.getCursor().line;
	var csrChar = editor.getCursor().ch;
	// In the case of an operation addition motion, no new line character is added.
	//if (selectElement.node.parent != "GRPADDMOTION") {
	// [9/11/2017] If add a CR/LF code, do not give it because the line saving process will be done.
	//selectedCommand = selectedCommand + CODELF;
	//}
  if ((selectedCommand.parent == 'GRPADDMOTION') ||
      (selectedCommand.parent == 'GRPVIA')) { // [STEP8] add via extend instruction.
		selectedCommand.str = ' ' + selectedCommand.str;
	}
	editor.replaceRange(selectedCommand.str,
		CodeMirror.Pos(csrLine, csrChar));
	// focus set to editor
	csrChar += selectedCommand.str.length;	// Cursor move to end of command string.
	editor.focus();
	editor.setCursor({ line: csrLine, ch: csrChar });
}
// Insert? No process.
function insertNo() {
	var editor = getPcEditor();
	var csrLine = editor.getCursor().line;
	var csrChar = editor.getCursor().ch;
	editor.focus();
	editor.setCursor({ line: csrLine, ch: csrChar });
}

$(function () {
	// open/close command list window
	$('#layoutarea').on('click touchend', '#closelistbtn', function (event) {
		if(event.type.indexOf('touch') >= 0){
			event.preventDefault();
		}
		// toggle open/close
		var txteditwinW = $('#prglistwin').width();	// before close
		var cmdlistwinW = $('#cmdlistwin').width();	// before close
		var leftSideW = txteditwinW + cmdlistwinW;	// before close
		var layoutW = $('#layoutarea').width();
		if (getCurrentLayout() == LAYOUTVERTICAL) {
			if (cmdwinOpen) {	// to close
				// resize text editor
				$('#closelistbtn').html('<img src="images/btn_listopen.png">');
				$('#cmdlistwin').css({ 'width': RATECMDVERTCLS });
				$('#prglistwin').css({ 'width': RATEPRGVERTCLS });
			} else { 	// to open
				$('#prglistwin').width(RATEPRGWINPCT);
				$('#cmdlistwin').width(RATECMDWINPCT);
				$('#closelistbtn').html('<img src="images/btn_listclose.png">');
			}
		} else {	// Horizontal
			if (cmdwinOpen) {	// to close
				// resize text editor
				$('#closelistbtn').html('<img src="images/btn_listopen.png">');
				$('#cmdlistwin').width(leftSideW * CMDWCMDOPEN);
				$('#prglistwin').width(leftSideW * PRGWCMDCLOSE);
			} else { 	// to open
				$('#prglistwin').width(leftSideW * WINRATELEFT);
				$('#cmdlistwin').width(leftSideW * WINRATERIGHT);
				$('#closelistbtn').html('<img src="images/btn_listclose.png">');
			}
		}
		cmdwinOpen = !cmdwinOpen;
	});

});
