function delBtnEvent() {
  var selRowNum = ioAsgnGrid.getSelectRow().end;
  if (selRowNum == undefined || selRowNum == 0) {
    return;
  }
  
  var tgtRec = ioAsgnGrid.getRecord(selRowNum).record;
  var startNum = Number(document.getElementById('startNumTb').getValue());
  var dlgMsg = resources['dlgMsgDelAsgn'];

  if (tgtRec[ASGN_CLM_NO].recVal != ASGN_ADD_STR) {
    var rackNum = document.getElementById('rackTb').getValue();
    var slotNum = document.getElementById('slotTb').getValue();
    var ioModName = getDevName(rackNum, slotNum);
    var msgIoNum = resources['AsgnGridNo'] + ":";
    var msgModType = resources['AsgnGridType'] + ":" + ioModName + "<br>" +
      resources['AsgnGridRack'] + ":" + rackNum + "&nbsp;&nbsp;" +
      resources['AsgnGridSlot'] + ":" + slotNum;
    var msgStartPt = '';

    switch (asgnIOType) {
      case TAB_DI:
      case TAB_DO:
        var numBits = document.getElementById('numBitsTb').getValue();
        msgIoNum += g_ary_tab_id[asgnIOType].name + "[" + startNum + "-" + (Number(numBits) + startNum - 1) + "]<br>";
        msgStartPt = "&nbsp;&nbsp;" + resources['AsgnGridStartNum'] + ":" + document.getElementById('startPointTb').getValue();
        break;
      case TAB_AI:
      case TAB_AO:
        msgIoNum += g_ary_tab_id[asgnIOType].name + "[" + startNum + "]<br>";
        msgStartPt = "&nbsp;&nbsp;" + resources['AsgnGridCh'] + ":" + document.getElementById('startPointTb').getValue();
        break;
      case TAB_GI:
      case TAB_GO:
        msgIoNum += g_ary_tab_id[asgnIOType].name + "[" + startNum + "]<br>";
        msgStartPt = "<br>" + resources['AsgnGridStartNum'] + ":" + document.getElementById('startPointTb').getValue() +
          "&nbsp;&nbsp;" + resources['AsgnGridNumPoints'] + ":" + document.getElementById('numberPointsTb').getValue();
        break;
    }
    dlgMsg += '<br><span style="display:block; height: 9px;"></span>' + msgIoNum + msgModType + msgStartPt;
  }
  var dlgBtnObj = {btnData:top.irpDlgBtnYN, btn1Cb:delRec};
  top.openCmpConfirm(webpage, window, 'delIOAsgn', dlgMsg, dlgBtnObj, true);

  function delRec() {
    var slctRow = ioAsgnGrid.getSelectRow().end;
    if (slctRow == undefined || slctRow == 0) {
      return;
    }
    setListSelectable('none');
    var slctRowNoVal = ioAsgnGrid.getRecord(slctRow).record[ASGN_CLM_NO].recVal;
    if (slctRowNoVal != ASGN_ADD_STR) {
      delIOAssign(g_ary_tab_id[asgnIOType].tab, startNum, updWindowDelEvnt);
    } else {
      updWindowDelEvnt();
    }
    function updWindowDelEvnt() {
      ioAsgnGrid.deleteRecord(slctRow);
      initAsgnElm();
      setListSelectable('auto');
    }
  }
}

