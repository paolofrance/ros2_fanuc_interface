.pltdlgbtnarea {
  position: absolute;
  display: flex;
  width: 100%;
  height: 70px;
  left: 0;
  bottom: 0;
  justify-content: space-around;
  align-items: center;
}

.pltdlgbtnarea > div {
  width: 33%;
  text-align: center;
}

.pltdlgbtnarea > div > .dlgbutton {
  margin: 0;
}

.pltbtnprev,
.pltbtncancel,
.pltbtnnext {
  background-color: #696969;
  border: 1px solid #696969;
	color: #ffffff;
	cursor: pointer;
}

.pltbtnnext:focus ,
.pltbtnprev:focus,
.pltbtncancel:focus {
  outline: 0;
  border-color: #32a5ff;
}

.pltbtnnext:active ,
.pltbtnprev:active,
.pltbtncancel:active {
	background-color: #0e6eb8;
}

.pltbtndisable {
  background-color: transparent;
  color: transparent;
}

.pltprevframe {
  width: 33%;
  float: left;
}
.pltcancelframe {
  width: 34%;
  float: left;
}
.pltnextframe {
  width: 33%;
  float: left;
}

.pltposinfoarea {
  width: 100%;
}
.pltrteposmsg{
  font-size: 16px;
  margin: 7px 10px 10px 10px;
  float: left;
  text-align: center;
}
.plttupbtn {
	height: 34px;
  width: 120px;
  font-size: 13px;
  border-radius: 3.4px;
  margin: 5px 8px;
  float: right;
  text-align: center;
  line-height: 30px;
}
.pltpos {
  color: #696969;
  width: 100%;
  font-size: 13px;
}
.plttupactiv {
	color: #FFFFFF;
  background-color: #696969;
  border: 1px solid #696969;
	cursor: pointer;
}
.plttupactiv:active {
  background-color: #0e6eb8;
}
.plttupinact {
	color: #696969;
