/* = Resize(Stretch) bar style = */
.resizebarh {
	cursor: row-resize;
}
.resizebarv {
	cursor: col-resize;
}
.resizehbarv {
	width: 99.5%;
	height: 20px;
	padding-top: 0px;
	background-color: #AFBBC9;
	color: #000000;
	cursor: row-resize;
	z-index: 9901;
}
.resizehbarv:after {
	content: "";
	display: block;
	clear: both;
}
.resizehbarh {
	width: 100%;
	min-width: 338px;
	/* No display in horizontalLayout */
	height: 0px;
	display: none !important;
	padding-top: 0px;
	color: #000000;
	cursor: row-resize;
}
.resizehbarh:after {
	content: "";
	display: block;
	clear: both;
}
.resizevbarv {
	height: 297px;
	/* No display in verticalLayout */
	width: 0px;
	display: none !important;
	padding-left: 0px;
	color: #000000;
	float: left;
	cursor: col-resize;
}
.resizevbarh {
	height: 100%;
	width: 20px;
	padding-left: 0px;
	color: #000000;
	cursor: col-resize;
}
