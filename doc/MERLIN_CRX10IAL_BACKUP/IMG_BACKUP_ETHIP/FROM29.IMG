}

function chgIOsimEvt(evt, io_type, io_num, io_sim) {
  if (io_num < g_current_item_no_start || io_num > g_current_item_no_end || io_type != g_cur_type) {
    return;
  }
  var nextVal;
  var curSim = Number(w2ui['ioarea'].get(Number(io_num) - g_current_item_no_start).simulate);
  if (io_sim == 1) {
    // Reflects the simulate mode, holding the current test mode status.
    nextVal = (curSim | SIM_MODE).toString();
  } else {
    // Reflects the unsimulate mode, holding the current test mode status.
    nextVal = (curSim & TEST_MODE).toString();
  }
  getIOsimCallback(IO_SUCCESS, io_type, io_num, nextVal);
}

function chgIOTestModeEvt(evt, io_type, io_num, io_sim) {
  if (io_type == 0) {
    var nextVal;
    var curSim;
    if (io_sim == 1) {
      for (i = g_current_item_no_start; i <= g_current_item_no_end; i++) {
        curSim = Number(w2ui['ioarea'].get(i - g_current_item_no_start).simulate);
        if ((curSim == STR_SIM_OFF) || (curSim == STR_SIM_ON)) {
          // Reflects the test mode, holding the current simulate mode status.
          nextVal = (curSim | TEST_MODE).toString();
          getIOsimCallback(IO_SUCCESS, g_cur_type, i, nextVal);
        }
      }
    } else {
      for (i = g_current_item_no_start; i <= g_current_item_no_end; i++) {
        curSim = Number(w2ui['ioarea'].get(i - g_current_item_no_start).simulate);
        if ((curSim == STR_SIM_TEST_U) || (curSim == STR_SIM_TEST_S)) {
          // Reflects the normal mode, holding the current simulate mode status.
          nextVal = (curSim & SIM_MODE).toString();
          getIOsimCallback(IO_SUCCESS, g_cur_type, i, nextVal);
        }
      }
    }
  }
}

