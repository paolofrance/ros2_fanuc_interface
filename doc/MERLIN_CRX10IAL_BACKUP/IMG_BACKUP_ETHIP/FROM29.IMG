    execProgramTable[funcName](args);
  }
}

/**
 * Make program select list.
 */
function selOptProgramList(selId) {
  var currentPg = getCurrentProgram();
  var $selForm = $('<div>', { id: 'prgform', 'class': 'proglistform' });
  var $selHeader = $('<div>', { id: 'proglisthdr', 'class': 'proglisthdr' });
  var $hdrNumber = $('<div>', { id: 'proglisthdr-nbr', 'class': 'proglisthdr-nbr' });
  $hdrNumber.html('No.');
  var $hdrTitle = $('<div>', { id: 'proglisthdr-ttl', 'class': 'proglisthdr-ttl' });
  $hdrTitle.html(resources['hdrProgName']);
  var $hdrType = $('<div>', { id: 'proglisthdr-typ', 'class': 'proglisthdr-typ' });
  $hdrType.html(resources['dlgAttrSubtype']);
  $selHeader.append($hdrNumber);
  $selHeader.append($hdrTitle);
  $selHeader.append($hdrType);
  $selForm.append($selHeader);
  var titleHeight = $('#proglisthdr-ttl').height();
  var $selBody = $('<div>', { id: 'proglistbody', name: 'proglistbody', class: 'proglistbody' });
  var progList = [];
  // Do not display the program name starting with "IPL_"
  var allProgram = getProgramFileList();
  var countDispProg = 0;
  for (var i = 0; i < allProgram.length; i++) {
    var progName = allProgram[i].name;
    if (!progName.match(/^IPL_.*/)) {
      var filterSlcted = sessionStorage.getItem('progattrFilter_' + window.frameElement.id);
      if ((isScrModeProgAttr()) && (filterSlcted !== null) && (filterSlcted != resources['filterAll'])) {
        var reg = new RegExp("^" + filterSlcted + ".*");
        if (!progName.match(reg)) {
          continue;
        }
      }
      progList[countDispProg] = allProgram[i];
      countDispProg++;
    }
  }
  var listIndex = 1;
  $(progList).each(function (i) {
    var name = progList[i].name;
    if ((name != currentPg) || (isScrModeProgAttr())) {
      var $selElemP = $('<div>', { id: 'progrow' + listIndex, 'class': 'proglistparent' });
      var $selElem1 = $('<div>',
        { id: 'progrow' + listIndex + '-1', 'class': 'proglisthdr-nbr proglistelem', 'value': name });
      $selElem1.html('' + listIndex);
      var $selElem2 = $('<div>',
        { id: 'progrow' + listIndex + '-2', 'class': 'proglisthdr-ttl proglistelem' });
      $selElem2.attr({ 'title': name });
      $selElem2.html(name);
      var $selElem3 = $('<div>',
        { id: 'progrow' + listIndex + '-3', 'class': 'proglisthdr-typ proglistelem' });
      $selElem3.html(progList[i].type);
      $selElemP.append($selElem1);
      $selElemP.append($selElem2);
      $selElemP.append($selElem3);
      $selBody.append($selElemP);
      listIndex++;
    }
  });
  $selForm.append($selBody);
  return $selForm.prop('outerHTML');
}

function selOptMacroList(selId) {
  var $selForm = $('<div>', { id: 'prgform', 'class': 'proglistform' });
  var $selHeader = $('<div>', { id: 'proglisthdr', 'class': 'proglisthdr' });
  var $hdrNumber = $('<div>', { id: 'proglisthdr-nbr', 'class': 'proglisthdr-nbr' });
