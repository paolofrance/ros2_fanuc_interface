		showList = [indElemNum + 1, indElemNum + 2, indElemNum + 3];
		var targetSel = selIdStr.replace('ipt', 'asst');
		$('#' + targetSel).val(selVal + '[]');
	} else {
		hideList = [indElemNum + 1, indElemNum + 2, indElemNum + 3];
	}
	changeShowHideCellControl((isDlgStr) ? DLGID_STR : DLGID_CMD, hideList, showList);
}

// Construction of the command dialog
function buildCommandDialog(dlgDef, cmdId, cmdStr) {
	// Get command parts table
	var cmdParts = getCommandParts();
	var focusId = null;
	// Decide Dialog type (prefix)
	var dlgPrefix = (dlgDef.id == DLGID_STR) ? 'str' : 'cmd';
	// Make dialog
	var dlgConf = $('<div>',
		{ id: dlgDef.id, 'class': 'commanddlg' });
	var dlgTtl = $('<div>',
		{ id: dlgDef.title, 'class': 'commandtitle' });
	var ttlArea = $('<label>', { 'class': 'dlgTtlText' });
	ttlArea.html(resources[dlgDef.titleRes]);
	dlgTtl.append(ttlArea);
	dlgConf.append(dlgTtl);
	var dlgFormId = (dlgDef.id == DLGID_STR) ? 'strinstform' : 'cmddlgform';
	var dlgForm = $('<form id=' + dlgFormId + '>');
	dlgForm.css({ 'text-align': 'center' });
	var dlgCage = $('<div>', { id: 'cmddlgcage' });
	dlgCage.css({ 'display': 'inline-block' });
	var dlgBody = $('<div>',
		{ id: 'cmddlgbody', 'class': 'cmddlgbody' });
	var dlgHdr = $('<div>',
		{ id: 'cmddlgth', 'class': 'cmddlgth' });
	// append message row.
	var dlgCmdMsg = $('<div>',
		{ id: 'cmddlgmsgth', 'class': 'cmddlgtbl cmddlgtblmsg' });
	var dlgCmdMsgTh = $('<span>');
	if (dlgDef.dlgMsg.length > 0) {
		dlgCmdMsgTh.html(resources[dlgDef.dlgMsg]);
	}
	dlgCmdMsg.append(dlgCmdMsgTh);
	dlgHdr.append(dlgCmdMsg);
	var dlgCmdTbl = $('<div>',
		{ id: dlgPrefix + 'dlgtblth', 'class': 'cmddlgtbl' });
	// build header row.
	var dlgCmdTrH = $('<div>',
		{ id: dlgPrefix + 'dlgtbltr-1', 'class': 'cmddlgtbltr' });
	var headerDef = dlgDef.headerDef;
	for (var i = 0; i < headerDef.length; i++) {
		var dlgCmdTblTh = $('<div>',
			{ id: dlgPrefix + 'dlgtblth-' + (i + 1), 'class': 'cmddlgtblth ' + headerDef[i].hdrcell });
		var dlgText = $('<div>', { id: dlgPrefix + 'hdr-' + (i + 1), 'class': 'cmddlgtbltxt' });
		if (headerDef[i].width) {
			dlgText.css({ 'width': headerDef[i].width });
		}
		dlgText.html(resources[headerDef[i].hdrid]);
		dlgCmdTblTh.append(dlgText);
		if (headerDef[i].hide) {
			dlgCmdTblTh.css({ 'display': 'none' });
		}
		dlgCmdTrH.append(dlgCmdTblTh);
	}
	dlgCmdTbl.append(dlgCmdTrH);
	// build form row.
	var dlgCmdTrD = $('<div>',
		{ id: dlgPrefix + 'dlgtbltr-2', 'class': 'cmddlgtbltr cmdcell-all' });
	var formDef = dlgDef.formDef;
	var frmTabIndex = 1;
	for (var i = 0; i < formDef.length; i++) {
		var formElementId = '';
		var dlgCmdTblTd = $('<div>',
			{ id: dlgPrefix + 'dlgtbltd-2-' + (i + 1), 'class': 'cmddlgtbltd ' + formDef[i].formcell });
		switch (formDef[i].formid) {
			case 'frmLabel':
				formElementId = dlgPrefix + 'lbl-1-' + (i + 1);
				var frmlabel = $('<span>', { 'id': formElementId, 'class': 'cmdcellitem' });
				frmlabel.html(formDef[i].lbltext);
				dlgCmdTblTd.append(frmlabel);
				break;
			case 'frmText':
				formElementId = dlgPrefix + 'str-1-' + (i + 1);
				var frmtext = $('<span>', { 'id': formElementId, 'class': 'cmdcellitem' });
				if (formDef[i].txtid.length > 0) {
					frmtext.html(cmdParts[formDef[i].txtid]);
				}
				dlgCmdTblTd.append(frmtext);
				break;
			case 'frmTextarea':
				formElementId = dlgPrefix + 'txt-1-' + (i + 1);
				var frmTextarea = $('<textarea>', { id: formElementId, name: formElementId });
				frmTextarea.css({ 'width': 'calc(100% - 7px)', 'height': '50px', 'float': 'left' });
				frmTextarea.attr({ tabindex: frmTabIndex });
				frmTabIndex++;
				var frmareawrap = $('<div>', { 'class': 'cmdcellitem' });
				frmareawrap.append(frmTextarea);
				dlgCmdTblTd.append(frmareawrap);
				break;
			case 'frmSelect':
				formElementId = dlgPrefix + 'sel-1-' + (i + 1);
				var frmsel = $('<select>',
					{ id: formElementId, name: formElementId, 'class': 'cmdcellitem' });
				if (formDef[i].call) {
					frmsel.attr({ 'onChange': formDef[i].call });
				}
				var sellist = formDef[i].selList;
				if (sellist != undefined) {
					for (var j = 0; j < sellist.length; j++) {
						var opttext = (formDef[i].selRcTbl) ? cmdParts[sellist[j]] : sellist[j];
						var frmopt = $('<option>', { value: opttext });
						frmopt.html(opttext);
						frmsel.append(frmopt);
					}
				}
				frmsel.attr({ tabindex: frmTabIndex });
				frmTabIndex++;
				dlgCmdTblTd.append(frmsel);
				break;
			case 'frmInput':
				var frmiptwrap = $('<div>', { 'class': 'cmdcellitem' });
				formElementId = dlgPrefix + 'ipt-1-' + (i + 1);
				if (formDef[i].asstid) {	// input box with 'assist select box'.
					var frmComboWrap = $('<div>', { 'class': 'cmdcombo' });
					var frmComboArrow = $('<button>', { 'class': 'cmdcomboarrow cmdcombo-triangledn-dgray' });
					frmComboWrap.append(frmComboArrow);
					var idComboSel = dlgPrefix + 'asst-1-' + (i + 1);
					var selChgFunc = formDef[i].selChange;
					var selComboClass = (isIE) ? 'cmdcomboselIE' : 'cmdcomboselect';
					var frmComboSelect = $('<select>', { 'class': 'cmdiptblock ' + selComboClass });
					var onChgStr = 'changeAsstCombo("' + cmdId + '", "' + idComboSel + '", "' + formElementId + '", "' + formDef[i].asstid + '", ' + selChgFunc + ')';
					frmComboSelect.attr({ 'onChange': onChgStr });
					frmComboSelect.attr({ 'id': idComboSel, 'name': idComboSel });
					switch (formDef[i].asstid) {
						case ASSIST_STD:	// Input assist standard item list
							frmComboSelect.append($('<option>').val('').text(''));
							$(asstIptStd).each(function (j) {
								var item = cmdParts[asstIptStd[j]];
								frmComboSelect.append($('<option>').val(item).text(item));
							});
							break;
						case ASSIST_REG:	// Input assist only register item
							frmComboSelect.append($('<option>').val('').text(''));
							$(asstIptReg).each(function (j) {
								var item = cmdParts[asstIptReg[j]];
								frmComboSelect.append($('<option>').val(item).text(item));
							});
							break;
						case ASSIST_PRG:	// Program list
							frmComboSelect.append($('<option>').val('').text(''));
							var progList = getProgramFileList();
							$(progList).each(function (j) {
								var item = progList[j].name;
								frmComboSelect.append($('<option>').val(item).text(item));
							});
							break;
					}
					frmComboWrap.append(frmComboSelect);
					var frmComboIpt = $('<div>', { 'class': 'cmdcombowrap' });
					var frminput = $('<input>',
						{
							type: 'text', name: formElementId, id: formElementId,
							'class': 'cmdcellitem cmdiptblock cmdcombotxt ui-widget-content ui-corner-all'
						});
					if (formDef[i].initStr) {
						frminput.attr({ value: cmdStr });
						frminput.css({ 'text-align': 'left', 'border-color': '#ABABAB' });
					} else {
						var alignStr = (formDef[i].align) ? formDef[i].align : 'right';
						frminput.css({ 'text-align': alignStr, 'border-color': '#ABABAB' });
					}
					var asstFuncDef = (formDef[i].asstfunc) ? formDef[i].asstfunc : 'inputChangeAsstBox';
					var onIptStr = asstFuncDef + '("' + formElementId + '",' + formDef[i].asstid + ')';
					frminput.attr({ 'onInput': onIptStr });
					frminput.attr({ tabindex: frmTabIndex });
					frmTabIndex++;
					frmComboIpt.append(frminput);
					frmComboWrap.append(frmComboIpt);
					frmiptwrap.append(frmComboWrap)
				} else {
					var frminput = $('<input>',
						{
							type: 'text', name: formElementId, id: formElementId,
							'class': 'cmdcellitem ui-widget-content ui-corner-all'
						});
					frminput.css({ 'padding': 0, 'width': formDef[i].width, 'height': '22px' });
					if (formDef[i].initStr) {
						frminput.attr({ value: cmdStr });
						frminput.css({ 'text-align': 'left', 'border': '1px solid #ABABAB' });
					} else {
						var alignStr = (formDef[i].align) ? formDef[i].align : 'right';
						frminput.css({ 'text-align': alignStr, 'border': '1px solid #ABABAB' });
					}
					if (formDef[i].iptAsstNum) {
						frminput.attr({ 'class': 'inputAsstNum' });
					}
					if (formDef[i].maxleng) {
						frminput.attr({ 'maxlength': formDef[i].maxleng });
					}
					frminput.attr({ tabindex: frmTabIndex });
					frmTabIndex++;
					frmiptwrap.append(frminput);
				}
				dlgCmdTblTd.append(frmiptwrap);
				break;
			default:
				debug(DBGERROR, 'Illegal command dialog format.');
		}
		if (headerDef[i].hide) {
			dlgCmdTblTd.css({ 'display': 'none' });
		}
		dlgCmdTrD.append(dlgCmdTblTd);
		if (formDef[i].focus) {
			focusId = formElementId;
		}
	}
	dlgCmdTbl.append(dlgCmdTrD);
	// build expand button row.
	var dlgCmdTrDx = $('<div>',
		{ id: dlgPrefix + 'dlgtbltr-3', 'class': 'cmddlgtbltr cmdcell-all' });
	var expDef = dlgDef.expandDef;
	for (var i = 0; i < expDef.length; i++) {
		var dlgCmdTblTd = $('<div>',
			{ id: dlgPrefix + 'dlgtbltd-3-' + (i + 1), 'class': 'cmddlgtbltd ' + expDef[i].expcell });
		if (expDef[i].expndid.length > 0) {
			dlgCmdTblTd.css({ 'padding': '0 0 5px 10px' });
			switch (expDef[i].expndid) {
				case 'cmdmixedlogic':
					// Layout Arithmetic operator, Value, ^Insert
					var expdiv = $('<div>');
					var expElemId = 'expsel-3-' + (i + 1);
					var expbtn = $('<button>',
						{
							type: 'button', id: 'cmdmixedlogic',
							'class': expDef[i].btncls + ' nocancel',
							value: resources['dlgBtnLogic']
						});
					expbtn.attr({
						'exptgtid': 'cmdtxt-1-' + (i + 1),
						'expopeid': 'expsel-3-' + (i + 1), 'expvalid': 'expipt-3-' + (i + 1)
					});
					expbtn.css({ 'font-size': '100%' });
					expbtn.html(resources['dlgBtnLogic']);
					expbtn.attr({ tabindex: frmTabIndex });
					frmTabIndex++;
					expdiv.append(expbtn);
					dlgCmdTblTd.append(expdiv);
					break;
			}
		}
		if (headerDef[i].hide) {
			dlgCmdTblTd.css({ 'display': 'none' });
		}
		dlgCmdTrDx.append(dlgCmdTblTd);
	}
	dlgCmdTbl.append(dlgCmdTrDx);
	dlgHdr.append(dlgCmdTbl);
	dlgBody.append(dlgHdr);
	// Footer
	var dlgFooter = $('<div>', { id: dlgPrefix + 'dlgfoot', 'class': 'cmddlgfoot' });
	var dlgFootTd = $('<div>',
		{ id: dlgPrefix + 'dlgtbltdf', 'class': 'cmddlgtbltdf' });
	var dlgFootTbl = $('<div>',
		{ id: dlgPrefix + 'dlgfttbl', 'class': 'cmddlgfttbl' });
	var dlgFooterPref = (dlgDef.id == DLGID_STR) ? 'strinst' : 'cmddlg';
	var dlgFootBtnOk = $('<div>', { 'class': 'cmddlgfootok' });
	var footokdiv = $('<div>');
	var footokbtn = $('<button>',
		{ type: 'button', id: dlgFooterPref + 'okbtn', 'class': 'cmddlgbutton okyes', value: resources['dlgBtnOk'] });
	footokbtn.html(resources['dlgBtnOk']);
	footokbtn.attr({ tabindex: frmTabIndex });
	frmTabIndex++;
	footokdiv.append(footokbtn);
	dlgFootBtnOk.append(footokdiv);
	var dlgFootBtnCancel = $('<div>', { 'class': 'cmddlgfootcancel' });
	var footcanceldiv = $('<div>');
	var footcancelbtn = $('<button>',
		{ type: 'button', id: dlgFooterPref + 'cancelbtn', 'class': 'cmddlgbutton nocancel', value: resources['dlgBtnCancel'] });
	footcancelbtn.html(resources['dlgBtnCancel']);
	footcancelbtn.attr({ tabindex: frmTabIndex });
	frmTabIndex++;
	footcanceldiv.append(footcancelbtn);
	dlgFootBtnCancel.append(footcanceldiv);
	dlgFootTbl.append(dlgFootBtnOk);
	dlgFootTbl.append(dlgFootBtnCancel);
	dlgFootTd.append(dlgFootTbl);
	dlgFooter.append(dlgFootTd);
	if (isScrModeEditorTab()) {
		dlgCage.append(dlgBody);
		dlgCage.append(dlgFooter);
		dlgForm.append(dlgCage);
	} else {
		dlgForm.append(dlgBody);
		dlgForm.append(dlgFooter);
	}
	dlgConf.append(dlgForm);
	$('body').append(dlgConf);
	// Initialize dialog items...
	if ((dlgDef.initElement != undefined) && (dlgDef.initElement.length > 0)) {
		(dlgDef.initElement)(cmdId);
	}
	// When the list has focus, the iPad is expanded and displayed,
	// so do not set the focus.
	if ((focusId != null) && (focusId.length > 0) && (!isTablet)) {
		if ($('#' + focusId).is(':visible')) {
			$('#' + focusId).focus();
		}
	}
}

