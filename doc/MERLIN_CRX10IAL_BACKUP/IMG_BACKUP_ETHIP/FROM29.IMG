function addFunc(event) {
  var li = event.item;
  var elem = document.getElementById(CUSTOM.treelistclass);
  var childSortable = li.getElementsByClassName(BASE.treelistclass);
  var nodes = li.getElementsByClassName('tree_node');

  /* Scrollbar size setting for custom tree. */
  setScrlbarHeight(CUSTOM.treemenutype);
  
  /*
    If the [section edit button] was inserted, replace it with a node.
    -> After that, text box is inserted.
  */
  if (li.classList.contains('button_li')) {
    convertSortable(li);
  }

  // set select_node
  function setSelectNode(){
    // for tree_node parent
    if (nodes.length) {
      for (var z = 0; z < nodes.length; z++) {
        var node = nodes[z];
        node.classList.add('select_node');
      }
    } else {
      // for tree_node
      if (li.getElementsByClassName('select_node').length != elem.getElementsByClassName('select_node').length) {
        for (var m = 0; m < nodes.length; m++) {
          var node = nodes[m];
          if (node.classList.contains('select_node')) {
            node.classList.remove('select_node');
          }
        }
      }
    }
  }
  
  // select color
  removeSelectedClass(BASE.treelistclass);
  removeSelectedClass(CUSTOM.treelistclass);
  setSelectNode();

  // Disable button
  updateDelBtnState();
  updateEditBtnState();
  setBtnDisabled(false, [restor_btn, update_btn]);

  if (!cloneFlag) {
    return;
  }

  /* 
    If the added element has a children tree, set the custom tree again.
  */
  if (childSortable.length) {
    for (var i = 0; i < childSortable.length; i++) {
      var ul = childSortable[i];

      for (var h = 0; h < sortableArray.length; h++) {
        if (ul == sortableArray[h].el) {
          var options = sortableArray[h].options;
          options.sort = true;
          options.onAdd = addFunc;
          options.onChoose = chooseFunc;
          options.onUnchoose = unChooseFunc;
          options.onUpdate = moveEndFunc;
          options.group.checkPull = checkFunc(undefined, true);
          options.group.checkPut = checkFunc(undefined);

          delete options.onClone;
          delete options.onEnd;

          sortableArray.splice(h, 1);

          cloneFlag = false;
          addFlag = true;
        }
      }
    }
  }

  // change className 'base_list' -> 'custom_list'
  for(var i = 0; i < childSortable.length ;i++ ){
    childSortable[i].className = CUSTOM.treelistclass;  
  }

  // 'base_list' , 'custom_list' click and open/close event initialize
  bindDbclickEvent(CUSTOM.treelistclass);
  bindOpenCloseEvent(BASE.treelistclass);
  bindOpenCloseEvent(CUSTOM.treelistclass);
  bindSelectEvent(BASE.treelistclass);
  bindSelectEvent(CUSTOM.treelistclass);

}

function checkFunc(value, pull) {
  return function (to, from, dragEl, evt) {
    var sameGroup = to.options.group.name && from.options.group.name && to.options.group.name === from.options.group.name;

    if (value == null && (pull || sameGroup)) {
      return true;
    } else if (value == null || value === false) {
      return false;
    } else if (pull && value === 'clone') {
      return value;
    } else if (typeof value === 'function') {
      return checkFunc(value(to, from, dragEl, evt), pull)(to, from, dragEl, evt);
    } else {
      var otherGroup = (pull ? to : from).options.group.name;
      return value === true || typeof value === 'string' && value === otherGroup || value.join && value.indexOf(otherGroup) > -1;
    }
  };
}

function findSelectedAncestor(li) {
  var treeElem = document.getElementById(CUSTOM.treelistid);
  var elem = li;
  var ancestor = null;
  var node;

  while (treeElem != elem) {
    if (elem.parentNode.getAttribute('data') == 'li') {
      node = elem.parentNode.children[0];
      if (node.classList.contains('parent') && node.classList.contains('select_node')) {
        ancestor = node;
      }
    }
    elem = elem.parentNode;
    continue;
  }

  return ancestor;
}

function convertSortable(li) {
  var btn = li.children[0];
  li.removeChild(btn);
  li.classList.remove('button_li');

  var parentNode = document.createElement('div');
  var ul = document.createElement('div');

  parentNode.className = 'tree_node parent';
  ul.className = BASE.treelistclass;
  li.appendChild(parentNode);
  li.appendChild(ul);

  new Sortable(ul, {
    group: 'sortable',
    animation: 180,
    onAdd: addFunc,
    onChoose: chooseFunc,
    onUnchoose: unChooseFunc,
    onUpdate: moveEndFunc,
    filter: "input",
  });

  appendEditBox(parentNode);
}

function setDisabledCstmTree(state){
  for (var i = 0; i < customArray.length; i++) {
    customArray[i].option("disabled", state);
  }
  if(sectionBtnObj.length > 0) {
    sectionBtnObj.option("disabled", state);
  }
}

function openCloseTree(event) {
  // If the selected color is not attached, the section will not be opened or closed.
  if(event.target.className.indexOf('select_node') == -1){
    return;
  }
  if (event.type.indexOf("touch") == 0) {
    event.preventDefault();
  }
  if ($('input').length != 0) {
    return;
  }
  var targetNode = event.currentTarget;
  var arrowElem = targetNode.children[0];
  var ul = targetNode.parentNode;
  var ulChildren = ul.children;
  var treeClass = ul.parentNode.className;
  var activNode = document.activeElement;

  /* When the selected section is in the [Input] state 
     or when another section is selected in the [Input] state.*/
  if( (event.target.tagName == EDITBOX_TAGNAME) 
    || (activNode.tagName == EDITBOX_TAGNAME)){
    return false;
  }

  if (arrowElem.classList.contains('open')) { // hasClass
    // Tree close
    arrowElem.classList.remove('open');
    arrowElem.classList.add('close');

    for (var i = 0; i < ulChildren.length; i++) {
      var node = ulChildren[i];
      if (!node.classList.contains('parent')) {
        node.style.display = 'none';
      }
    }
    
  } else {
    // Tree open
    arrowElem.classList.remove('close');
    arrowElem.classList.add('open');

    for (var i = 0; i < ulChildren.length; i++) {
      var node = ulChildren[i];
      if (!node.classList.contains('parent')) {
        node.style.display = 'block';
      }
    }
  }

  if(treeClass == BASE.treelistclass){
    setScrlbarHeight(BASE.treemenutype);
  } else if(treeClass == CUSTOM.treelistclass){
    setScrlbarHeight(CUSTOM.treemenutype);
  }

}

function isNgSectionName(targetValue){
  var upvalue = targetValue.toUpperCase().trim();
  if(upvalue.match(ngStr) || !upvalue) {
    return true;
  } else {
    return false;
  }
}

// Color selected node.
function selectTreeNode(event) {
  if (event.type != undefined){
    if(event.type.indexOf("touch") == 0) {
      event.preventDefault();
    }
  }
  if($('input').length != 0){
    return;
  }
  // If there is an 'input'tag, the selected color is not specified.
  if(hasErrSec()) return;
  var targetNode = event.currentTarget;
  var ul;
  var childrenNodes;
  var node;

  // Reset 'select_node'
  removeSelectedClass(BASE.treelistclass);
  removeSelectedClass(CUSTOM.treelistclass);

  // Set 'select_node'
  if (targetNode.classList.contains('parent')) {
    ul = targetNode.parentNode;
    childrenNodes = ul.getElementsByClassName('tree_node');

    for (var i = 0; i < childrenNodes.length; i++) {
      node = childrenNodes[i];
      node.classList.add('select_node');
    }
  } else {
    targetNode.classList.add('select_node');
  }
  updateDelBtnState();
  updateEditBtnState();
}

function removeSelectedClass(treeId) {
  var elem = document.getElementById(treeId);
  var nodes = elem.getElementsByClassName('tree_node');
  var node;
  
  if (nodes.length) {
    for (var i = 0; i < nodes.length; i++) {
      node = nodes[i];

      if (node.classList.contains('select_node')) {
        node.classList.remove('select_node');
      }
    }
  }
}

// Click event set section
function bindOpenCloseEvent(treeId) {
  var elem = document.getElementById(treeId);
  var parenNodes = elem.getElementsByClassName('parent');

  if (parenNodes.length) {
    for (var i = 0; i < parenNodes.length; i++) {
      var parentNode = parenNodes[i];
      parentNode.removeEventListener('touchend', openCloseTree);
      parentNode.removeEventListener('click', openCloseTree);
      parentNode.addEventListener('touchend', openCloseTree, false);
      parentNode.addEventListener('click', openCloseTree, false);
    }
  }
}

function bindDbclickEvent(treeId){
  var elem = document.getElementById(treeId);
  var parenNodes = elem.getElementsByClassName('parent');

  if (parenNodes.length) {
    for (var i = 0; i < parenNodes.length; i++) {
      var parentNode = parenNodes[i];
      // for PC
      parentNode.removeEventListener('dblclick', clickEditBtn);
      parentNode.addEventListener('dblclick', clickEditBtn, false);
    }
  }
}

// Click event set 'tree_node'
function bindSelectEvent(treeId) {
  var elem = document.getElementById(treeId);
  var nodes = elem.getElementsByClassName('tree_node');
  var node;

  if (nodes.length) {
    for (var i = 0; i < nodes.length; i++) {
      node = nodes[i];
      node.removeEventListener('touchstart', selectTreeNode);
      node.addEventListener('touchstart', selectTreeNode, false);
      node.removeEventListener('mousedown', selectTreeNode);
      node.addEventListener('mousedown', selectTreeNode, false);
    }
  }
}

