==============================================================
*/
function setAddProg(param) {
  var split_param = param[1].split(',');
  var order;
  var speed_method;
  var spd_param;
  var spd_unit;
  var stop;
  var stop_param;
  var spd_unit_tbl = [
    'mm/sec',
    'cm/min',
    'inch/min',
    'deg/sec',
    'sec',
    'msec',
    '%'
  ]
  if (split_param[0].indexOf('(') != -1) {
    for (var i = 0; i < split_param.length; i++) {
      if (split_param[i].indexOf('(') != -1) {
        split_param[i] = split_param[i].replace(/\(|\)/g, '');
      }
    }
  }
  //Set speed method
  if (split_param[2] == "1") {
    speed_method = "";
  } else if (split_param[2] == "2") {
    speed_method = ORDER_REGI;
  } else if (split_param[2] == "3") {
    speed_method = ORDER_REGI + "[" + ORDER_REGI;
  } else if (split_param[2] == "4") {
    speed_method = langResource.ihmieditor_order_weld_speed_c + " ";
  }
  //Set speed param
  if (split_param[2] == "2" || split_param[2] == "3") {
    spd_param = "[" + split_param[3] + "]";
  } else if(split_param[2] == "4"){
    spd_param = "";
  } else {
    spd_param = split_param[3];
  }
  //Set speed unit
  if (split_param[2] == "4") {
    spd_unit = "";
  } else {
    if (split_param[4] == "1") {
      spd_unit = spd_unit_tbl[0];
    } else if (split_param[4] == "2") {
      spd_unit = spd_unit_tbl[1];
    } else if (split_param[4] == "3") {
      spd_unit = spd_unit_tbl[2];
    } else if (split_param[4] == "4") {
      spd_unit = spd_unit_tbl[3];
    } else if (split_param[4] == "5") {
      spd_unit = spd_unit_tbl[4];
    } else if (split_param[4] == "6") {
      spd_unit = spd_unit_tbl[5];
    } else if (split_param[4] == "7") {
      spd_unit = spd_unit_tbl[6];
    }
  }
  //Set stop
  if (split_param[5] == "1") {
    stop = ORDER_FINE;
  } else if (split_param[5] == "2") {
    stop = ORDER_CNT;
  } else if (split_param[5] == "3") {
    stop = ORDER_CNT + " " + ORDER_REGI;
  } else if (split_param[5] == "4") {
    stop = langResource.ihmieditor_detail_dispcr_a_c;
  }
  //Set stop param
  if (split_param[5] == "1") {
    stop_param = "";
  } else if (split_param[5] == "3") {
    stop_param = "[" + split_param[6] + "]";
  } else {
    stop_param = split_param[6];
  }
  //Combining parameters
  if (split_param[1] == 1) {//Liner
    order = ORDER_LINE + " P[N] " + speed_method + spd_param + spd_unit + " " + stop + stop_param;
  } else if (split_param[1] == 2) {//Joint
    order = ORDER_JOINT + " P[N] " + speed_method + spd_param + spd_unit + " " + stop + stop_param;
  }
  return order;
}

