/*
 * Check Update Payload data when change value
 */
function checkDataValue(chkData, key) {
  if(isErrData(chkData, key)) {
    switch (key) {
      case 'comment':
        return resources['ERRTooManyChars'];
      case 'payload':
        return resources['ERRPayloadValue'];
      case 'x':
      case 'y':
      case 'z':
        return resources['ERRXYZValue'];
      // Check IX,IY,IZ(Min, Max)
      case 'ix':
      case 'iy':
      case 'iz':
        return resources['ERRIXIYIZValue'];
    }
  }
  return "";
}
function isErrData(chkData, key) {
  switch (key) {
    // Check Comment Length
    case 'comment':
      if (countChar(chkData) > PL_COMMENT_MAX){
        return true;
      }
      break;
    // Check Payload Value(Min, Max)
    case 'payload':
      if ((chkData === "") || isNaN(chkData) || (parseFloat(chkData) < PAYLOAD_MIN) || (parseFloat(chkData) > PAYLOAD_MAX)) {
        return true;
      }
      break;
    // Check X,Y,Z(Min, Max)
    case 'x':
    case 'y':
    case 'z':
      if ((chkData === "") || isNaN(chkData) || (parseFloat(chkData) < PAYLOAD_POS_MIN) || (parseFloat(chkData) > PAYLOAD_POS_MAX)) {
        return true;
      }
      break;
    // Check IX,IY,IZ(Min, Max)
    case 'ix':
    case 'iy':
    case 'iz':
      if ((chkData === "") || isNaN(chkData) || (parseFloat(chkData) < -PAYLOAD_INERTIA_MIN) || (parseFloat(chkData) > PAYLOAD_INERTIA_MAX)) {
        return true;
      }
      break;
  }
  return false;
}
function calcDispVal(plData, key){
  switch(key) {
    case 'comment':
      return plData.indexOf('*') >= 0 ? '' : plData;

    case 'payload':
      return Number(plData);

    case 'ix':
    case 'iy':
    case 'iz':
      // kgcms^2 -> kgms^2
      return Number(plData) * 0.0001;

    case 'x':
    case 'y':
    case 'z':
      // cm -> m
      return Number(plData) * 0.01;
  }
}

function calcSysvarVal(plData, key){
  switch(key) {
    case 'comment':
      return multByteStringSlice(plData, PL_COMMENT_MAX);
    case 'payload':
      return plData;

    case 'ix':
    case 'iy':
    case 'iz':
      // kgms^2 -> kgcms^2
      return Number(plData) / 0.0001;

    case 'x':
    case 'y':
    case 'z':
      // m -> cm
      return Number(plData) / 0.01;
  }
}

/*
 * Initializing Payload data when change page
 */
function initPayloadPage() {
  var selType = payloadGrid.getSelectType();
  var selRng;
  switch (selType) {
    case SELGRID_ROW:
      selRng = payloadGrid.getSelectRow();
      break;
    case SELGRID_CELL:
      selRng = payloadGrid.getSelectRange();
      break;
  }

  var linebuff = [];
  for (var i = g_current_item_no_start; i <= g_current_item_no_end; i++) {
    var newline = createrow();
    newline.record[COLUMN_INDEX].recVal = i;
    linebuff.push(newline);
  }
  if (payloadGrid.getRecord(1) != null) {
    payloadGrid.clearGrid();
    if (linebuff.length != 0) {
      payloadGrid.drawRecord(linebuff);
    }
  } else {
    payloadGrid.refresh(document, 'payloadarea', payloadTableDef, linebuff);
  }
  
  switch (selType){
    case SELGRID_ROW:
      selRng.end = selRng.start;
      payloadGrid.setSelectRow(selRng);
      break;
    case SELGRID_CELL:
      payloadGrid.setSelectCell(selRng.start.row, PAYLOAD_CLM_ARR[selRng.start.col]);
      break;
  }

  // Get payload datas.
  var arg = {};
  arg.gn = g_select_group;
  arg.sn = g_current_item_no_start;
  arg.nm = (g_current_item_no_end - g_current_item_no_start) + 1;
  top.rpcmc_chkkey(null, top.mor_ss_c, top.SYSNAME_C,'$DCSS_CLLB[1].$EF_ENABLE', getEfenValEnbCB, arg);

  //FSI 20181207 Readjust the screen size.
  $(window).trigger('resize');
}

