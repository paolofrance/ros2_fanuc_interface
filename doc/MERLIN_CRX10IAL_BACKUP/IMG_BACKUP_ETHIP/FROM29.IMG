/*
 * Get I/O Simulate Callback
 */
function getIOsimCallback(sts, type, index, sim_val, callbackarg1, callbackarg2) {
    var recid = index - g_current_item_no_start;
    var cur_val = "";
    var next_val = "";
    if (index < g_current_item_no_start
        || index > g_current_item_no_end
        || type != g_cur_type) {
        return;
    }
    if (type == IO_TYPE_FLAG) {
        return;
    }
    if (sts == IO_SUCCESS) {
        next_val = sim_val;
    } else if (sts == IOSTAT_NOT_IO_ALLOC) {
        next_val = STR_STS_NONE;
    } else if (sts == IOSTAT_PRIO_023) {
        next_val = STR_STS_NONE;
    } else {
        // no action
    }
    cur_val = w2ui['ioarea'].get(recid).simulate;
    if (cur_val != next_val) {
        w2ui['ioarea'].get(recid).simulate = next_val;
        setIOSimColor(recid, next_val.toString());
    }
}

/*
 * Get IO Comment Callback
 */
function getIOcommentCallback(status, type, index, str) {
    var recid = index - g_current_item_no_start;
    var next_com = "";
    if (index < g_current_item_no_start
        || index > g_current_item_no_end
        || type != g_cur_type) {
        return;
    }
    if (status == IO_SUCCESS) {
        next_com = str;
    } else if (status == IOSTAT_IO_COMMENT_NULL) {
        next_com = "";
    } else if (status == IOSTAT_PRIO_023) {
        next_com = "";
    }
    cur_com = w2ui['ioarea'].get(recid).comment;
    if (cur_com != next_com) {
        w2ui['ioarea'].get(recid).comment = next_com;
        w2ui['ioarea'].get(recid).w2ui.style[COLUMN_COMMENT] = "background-color: #ffffff; color:#000000";
        w2ui['ioarea'].refreshCell(recid, 'comment');
    }
}

/*
 * Set IO Value Callback
 */
function setIOvalueCallback(status, type, index, original_val) {
    var recid = index - g_current_item_no_start;
    if (index < g_current_item_no_start
        || index > g_current_item_no_end
        || type != g_cur_type) {
        return;
    }
    if (status == IO_SUCCESS) {
        switch (type) {
            case IO_TYPE_AIN:
            case IO_TYPE_AOUT:
            case IO_TYPE_GIN:
            case IO_TYPE_GOUT:
                getIOvalue(g_cur_type, index, updIOvalue, null, null);
                break;
            case IO_TYPE_DIN:
            case IO_TYPE_DOUT:
            case IO_TYPE_RIN:
            case IO_TYPE_ROUT:
            case IO_TYPE_FLAG:
                setTimeout(function () {
                  getIOvalue(g_cur_type, index, updIOvalue);
                }, 100);
                break;
            default:
                break;
        }
    } else if (status == IOSTAT_PRIO_017) {
        updIOvalue(IO_SUCCESS, type, index, original_val);
        switch (type) {
          case IO_TYPE_DIN:
          case IO_TYPE_DOUT:
          case IO_TYPE_RIN:
          case IO_TYPE_ROUT:
          case IO_TYPE_FLAG:
              setIOStsColor(recid, original_val);
              break;
        }
        top.openCmpAlert(window, 'errnoAllocateDlg', resources['ERRNoAllocate']);
    } else {
        switch (type) {
            case IO_TYPE_DIN:
            case IO_TYPE_DOUT:
            case IO_TYPE_RIN:
            case IO_TYPE_ROUT:
            case IO_TYPE_FLAG:
                w2ui['ioarea'].get(recid).w2ui.style[COLUMN_STATUS] = "background-color: #e60012";
                w2ui['ioarea'].refreshCell(recid, 'state');
                break;
            case IO_TYPE_AIN:
            case IO_TYPE_AOUT:
            case IO_TYPE_GIN:
            case IO_TYPE_GOUT:
                w2ui['ioarea'].get(recid).w2ui.style[COLUMN_VALUE] = "background-color: #e60012";
                w2ui['ioarea'].refreshCell(recid, 'value');
                break;
        }
    }
}

