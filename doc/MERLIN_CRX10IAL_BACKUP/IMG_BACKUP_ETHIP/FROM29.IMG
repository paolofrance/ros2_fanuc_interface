function sendFileTd(fileName, fileData, fromFilePath, callback) {
  // Ready to xhr.
  $.ajax({
      url: "/td?file=" + fileName,
      type: "POST",
      data: fileData,
      processData: false,
      contentType: false,
      success: function (res) {
        var arg = {};
        arg.callback = null;
        arg.type = 'TD';
        sendXmlCopy(IO_SUCCESS, fileName, fromFilePath, callback);
      },
      error: function (xmlhttprequest, textstatus, errorthrown) {
          console.log(xmlhttprequest);
          console.log(textstatus);
          console.log(errorthrown);
          callback(-1, fileName);
      }
  });
}

