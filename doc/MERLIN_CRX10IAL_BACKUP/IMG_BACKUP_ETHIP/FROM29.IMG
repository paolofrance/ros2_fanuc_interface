// Language Resource Initialize. (except toolbar)
function initEachWindowResource() {
    for (var i = 0; i < resourceEachIOTitle.length; i++) {
        var rmt = resourceEachIOTitle[i];
        var idStr = '#' + rmt.id;
        var resourceText = resources[rmt.resId];
        if (rmt.append == 'before') { // Message append to already existed text.
            resourceText = resourceText + $(idStr).html(); //	append text
        } else if (rmt.append == 'after') {
            resourceText = $(idStr).html() + resourceText; //	append text
        }
        $(idStr).html(resourceText);
    }

    //debug(DBGDUMP, 'Init window resource.');
    for (var i = 0; i < resourceEachIOTable.length; i++) {
        var rmt = resourceEachIOTable[i];
        var idStr = '#' + rmt.id;
        var resourceText = resources[rmt.resId];
        if (rmt.append == 'before') { // Message append to already existed text.
            resourceText = resourceText + $(idStr).html(); //	append text
        } else if (rmt.append == 'after') {
            resourceText = $(idStr).html() + resourceText; //	append text
        }
        $(idStr).html(resourceText);
    }

    for (var i = 0; i < resource_io_status.length; i++) {
        var rmt = resource_io_status[i];
        for (var j = 0; j < g_io_status.length; j++) {
            if (g_io_status[j].id == rmt.id) {
                g_io_status[j].text = resources[rmt.resId];
            }
        }
    }

    for (var i = 0; i < resource_io_simulate.length; i++) {
        var rmt = resource_io_simulate[i];
        for (var j = 0; j < g_io_simulate.length; j++) {
            if (g_io_simulate[j].id == rmt.id) {
                g_io_simulate[j].text = resources[rmt.resId];
            }
        }
    }

    var coldata = w2ui['ioarea'].columns;
    for (var i = 0; i < coldata.length; i++) {
        var rmt = resourceEachIOTable[i];
        coldata[i]['caption'] = resources[rmt.resId];
        if (coldata[i].field == 'state') {
            coldata[i].editable = { type: 'select', items: [{ id: '', text: '' }].concat(g_io_status) };
        }
    }
    w2ui['ioarea'].refresh();
    //debug(DBGDUMP, 'Finish windows resource.');
}

function initIOAsgnWindowResource(ioType) {
  for (var i = 0; i < resource_io_asgn.length; i++) {
    var rmt = resource_io_asgn[i];
    $("#" + rmt.id)[0].innerText = resources[rmt.resId];
  }
  switch (ioType) {
    case TAB_DI:
    case TAB_DO:
      $("#startNumLbl")[0].innerText = resources['AsgnStartNum'];
      $("#startPointLbl")[0].innerText = resources['AsgnStartPoint'];
      break;
    case TAB_AI:
