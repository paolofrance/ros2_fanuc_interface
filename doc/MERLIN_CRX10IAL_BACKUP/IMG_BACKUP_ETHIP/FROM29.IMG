function setPayloadDataIFCB(status, lim_err, pld_stat, mode, setPldInf, backUpInf) {
  if (status == ER_PLDSET_FAIL) {
    top.openCmpAlert(window, 'pldERRPldsetFailDlg', top.irpDlgRes["dlgERRPldsetFail"]);
    cancelIFChange();
    return;
  } else if (status != top.IO_SUCCESS) {
    console.log('Error on /COMET/rpc?func=PGPLDUTL : status [' + status +'], mode [' + mode + ']');
    cancelIFChange();
    return;
  }

  if (mode == PLDCHK) {
    if (lim_err == 1) {
      top.openCmpAlert(window, 'pldERRLimDlg', top.irpDlgRes['dlgERRLim']);
      cancelIFChange();
    } else if (lim_err == 0 && pld_stat == ER_OVERLOAD) {
      top.openCmpConfirm(webPage, window, 'pldERRLimOverLoadDlg', top.irpDlgRes["dlgERRLimOverLoad"], {btnData:top.irpDlgBtnYN, btn1Cb:pldMoveCheck, btn2Cb:cancelIFChange, closeBtnCb:cancelIFChange});
    } else if (lim_err == 0 && pld_stat == ER_CLOSE_CAPACITY) {
      top.openCmpConfirm(webPage, window, 'pldERRLimCloseCapacityDlg', top.irpDlgRes["dlgERRLimCloseCapacity"], {btnData:top.irpDlgBtnYN, btn1Cb:pldMoveCheck, btn2Cb:cancelIFChange, closeBtnCb:cancelIFChange});
    } else {
      pldMoveCheck();
    }
  }

  function cancelIFChange() {
    for (var i = 0; i < backUpInf.length; i++) {
      var record = payloadGrid.getRecord(backUpInf[i].row);
      if (record == undefined) break;
      if (backUpInf[i].col != 'comment') {
        payloadGrid.setCellValue(backUpInf[i].row, backUpInf[i].col, backUpInf[i].val);
      }
    }
  }

  function pldMoveCheck() {
    // PLDSET_MOVECHK
    setPayloadDataIF(g_select_group, setPldInf.index, PLDSET_MOVECHK, setPayloadDataIFCB, setPldInf, backUpInf);
  }
