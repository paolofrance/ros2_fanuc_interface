/* === Processing specialized for each command === */

// Selected item judge whether an indirect item.
function isIndirectItem(asstStr) {
	var sqrBraLf = asstStr.indexOf('[');
	var sqrBraRt = asstStr.indexOf(']');
	return ((sqrBraLf >= 0) && (sqrBraRt >= 0));
}
// Input string judge whether an indirect item.
function isStringIndirect(asstStr, asstType) {
	var cmdParts = getCommandParts();
	var asstList = (asstType == ASSIST_REG) ? asstSelReg : asstSelStd;
	var rsIndCmd = [];
	$(asstList).each(function (j) {
		rsIndCmd.push(cmdParts[asstList[j]]);
	});
	return ($.inArray(asstStr, rsIndCmd) >= 0);
}

/**
 * Initialize dialog control.
 */
// Motion instruction. : Pattern 1
function initMotionDialog(cmdId) {
	var cmdParts = getCommandParts();
	var initCmd = '';
	var initFineCnt = '';
	switch (cmdId) {
		case 'CMDJPFINE':
			break;
		case 'CMDJPCNT':
			initCmd = cmdParts['txtMotionJ'];
			initFineCnt = cmdParts['txtCnt'];
			break;
		case 'CMDLPFINE':
			initCmd = cmdParts['txtMotionL'];
			break;
		case 'CMDLPCNT':
			initCmd = cmdParts['txtMotionL'];
			initFineCnt = cmdParts['txtCnt'];
			break;
		case 'CMDCPFINE':
			initCmd = cmdParts['txtMotionA'];
			break;
		case 'CMDCPCNT':
