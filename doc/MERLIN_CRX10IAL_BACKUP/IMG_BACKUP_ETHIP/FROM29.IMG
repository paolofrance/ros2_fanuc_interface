  width: 100%;
  height: 100%;
  background-color: #e1e7ec;
  margin: 0px;
  box-sizing: border-box;
  touch-action: none;
}

.screenoverlayCls{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 9988;
  background-color: rgba(0, 0, 0, 0.2);
}
.menucstm_area{
  height: 100%;
  overflow: auto;
}

button {
  border: none;
  cursor: pointer;
  appearance: none;
  padding: 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background-color: #67727e;
  text-align: left;
  color: #ffffff;
  font-size: 16px;
  user-select: none;
  -webkit-user-select:none;
}

button:focus {
  outline: none;
}

button:disabled {
  pointer-events: none;
}

#btnyes ,#btnno {
  display: inline-flex;
  font-size: 16px;
}

#section_button:focus,
.button_area.top > button:focus,
.button_area.bottom > button:focus {
  outline: solid 1px #32a5ff;
}

.button_area.top > button:active,
.button_area.bottom > button:active {
  background-color: #0e6eb8;
}

.button_area.top > button:disabled:active,
.button_area.bottom > button:disabled:active {
  background-color: #c7ced7;
  color: #67727e;
}

/* For main screen */
.main_area {
  overflow: hidden;
  display: flex;
  height: 65%;
  width: 90%;
  padding-top: 20px;
  margin-left: 5%;
  margin-right: 5%;
  min-height: 280px;
  min-width: 625px;
}

.middle_contents {
  display: flex;
  width: 10%;
  height: calc(100% - 56px);
  justify-content: center;
  align-items: center;
  min-width: 50px;
}

.left_contents {
  width: 45%;
  height: calc(100% - 56px);
  min-width: 240px;
}

.right_contents {
  width: 45%;
  height: calc(100% - 56px);
  min-width: 240px;
}

.header_area {
  width: 100%;
  height: 50px;
  line-height: 50px;
  font-size: 20px;
  font-weight: 70%;
}


.tree_contents {
  width: 100%;
  height: calc(100% - 50px);
  min-width: 270px;
}


.tree_area {
  width: calc(100% - 6px);
  border: solid 3px #97a1ae;
  background-color: #ffffff;
  overflow-y: scroll;
  overflow-x: hidden;
  min-height: 150px;
  -ms-overflow-style: none;
  padding-bottom: 2px;
}

.tree_area.base {
  height: 100%;
  width: 90%;
  float: left;
}

.tree_area.custom {
  height: 100%;
  width: 90%;
  float: left;
}

/* scrollbar */
.tree_area::-webkit-scrollbar {
  display: none;
}

.scroll_area {
  position: relative;
  height: 100%;
  width: 20px;
  float: left;
  margin: 1px;
}

.scrollbar {
  position: absolute;
  z-index: 1000;
  background: #ccc;
  height: 0%;
  width: 20px;
  top: 0%;
  bottom: 0%;
  margin: 2px;
  padding: 0%;
  box-sizing: border-box;
}

.drag {
  z-index: 1001;
}

.arrow {
  position: relative;
}

.arrow::after {
  position: absolute;
  top: 0;
  left: -40px;
  width: 50px;
  height: 50px;
  border-top: solid 5px #67727e;
  border-right: solid 5px #67727e;
  content: '';
  transform:rotate(45deg);
}

.bottom_area{
  height: 20%;
  width: 100%;
  min-width: 680px;
}

.button_contents {
  position: relative;
  height: 100%;
  width: 450px;
  float: right;
  margin-right: 8%;
}

.button_area {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 49%;
  float: right;
  min-height: 60px;
  width: 100%;
  margin-bottom: 2%;
}

.bottom_button, .section_button {
  width: 45%;
  height: 60%;
  min-width: 140px;
  min-height: 60px;
  margin: 5px;
  user-select: none;
  -webkit-user-select: none;
}

.button_ico {
  width: 30px;
  height: 30px;
}

.section_button > div {
  width: 100%;
  height: 100%;
}

.section_button > div > button {
  width: 100%;
  height: 100%;
}

button:disabled {
  background-color: #c7ced7;
  color: #67727e;
}

/* For tree */
.base_list, .custom_list {
  margin-left: 30px;
}

/* For displaying items when scrolling horizontally */
#base_list ,#custom_list{
  min-width: 100%;
}

.tree_node {
  position: relative;
  height: 40px;
  width: calc(100% - 30px);
  min-width: 80px;
  line-height: 40px;
  margin-top: 2px;
  background-color: #67727e;
  color: #ffffff;
  z-index: 10;
  font-size: 16px;
  white-space: nowrap;
}

.tree_node.parent {
  background-color: #97a1ae;
}

.tree_node>span {
  padding-left: 10px;
}

.tree_ico {
  display: inline-block;
  vertical-align: middle;
  margin-right: 10px;
  margin-left: 10px;
  width: 0;
  height: 0;
}

.tree_ico.open {
  border-top: 13px solid #ffffff;
  border-right: 7px solid transparent;
  border-left: 7px solid transparent;
  border-bottom: 0px solid transparent;
}

.tree_ico.close {
  border-top: 7px solid transparent;
  border-right: 0px solid transparent;
  border-left: 13px solid #ffffff;
  border-bottom: 7px solid transparent;
}

.select_node {
  background-color: #0e6eb8 !important;
}

#focus_button {
  background-color: transparent;
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  outline: 0;
  z-index: 999;
}

/* For edit text box */
.editbox {
  position: absolute;
  top: 0;
  left: 0;
  width: calc(100% - 6px);
  height: 89%;
  border: solid 1px #97a1ae;
  font-size: 16px;
}

.editbox:focus {
  outline: 0;
  border-color: #6fc7d5;
}

.editbox::selection {
  background: #0e6eb8;
  color: #ffffff;
}

#editbox_tooltip {
  display: block;
  position: absolute;
  z-index: 9999;
  color: #000000;
  font-size: 15px;
  background: rgba( 255, 255, 255, 0.9);
  border-radius: 2px;
  box-shadow: 2px 3px 3px rgba(0, 0, 0, 0.2);
  top: 48px;
  line-height: 20px;
  padding: 10px;
  
}
  
#editbox_tooltip::before {
  content: "";
  display: block;
  position: absolute;
  width: 0;
  height: 0;
}

#editbox_tooltip::after {
  content: "";
  display: block;
  position: absolute;
  width: 0;
  height: 0;
  border-left: 8px solid transparent;
  border-right: 8px solid transparent;
}

/*top arrow*/
.edit_arrow_lt::after{
  top: -8px;
  left: 6px;
  border-left: 8px solid transparent;
  border-right: 8px solid transparent;
  border-bottom: 8px solid rgba( 255, 255, 255, 0.9);
  border-top: 0px solid transparent;
}

.edit_arrow_lt::before{
  top: -7px;
  left: 10px;
  border-left: 7px solid transparent;
  border-right: 7px solid transparent;
  border-bottom: 7px solid rgba( 153, 153, 153, 0.2);
  border-top: 0px solid transparent;  
}

/*bottom arrow*/
.edit_arrow_lb::before {
  bottom: -8px;
  left: 6px;
  border-top: 9px solid rgba( 153, 153, 153, 0.2);
  border-bottom: 0px solid transparent;
  border-left: 9px solid transparent;
  border-right: 9px solid transparent;
}

.edit_arrow_lb::after {
  bottom: -7px;
  left: 6px;
  border-top: 8px solid rgba( 255, 255, 255, 0.9);
  border-bottom: 0px solid transparent;
}

#pgdelete {
  font-size: 17px;
  padding: 8px 0px 8px 0px;
  margin: 10px 10px 0px 10px;
