/* == Pickup message from resource.
      If nothing, resource id is output message text. == */
function pickupDialogMessage(resId, dlgDef) {
  var dlgMsg = "";
  if ((resId.length > 0) || (dlgDef.textRes.length > 0)) {
    dlgMsg = resources[(resId.length > 0) ? resId : dlgDef.textRes];
    if ((dlgMsg == undefined) || (dlgMsg.length == 0)) {
      dlgMsg = resId;
    }
  }
  return dlgMsg;
}
/* === Dialog configure === */
function configDialog(dlgId, textResId) {
  // Search dialog define
  var dlgDef = null;
  for (var i = 0; i < dialogDefinitionTable.length; i++) {
    dlgDef = dialogDefinitionTable[i];
    var idStr = '#' + dlgDef.id;
    if (idStr === dlgId) {	// target dialog?
      break;
    }
  }
  if (dlgDef == null) {
    debug(DBGERROR, "Illega Dialog ID:" + dlgId);
    return;
  }
  // Make dialog
  var dlgConf = '';
  if(dlgId == '#appleditordlg'){
    dlgConf = $('<div>',
    { id: dlgDef.id, 'class': 'modaldlg appldlgsize' });
  } else{
    dlgConf = $('<div>',
    { id: dlgDef.id, 'class': 'modaldlg' });
  }
  if (isScrModeProgAttr()) {
    var dlgTtl = $('<div>',
    { id: dlgDef.title, 'class': 'attrdlgtitlebar' });
  } else {
    var dlgTtl = $('<div>',
    { id: dlgDef.title, 'class': 'dlgtitlebar' });
  }
  var ttlArea = $('<label>', { 'class': 'dlgTtlText' });
  ttlArea.html(resources[dlgDef.titleRes]);
  dlgTtl.append(ttlArea);
  if(dlgId == '#posregconferr') {
    var ttlCancel = $('<div>', { id: 'dlgTtlCancel', 'class': 'dlgttlbarbtn' });
    var ttlBtn = $('<img>',
      { id: 'ttlCancelBtn', 'src': '/frh/jcgtp/closeicon.png', 'class': 'dlgttlbtnimg' });
    ttlCancel.append(ttlBtn);
    dlgTtl.append(ttlCancel);
  }
  dlgConf.append(dlgTtl);
  var dlgMsg = $('<p>');
  dlgMsg.attr('id', dlgDef.dlgTextId);

  if(dlgId == '#appleditordlg'){
    dlgMsg.addClass('dlgcontent appl');
  } else{
    dlgMsg.addClass('dlgcontent');
  }
  if ((dlgDef.msgClass != undefined) && (dlgDef.msgClass.length > 0)) {
    dlgMsg.addClass(dlgDef.msgClass);
  }
  var innerMsg = ""; //resources[dlgDef.message];
  if (dlgDef.append != 'after') {
    innerMsg = pickupDialogMessage(textResId, dlgDef);
  }
  if ((dlgDef.msgForm != undefined) && (dlgDef.msgForm.length > 0)) {
    innerMsg += dlgDef.msgForm;
  }
  if (dlgDef.selOptBuild != undefined) {
    var formText = (dlgDef.selOptBuild.func)(dlgDef.selOptBuild.id);
    innerMsg += formText;
  }
  if (dlgDef.append == 'after') {
    innerMsg += pickupDialogMessage(textResId, dlgDef);
  }
  dlgMsg.html(innerMsg);
  dlgConf.append(dlgMsg);
  var dlgBtn = $('<div align="center" style="clear: both;">');
  for (var j = 0; j < dlgDef.btnDef.length; j++) {	// Yes/No/OK/Cancel
    var btnClass = "dlgbutton";
    btnClass += (dlgDef.btnDef[j].type == "ok") ?
      ((dlgDef.btnDef[j].disabled) ? " okyesdis" : " okyes") : " nocancel";
    btnClass += " btn" + dlgDef.btnDef[j].type;
    var btnObj = $('<button>',
      { type: 'button', id: dlgDef.btnDef[j].id, 'class': btnClass, value: resources['dlgBtnOk'] });
    btnObj.attr({ 'call': dlgDef.btnDef[j].call });
    if (dlgDef.btnDef[j].disabled) {
      btnObj.prop('disabled', true);
    }
    btnObj.html(resources[dlgDef.btnDef[j].face]);
    dlgBtn.append(btnObj);
  }
  dlgConf.append(dlgBtn);
  $('body').append(dlgConf);
  if (((dlgId == '#getsvasprogdlg') || (dlgId == '#getrenameprogdlg') || (dlgId == '#getprogdlg')) && // input element
  (isiPad)) { // for fix bug iOS display collapse
    $('.dlgcontent').css({
      'box-sizing': 'border-box'
    });
  }
}

