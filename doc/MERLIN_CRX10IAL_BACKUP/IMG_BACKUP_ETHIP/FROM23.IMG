/* 4.6
 * Pause Program.
 * Annotation : Delay processing is necessary to make temporary stop work.
 */
function pauseProgram() {
	top.sendKey("Hold", 1);
	setTimeout(function () { // add timeout
		top.sendKey("Hold", 0);
		setTimeout(function () { // add timeout
			top.sendKey("R-shift", 0);
			top.sendKey("L-shift", 0);
		}, 100);
	}, 500);
}

/* 4.7
 * Stop Program.
 * (IN) prog_name : Program name.
 * (IN) callback : A callback function to stop excute.
 *          arg1 : Status code.
 *          arg2 : Program name.
 */
function abortProgram(prog_name, callback, cbArg) {
	if (!top.g_jitpky2) { // [Add] by FAC
		// This is not the iPendant connection
		return;
	}
	$.getJSON("/COMET/rpc?func=PGABORT&task_name=" + encodeASCII(prog_name), function (json) {
		if (json.FANUC.RPC != undefined) {
			json.FANUC.RPC[0].status = json.FANUC.RPC[0].status.toLowerCase();
			if (json.FANUC.RPC[0].status != IO_SUCCESS) {
				top.rpcmc_rprintf("stopProgram error status");
				handle_error(json.FANUC.RPC[0].status);
			}
			top.sendKey("R-shift", 0);
			if (callback != undefined) {
				callback(json.FANUC.RPC[0].status, prog_name, cbArg);  // omit line_num
			}
		} else {
      top.rpcmc_rprintf("[rpcmc] unexpected reply " + JSON.stringify(json));
    }
  }).fail(function (xhr, textStatus, errorThrown) {
    putIFFailLog(xhr, textStatus, errorThrown);
    callback(IO_COMFAILURE, prog_name, cbArg);
  });
}

/* 4.8
 * Run Task
 * (IN) task_name      : Run task name.
 * (IN) cur_task_name  : Current task name.
 * (IN) line_num       : Line num.
 * (IN) exec_type      : Execution type.
                         PG_FWD_EXE_C = Move forward
                         PG_NBWD_EXE_C = Retreat
                         PG_SUPERRUN_C = Supervisor motion
 * (IN) pause_on_shift : Pause on shift
                         TRUE(1) = Pause when the shift key is released.
                         FALSE(0) = Continues even if you release the Shift key.
 * (IN) tp_motion      : Tp motion
                         TRUE(1) = Only executed if TP is enabled.
                         FALSE(0) = Execute even if TP is invalid.
 * (IN) callback       : A callback function that is called after task execution starts.
 *          arg1       : Status code.
 *          arg2       : Run task name.
 * (IN) cbArg          : Callback argument object.
 */
function runTask(task_name, cur_task_name, line_num, exec_type, pause_on_shift, tp_motion, wait_type, callback, cbArg) {
	$.getJSON("/COMET/rpc?func=RUN_TASK&task_name=" + encodeASCII(task_name) + "&cur_task_name=" + encodeASCII(cur_task_name) + "&line_num=" + line_num +"&exec_type=" + exec_type +"&pause_on_shift=" + pause_on_shift +"&tp_motion=" + tp_motion +"&wait_type=" + wait_type, function(json) {
		if (json.FANUC.RPC != undefined) {
			json.FANUC.RPC[0].status = json.FANUC.RPC[0].status.toLowerCase();
			if (callback != undefined) {
				callback(json.FANUC.RPC[0].status, task_name, cbArg);
			}
			else {
        top.rpcmc_rprintf("callback undefined");
			}
		} else {
			top.rpcmc_rprintf("[rpcmc] unexpected reply " + JSON.stringify(json));
		}
	}).fail(function (xhr, textStatus, errorThrown) {
		putIFFailLog(xhr, textStatus, errorThrown);
		callback(IO_COMFAILURE, task_name, cbArg);
	});
}

