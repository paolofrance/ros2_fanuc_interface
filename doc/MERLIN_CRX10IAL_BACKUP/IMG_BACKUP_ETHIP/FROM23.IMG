/* 6.2
 * Add or replace multi group position data to program
 * (IN) prog_name : Program name
 * (IN) in_buf : ASCII format position data.
 *      ex) var pos = 'P[1:"Pick Standby"]' +
 *                    '{GP1:UF:0,UT:1,CONFIG:\'N%20U%20T ,0,0,0\',' +
 *                    'X=1807.000 mm,Y=0.000 mm,Z=1300.000 mm,' +
 *                    '...'
 *                    'GP2:UF:0, UT:1, J1=0.000 mm, E1=100.000 mm};';
 * (IN) callback  : A callback function to set position.
 *                  arg1: response object
 */
function editPosMulti(progName, inBuf, callback) {
  // make xml
  var posXml = '<?xml version="1.0" encoding="' + irprogapi.global.langEncoding[irprogapi.global.currentLang] + '"?>\r';
  posXml += '<PROGRAM name="' + encodeASCII(progName) + '">\r';
  posXml += '<POS>\r';
  posXml += inBuf;
  posXml += '</POS>\r</PROGRAM>';
  // create blob
  var fileData = irprogapi.inner.createBlob(posXml);
  // post xml
  var xhr = new XMLHttpRequest();
  var postUrl = 'http://' + location.host + '/SOFTPART/XMLJSON?func=EDITPRG';
  xhr.open('POST', postUrl, true);
  xhr.onload = function() {
    var response = JSON.parse(this.response);
    if (callback != undefined) {
      callback(response);
    } else {
      top.rpcmc_rprintf("[rpcmc] undefined callback: response=" + this.response);
    }
  };

  // Send file
  xhr.send(fileData);
}

