          if (status == IO_SUCCESS) {
            for (var rio_index = 0; rio_index < count; rio_index = rio_index + PORT_INDENT_RIO) {
              if ((value[rio_index].val != "0") && (value[rio_index].val != "1")) {
                rio_max_value = rio_index;
                break;
              }
            }
          } else {
            top.rpcmc_rprintf("Error: Failed to get RI / RO data.");
            rio_max_value = 0;
          }
          irpapi.global.ioMaxNum[index].maxvalue = rio_max_value;
          irpapi.global.ioMaxNum[index].complete = true;
          irpapi.inner.checkIOMaxNum(callback);
        }
      },
      /*
      * Search IO Index
      * (IN) iotype : IO Type
      * Return : index of iotype
      */
      getIOindex: function(iotype) {
        var ioIndex = 0;
        for ( ; ioIndex < irpapi.global.ioMaxNum.length; ioIndex++) {
          if (irpapi.global.ioMaxNum[ioIndex].iotype.test(iotype)) {
            break;
          }
        }
        return ioIndex;
      },
      /*
      * Analyze position data after convert XVR CallBack
      * (IN) getText : XML text.
      *      callbackarg : Argument object for callback.
      *          .type : Data type (POSREG/UTOOL/UFRAME)
      *          .callback : Callback functio when data converted.
      */
      convertXVRCallBack: function(status, getText, callbackarg) {
        var str_xml = '';
        var filelines = getText.split(CODELF);
        var dom_parser = new DOMParser();
        var xmlErrorFlg = false;
        for (var i = 0; i < filelines.length; i++) {
          str_xml = str_xml + filelines[i] + '\n';
        }
        var xml_obj = null;
        try {
          xml_obj = dom_parser.parseFromString(str_xml, 'text/xml');
        } catch (e) {
          irpapi.inner.apiErrorLog(e);
        }
        if ((xml_obj == null) || ((xml_obj != null) && (xml_obj.getElementsByTagName('parsererror').length))) {
          str_xml = irpapi.inner.delErrData(str_xml, filelines);
          xml_obj = dom_parser.parseFromString(str_xml, 'text/xml');
          xmlErrorFlg = true;
          irpapi.inner.apiErrorLog('convertXVRCallBack: xml error.');
        } else if ((navigator.userAgent.toLowerCase().indexOf('msie') != -1) ||
          (navigator.userAgent.toLowerCase().indexOf('trident') != -1)) { // IE
          var str_xml_IE = irpapi.inner.delIEGrbl(str_xml, filelines);
          if (str_xml_IE != str_xml) { // It is a process for when the garbled error which does not become an error in IE has occurred
            xml_obj = dom_parser.parseFromString(str_xml_IE, 'text/xml');
          }
        }
        irpapi.global.xmlErrorLog.flg = xmlErrorFlg;
        // Success Get XML Data.
        if (xml_obj.getElementsByTagName('ARRAY').length != 0) {
          var pos_data = {};
          var pos_elem = xml_obj.getElementsByTagName('ARRAY');
          // For position data
          switch (callbackarg.type) {
            case REG_TYPE_POS:
              for (var j = 0; j < pos_elem.length; j++) {
                if (pos_elem[j].getAttributeNode('name').value.match(/\$POSREG/)) {
                  pos_data[pos_elem[j].getAttributeNode('name').value.replace('$POSREG[', '').replace(']', '')] = analyzePosRegData(pos_elem[j].childNodes[0].data);
                }
              }
              break;
            case FRAME_TYPE_UTOOL:
              for (var jt = 0; jt < pos_elem.length; jt++) {
                if (pos_elem[jt].getAttributeNode('name').value.match(/\$MNUTOOL/)) {
                  pos_data[pos_elem[jt].getAttributeNode('name').value.replace('$MNUTOOL[', '').replace(']', '')] = analyzePosRegData(pos_elem[jt].childNodes[0].data);
                }
              }
              break;
            case FRAME_TYPE_UFRAME:
              for (var jf = 0; jf < pos_elem.length; jf++) {
                if (pos_elem[jf].getAttributeNode('name').value.match(/\$MNUFRAME/)) {
                  pos_data[pos_elem[jf].getAttributeNode('name').value.replace('$MNUFRAME[', '').replace(']', '')] = analyzePosRegData(pos_elem[jf].childNodes[0].data);
                }
              }
              break;
            default:
              irpapi.inner.apiErrorLog('convertXVRCallBack: Illegal callbackarg.type.(' + callbackarg.type + ')');
              return;
          }
          callbackarg.callback(pos_data, xmlErrorFlg);
          // For comment data
        } else if (xml_obj.getElementsByTagName('VAR').length != 0) {
          var cmt_data = {};
          var cmt_elem = xml_obj.getElementsByTagName('VAR');
          switch (callbackarg.type) {
            case FRAME_TYPE_UTOOL:
            case FRAME_TYPE_UFRAME:
              for (var ju = 0; ju < cmt_elem.length; ju++) {
                if (cmt_elem[ju].getAttributeNode('name').value.match(/SETUP_DATA/)) {
                  cmt_data[cmt_elem[ju].getAttributeNode('name').value.replace('SETUP_DATA[', '').replace('].$COMMENT', '')] = cmt_elem[ju].textContent;
                }
              }
              break;
            default:
              irpapi.inner.apiErrorLog('convertXVRCallBack: Illegal callbackarg.type.(' + callbackarg.type + ')');
              return;
          }
          callbackarg.callback(cmt_data, xmlErrorFlg);
        } else { // data none
          callbackarg.callback({}, xmlErrorFlg);
        }

        // Analyze Position Register String Array Data to Object.
        function analyzePosRegData(data) {
          var rtn_value = {};
          // PosReg data analyze.
          var target_key = '';    // Target Key
          var target_value = '';  // Target Value
          var value_temp = '';    // Value Temp(Next Key)
          for (var index = 0; index < data.length; index++) {
            switch (data[index]) {
              case '\'':  // Comment
                if ("comment" in rtn_value) {
                  rtn_value[target_key] = target_value;
                  target_key = "";
                  target_value = "";
                } else {
                  target_value = "";
                  target_key = "comment";
                  rtn_value[target_key] = "";
                }
                break;
              case ':':   // Delimiter
              case '=':   // Delimiter(Axis)
                if (target_key == "comment") {
                  target_value = target_value + data[index];
                } else {
                  if (target_key != "") {
                    rtn_value[target_key] = target_value.trim();
                  }
                  target_key = value_temp.trim();
                  target_value = "";
                  value_temp = "";
                }
                break;
              case '\n':  // Enter
                if (target_key != "") {
                  rtn_value[target_key] = (target_value + value_temp).trim();
                  target_key = "";
                  target_value = "";
                  value_temp = "";
                }
                break;
              default:    // Character and Space
                if (target_key == "comment") {
                  target_value = target_value + data[index];
                } else {
                  if ((data[index] != ' ') && (value_temp.slice(-1) == ' ')) {
                      target_value = target_value + value_temp;
                      value_temp = "";
                  }
                  value_temp = value_temp + data[index];
                }
                break;
            }
          }
          if (target_key != "") {
            rtn_value[target_key] = (target_value + value_temp).trim();
          }
          // Axis Data => {data:000.000, unit:"deg"}
          for (var key in rtn_value) {
            if (key.match(/J\d/) || key.match(/EXT\d/)) {
              var value = rtn_value[key].split(' ');
              rtn_value[key] = { data: value[0], unit: value[1] };
            }
          }
          return rtn_value;
        }
      },
      /*
      * Analyze system value data after convert XVR CallBack by list
      * (IN) getText : XML text.
      *      callbackarg : Argument object for callback.
      *          .reqSysVals : request system values.
      *          .callback   : Callback functio when data converted.
      */
     convertXVRCallBackByList: function(status, getText, callbackarg) {
        var str_xml = '';
        var filelines = getText.split(CODELF);
        var dom_parser = new DOMParser();
        var resonses = {};
        var sysval_name;
        var regexps = [];

        for (var i = 0; i < filelines.length; i++) {
          str_xml = str_xml + filelines[i] + '\n';
        }
        var xml_obj = null;
        var xmlErrorFlg = false;
        try {
          xml_obj = dom_parser.parseFromString(str_xml, 'text/xml');
        } catch (e) {
          irpapi.inner.apiErrorLog(e);
        }
        if ((xml_obj == null) || ((xml_obj != null) && (xml_obj.getElementsByTagName('parsererror').length))) {
          str_xml = irpapi.inner.delErrData(str_xml, filelines);
          xml_obj = dom_parser.parseFromString(str_xml, 'text/xml');
          xmlErrorFlg = true;
          irpapi.inner.apiErrorLog('convertXVRCallBack: xml error.');
        } else if ((navigator.userAgent.toLowerCase().indexOf('msie') != -1) ||
          (navigator.userAgent.toLowerCase().indexOf('trident') != -1)) { // IE
          var str_xml_IE = irpapi.inner.delIEGrbl(str_xml, filelines);
          if (str_xml_IE != str_xml) { // It is a process for when the garbled error which does not become an error in IE has occurred
            xml_obj = dom_parser.parseFromString(str_xml_IE, 'text/xml');
          }
        }
        irpapi.global.xmlErrorLog.flg = xmlErrorFlg;
        regexps = createRegExpList(callbackarg.reqSysVals);
        // Success Get XML Data.
        if (xml_obj.getElementsByTagName('VAR').length != 0) {
          var var_elems = xml_obj.getElementsByTagName('VAR');
          for (var jva = 0; jva < var_elems.length; jva++) {
            sysval_name = var_elems[jva].getAttributeNode('name').value;
            if (isMatchReqRes(sysval_name, regexps)) {
              resonses[sysval_name] = var_elems[jva].textContent;
            }
          }
        }
        if (xml_obj.getElementsByTagName('ARRAY').length != 0) {
          var arry_elems = xml_obj.getElementsByTagName('ARRAY');
          for (var jar = 0; jar < arry_elems.length; jar++) {
            sysval_name = arry_elems[jar].getAttributeNode('name').value;
            if (isMatchReqRes(sysval_name, regexps)) {
              resonses[sysval_name] = arry_elems[jar].textContent;
            }
          }
        }
        callbackarg.callback(resonses, xmlErrorFlg);

        function createRegExpList(reqSysVals) {
          var convStr = [];
          if (!reqSysVals) {
            return convStr;
          }
          if (reqSysVals.system && (reqSysVals.system.length > 0)) {
            for (var i = 0; i < reqSysVals.system.length; i++) {
              convStr.push(escapeRegExp(reqSysVals.system[i]));
            }
          }
          if (reqSysVals.posreg && (reqSysVals.posreg.length > 0)) {
            for (var k = 0; k < reqSysVals.posreg.length; k++) {
              convStr.push(escapeRegExp(reqSysVals.posreg[k]));
            }
          }
          if (reqSysVals.numreg && (reqSysVals.numreg.length > 0)) {
            for (var n = 0; n < reqSysVals.numreg.length; n++) {
              convStr.push(escapeRegExp(reqSysVals.numreg[n]));
            }
          }
          function escapeRegExp(string) {
            var reRegExp = /[\\^$.*+?()[\]{}|]/g;
            var reHasRegExp = new RegExp(reRegExp.source);
            return (string && reHasRegExp.test(string))
                ? string.replace(reRegExp, '\\$&')
                : string;
          }
          return convStr;
        }

        function isMatchReqRes(sysval_name, regexps) {
          var match_result;
          for (var i = 0; i < regexps.length; i++) {
            match_result = sysval_name.match(new RegExp(regexps[i]));
            if (match_result !== null) {
              regexps.splice(i, 1);
              return true;
            }
          }
          return false;
        }
      },
      /*
      * Single quotes may be missing, and if the pattern is matched,
      * a single quote is added to the end of the string.
      * When IE.
      * (IN) str_xml : XML string.
      *      filelines : File lines array.
      * return : added string.
      */
      delIEGrbl: function (str_xml, filelines) {
        var repStr = str_xml; // Data after error deletion
        var repBefStr = {
            before: /[^\']\s+\*{8}<\/ARRAY>/,
            after: "' ********</ARRAY>"
        };
        
        if (repBefStr['before'].test(str_xml)) {
          repStr = '';
          for (var i = 0; i < filelines.length; i++) { // Processing for when single quotes are garbled.
            filelines[i] = filelines[i].replace(repBefStr.before, repBefStr.after); // Add single quote
            repStr = repStr + filelines[i] + '\n';
          }
        }
        return repStr;
      },
      /*
      * If the position data have error, delete comment data.
      * (IN) str_xml : XML string.
      *      filelines : File lines array.
      * return : Data after error deletion.
      */
      delErrData: function(str_xml, filelines) {
        var repStr = ''; // Data after error deletion
        var matchStr; // For loop processing
        var repBefStr = { before: '', after: '' }; // before: String before replacement, after: String after replacement

        if (str_xml.indexOf('$COMMENT" prot ="RW">') !== -1) { // for uframe, utool, refpositions
          repBefStr = {
            before: /\$COMMENT\"\sprot\s=\"RW\"><\/VAR>\r\n/,
            after: '$COMMENT\" prot =\"RW\">********</VAR>\r\n'
          };
          if (str_xml.indexOf('$REFPOS') !== -1) {
            repBefStr.after = '$COMMENT\" prot =\"RW\">********</FIELD>\r\n';
          }
          repStr = delGrblChar(filelines, false); // Processing for when filelines are garbled.
        } else if (str_xml.indexOf('$POSREG') !== -1) { // for posregister
          repBefStr = {
            before: /prot\s=\"RW\">\s*\'.+?\'+/,
            after: " prot =\"RW\">  ''"
          };
          repStr = delGrblChar(filelines, true); // Processing for when filelines are garbled.
        }

        if (repBefStr.before !== '') { // Process only the line that contains comment
          matchStr = repStr.replace(repBefStr.before, repBefStr.after);
          while (matchStr !== repStr) {
            matchStr = matchStr.replace(repBefStr.before, repBefStr.after);
            repStr = repStr.replace(repBefStr.before, repBefStr.after);
          }
        }

        return repStr;

        // Delete garbled(Mojibake) character.
        function delGrblChar(filelines, posreg) {
          var repStr = ''; // Data after error deletion
        
          if (posreg) {
            for (var i = 0; i < filelines.length; i++) { // Processing for when single quotes are garbled.
              if (filelines[i].indexOf("'") !== -1) { // Process only the line that contains single quotes from the original.
                filelines[i] = filelines[i].replace("\r", "' \r"); // Add single quote
                filelines[i] = filelines[i].replace("</ARRAY>'", "'</ARRAY>"); // Add single quote
                filelines[i] = filelines[i].replace(" ********'</ARRAY>", "' ********</ARRAY>"); // Delete unnecessary single quote
              }
              repStr = repStr + filelines[i] + '\n';
            }
          } else {
            for (var j = 0; j < filelines.length; j++) { // Processing for when '<' was garbled.
              if (filelines[j].indexOf('COMMENT') !== -1) { // Process only the line that contains comment from the original.
                var headStr = filelines[j].substr(0, filelines[j].indexOf('$COMMENT" prot ="RW">') + 21);
                filelines[j] = headStr + '</VAR>\r';
              }
              repStr = repStr + filelines[j] + '\n';
            }
          }
        
          return repStr;
        }
      },
      /*
       * Create Blob for sending data.
       * (IN) str : String type data.
       * return : Blob object.
       */
      createBlob: function(str) {
        var blob;
        var codeArr = [];
        for (var i = 0; i < str.length; i++) {
          codeArr.push(str.charCodeAt(i));
        }
        var sendData = codeArr;
        var encArr = '';
        var blobEnc = irpapi.getBlobEncoding();
        if (blobEnc === undefined) {
          irpapi.inner.apiErrorLog('createBlob: Illegal current lang.(' + irpapi.global.currentLang + ')');
          blobEnc = ENC_UTF8; // default encoding
        }
        encArr = Encoding.convert(codeArr, blobEnc, 'UNICODE');
        sendData = new Uint8Array(encArr);
        blob = new Blob([sendData], { type: 'text/plain;' });
        return blob;
      },
      func: {
        // Skip to the specified character
        skipToSpecChar: function(chIndex, str, toChar) {
          for (; chIndex < str.length; chIndex++) {
            if (str.charAt(chIndex) == toChar) {
              break;
            }
          }
          return chIndex;
        },
        // Skip while the specified character appears
        skipWhileSpecChar: function(chIndex, str, toChar) {
          for (; chIndex < str.length; chIndex++) {
            if (str.charAt(chIndex) != toChar) {
              break;
            }
          }
          return chIndex;
        },
        // Back to the specified character
        backToSpecChar: function(chIndex, str, toChar) {
          for (; chIndex > 0; chIndex--) {
            if (str.charAt(chIndex) == toChar) {
              break;
            }
          }
          return chIndex;
        },
        // Extract character strings up to blank
        extractChars: function(chIndex, str) {
          var extWord = '';
          for (; chIndex < str.length; chIndex++) {
            var targetChar = str.charAt(chIndex);
            if (targetChar == ' ') {
              break;
            }
            extWord += targetChar;
          }
          var strInfo = {};
          strInfo.chIndex = chIndex;
          strInfo.word = extWord;
          return strInfo;
        }
      },
      /*
       * Call DCS apply process callback function.
       * (IN) callback : Callback function
       *      status : Callback status code
       *      proc : The processing that caused it
       *      cbArgs : Argument object for callback
       *      cbArg2 : Argument object for callback
       */
      callDCSCallback: function(callback, status, proc, cbArgs, cbArg2) {
        if ((callback != undefined) && (typeof callback === 'function')) {
          callback(status, proc, cbArgs, cbArg2);
        }
      }
    }, // -----inner
    // Functions-----
    /*
    * Get lang_suffix from system variable.
    * (IN) callback : Callback function when after setting language suffix.
    *      cbArg : Argument object for callback.
    */
    setCurrentLanguage: function(callback, cbArg) {
      top.rpcmc_getVar(top.SYSNAME_C, '$DICT_CONFIG.$LANG_SUFFIX', currentLanguageCallback, callback, cbArg);

      /* Current Language Callback */
      function currentLanguageCallback(progname, varname, typecode, value, callback, cbarg2) {
        var tempLang = '';
        tempLang = value.slice(0);
        irpapi.global.currentLang = tempLang;
        if (irpapi.global.currentLang == LANG_KN) {
          irpapi.global.currentLang = LANG_JP;
        }
        irpapi.inner.apiDebugLog('Current Language:' + irpapi.global.currentLang);
        if ((callback != undefined) && (typeof callback === 'function')) {
          callback(cbarg2);
        }
      }
    },
    /*
    * Get Current Language.
    * return : Current language (suffix).
    */
    getCurrentLanguage: function() {
      return irpapi.global.currentLang;
    },
    /*
    * Check Current Language and Character bytes
    * (IN) str          : character string
    *      selectRegExp : 0 or 1
    * (OUT)regExpRes    : true or false
    */
    checkLangAndCharBytes: function(str, selectRegExp) {
      var RegExp = (selectRegExp == 0) ? /[^\x00-\x7E]+/g : /[^\x01-\x7E]/;
      if ((irpapi.getCurrentLanguage() != LANG_JP && irpapi.getCurrentLanguage() != LANG_CH) && str.match(RegExp)) {
        return true;
      }
      return false;
    },
    /*
    * Gets the language encoding for the current language.
    * return : ex) eg:UTF-8, jp:Shift-JIS, ch:gb2312, etc...
    */
    getLangEncoding: function() {
      return LANG_INFO.langEnc[irpapi.global.currentLang];
    },
    /*
    * Gets the Blob language encoding for the current language.
    * return : ex) eg:UTF-8, jp:SJIS, ch:UTF-8, etc...
    */
    getBlobEncoding: function() {
      return LANG_INFO.blobEnc[irpapi.global.currentLang];
    },
    /*
    * Load Active Error File(ERRACT.LS)
    * (IN) callback : Callback function when file is loaded.
    */
    loadActiveErrorFile: function(callback) {
      top.loadFile('ERRACT.LS', irpapi.inner.loadErrorFileCallback, callback); // io
    },
    /*
    * Load Error History File(ERRALL.LS)
    * (IN) callback : Callback function when file is loaded.
    */
    loadErrorHistoryFile: function(callback) {
      top.loadFile('ERRALL.LS', irpapi.inner.loadErrorFileCallback, callback); // io
    },
    /*
    * (IN) callback : Callback function when program list is get.
    */
    getProgWithCmtList: function (callback) {
      // Call Interface of get program list
      var subTypeIdx = 0;
      // use attribute configuration
      var SUB_TYPE_NUM = [top.MM_SUB_UNDEF_C, top.MM_SUB_MACRO_C, top.MM_SUB_COND_C];
      irpapi.global.progWithCmtFileList = [];
      getProgWithCmtSub(SUB_TYPE_NUM[subTypeIdx]);
      function getProgWithCmtSub(subType) {
        lstProg(top.TV_PRGCMT_ALL, top.MM_MNEPRG_C, subType, prgListAddCB);
      }
      function prgListAddCB(datatype, prglist, listNum) {
        var result = makeProgWithCmtListArray(prglist);
        if ((result) && (typeof callback === 'function')) {
          callback(irpapi.global.progWithCmtFileList);
        }
      }
      function makeProgWithCmtListArray(prglist) {
        // Insert to list.
        // Preprocessing of list construction
        for (var i = 0; i < prglist.length; i++) {
          var regularName = prglist[i].split('/')[0];
          if ((regularName.length > 0) && (!regularName.startsWith('*')) && (!regularName.match(/^IPL_.*/))) {	// Not found in array
              // add regular program to list
              var progWithCmtInfo = {};
              progWithCmtInfo['name'] = regularName;
              progWithCmtInfo['cmt'] = prglist[i].split('/')[1];
              irpapi.global.progWithCmtFileList[irpapi.global.progWithCmtFileList.length] = progWithCmtInfo;
          }
        }
        subTypeIdx++;
        if (subTypeIdx < SUB_TYPE_NUM.length) {
          getProgWithCmtSub(SUB_TYPE_NUM[subTypeIdx]);
          return false;	// May be need.
        } else {
          irpapi.global.progWithCmtFileList.sort(function (one, two) {
            return (one.name < two.name) ? -1 : 1;
          });
          return true;
        }
      }
    },
    /*
    * (IN) target : Program to be selected.
    *      selMode : 0 => search by comment
    *                1 => search by Number
    *                2 => search by Program name
    *      return : found => Program name
    *               not found => false (boolean) 
    */
    searchProg: function (target, selMode) {
      var MODE = {
        CMT: 0,
        NUM: 1,
        NAME: 2
      }
      var progObj = {
        name: false,
        cmt: ''
      }
      target = String(target).replace(/\s+/g, '').toUpperCase();
      if (target.length < 1) {
        return progObj;
      }

      switch(selMode) {
        case MODE.CMT:
          for (var i = 0; i < irpapi.global.progWithCmtFileList.length; i++) {
            if (target === irpapi.global.progWithCmtFileList[i].cmt.replace(/\s+/g, '').toUpperCase()) {
              progObj = irpapi.global.progWithCmtFileList[i];
              break;
            }
          }
          break;
        case MODE.NUM:
          var num = parseInt(target);
          if (isNaN(num)) {
            var strArray = target.split("").reverse();
            if (!isNaN(strArray[0])) {
              num = strArray[0];
              for (var i = 1; i < strArray.length; i++) {
                if (!isNaN(strArray[i])) {
                  num = strArray[i] + num;
                } else {
                  break;
                }
              }
              num = parseInt(num);
            }
          }
          if ((!isNaN(num)) && (num > 0) && (num <= irpapi.global.progWithCmtFileList.length)) {
            progObj = irpapi.global.progWithCmtFileList[num - 1];
          }
          break;
        case MODE.NAME:
          for (var i = 0; i < irpapi.global.progWithCmtFileList.length; i++) {
            if (target === irpapi.global.progWithCmtFileList[i].name.replace(/\s+/g, '').toUpperCase()) {
              progObj = irpapi.global.progWithCmtFileList[i];
              break;
            }
          }
          break;
      }
      return progObj;
    },
    /*
    * Load Specified Program File(PROGRAMNAME.LS)
    * (IN) progName : Program file name.
    *      callback : Callback function when program file is loaded.
    */
    loadProgramFile: function(progName, callback) {
      top.loadFile(progName, loadProgramFileCallback, callback); // io

      /* (inner)
      * Callback (Load Program File)
      */
      function loadProgramFileCallback(status, fileName, fileText, lineCount, callback) {
        if (status != IO_SUCCESS) {
          document.body.style.cursor = 'auto';
          return;
        }
        convertTextData(fileText, lineCount, callback, fileName);
      }
      /* (inner)
      * convert Text Data
      * Since convert the program text (mainly comments) is not only by Shift-JIS, 
      * it is decoded using the FileReader function.
      * (IN) textData : Text data. (Base64 format)
      *      lineCount : Text line count.
      *      callback : Callback function when the program text is converted.
      *      callbackarg : Argument object added when calling the callback function.
      */
      function convertTextData(textData, lineCount, callback, callbackarg) {
        var bytesArray = [];
        var text = '';
        var blobData = [];
        irpapi.global.decodedData = [];
        irpapi.global.decodedCount = 0;

        if (lineCount > 0) { // If online data read, need to decode.
          for (var txtIdx = 0; txtIdx < textData.length; txtIdx++) {
            text = window.atob(textData[txtIdx].buf);
            bytesArray = [];
            for (var convIdx = 0; convIdx < text.length; convIdx++) {
              var tempCharCode = text.charCodeAt(convIdx);
              if (tempCharCode > 0x00) { // Not null character
                bytesArray[convIdx] = tempCharCode;
              }
            }
            blobData[txtIdx] = new Blob([new Uint8Array(bytesArray)]);
          }

          var reader = new FileReader();
          irpapi.global.decodedCount = 0;
          reader.addEventListener('loadend', function () {
            resultLoadFile(reader.result, irpapi.global.decodedCount, blobData, callback, callbackarg);
          });
          reader.readAsText(blobData[irpapi.global.decodedCount], irpapi.global.langEncoding[irpapi.global.currentLang]);
        } else { // For offline reading No need to decode.
          irpapi.global.decodedData = textData;
          callback(irpapi.global.decodedData, callbackarg);
        }

        /*
        * Event listener to get the decoded text with the specified encoding.
        * (IN) result : Decoded data
        *      decodedCount : Counter
        *      blobData : Binary data generated with Base64 character code data.
        *      callback : Callback function when program text is converted.
        *          arg1 : decoded data
        *          arg2 : callback argument
        *      callbackarg : Argument object added when calling the callback function.
        */
       function resultLoadFile(result, decodedCount, blobData, callback, callbackarg) {
          irpapi.global.decodedData[decodedCount] = result;
          decodedCount++;
          irpapi.global.decodedCount = decodedCount;
          if (decodedCount >= blobData.length) {
            callback(irpapi.global.decodedData, callbackarg);
          } else {
            var reader = new FileReader();
            reader.addEventListener('loadend', function () {
              resultLoadFile(reader.result, decodedCount, blobData, callback, callbackarg);
            });
            reader.readAsText(blobData[decodedCount], irpapi.global.langEncoding[irpapi.global.currentLang]);
          }
        }
      }
    },
