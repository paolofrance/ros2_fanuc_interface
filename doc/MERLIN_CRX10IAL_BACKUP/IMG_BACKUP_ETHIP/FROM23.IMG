/**
 * Exchange Position Data / Position Register.
 * Cartesian to Joint, Joint to Cartesian.
 * Usage : At first, set data to exchgInfo,
 *         2nd, call exchangePosFrame(),
 *         3rd, set converted data to table. (by callback function)
 * (IN) exchgInfo : Exchange Record Data Information. (!!Set in advance!!) (See execExchangePosFrame())
 *                  .srcRec : Source Record data.  = w2ui[tbl].get(recid);
 *                  .newRec : Target new record. (Set the default value in advance.)
 *                  .exchgTo : Convert to COORDINATECART/COORDINATEJOINT?
 *                  .cb : Callback function to receive the converted record data.
 *                  .tbl : Position Table Kind. 0=Position Data, 1=Position Register.
 *                  .grp : Group number.
 *                  .robot : Robot Axes count.
 *                  .ext : Extend Axes count.
 *                  .unit : Current group Axes unit array. [J1, J2, J3, J4, J5, J6, EXT1, EXT2, EXT3]
 *                  .UFVal : Target UF value. ex) PosReg = 'F'
 *                  .UTVal : Target UT value. ex) same as above.
 *                  .orgData :exchgInfo.orgData is for REPRE of position register.
 */
// After get the exchange position and converting it, call the callback function.
function exchangePosFrame(exchgInfo) {
  // Construct Exchange Input Position Data
  var posStr = constructExchangePosText(exchgInfo)
  // Call interface.
  top.rpcmc_mnchgrep(posStr, exchangePosCallback, exchgInfo); // rpcmc
}
function exchangePosCallback(status, posDataStr, exchgInfo) {
  // Analyze get string of exchanged data.
  var exchgRec = exchgInfo.newRec;
  if ((status == IO_SUCCESS) && (posDataStr != undefined) && (posDataStr.length > 0)) {
    if (exchgInfo.exchgTo == COORDINATECART) {
      var anlArg = {};
      anlArg.rec = exchgRec;
      anlArg.tblKind = exchgInfo.tbl;
      exchgRec = analyzeCartesianPosition(posDataStr, anlArg);
    } else if (exchgInfo.exchgTo == COORDINATEJOINT) {
      exchgRec = analyzeJointPosition(posDataStr, exchgRec);
      exchgRec.rep = POS_REP_JOINT;
      exchgRec.force = true;
    } else {
      exchgRec = null;
    }
    if (exchgRec != null) {
      // Set UF / UT value according to table kind and current value.
      if (exchgInfo.tbl == POSTBLKIND_DATA) {
        // If the UF / UT value has already been set, it use the set value.
        // Otherwise, the default value is used.
        exchgRec.UF = (exchgRec.UF !== '') ? exchgRec.UF : getUfUtSystemValue(exchgRec.GP).UF;
        exchgRec.UT = (exchgRec.UT !== '') ? exchgRec.UT : getUfUtSystemValue(exchgRec.GP).UT;
      } else {
        exchgRec.UF = POS_REG_UF_VAL;
        exchgRec.UT = POS_REG_UT_VAL;
      }
    }
  } else {
    exchgRec = null;
  }
  // Call Original callback function
  exchgInfo.cb(status, exchgRec, exchgInfo.newRec.tbl, exchgInfo.orgData);
}
