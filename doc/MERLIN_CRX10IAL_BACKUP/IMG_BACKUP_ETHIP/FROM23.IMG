/**
 * Get All Frame Value
 * (IN) type     : frame type(utool, uframe)
 *      cmt_flag : Comment flag(Whether the data to be acquired is a comment.)
 *      grp_num  : group number
 *      index    : index
 *      number   : (ex: index=1, num=3 -> get frame index {1,2,3})
 *      callback : callback function to get frame.
 */
function getFrameValue(type, cmt_flag, grp_num, index, num, callback) {
  var arg = {};
  arg.callback = callback;
  arg.type = type;
  arg.cmt_flag = cmt_flag;
  var utool_id = 1;
  var uframe_id = 3;
  // Header
  var frame_xml = '<?xml version="1.0" encoding="' + langEncoding[currentLang] + '"?>\r';
  if (cmt_flag) {
    frame_xml = frame_xml +
      '<XMLCFG>\r<PROG name="TPFDEF">\r';
    switch (type) {
      case FRAME_TYPE_UTOOL:
        for (var i = index; i < (index + num); i++) {
          frame_xml = frame_xml +
            '<VAR name="SETUP_DATA[' + grp_num + ',' + utool_id + ',' + i + '].$COMMENT" />\r';
        }
        break;
      case FRAME_TYPE_UFRAME:
        for (var i = index; i < (index + num); i++) {
          frame_xml = frame_xml +
            '<VAR name="SETUP_DATA[' + grp_num + ',' + uframe_id + ',' + i + '].$COMMENT" />\r';
        }
        break;
      default:
        break;
    }
  } else {
    frame_xml = frame_xml +
      '<XMLCFG>\r<PROG name="*SYSTEM*">\r';
    switch (type) {
      case FRAME_TYPE_UTOOL:
        for (var i = index; i < (index + num); i++) {
          frame_xml = frame_xml + '<VAR name="$MNUTOOL[' + grp_num + ',' + i + ']" />\r';
        }
        break;
      case FRAME_TYPE_UFRAME:
        for (var i = index; i < (index + num); i++) {
          frame_xml = frame_xml + '<VAR name="$MNUFRAME[' + grp_num + ',' + i + ']" />\r';
        }
        break;
      default:
        break;
    }
  }
  frame_xml = frame_xml + '</PROG>\r</XMLCFG>';

  // Create Blob frame CVR
  var file_cvr = new Blob([frame_xml], { type: "text/plain" });
  if (cmt_flag) {
    getXHR(cvrFileNameCmt, file_cvr, convertXVRCallBack, arg);
  } else {
    getXHR(cvrFileName, file_cvr, convertXVRCallBack, arg);
  }
}

