    callback(IO_COMFAILURE, [], 0);
	});
}

/* 5.2
 * Load file. 
 * (IN) file_spec : File name. (String)
 * (IN) callback : A callback function to receive the text.
 *          arg1 : status 0=success, !0=Error.
 *          arg2 : File name.
 *          arg3 : text data. (String array (Base64 encoded string))
 *          arg4 : line count. (int)
 * (IN) callbackarg : A callback uniq arugment.
 */
function loadFile(file_spec, callback, callbackarg, callbackarg2) {
  var loadFileName = 'MD:\\' + encodeASCII(file_spec);
  $.getJSON("/COMET/rpc?func=GET_RAW_FILE&file=" + loadFileName, function (json) {
    if (json.FANUC.RPC != undefined) {
      json.FANUC.RPC[0].status = json.FANUC.RPC[0].status.toLowerCase();
      var newlines = []; // add initialize
      var n_lines = 0; // add initialize
      if (json.FANUC.RPC[0].status == IO_SUCCESS) {
        newlines = json.FANUC.RPC[0].lines;
        n_lines = newlines.length;
      } else {
        top.rpcmc_rprintf("Error: GET_RAW_FILE(" + loadFileName + ")");
      }
      callback(json.FANUC.RPC[0].status, file_spec, newlines, n_lines, callbackarg, callbackarg2);
    } else {
      top.rpcmc_rprintf("[rpcmc] unexpected reply " + JSON.stringify(json));
    }
  }).fail(function (xhr, textStatus, errorThrown) {
    putIFFailLog(xhr, textStatus, errorThrown);
    callback(IO_COMFAILURE, file_spec, [], 0, callbackarg, callbackarg2);
  });
}

/**
 * Derived function of loadFile.
 * Use when there is a file that ignores the error.
 * (IN) file_spec : File name. (String)
 * (IN) bypErrStr : String to ignore errors if at the beginning of a string. (String)
 * (IN) callback : A callback function to receive the text.
 *          arg1 : status 0=success, !0=Error.
 *          arg2 : File name.
 *          arg3 : text data. (String array (Base64 encoded string))
 *          arg4 : line count. (int)
 * (IN) callbackarg : A callback uniq arugment.
 */
function loadFileBypErr(file_spec, bypErrStr, callback, callbackarg, callbackarg2) {
  var loadFileName = 'MD:\\' + encodeASCII(file_spec);
  $.getJSON("/COMET/rpc?func=GET_RAW_FILE&file=" + loadFileName, function (json) {
    if (json.FANUC.RPC != undefined) {
      json.FANUC.RPC[0].status = json.FANUC.RPC[0].status.toLowerCase();
      var newlines = []; // add initialize
      var n_lines = 0; // add initialize
      if (json.FANUC.RPC[0].status == IO_SUCCESS) {
        newlines = json.FANUC.RPC[0].lines;
        n_lines = newlines.length;
      } else {
        if (!file_spec.startsWith(bypErrStr)) top.rpcmc_rprintf("Error: GET_RAW_FILE(" + loadFileName + ")");
      }
      callback(json.FANUC.RPC[0].status, file_spec, newlines, n_lines, callbackarg, callbackarg2);
    } else {
      top.rpcmc_rprintf("[rpcmc] unexpected reply " + JSON.stringify(json));
