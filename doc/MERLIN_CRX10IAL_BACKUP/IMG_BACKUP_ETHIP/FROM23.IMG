  APP_VER: 1.16,
  PRGSLCTPOP_OPRT_FLG: '#modeOprtFlg',
  POP_BTN_ID:'#popBtn',

  startRecogn: function() {
    if (top.mainfrm.prim.micLangElem.classList.contains('opened') == true) {
      top.mainfrm.prim.micLangElem.click();
    }
    top.mainfrm.prim.micTgl.refresh(true, false);
    top.mainfrm.prim.micLangElem.refresh(top.mainfrm.prim.selectOptionList, top.mainfrm.prim.micLangElem.getValue(), null, true);
  },

  stopRecogn: function() {
    var mainMicTgl = $('iframe').contents().find('#prim').contents().find('#micTgl');
    if (mainMicTgl.length == 0) return;
    top.mainfrm.prim.micTgl.refresh(false, false);
    top.mainfrm.prim.micLangElem.refresh(top.mainfrm.prim.selectOptionList, top.mainfrm.prim.micLangElem.getValue(), null, false);
  },

  callReset: function() {
    var resetbtn_elem = $('iframe').contents().find('#prim').contents().find('#resetOprtBtn');
    voiceApi.clickCommand(resetbtn_elem, true);
  },

  //for program select popup api
  callSelect: function(searchTargetWord) {
    var popupFrameLength = document.getElementsByClassName("popup-frame").length;
    var progoprt_elem = $('iframe').contents().find('#ihcpPopupContents').contents();
    if (progoprt_elem.find(voiceApi.PRGSLCTPOP_OPRT_FLG).val() == 'true' || popupFrameLength == 0) {
      searchTargetWord = String(searchTargetWord).replace(/\s+/g, '').toUpperCase();
      var slctbtn_elem = $('iframe').contents().find('#prim').contents().find('#slctOprtBtn');
      if (!searchTargetWord && progoprt_elem.find('#progListPop').css('display') != 'block') {
        voiceApi.clickCommand(slctbtn_elem, true);
      } else {
        var prog_obj = top.irprogapi.searchProg(searchTargetWord,0);
        if (!prog_obj.name) {
          prog_obj = top.irprogapi.searchProg(searchTargetWord,1);
        }
        if (typeof prog_obj.name == 'string') {
          if (progoprt_elem.find('#progListPop').css('display') == 'block') { 
            voiceApi.callCancel();
          }
          top.checkRunState(prog_obj.name, true);
        } else {
          if (!searchTargetWord) {
            // If the searchTargetWord is blank
            voiceApi.reqSpeech(top.mainfrm.prim.getSpeechMsg(top.mainfrm.prim.MSG.SLCT_POP));
          } else {
            // If the searchTargetWord does not exist
            voiceApi.reqSpeech(top.mainfrm.prim.getSpeechMsg(top.mainfrm.prim.MSG.SLCT_ERROR));
          }
        }
      }
    } else {
      voiceApi.reqSpeech('');
    }
  },

  speechSlctRes: function(pgName) {
    var pgCmt = top.irprogapi.searchProg(pgName, 2).cmt;
    var speechingProg = (pgCmt !== '') ? pgCmt : pgName;
    if (Number(top.mainfrm.prim.micLangElem.getValue()) == top.mainfrm.prim.VOICE_LANG.JP) {
      voiceApi.reqSpeech(speechingProg + top.mainfrm.prim.getSpeechMsg(top.mainfrm.prim.MSG.SLCT_SUCCESS));//jp
    } else {
      voiceApi.reqSpeech(top.mainfrm.prim.getSpeechMsg(top.mainfrm.prim.MSG.SLCT_SUCCESS) + speechingProg);//eg
    }
  },

  reqSpeech: function(msg) {
    if (top.isTabTP()) {
      try {
        if (irprogapi.global.tabAppVersion >= voiceApi.APP_VER) {
          setTimeout(function() {
            android.ApiRequestSpeech(msg);
          }, 0);
        }
      } catch (e) {}
    }
  },

  callNumber: function(prog_number) {
    var progoprt_elem = $('iframe').contents().find('#ihcpPopupContents').contents();
    var proglistpop = progoprt_elem.find('#progListPop');
    if (progoprt_elem.find(voiceApi.PRGSLCTPOP_OPRT_FLG).val() == 'true') {
      var tr_elem = progoprt_elem.find('tr');
      if ((prog_number > 0) && ((tr_elem.length - 1) >= prog_number)) {//(tr_elem.length - 1) means number of programs.
        $(tr_elem.find('.proglistbody-nbr')).each(function() {
          if ($(this).text() == prog_number) {
            tr_elem.removeClass('ihcp-listview-selected ihcp-listview-focused');
            $(this).parent().addClass('ihcp-listview-selected ihcp-listview-focused');
            proglistpop.animate({scrollTop:tr_elem.height() * $(this).parent().index()},100);
            progoprt_elem.find('#okBtn').removeClass('disabled');
            progoprt_elem.find('#okBtn').prop('disabled',false);
            return false;
          }
        });
      } else {
        voiceApi.reqSpeech(top.mainfrm.prim.getSpeechMsg(top.mainfrm.prim.MSG.SLCT_ERROR));//not found
      }
    }
  },

  callOK: function() {
    var progoprt_elem = $('iframe').contents().find('#ihcpPopupContents').contents();
    if (progoprt_elem.find(voiceApi.PRGSLCTPOP_OPRT_FLG).val() == 'true') {
      progoprt_elem.find('#okBtn').trigger('click');
    } else if (progoprt_elem.find(voiceApi.POP_BTN_ID).val() == 'OkCancel') {
      progoprt_elem.find('#pstvBtn').trigger('click');
    }
  },

  callCancel: function() {
    var progoprt_elem = $('iframe').contents().find('#ihcpPopupContents').contents();
    if (progoprt_elem.find(voiceApi.PRGSLCTPOP_OPRT_FLG).val() == 'true') {
      progoprt_elem.find('#cancelBtn').trigger('click');
    } else if (progoprt_elem.find(voiceApi.POP_BTN_ID).val() == 'OkCancel') {
      progoprt_elem.find('#ngtvBtn').trigger('click');
    }
  },

  callNext: function() {
    var progoprt_elem = $('iframe').contents().find('#ihcpPopupContents').contents();
    var proglistpop = progoprt_elem.find('#progListPop');
    var tr_elem  =  progoprt_elem.find('tr');
    if (progoprt_elem.find(voiceApi.PRGSLCTPOP_OPRT_FLG).val() == 'true') {
      proglistpop.animate({scrollTop:proglistpop.scrollTop() + (tr_elem.height()*5)},100);
    }
  },

  callPrev: function() {
    var progoprt_elem = $('iframe').contents().find('#ihcpPopupContents').contents();
    var proglistpop = progoprt_elem.find('#progListPop');
    var tr_elem  =  progoprt_elem.find('tr');
    if (progoprt_elem.find(voiceApi.PRGSLCTPOP_OPRT_FLG).val() == 'true') {
      proglistpop.animate({scrollTop:proglistpop.scrollTop() - (tr_elem.height()*5)},100);
    }
  },

  callYes: function() {
    var progoprt_elem = $('iframe').contents().find('#ihcpPopupContents').contents();
    if (progoprt_elem.find(voiceApi.POP_BTN_ID).val() == 'YesNo') {
      progoprt_elem.find('#pstvBtn').trigger('click');
    }
  },

  callNo: function() {
    var progoprt_elem = $('iframe').contents().find('#ihcpPopupContents').contents();
    if (progoprt_elem.find(voiceApi.POP_BTN_ID).val() == 'YesNo') {
      progoprt_elem.find('#ngtvBtn').trigger('click');
    }
  },

  //for program start popup api
  callStart: function() {
    if (document.getElementsByClassName("popup-frame").length == 0) {
      var startbtn_elem = $('iframe').contents().find('#prim').contents().find('#startOprtBtn');
      top.mainfrm.prim.startPrgOprt('callStart');
      voiceApi.clickCommand(startbtn_elem, false);
    } else {
      voiceApi.reqSpeech('');
    }
  },

  callStop: function() {
    var stopbtn_elem = $('iframe').contents().find('#prim').contents().find('#stopOprtBtn');
    voiceApi.clickCommand(stopbtn_elem, true);
  },

  callAbort: function() {
    var abortbtn_elem = $('iframe').contents().find('#prim').contents().find('#abortOprtBtn');
    voiceApi.clickCommand(abortbtn_elem, true);
  },

  clickCommand: function (btnElem, isClickBtn) {
    if (isClickBtn) {
      btnElem.trigger('click');
    }
    btnElem.addClass('voiceExecution');
    setTimeout(function() {
      btnElem.removeClass('voiceExecution');
    },500);
  }
