        // Extract XML Data.
        var PMCDevData = [];
        if (xml_obj.getElementsByTagName("ARRAY").length != 0) {
          var array_elem = xml_obj.getElementsByTagName("ARRAY");
          for (var j = 0; j < array_elem.length; j++) {
            var tmpData = {};
            var field_elem = array_elem[j].getElementsByTagName("FIELD");
            for (var k = 0; k < field_elem.length; k++) {
              var fieldName = field_elem[k].getAttributeNode("name").value;
              switch (fieldName) {
                case '$OCCPY':
                  tmpData.occpy = field_elem[k].textContent;
                  break;
                case '$RACK':
                  tmpData.rack = field_elem[k].textContent;
                  break;
                case '$SLOT':
                  tmpData.slot = field_elem[k].textContent;
                  break;
                case '$IO_TYPE':
                  tmpData.ioType = field_elem[k].textContent;
                  break;
              }
            }
            if (tmpData.occpy != '0') {
              PMCDevData.push(tmpData);
            }
          }
        }
        callback(PMCDevData);
      }
    },

    getBitInfo: function(num) {
      if (num == undefined || num == 0) return [];
      if (typeof num === "string") num = parseInt(num, 10);
      var idxes = [];
      var bit = "";
      var bits = num.toString(2);
      var maxBitIdx = bits.length - 1;
      for (var i = 0; i < bits.length; i++) {
        bit = bits[maxBitIdx - i];
        idxes.push(bit === "1");
      }
      return idxes;
    },

    convNumFromFlags: function(flags) {
      if (flags == undefined) {
        return null;
      }
      var flag = "";
      var bits = "";
      for (var i = 0; i < flags.length; i++) {
        if (flags[i]) {
          flag = "1";
        } else {
          flag = "0";
        }
        bits = flag + bits;
      }
      return parseInt(bits, 2);
    },

