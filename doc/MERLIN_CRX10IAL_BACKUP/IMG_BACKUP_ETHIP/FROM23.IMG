/* ===== File access I/F ===== */

/* 17.1
 * Send specified file to MD device.
 * (IN) fileSpec : File name. (String)
 * (IN) fileData : Send File Data.
 * (IN) callback : Callback function.
 * (IN) callbackarg : Callback argument.
 */
function sendFile(fileSpec, fileData, callback, callbackarg) {
  // Ready to xhr.
  $.ajax({
    url: "/md?file=" + fileSpec,
    type: "POST",
    data: fileData,
    processData: false,
    contentType: false,
    success: function (res) {
      //console.log('file uploaded' + res);
      callback(IO_SUCCESS, callbackarg);
    },
    error: function (xmlhttprequest, textstatus, errorthrown) {
      putIFFailLog(xmlhttprequest, textstatus, errorthrown);
      callback(IO_COMFAILURE, callbackarg);
    }
  });
}

/* 17.2
 * Post XHR file.
 *   Use XMLHttpRequest.
 * (IN) fileSpec : File name.
 * (IN) fileData : File data.
 * (IN) callback : Callback function.
 * (IN) callbackarg : Callback argument.
 */
function postXHR(fileSpec, fileData, callback, callbackarg) {
  // Ready to xhr.
  var xhr = new XMLHttpRequest();
  // var parts = location.host.split(':');
  var sentFile = true;
  var postUrl = 'http://' + location.host + '/SOFTPART/XMLJSON?func=SETVAR';
  xhr.open('POST', postUrl, true);
  xhr.onload = function () {
    if (this.status == 200) {
      if (sentFile) {
        sentFile = false;
        // Send url request
        xhr.abort();
        postUrl = 'http://' + location.host + '/SOFTPART/BOGUS?file=' + fileSpec;
        xhr.open("GET", postUrl, true);
        xhr.send();
      } else {
        callback(IO_SUCCESS, callbackarg);
      }
    } else {
      console.log('file upload error: ', this.responseText);
      callback(IO_COMFAILURE, callbackarg);
    }
  };

  // Send file
  xhr.send(fileData);
}

/* 17.3
 * Get XHR file.
 *   Use XMLHttpRequest.
 * (IN) fileSpec : File name.
 * (IN) fileData : File data.
 * (IN) callback : Callback function.
 * (IN) callbackarg : Callback argument.
 */
function getXHR(fileSpec, fileData, callback, callbackarg) {
  // Prepare XHR
  var xhr = new XMLHttpRequest();
  // var parts = location.host.split(':');
  var sentFile = true;
  var postUrl = 'http://' + location.host + '/SOFTPART/XMLJSON?func=GETVAR';
  xhr.open('POST', postUrl, true);
  xhr.onload = function () {
    if (this.status == 200) {
      if (sentFile) {
        sentFile = false;
        callback(IO_SUCCESS, this.responseText, callbackarg);
        // Send url request
        xhr.abort();
        postUrl = 'http://' + location.host + '/SOFTPART/BOGUS?file=' + fileSpec;
        xhr.open("GET", postUrl, true);
        xhr.send();
      } else {
        // console.log('softpart called: ', this.responseText);
      }
    } else {
      console.log('file upload error: ', this.responseText);
      callback(IO_COMFAILURE, this.responseText, callbackarg);
    }
  };

  // Send file
  xhr.send(fileData);
}

