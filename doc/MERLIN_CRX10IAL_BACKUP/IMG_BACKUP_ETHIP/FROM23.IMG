    /*
    * Get current position data.
    * Acquire the position data of both Cartesian and Joint in one call and call the callback.
    * (IN) grp_num    : Group number
    *      callback   : A callback function when current position is got.
    *      callbackarg: callback arugment
    */
    getCurrentPosition: function(grp_num, callback, callbackarg) {
      var arg = {};
      arg.callback = getCartJointCallback;
      arg.orgcallback = callback;
      arg.callbackarg = callbackarg;
      get_curpos(top.KXYZWPR, top.TXML_TP_REC_TYPE, grp_num, currentPosRegCallback, arg); // io
      get_curang(grp_num, currentAngCallback, arg); // io

      /* Current Position Register Callback(Cartesian coordinate). */
      function currentPosRegCallback(pos_rep, pos_type, grp_num, str, arg) {
        var posdata = getPosRegData(str);
        arg.callback(posdata, arg, POS_REP_CART);
      }
      /* Current Position Register Callback(Joint coordinate). */
      function currentAngCallback(grp_num, str, arg) {
        var posdata = irpapi.getPosRegJointData(str);
        arg.callback(posdata, arg, POS_REP_JOINT);
      }
      /* Get current position data. */
      function getCartJointCallback(posdata, arg, rep) {
        if (rep == POS_REP_CART) {
          arg.cartdata = posdata;
        } else if (rep == POS_REP_JOINT) {
          arg.jointdata = posdata;
        }
        if ((rep == POS_REP_CART) || (rep == POS_REP_JOINT)) {
          if (irpapi.global.getPosFlag.indexOf(arg.callbackarg) > -1) {
            // If the flag is on, it enters the processing of getCurrentConfigCallback.
            irpapi.global.getPosFlag.splice(irpapi.global.getPosFlag.indexOf(arg.callbackarg), 1);
            arg.orgcallback(arg.cartdata, arg.jointdata, arg.callbackarg);
          } else {
            // Set flag
            irpapi.global.getPosFlag.push(arg.callbackarg);
          }
        }
      }
      function getPosRegData(str) {
        var posdata = {};
        posdata[REF_POS_HEAD[REFPOSKEY_GROUP]] = '';
        posdata[REF_POS_HEAD[REFPOSKEY_CONFIG]] = '';
        posdata[REF_POS_CART[0]] = '';
        posdata[REF_POS_CART[1]] = '';
        posdata[REF_POS_CART[2]] = '';
        posdata[REF_POS_CART[3]] = '';
        posdata[REF_POS_CART[4]] = '';
        posdata[REF_POS_CART[5]] = '';
        var innerFunc = irpapi.inner.func;
        // Position data analize.
        var target_word = ''; // Target Word
        // Repeat for the number of characters
        var index = str.indexOf(REF_POS_HEAD[REFPOSKEY_GROUP] + ":");
        for (; index < str.length; index++) {
          index = innerFunc.skipWhileSpecChar(index, str, ' '); // Skip ' '
          // Extract up to ':'
          var str_info = {};
          var ch = str.charAt(index);
          if (ch == ':') {
            index++;
            if (target_word == REF_POS_HEAD[REFPOSKEY_GROUP]) {
              index = innerFunc.skipWhileSpecChar(index, str, ' '); // Skip ' '
              str_info = innerFunc.extractChars(index, str); // Extract characters
              posdata[target_word] = parseInt(str_info.word); // Set group number
              index = str_info.chIndex; // Update index
            } else if (target_word == REF_POS_HEAD[REFPOSKEY_CONFIG]) {
              // Retrieve up to the next ':' and return from ':' to ' '.
              var idx_start = index;
              index = innerFunc.skipToSpecChar(index, str, ':');
              index = innerFunc.backToSpecChar(index - 1, str, ' ');
              // Make the extracted string to string of 'Config'
              posdata[target_word] = str.substring(idx_start, index).trim();
            } else { // X to R
              index = innerFunc.skipWhileSpecChar(index, str, ' '); // Skip ' '
              str_info = innerFunc.extractChars(index, str); // Extract characters
              posdata[target_word] = parseFloat(str_info.word);
              index = str_info.chIndex;
            }
            target_word = '';
          } else {
            target_word += ch;
          }
        }
        return posdata;
      }
    },
    getPosRegJointData: function(str) {
      var posdata = {};
      posdata[REF_POS_HEAD[REFPOSKEY_GROUP]] = '';
      posdata[REF_POS_JOINT[0]] = '';
      posdata[REF_POS_JOINT[1]] = '';
      posdata[REF_POS_JOINT[2]] = '';
      posdata[REF_POS_JOINT[3]] = '';
      posdata[REF_POS_JOINT[4]] = '';
      posdata[REF_POS_JOINT[5]] = '';
      posdata[REF_POS_EXT[0]] = '';
      posdata[REF_POS_EXT[1]] = '';
      posdata[REF_POS_EXT[2]] = '';
      var innerFunc = irpapi.inner.func;
      // Position data analize.
      // Repeat for the number of characters
      var index = str.indexOf(REF_POS_HEAD[REFPOSKEY_GROUP] + ":");
      index = innerFunc.skipToSpecChar(index, str, ' ') + 1;
      var str_info = {};
      str_info = innerFunc.extractChars(index, str); // Extract characters
      posdata[REF_POS_HEAD[REFPOSKEY_GROUP]] = parseInt(str_info.word); // Set group number
      index = str_info.chIndex + 1;
      for (; index < str.length; index++) {
        index = innerFunc.skipToSpecChar(index, str, ' ') + 1; // Skip to '='
        index = innerFunc.skipWhileSpecChar(index, str, ' '); // Skip to value
        str_info = innerFunc.extractChars(index, str); // Extract characters
        if (str_info.word.indexOf(':') != -1) {
          str_info.word = str_info.word.replace(/:/g, "");
        }
        var target_key = str_info.word.trim();
        if (target_key.length == 0) {
          continue; // Illegal keyword
        }
        index = str_info.chIndex;
        index = innerFunc.skipWhileSpecChar(index, str, ' '); // Skip to value
        index = innerFunc.skipWhileSpecChar(index, str, '='); // Skip to value
        index = innerFunc.skipWhileSpecChar(index, str, ' '); // Skip to value
        str_info = innerFunc.extractChars(index, str);
        posdata[target_key] = parseFloat(str_info.word);
        index = str_info.chIndex + 1;
        index = innerFunc.skipToSpecChar(index, str, ' ') + 1; // Skip unit
      }
      return posdata;
    },
    /*
    * Get String Register Value XVR
    * (IN) index  : register number (1, 2, ...).
    *      num : (ex: index=1, num=3 -> get strreg index {1,2,3})
    *      callback : callback function to get position.
    */
    getStrRegValueXVR: function (index, num, callback) {
      var reg_cvr =
        '<?xml version="1.0" encoding="' + irpapi.global.langEncoding[irpapi.global.currentLang] + '"?>\r' +
        '<XMLCFG>\r<PROG name="*STRREG*">\r';
      for (var i = 0; i < num; i++) {
        reg_cvr = reg_cvr + '<VAR name="$STRREG[' + (i + index) + ']" />\r';
      }
      reg_cvr = reg_cvr + '</PROG>\r</XMLCFG>';
    
      // Create strreg.cvr
      var file_cvr = new Blob([reg_cvr], { type: "text/plain" });
      getXHR(irpapi.global.cvrFileName, file_cvr, convertRegXVRCallBack, callback);
    
      // Analyze num data after convert XVR CallBack
      function convertRegXVRCallBack(status, reg_xvr, callback) {
        var xml_obj = null;
        var dom_parser = new DOMParser();
        try {
          xml_obj = dom_parser.parseFromString(reg_xvr, 'text/xml');
        } catch (e) {
          irpapi.inner.apiErrorLog(e);
        }
        if ((xml_obj == null) || (xml_obj.getElementsByTagName('parsererror').length)) {
          irpapi.inner.apiErrorLog('convertStrRegXVRCallBack: xml error.');
        }
        // Analyze XML Data.
        if (xml_obj.getElementsByTagName("ARRAY").length != 0) {
          var arrayElem = xml_obj.getElementsByTagName('ARRAY');
          var regData = {};
          for (var j = 0; j < arrayElem.length; j++) {
            var textData = arrayElem[j].textContent;

            var strRegNum = {};
            var strRegNum = arrayElem[j].getAttributeNode("name").value.replace("$STRREG[", "").replace("]", "");

            var setRegCmt = (textData.match(/\'.+?\'/))
            setRegCmt = setRegCmt == null ? "" : setRegCmt[0].replace(/'/g,"");
            textData = textData.replace(setRegCmt, "");
            var strRegVal = textData.substring(textData.indexOf(":") + 1)
            var inputData = {
              value: strRegVal == null ? "" : strRegVal,
              comment: setRegCmt == null ? "" : setRegCmt,
            };

            regData[strRegNum] = inputData;
          }
        }
        callback(status, regData);
      }
    },
    
