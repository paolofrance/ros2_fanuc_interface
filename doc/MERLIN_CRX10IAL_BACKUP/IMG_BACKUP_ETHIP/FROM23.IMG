// Convert to ASCII code. for IE.
function encodeASCII(str) {
  if(top.g_msie) {
    var codeArr = [];
    var encodeStr = str;
    var detectStr = top.Encoding.detect(encodeStr);
    if (detectStr != "UNICODE" && detectStr != "ASCII") {
      for (var i = 0; i < encodeStr.length; i++) {
        codeArr.push(encodeStr.charCodeAt(i));
      }
      var encArr = top.Encoding.convert(codeArr, 'ASCII', detectStr);
      encodeStr = top.Encoding.urlEncode(encArr);
    }
    return encodeStr;
  } else {
    return str;  
  }
}

