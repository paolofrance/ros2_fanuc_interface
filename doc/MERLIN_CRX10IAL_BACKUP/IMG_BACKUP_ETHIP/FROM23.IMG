      }
      // Position data
      if (write_data_list[i].rep == POS_REP_CART) {
        posreg_xml = posreg_xml +
          '  X:   ' + parseFloat(write_data_list[i].X).toFixed(6) + '   Y:     ' + parseFloat(write_data_list[i].Y).toFixed(6) + '   Z:   ' + parseFloat(write_data_list[i].Z).toFixed(6) + '\r' +
          '  W:   ' + parseFloat(write_data_list[i].W).toFixed(6) + '   P:     ' + parseFloat(write_data_list[i].P).toFixed(6) + '   R:   ' + parseFloat(write_data_list[i].R).toFixed(6) + '\r';
      } else if (write_data_list[i].rep == POS_REP_BOTH) {
        var format_axis = "";
        posreg_xml = posreg_xml +
          '  X:   ' + parseFloat(write_data_list[i].X).toFixed(6) + '   Y:     ' + parseFloat(write_data_list[i].Y).toFixed(6) + '   Z:   ' + parseFloat(write_data_list[i].Z).toFixed(6) + '\r' +
          '  W:   ' + parseFloat(write_data_list[i].W).toFixed(6) + '   P:     ' + parseFloat(write_data_list[i].P).toFixed(6) + '   R:   ' + parseFloat(write_data_list[i].R).toFixed(6) + '\r';
        for (var j = 0; j < write_data_list[i].axes; j++) {
          var joint = "EXT" + (j + 1);
          format_axis = format_axis + joint + ": " + ("         " + parseFloat(write_data_list[i][joint].data).toFixed(6)).slice(-12) + " " + write_data_list[i][joint].unit + " ";
        }
        if (format_axis != "") {
          posreg_xml = posreg_xml + '  ' + format_axis + '\r';
        }
      } else if (write_data_list[i].rep == POS_REP_JOINT) {
        var format_axis = "";
        for (var j = 0; j < write_data_list[i].axes; j++) {
          var joint;
          if (j < write_data_list[i].rob_axes) {
            //Jxx
            joint = "J" + (j + 1);
            format_axis = format_axis + joint + " = " + ("         " + parseFloat(write_data_list[i][joint].data).toFixed(6)).slice(-12) + " " + write_data_list[i][joint].unit + " ";
          } else {
            //EXTxx
            joint = "EXT" + (j - write_data_list[i].rob_axes + 1);
            format_axis = format_axis + joint + ": " + ("         " + parseFloat(write_data_list[i][joint].data).toFixed(6)).slice(-12) + " " + write_data_list[i][joint].unit + " ";
          }
          if (((j + 1) % 3) == 0) {
            posreg_xml = posreg_xml + '  ' + format_axis + '\r';
            format_axis = "";
          }
        }
        if (format_axis != "") {
          posreg_xml = posreg_xml + '  ' + format_axis + '\r';
        }
      }
      // Footer
      posreg_xml = posreg_xml +
        '  </ARRAY>\r';
    }
  }
  // Footer
  posreg_xml = posreg_xml +
    ' </PROG>\r' +
    '</XMLVAR>';
  // Create File PosRegCVR
  var file_xvr = createBlob(posreg_xml);
  postXHR(xvrFileName, file_xvr, callback);
  var reader = new FileReader();
  reader.addEventListener("loadend", function () {});
  reader.readAsArrayBuffer(file_xvr);
}

/**
 * Create Blob for sending data.
 * (IN) str  : String type data.
 * return : Blob object.
*/
function createBlob(str) {
  var blob;
  var codeArr = [];
  for (var i = 0; i < str.length; i++) {
    codeArr.push(str.charCodeAt(i));
  }
  var sendData = codeArr;
  var encArr = '';
  var blobEnc = getBlobEncoding();
  if (blobEnc === undefined) {
    console.log('createBlob: Illegal current lang.(' + currentLang + ')');
    blobEnc = ENC_UTF8; // default encoding
  }
  encArr = Encoding.convert(codeArr, blobEnc, 'UNICODE');
  sendData = new Uint8Array(encArr);
  blob = new Blob([sendData], { type: "text/plain;" });
  return blob;
}

