/* 1.14
 * Change the tp mode.
 * (IN) code     : Source program name
 * (IN) mode     : Destination program name 
 * (IN) check    : Destination program name
 * (IN) callback : A callback function to change the tp mode.
 *          arg1 : Response code.
 *          arg2 : Status code.
 *          arg3 : A result to change the tp mode.
 */
function chgTpMode(code, mode, check, callback) {
  $.getJSON("/COMET/rpc?func=TPMODE_CHG&code=" + code + "&mode=" + mode + "&check=" + check, function (json) {
    if (json.FANUC.RPC != undefined) {
      json.FANUC.RPC[0].status = json.FANUC.RPC[0].status.toLowerCase();
      if (callback != undefined) { 
        callback(json.FANUC.RPC[0].rpc, json.FANUC.RPC[0].status, json.FANUC.RPC[0].result);
      }
    } else {
      rpcmc_rprintf("[rpcmc] unexpected reply " + JSON.stringify(json));
    }
  });
}

