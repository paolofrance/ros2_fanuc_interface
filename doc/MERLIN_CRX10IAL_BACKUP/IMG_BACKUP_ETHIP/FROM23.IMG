    /*
     * Get specified group Num of axis and num of robot axes.
     * (IN) grp : Group number
     *      callback : A callback function to get axes
     */
    getNumAxes: function(grp, callback) {
      var arg = {};
      arg.grp = grp;
      arg.callback = callback;
      var sysvar = '$SCR_GRP[' + grp + '].$NUM_AXES';
      top.rpcmc_getVar(top.SYSNAME_C, sysvar, getNumAllAxesCallback, arg);
    
      /* Get specified group Num of axis callback. */
      function getNumAllAxesCallback(prog, sysvar, type, val, callbackarg) {
        var arg = callbackarg;
        arg.numAxes = Number(val);
        sysvar = '$SCR_GRP[' + arg.grp + '].$NUM_ROB_AXS';
        top.rpcmc_getVar(top.SYSNAME_C, sysvar, getNumRobotAxesCallback, arg);
      }
      /* Get specified group Num of axis and num of robot axes. */
      function getNumRobotAxesCallback(prog, sysvar, type, val, callbackarg) {
        var numRobotAxes = Number(val);
        var numExtAxes = callbackarg.numAxes - numRobotAxes;
        callbackarg.callback(callbackarg.grp, numRobotAxes, numExtAxes);
      }
    },
    /*
     * Set Frame Maximum Number(UTool, UFrame)
     * (IN) callback function when data received
     */
    setFrameMaxNum: function(callback) {
      top.rpcmc_getVar(top.SYSNAME_C, '$SCR.$MAXNUMUTOOL', getframeMaxNumCallback, callback);
      top.rpcmc_getVar(top.SYSNAME_C, '$SCR.$MAXNUMUFRAM', getframeMaxNumCallback, callback);
    
      /* Callback(Set Frame Maximum Number) */
      function getframeMaxNumCallback(progname, varname, typecode, value, callback) {
        if (varname == '$SCR.$MAXNUMUTOOL') {
          irpapi.global.utoolMaxNum = Number(value.slice(0));
          irpapi.inner.apiDebugLog('UTool Max Num:' + irpapi.global.utoolMaxNum);
        } else {
          irpapi.global.uframeMaxNum = Number(value.slice(0));
          irpapi.inner.apiDebugLog('UFrame Max Num:' + irpapi.global.uframeMaxNum);
        }
        if ((irpapi.global.utoolMaxNum != null) && (irpapi.global.uframeMaxNum != null)) {
          callback();
