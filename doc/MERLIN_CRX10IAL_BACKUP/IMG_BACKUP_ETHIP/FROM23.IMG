    /* [Add]
    * Get all group robot axes data.
    */
    getAllAxisData: function() {
      var axisValArr = [];
      var defHeader = '$SCR_GRP[';
      var refAxisVar = irpapi.global.axisData;

      for (var i = 0; i < irpapi.getGroupCount(); i++){ // for all group (Need total number of groups)
        axisValArr.push(defHeader + (i + 1) + '].$NUM_AXES');
        axisValArr.push(defHeader + (i + 1) + '].$NUM_ROB_AXS');
      }
      getSysValXHR(axisValArr, getAllAxsCB);

      // get $NUM_ROB_AXS and $NUM_AXES callback
      function getAllAxsCB(status, getText) {
        var xmlObj = getSysValXmlData(getText, 'VAR');
        if (xmlObj == null) { return; } // Add error messages if needed.

        // Setting the variables of axis count that have been retrieved.
        var dataArr = xmlObj.getElementsByTagName('VAR');
        for (var i = 0; i < dataArr.length; i++) {
          var valName = $(dataArr[i]).attr('name');
          var dataKey = ((valName.indexOf('ROB') > -1) ? 'robot' : 'total');
          refAxisVar[dataKey][valName.match(/[0-9]+/g).join('')] = parseInt($(dataArr[i]).text());
        }
        getAllAxisUnit();
      }

      // get $ROTARY_AXS
      function getAllAxisUnit() {
        var rotaryValArr = [];
        for (var k = 0; k < irpapi.getGroupCount(); k++){
          refAxisVar.ext[(k + 1)] = refAxisVar.total[(k + 1)] - refAxisVar.robot[(k + 1)]; // extra axis
          for (var j = 0; j < REF_POS_AXIS.length; j++) { // 9
            rotaryValArr.push(defHeader + (k + 1) + '].$ROTARY_AXS[' + (j + 1) + ']');
          }
        }
        getSysValXHR(rotaryValArr, getAxesUnitCallback);
      }

      // get $ROTARY_AXS callback
      function getAxesUnitCallback(status, getText, cbArgs) {
        var xmlObj = getSysValXmlData(getText, 'ARRAY');
        if (xmlObj == null) { return; } // Add error messages if needed.

        // Setting the variables of axis unit that have been retrieved.
        var dataArr = xmlObj.getElementsByTagName("ARRAY");
        for (var i = 0; i < dataArr.length; i++) { // Format the data and record the units of position
          var valGrp = Number($(dataArr[i]).attr('name').slice(defHeader.length, defHeader.length + 1)); // group number
          var valName = $(dataArr[i]).attr('name').replace(defHeader + valGrp + '].', '').match(/[0-9]+/g).join(''); // for index
          refAxisVar.unit[valGrp] = refAxisVar.unit[valGrp] || []; // init
          refAxisVar.unit[valGrp][Number(valName) - 1] = (($(dataArr[i]).text().toLowerCase() == 'true') ? 'deg' : 'mm');
        }

        // Ties the name of a valid axis to its unit.
        for (var k = 1; k <= irpapi.getGroupCount(); k++){
          refAxisVar.valid[k] = { 'cart': {}, 'joint': {} };
          refAxisVar.valid[k].cart.axis = [].concat(REF_POS_CART).concat(REF_POS_EXT.slice(0, refAxisVar.ext[k]));
          refAxisVar.valid[k].cart.unit = ['mm', 'mm', 'mm', 'deg', 'deg', 'deg'].concat(refAxisVar.unit[k].slice(refAxisVar.robot[k], refAxisVar.total[k]));
          refAxisVar.valid[k].joint.axis = [].concat(REF_POS_JOINT.slice(0, refAxisVar.robot[k])).concat(REF_POS_EXT.slice(0, refAxisVar.ext[k]));
          refAxisVar.valid[k].joint.unit = refAxisVar.unit[k].slice(0, refAxisVar.total[k]);
        }
      }

      // Create Blob frame CVR
      function getSysValXHR(sysValArr, callback, arg) {
        var frameXml = "";
        frameXml += '<?xml version="1.0" encoding="UTF-8"?>\r';
        frameXml += '<XMLCFG>\r<PROG name="*SYSTEM*">\r';
        frameXml += '<VAR name="' + sysValArr.join('" />\r<VAR name="') + '" />\r';
        frameXml += '</PROG>\r</XMLCFG>';

        var fileCvr = new Blob([frameXml], { type: 'text/plain' });
        top.getXHR('apiSysVal.cvr', fileCvr, callback, arg);
      }

      // parse xml
      function getSysValXmlData(getText, tagName) {
        var xmlObj = null;
        var dom = new DOMParser();
        try {
          xmlObj = dom.parseFromString(getText, 'text/xml');
        } catch (e) {
          alert('getXML err');
        }
        if (xmlObj != null) { // success get xml data.
          if(xmlObj.getElementsByTagName('parsererror').length > 0) {
            alert('xml parse error');
          }
          if(xmlObj.getElementsByTagName(tagName).length == 0) {
            xmlObj = null; // Illegal XML data.
          }
        }
        return xmlObj;
      }
