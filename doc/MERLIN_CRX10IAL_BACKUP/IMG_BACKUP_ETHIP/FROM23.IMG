  } else if (codeList.iocnct.length > 0) {
    codeSpecifiedStr = 'new_code3=' + codeList.iocnct;
  }
  $.getJSON("/COMET/rpc?func=DCS_CODE_CHANGE&" + codeSpecifiedStr + "&curr_code=" + currentCode,
    function (json) {
      if (json.FANUC.RPC != undefined) {
        var rpcStatus = json.FANUC.RPC[0].status;
        if (rpcStatus != IO_SUCCESS) {
          top.rpcmc_rprintf('Error: DCS_CODE_CHANGE status(' + rpcStatus + ')');
