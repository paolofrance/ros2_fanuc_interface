/* === Dialog definition === */
/*
 = This table  : Definition data for dialog construction.
 = Resource ID : ID when referring to the language resource table.
 = Append : before=append before msgForm, after=append after msgForm.
 = Button : type ok = Positive choice. ng = Negative choice. cancel = Cancel.
 =          id   Button id.
 =          face Yes = YES(Hai), No = NO(Iie), Cancel = Cancel
 =          call Callback function when each button is selected.
*/
var dialogDefinitionTable = [{
    id: 'yesnodlg',
    title: 'dlgttlconf',
    titleRes: 'dlgTtlConf',
    dlgTextId: 'pgdelete',
    textRes: 'dlgText',
    append: 'no',
    msgForm: '',
    formValue: '',
    btnDef: [
        { type: 'yes', id: 'btnyes', face: 'dlgBtnYes' },
        { type: 'no', id: 'btnno', face: 'dlgBtnNo' }]
    },{
    id: 'nobtndlg',
    title: 'dlgttlconf',
    titleRes: 'dlgTtlConf',
    dlgTextId: 'pgdelete',
    textRes: 'dlgText',
    append: 'no',
    msgForm: '',
    formValue: '',
    btnDef: []
}, {
    id: 'continuedlg',
    title: 'dlgttlconf',
    titleRes: 'dlgTtlConf',
    dlgTextId: 'pgdelete',
    textRes: 'dlgText',
    append: 'no',
    msgForm: '',
    formValue: '',
    btnDef: [
        { type: 'no', id: 'btnno', face: 'dlgBtnCancel' },
        { type: 'yes', id: 'btnyes', face: 'dlgBtnContinue' }
    ]
}
];

/**
 * Execute specified callback function.
 **/
function executeProgramMethod(funcName, args) {
    if ((funcName != null) && (funcName.length > 0) &&
        (execProgramTable[funcName] !== undefined)) {
        execProgramTable[funcName](args);
    }
}

/*
 * Save tempolary editting flag. 
 * Resume when Save eq No and Create Prog eq cancel.
 */
var tempEditPrg = false;
var tempEditPos = false;

function resumePrgPosModify() {
    setProgramModified(tempEditPrg);
    setPositionModified(tempEditPos);
}

/* === Dialog configure === */
function configDialog(dlgId, dlgTextData) {
    // Search dialog define
    var dlgDef = null;
    for (var i = 0; i < dialogDefinitionTable.length; i++) {
        dlgDef = dialogDefinitionTable[i];
        var idStr = '#' + dlgDef.id;
        if (idStr === dlgId) { // target dialog?
            break;
        }
    }
    if (dlgDef == null) {
        debug(DBGERROR, "Illega Dialog ID:" + dlgId);
        return;
    }
    // Make dialog
    var dlgConf = $('<div>', { id: dlgDef.id, 'class': 'modaldlg' });
    var dlgTtl = $('<div>', { id: dlgDef.title, 'class': 'dlgtitlebar' });
    var ttlArea = $('<label>', { 'class': 'dlgTtlText'});
    ttlArea.html(resources[dlgDef.titleRes]);
    dlgTtl.append(ttlArea);
    var ttlCancel = $('<button>', { id: 'dlgTtlCancel', 'class': 'dlgttlbarbtn', 'style': 'background-image: url(/frh/jcgtp/closeicon.png);' });
    dlgTtl.append(ttlCancel);
    dlgConf.append(dlgTtl);
    var dlgMsg = $('<p>');
    dlgMsg.attr('id', dlgDef.dlgTextId);

    dlgMsg.addClass('dlgcontent');

    if ((dlgDef.msgClass != undefined) && (dlgDef.msgClass.length > 0)) {
        dlgMsg.addClass(dlgDef.msgClass);
    }
    var innerMsg = ""; //resources[dlgDef.message];
    if (dlgDef.append != 'after') {
        innerMsg = (dlgTextData.length > 0) ? dlgTextData : resources[dlgDef.textRes];
    }

    if ((dlgDef.msgForm != undefined) && (dlgDef.msgForm.length > 0)) {
        innerMsg += dlgDef.msgForm;
    }
    if (dlgDef.append == 'after') {
        innerMsg = (dlgTextData.length > 0) ? dlgTextData : resources[dlgDef.textRes];
    }

    dlgMsg.html(innerMsg);
    dlgConf.append(dlgMsg);
    var dlgBtn = $('<div align="center" style="clear: both;">');
    var btnDefText = '';
    for (var j = 0; j < dlgDef.btnDef.length; j++) { // Yes/No/OK/Cancel
        var btnClass = "dlgbutton";
        btnClass += ((dlgDef.btnDef[j].type == "ok") || (dlgDef.btnDef[j].type == "yes")) ? ((dlgDef.btnDef[j].disabled) ? "" : " okyes") : " nocancel";
        btnClass += " btn" + dlgDef.btnDef[j].type;
        var btnObj = $('<button>', { type: 'button', id: dlgDef.btnDef[j].id, 'class': btnClass, value: resources['dlgBtnOk'] });
        if (dlgDef.btnDef[j].disabled) {
            btnObj.prop('disabled', true);
        }
        btnObj.html(resources[dlgDef.btnDef[j].face]);
        dlgBtn.append(btnObj);
        if (dlgDef.btnDef[j].call != undefined) {
