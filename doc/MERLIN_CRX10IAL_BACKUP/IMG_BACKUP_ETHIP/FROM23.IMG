/**
 * Set Sysvar for Reference posisions.
 * (IN) grp_num : group number.
 *      write_data_list : write data list array.
 *      callback : callback function to get position.
 */
function setRefPosValue(grp_num, write_data_list, callback) {
  // Create Write RefPos Data
  var refpos_xml;
  // Header
  refpos_xml = '<?xml version="1.0" encoding="' + langEncoding[currentLang] + '"?>\r' + '<XMLVAR>\r';
  refpos_xml += ' <PROG name="*SYSTEM*">\r' + '  <VAR name="$REFPOS' + grp_num + '">\r';

  for (var i = 0; i < write_data_list.length; i++) {
    var field_str = [];
    var head_arr = '    <ARRAY name = "$REFPOS' + grp_num + '[' + write_data_list[i].record[COLUMN_INDEX].recVal + ']';
    var head_var = '      <FIELD name="';

    for (var k = 0; k <= REF_POS_VAR.length; k++) {
      switch (k) {
        case COLUMN_INDEX:
          field_str.push('    <ARRAY name="$REFPOS' + grp_num + '[' + write_data_list[i].record[COLUMN_INDEX].recVal + ']">\r');
          break;
        case COLUMN_AXIS1:
        case COLUMN_RANGE1:
          var write_arr = '';
          var axis_name = ((k == COLUMN_AXIS1) ? "axis" : "range");
          for (var j = 1; j <= REF_POS_AXIS.length; j++) {
            var unit_val = ((write_data_list[i]["unit" + j] == 'TRUE') ? (Math.PI / 180) : 1);
            var axis_num = exponentialAxis(parseFloat(write_data_list[i].record[REF_POS_CLM_ARR.indexOf(axis_name + j)].recVal * unit_val).toExponential(9));
            write_arr += '    ' + head_arr + '.' + REF_POS_VAR[k - 1] + '[' + j + ']" prot ="RW">';
            write_arr += axis_num + '</ARRAY>\r';
          }
          field_str.push(write_arr);
          break;
        case COLUMN_COMMENT:
          if ((write_data_list[i].force) || (write_data_list[i].record[COLUMN_COMMENT].recVal === "")) {
            var write_cmt = '********</FIELD>\r';
          } else {
            var write_cmt = write_data_list[i].record[COLUMN_COMMENT].recVal + '</FIELD>\r';
          }
          field_str.push(head_var + REF_POS_VAR[k - 1] + '" prot ="RW" force ="TRUE">' + write_cmt);
          break;
        case COLUMN_INRANGE:
        case COLUMN_ENABLE:
        case COLUMN_HOME:
        case COLUMN_SIGNAL_IO:
        case COLUMN_SIGNAL_IO_NUM:
          var write_var = write_data_list[i].record[k].recVal + '</FIELD>\r';
          field_str.push(head_var + REF_POS_VAR[k - 1] + '" prot ="RW">' + write_var);
          break;
        default:
          break;
      }
    }
    refpos_xml += field_str[COLUMN_INDEX] + field_str[COLUMN_COMMENT] + field_str[COLUMN_ENABLE];
    refpos_xml += field_str[COLUMN_INRANGE] + field_str[COLUMN_SIGNAL_IO] + field_str[COLUMN_SIGNAL_IO_NUM];
    refpos_xml += '      <FIELD name="$PERCHPOS">\r' + field_str[COLUMN_AXIS1] + '      </FIELD>\r';
    refpos_xml += '      <FIELD name="$PERCHTOL">\r' + field_str[COLUMN_RANGE1] + '      </FIELD>\r';
    refpos_xml += field_str[COLUMN_HOME] + '    </ARRAY>\r';
  }
  // Footer
  refpos_xml = refpos_xml + '  </VAR>\r' + ' </PROG>\r' + '</XMLVAR>';
  // Create File PosRegCVR
  var file_xvr = createBlob(refpos_xml);
  postXHR(xvrFileName, file_xvr, callback);
  var reader = new FileReader();
  reader.addEventListener("loadend", function () {});
  reader.readAsArrayBuffer(file_xvr);
}

function exponentialAxis(num) {
  var axis_str = String(num)
  if (axis_str.indexOf("+") > 0) {
    var index = axis_str.indexOf("+");
  } else if (axis_str.indexOf("-") > 1) {
    var index = axis_str.indexOf("-");
  } else {
    return num;
  }
  var front_str = axis_str.substring(0, index + 1);
  var back_str = axis_str.slice(index + 1);
  if (back_str.length < 2) {
    back_str = "0" + axis_str.slice(index + 1);
  }
  front_str += back_str;
  return front_str;
}

