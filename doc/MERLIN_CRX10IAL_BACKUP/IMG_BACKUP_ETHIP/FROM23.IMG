    */
    setMachinelockValue: function(machinelock_obj, callback) {
      // Create Write Test Mode Value.
      var machinelock_xml;
      // Header
      machinelock_xml = '<?xml version="1.0" encoding="' + irpapi.global.langEncoding[irpapi.global.currentLang] + '"?>\r' + '<XMLVAR>\r';
      machinelock_xml += ' <PROG name="*SYSTEM*">\r';

      // MACHINELOCK value setting.
      for (var i = 1; i <= irpapi.getGroupCount(); i ++){
        machinelock_xml += '  <VAR name="$MCR_GRP['+ i +'].$MACHINELOCK" prot ="RW">' + machinelock_obj['chkGrp' + i] + '</VAR>\r';
      }

      //Footer
      machinelock_xml += ' </PROG>\r' + '</XMLVAR>';

      // Callback Setting
      var callbackVar;
      if(typeof callback === 'function'){
        callbackVar = callback;
      } else {
        callbackVar = function (){};
      }

      // Create File MACHINELOCK value.
      var file_xvr = irpapi.inner.createBlob(machinelock_xml);
      postXHR("pnlml.xvr", file_xvr, callbackVar);
      var reader = new FileReader();
      reader.addEventListener('loadend', function () {});
      reader.readAsArrayBuffer(file_xvr);
    },

    /* 
    * Function to get MACHINELOCK value.
    * (IN)  .callback : Callback function
    */
    getMachinelockValue: function(callback) {
      // Header
      var machinelock_xml = '<?xml version="1.0" encoding="' + irpapi.global.langEncoding[irpapi.global.currentLang] + '"?>\r';
      machinelock_xml = machinelock_xml + '<XMLCFG>\r<PROG name="*SYSTEM*">\r';
      for (var i = 1; i <= irpapi.getGroupCount(); i++){
        machinelock_xml = machinelock_xml + '<VAR name="$MCR_GRP[' + i + '].$MACHINELOCK" />\r';
      }
      machinelock_xml = machinelock_xml + '</PROG>\r</XMLCFG>';

      // Create CVR.
      var file_cvr = new Blob([machinelock_xml], { type: 'text/plain' });
      getXHR('pnlml.cvr', file_cvr, convertMachinelock, callback);

      function convertMachinelock(status, getText, callback) {
        var str_xml = getText;
        var filelines = getText.split(CODELF);
        var xml_obj = null;
        var dom_parser = new DOMParser();
        irpapi.global.xmlErrorLog.flg = false;
        try {
          xml_obj = dom_parser.parseFromString(str_xml, 'text/xml');
        } catch (e) {
          irpapi.inner.apiErrorLog(e);
        }
        if ((xml_obj == null) || (xml_obj.getElementsByTagName('parsererror').length)) {
          str_xml = irpapi.inner.delErrData(str_xml, filelines);
          xml_obj = dom_parser.parseFromString(str_xml, 'text/xml');
          irpapi.global.xmlErrorLog.flg = true;
          irpapi.inner.apiErrorLog('convertMachinelock: xml error.');
        }
        // Success Get XML Data.
        var varElem = xml_obj.getElementsByTagName('VAR');
        var valList = [];
        for (var l = 0; l < varElem.length; l++) {
          valList.push(varElem[l].childNodes[0].data);
        }
        callback(valList);
      }
    },
