    /*
    * Get All refpos Value
    * (IN) cmt_flag: Comment flag(Whether the data to be acquired is a comment.) 
    *      grp_num: group number
    *      index: index (Normally 1)
    *      number   : total number of data
    *      callback : callback function to get frame.
    */
    getRefposValue: function(cmt_flag, grp_num, callback) {
      var arg = {};
      arg.type = REF_TYPE_POS;
      arg.grp = grp_num;
      arg.callback = callback;
      arg.cmt_flag = cmt_flag;

      // Header
      var frame_xml = '<?xml version="1.0" encoding="' + irpapi.global.langEncoding[irpapi.global.currentLang] + '"?>\r';
      frame_xml = frame_xml + '<XMLCFG>\r<PROG name="*SYSTEM*">\r<VAR name="$REFPOS' + grp_num + '" />\r';
      frame_xml = frame_xml + '</PROG>\r</XMLCFG>';

      // Create Blob frame CVR
      var file_cvr = new Blob([frame_xml], {
        type: 'text/plain'
      });
      getXHR(irpapi.global.cvrFileName, file_cvr, convertRefXVRCallBack, arg);  // io

      /* (inner)
      * Analyze refposition data after convert XVR CallBack
      * (IN) callback : callback function to get position.
      */
      function convertRefXVRCallBack(status, getText, callbackarg) {
        var str_xml = getText;
        var filelines = getText.split(CODELF);
        var xml_obj = null;
        var dom_parser = new DOMParser();
        var xmlErrorFlg = false;
        try {
          xml_obj = dom_parser.parseFromString(str_xml, 'text/xml');
        } catch (e) {
          irpapi.inner.apiErrorLog(e);
        }
        if ((xml_obj == null) || (xml_obj.getElementsByTagName('parsererror').length)) {
          str_xml = irpapi.inner.delErrData(str_xml, filelines);
          xml_obj = dom_parser.parseFromString(str_xml, 'text/xml');
          xmlErrorFlg = true;
          irpapi.inner.apiErrorLog('convertRefXVRCallBack: xml error.');
        }
        irpapi.global.xmlErrorLog.flg = xmlErrorFlg;
        // Success Get XML Data.
        if ((xml_obj.getElementsByTagName('ARRAY').length != 0) &&
            (xml_obj.getElementsByTagName('FIELD').length != 0)) {

          var perch_elem = xml_obj.getElementsByTagName('ARRAY');
          var str_elem = xml_obj.getElementsByTagName('FIELD');
          var var_elem = xml_obj.getElementsByTagName('VAR');
          var grp_num = var_elem[0].getAttributeNode('name').value.replace(/[^0-9]/g, '');
          var data_max_num = perch_elem[perch_elem.length - 1].getAttributeNode('name').value.match(/\[.+?\]/);
          data_max_num = data_max_num[0].slice(1, -1);
          var PERCH_VAR_ARR = REF_POS_PERCH_VAR.concat(REF_POS_PERCH_VAR);
          var row_num = 0;
          var col_num = 0;
          var all_ref_data = [];
          var all_ref_arr = {};

          for (var k = 0; k < Number(data_max_num); k++) {
            all_ref_data.push({
              '$ATPERCH': '',
              '$ENABLED': '',
              '$COMMENT': '',
              '$HOMEPOS': '',
              '$DOUT_TYPE': '',
              '$DOUT_INDX': '',
              '$PERCHPOS': { 'J1': 0, 'J2': 0, 'J3': 0, 'J4': 0, 'J5': 0, 'J6': 0, 'J7': 0, 'J8': 0, 'J9': 0 },
              '$PERCHTOL': { 'J1': 0, 'J2': 0, 'J3': 0, 'J4': 0, 'J5': 0, 'J6': 0, 'J7': 0, 'J8': 0, 'J9': 0 }
            });
          }

          for (var i = 0; i < perch_elem.length; i++) {
            if (perch_elem[i].getAttributeNode('name').value.indexOf('$PERCHPOS[') > 0) {
              all_ref_data[row_num]['$PERCHPOS'][PERCH_VAR_ARR[col_num]] = perch_elem[i].childNodes[0].data;
              col_num++;
            } else if (perch_elem[i].getAttributeNode('name').value.indexOf('$PERCHTOL[') > 0) {
              all_ref_data[row_num]['$PERCHTOL'][PERCH_VAR_ARR[col_num]] = perch_elem[i].childNodes[0].data;
              col_num++;
            }
            if (col_num == PERCH_VAR_ARR.length) {
              row_num++;
              col_num = 0;
            }
          }

          row_num = 0;
          col_num = 0;
          for (var j = 0; j < str_elem.length; j++) {
            var str_val = str_elem[j].getAttributeNode('name').value;
            switch (str_val) {
              case REF_POS_VAR[COLUMN_REFPOS_AXIS1 - 1]:
              case REF_POS_VAR[COLUMN_REFPOS_RANGE1 - 1]:
                break;
              case REF_POS_VAR[COLUMN_REFPOS_INRANGE - 1]:
              case REF_POS_VAR[COLUMN_REFPOS_ENABLE - 1]:
              case REF_POS_VAR[COLUMN_REFPOS_COMMENT - 1]:
              case REF_POS_VAR[COLUMN_REFPOS_HOME - 1]:
              case REF_POS_VAR[COLUMN_REFPOS_SIGNAL_IO - 1]:
              case REF_POS_VAR[COLUMN_REFPOS_SIGNAL_IO_NUM - 1]:
                var next_val = ((typeof str_elem[j].childNodes[0] === 'undefined') ? '' : str_elem[j].childNodes[0].data);
                all_ref_data[row_num][str_val] = next_val;
                col_num++;
                break;
              default:
                irpapi.inner.apiErrorLog('convertRefXVRCallBack: Illegal attribute name.(' + str_val + ')');
                break;
            }
            if (col_num == REF_POS_VAR.length - 2) {
              all_ref_arr[grp_num + ',' + (row_num + 1)] = all_ref_data[row_num];
              row_num++;
              col_num = 0;
            }
          }
          callbackarg.callback(Number(data_max_num), all_ref_arr, xmlErrorFlg);
        }
      }
    },
    /*
    * Set Sysvar for Reference posisions.
    * (IN) grp_num : group number.
    *      write_data_list : write data list array.
    *      callback : callback function to get position.
    */
    setRefPosValue: function(grp_num, write_data_list, callback) {
      // Create Write RefPos Data
      var refpos_xml;
      // Header
      refpos_xml = '<?xml version="1.0" encoding="' + irpapi.global.langEncoding[irpapi.global.currentLang] + '"?>\r' + '<XMLVAR>\r';
      refpos_xml += ' <PROG name="*SYSTEM*">\r' + '  <VAR name="$REFPOS' + grp_num + '">\r';

      for (var i = 0; i < write_data_list.length; i++) {
        var field_str = [];
        var head_arr = '    <ARRAY name = "$REFPOS' + grp_num + '[' + write_data_list[i].record[COLUMN_REFPOS_INDEX].recVal + ']';
        var head_var = '      <FIELD name="';

        for (var k = 0; k <= REF_POS_VAR.length; k++) {
          switch (k) {
            case COLUMN_REFPOS_INDEX:
              field_str.push('    <ARRAY name="$REFPOS' + grp_num + '[' + write_data_list[i].record[COLUMN_REFPOS_INDEX].recVal + ']">\r');
              break;
            case COLUMN_REFPOS_AXIS1:
            case COLUMN_REFPOS_RANGE1:
              var write_arr = '';
              var axis_name = ((k == COLUMN_REFPOS_AXIS1) ? 'axis' : 'range');
              for (var j = 1; j <= REF_POS_AXIS.length; j++) {
                var unit_val = ((write_data_list[i]['unit' + j] == 'TRUE') ? (Math.PI / 180) : 1);
                var axis_num = exponentialAxis(parseFloat(write_data_list[i].record[REF_POS_CLM_ARR.indexOf(axis_name + j)] * unit_val).toExponential(9));
                write_arr += '    ' + head_arr + '.' + REF_POS_VAR[k - 1] + '[' + j + ']" prot ="RW">';
                write_arr += axis_num + '</ARRAY>\r';
              }
              field_str.push(write_arr);
              break;
            case COLUMN_REFPOS_COMMENT:
                var write_cmt = '';
              if ((write_data_list[i].force) || (write_data_list[i].record[COLUMN_REFPOS_COMMENT].recVal === '')) {
                write_cmt = '********</FIELD>\r';
              } else {
                write_cmt = write_data_list[i].record[COLUMN_REFPOS_COMMENT].recVal + '</FIELD>\r';
              }
              field_str.push(head_var + REF_POS_VAR[k - 1] + '" prot ="RW" force ="TRUE">' + write_cmt);
              break;
            case COLUMN_REFPOS_INRANGE:
            case COLUMN_REFPOS_ENABLE:
            case COLUMN_REFPOS_HOME:
            case COLUMN_REFPOS_SIGNAL_IO:
            case COLUMN_REFPOS_SIGNAL_IO_NUM:
              var write_var = write_data_list[i].record[k].recVal + '</FIELD>\r';
              field_str.push(head_var + REF_POS_VAR[k - 1] + '" prot ="RW">' + write_var);
              break;
            default:
              irpapi.inner.apiErrorLog('setRefPosValue: Illegal REF_POS_VAR.(' + k + ')');
              break;
          }
        }
        refpos_xml += field_str[COLUMN_REFPOS_INDEX] + field_str[COLUMN_REFPOS_COMMENT] + field_str[COLUMN_REFPOS_ENABLE];
        refpos_xml += field_str[COLUMN_REFPOS_INRANGE] + field_str[COLUMN_REFPOS_SIGNAL_IO] + field_str[COLUMN_REFPOS_SIGNAL_IO_NUM];
        refpos_xml += '      <FIELD name="$PERCHPOS">\r' + field_str[COLUMN_REFPOS_AXIS1] + '      </FIELD>\r';
        refpos_xml += '      <FIELD name="$PERCHTOL">\r' + field_str[COLUMN_REFPOS_RANGE1] + '      </FIELD>\r';
        refpos_xml += field_str[COLUMN_REFPOS_HOME] + '    </ARRAY>\r';
      }
      // Footer
      refpos_xml = refpos_xml + '  </VAR>\r' + ' </PROG>\r' + '</XMLVAR>';
      // Create File PosRegCVR
      var file_xvr = irpapi.inner.createBlob(refpos_xml);
      postXHR(irpapi.global.xvrFileName, file_xvr, callback); // io
      var reader = new FileReader();
      reader.addEventListener('loadend', function () {});
      reader.readAsArrayBuffer(file_xvr);

      // (inner)
      // Add leading zeros to single-digit exponential representations.
      // ex)  1.000e+9 ->  1.000e+09
      //     -1.001e-5 -> -1.001e-05
      function exponentialAxis(num) {
        var axis_str = String(num);
        var index = 0;
        if (axis_str.indexOf('+') > 0) {
          index = axis_str.indexOf('+');
        } else if (axis_str.indexOf('-') > 1) {
          index = axis_str.indexOf('-');
        } else if (axis_str.indexOf('-') == 0) { // 1st char is minus.
          index = axis_str.indexOf('-', 1); // check minus value and minus exponential
        }
        if (index <= 0) { // Not e{+|-}n
          return num;
        }
        var front_str = axis_str.substring(0, index + 1);
        var back_str = axis_str.slice(index + 1);
        if (back_str.length < 2) {
          back_str = '0' + axis_str.slice(index + 1);
        }
        front_str += back_str;
        return front_str;
      }
    },

