    confirmConfigAxisNum: function(confData, digits) {
      var conf = confData.split(',');
      var numCount = 0;
      var regexp = /[0-9]+/;
      for (var num = conf.length - 1; num > 0; num--) {
        if (regexp.test(conf[num])) {
          numCount++;
        }
      }
      if (numCount != (digits || irpapi.global.confNumofDigits)) {
        return false; // Get Config digits and Put out an error dialog when returns.
      }
      return confData;
    },
