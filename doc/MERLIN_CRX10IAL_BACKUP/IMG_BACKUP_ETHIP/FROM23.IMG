    },
    /*
     * Get All Frame Value
     * (IN) type     : frame type(utool, uframe)
     *      cmt_flag : Comment flag(Whether the data to be acquired is a comment.)
     *      grp_num  : group number
     *      index    : index
     *      num      : (ex: index=1, num=3 -> get frame index {1,2,3})
     *      callback : callback function to get frame.
     */
    getFrameValue: function(type, cmt_flag, grp_num, index, num, callback) {
      var arg = {};
      arg.callback = callback;
      arg.type = type;
      arg.cmt_flag = cmt_flag;
      var utool_id = 1;
      var uframe_id = 3;
      // Header
      var frame_xml = '<?xml version="1.0" encoding="' + irpapi.global.langEncoding[irpapi.global.currentLang] + '"?>\r';
      if (cmt_flag) {
        frame_xml = frame_xml +
          '<XMLCFG>\r<PROG name="TPFDEF">\r';
        switch (type) {
          case FRAME_TYPE_UTOOL:
            for (var iutt = index; iutt < (index + num); iutt++) {
              frame_xml = frame_xml +
                '<VAR name="SETUP_DATA[' + grp_num + ',' + utool_id + ',' + iutt + '].$COMMENT" />\r';
            }
            break;
          case FRAME_TYPE_UFRAME:
            for (var iuft = index; iuft < (index + num); iuft++) {
              frame_xml = frame_xml +
                '<VAR name="SETUP_DATA[' + grp_num + ',' + uframe_id + ',' + iuft + '].$COMMENT" />\r';
            }
            break;
          default:
            irpapi.inner.apiErrorLog('getFrameValue: Illegal frame type.(' + type + ')');
            break;
        }
      } else {
        frame_xml = frame_xml +
          '<XMLCFG>\r<PROG name="*SYSTEM*">\r';
        switch (type) {
          case FRAME_TYPE_UTOOL:
            for (var iuts = index; iuts < (index + num); iuts++) {
              frame_xml = frame_xml + '<VAR name="$MNUTOOL[' + grp_num + ',' + iuts + ']" />\r';
            }
            break;
          case FRAME_TYPE_UFRAME:
            for (var iufs = index; iufs < (index + num); iufs++) {
              frame_xml = frame_xml + '<VAR name="$MNUFRAME[' + grp_num + ',' + iufs + ']" />\r';
            }
            break;
          default:
            irpapi.inner.apiErrorLog('getFrameValue: Illegal frame type.(' + type + ')');
            break;
        }
      }
      frame_xml = frame_xml + '</PROG>\r</XMLCFG>';
    
      // Create Blob frame CVR
      var file_cvr = new Blob([frame_xml], { type: 'text/plain' });
      var getXhrCvrFile = (cmt_flag) ? irpapi.global.cvrFileNameCmt: irpapi.global.cvrFileName;
      getXHR(getXhrCvrFile, file_cvr, irpapi.inner.convertXVRCallBack, arg); // io
    },
    /*
     * Set Frame Value
     * (IN) type : frame type(utool, uframe)
     *      grp_num : group number.
     *      write_data_list : write data list array.
     *      callback : callback function to get position.
    */
    setFrameValue: function(type, grp_num, write_data_list, callback) {
      var frame_xml = '';
      var file_xvr;
      var reader;
      var type_str = convFrameTypeIntToStr(type);
      // Set the frame type for the interface.(UTOOL: 1, UFRAME: 3)
      var if_frame_type = 1;
      if (type == FRAME_TYPE_UFRAME) {
        if_frame_type = 3;
      }
      // Create Write Frame Data
      // Remove 'X' && 'Comment' block. Because else block is correct processing.
      // Update either comment or position data.
      // Header
      frame_xml =
        '<?xml version="1.0" encoding="' + irpapi.global.langEncoding[irpapi.global.currentLang] + '"?>\r' +
        '<XMLVAR>\r';
      if ('Comment' in write_data_list[0]) {// In case of updating only comment data.
        // comment data
        frame_xml = frame_xml + ' <PROG name="TPFDEF">\r';
        for (var j = 0; j < write_data_list.length; j++) {
          if (write_data_list[j].force) {
            frame_xml = frame_xml +
              '  <VAR name="SETUP_DATA[' + grp_num + ',' + if_frame_type + ',' + write_data_list[j].id + '].$COMMENT" prot ="RW" force ="TRUE">********</VAR>\r';
          } else {
            if (write_data_list[j].Comment === '') {
              write_data_list[j].Comment = ' ';
            }
            frame_xml = frame_xml +
              '  <VAR name="SETUP_DATA[' + grp_num + ',' + if_frame_type + ',' + write_data_list[j].id + '].$COMMENT" prot ="RW">' + write_data_list[j].Comment + '</VAR>\r';
          }
        }
      } else {// In case of updating only position data.
        // position data
        frame_xml = frame_xml + ' <PROG name="*SYSTEM*">\r';
        for (var k = 0; k < write_data_list.length; k++) {
          frame_xml = frame_xml +
            '  <ARRAY name = "$' + type_str + '[' + grp_num + ',' + write_data_list[k].id + ']" prot = "RW">\r' +
            '  gnum: ' + write_data_list[k].gnum + ' rep: 1 axes: 0 utool: 255 uframe: 255 Config: ' + write_data_list[k].Config + '\r' +
            '  X:   ' + parseFloat(write_data_list[k].X).toFixed(6) + '   Y:     ' + parseFloat(write_data_list[k].Y).toFixed(6) + '   Z:   ' + parseFloat(write_data_list[k].Z).toFixed(6) + '\r' +
            '  W:   ' + parseFloat(write_data_list[k].W).toFixed(6) + '   P:     ' + parseFloat(write_data_list[k].P).toFixed(6) + '   R:   ' + parseFloat(write_data_list[k].R).toFixed(6) + '\r' +
            '  </ARRAY>\r';
        }
      }
      // Footer
      frame_xml = frame_xml +
        ' </PROG>\r' +
        '</XMLVAR>';
  
      // Create File Frame CVR
      file_xvr = irpapi.inner.createBlob(frame_xml);
      var postXhrXvrfile = ('Comment' in write_data_list[0]) ? irpapi.global.xvrFileNameCmt: irpapi.global.xvrFileName;
      postXHR(postXhrXvrfile, file_xvr, callback); // io
      reader = new FileReader();
      reader.addEventListener('loadend', function () {});
      reader.readAsArrayBuffer(file_xvr);
    
      /* Convert the frame type from integer to string. */
      function convFrameTypeIntToStr(type_int) {
        var type_str = (type_int == FRAME_TYPE_UTOOL) ? FRAME_STR_UTOOL: FRAME_STR_UFRAME;
        return type_str;
      }
    },
    /*
    * Set all frame value (for two file) support process.
    * (IN) type     : frame type(utool, uframe)
