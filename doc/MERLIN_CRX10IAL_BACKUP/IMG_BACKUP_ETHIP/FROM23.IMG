    /*
     * Check the contents and apply the DCS settings.
     * (IN) popupWin : Object : Popup target window. (If null, set it to the top.)
     *      applyCallback : Function : Callback fnction after completion.
     *      cbArgs : Object : Callback argument.
     * applyCallback(status, procCode, cbArgs)
     *     status String Server access status.
     *     procCode Number Process code. refer to CONFAPPLY_XXXX.
     *     cbArgs Object Callback argument.
     */
    applyConfirmDCS: function(popupWin, applyCallback, cbArgs) {
      var targetWin = (popupWin != null) ? popupWin : parent;
      if (cbArgs == undefined) {
        cbArgs = {};
      }
      // Check apply enable
      top.checkApplyEnable(cbCheckApplyEnable, cbArgs); // irprog_io.js

      // callback of check apply enable.
      function cbCheckApplyEnable(status, result, request, cbArgs) {
        if (status != top.IO_SUCCESS) { // Illegal response of check apply.
          irpapi.inner.callDCSCallback(applyCallback, status, result, cbArgs);
          return;
        }
        var resultNum = (typeof result === 'string') ? parseInt(result) : result;
        if (resultNum === 0) { // Normal
          irpapi.global.dcsApplyPopupWin = targetWin;
          irpapi.global.dcsApplyCallback = applyCallback; // save callback function.
          irpapi.global.dcsApplyCbArgs = cbArgs; // save callback argument.
          var config = {};
          config.popupWin = targetWin;
          config.reqCode = (typeof request.code === "string") ? parseInt(request.code) : request.code;
          config.reqApply = (typeof request.apply === "string") ? parseInt(request.apply) : request.apply;
          irpapi.openPasswordPopup(config, cbOpenPasswordPopup, cbArgs); // Open [Input pass-number popup]
        } else { // error...
          irpapi.openAlertMessage(targetWin, irpapi.global.applyDCSPopupRes[resultNum].dicId, '', function() {
            irpapi.inner.callDCSCallback(applyCallback, status, irpapi.global.applyDCSPopupRes[resultNum].procCode, cbArgs);
          });
        }
      }
      // callback of input password.
      function cbOpenPasswordPopup(status, proc, cbArgs) {
        if (status != top.IO_SUCCESS) {
          irpapi.inner.callDCSCallback(applyCallback, status, proc, cbArgs);
          return;
        }
        if (proc != CONFAPPLY_SUCCESS) {
          irpapi.inner.callDCSCallback(applyCallback, status, proc, cbArgs);
          return;
        }
        // Call next process.
        irpapi.openApplyResultPopupProc(targetWin, applyCallback, cbArgs);
      }
    },
    /* Process of PasswordPopup
       config Object Popup configuration object
              .popupWin Object Popup target window. (If null, set it to the top.)
              .header String (default none)
              .message String (default 'Please enter code number.')
              .doApply Boolean false=No Apply (callback code number) (default true:exec apply)
              .codeKind Number 0=Master, 1=Base, 2=Pos/Speed, 4=I/O connectm, -1=Clear.
              .reqCode Number 0=Only Master, 1 - 7= each item (bit), 128=Prev/Current master
              .digit Number (default 4)
       pupCallback Function (status, procCode, cbArgs, codeNumber)
                            when doApply is false, set code number to codeNumber.
                            codeNumber = {master: 'nnnn', newcode: 'nnnn', confcode: 'nnnn'};
       cbArgs Object callback arguments
    */
    openPasswordPopup: function(config, pupCallback, cbArgs) {
      var popupPos = parent;
      var pupWin = (config.popupWin != null) ? config.popupWin : parent;

      // if reqCode undefined, reqCode set to master
      if (config.reqCode === undefined) {
        config.reqCode = 0;
      }
      // if doApply undefined, doApply set to false
      if (config.doApply === undefined) {
        config.doApply = true;
      }
      // if input only, reqCode set to master
      if (!config.doApply) {
        config.reqCode = 0;
      }
      if (config.codeKind === undefined) {
        config.codeKind = 0; // Master code.
      }
      // Master only or Master and each item.
      var iframeSrc =
          ((!config.doApply) && (config.codeKind >= 0)) ? '/frh/jcgtp/dcs_passcdset_popup.stm' :
          (config.reqCode == 128) ? '/frh/jcgtp/dcs_passrestore_popup.stm' :
          (config.reqCode == 0) ? '/frh/jcgtp/dcs_password_popup.stm' :
          '/frh/jcgtp/dcs_passeach_popup.stm';
      var pupItemId = ['base', 'posspd', 'iocnct']; // Each items
      var pupItemElem = []; // Save elements for each item
      var pupCodeElem = []; // Save elements for code setting
      var pupRstrElem; // Save elements for restore

      var initPasswordPopup = function (iframe) { // Initial setting of popup.
        var pwPopupFrame = iframe.contentWindow;
        var passTextBox = pwPopupFrame.document.getElementById('api_pwdtext_master');
        var passOkButton = pwPopupFrame.document.getElementById('buttonOk'); // OK button

        // Initialize target popup.
        pwPopupFrame.initTargetPopup(iframe, popupPos, config);

        // Save the element for removing the focus of the soft keyboard.
        if (!config.doApply) {
          // Code setting popup textbox
          pupCodeElem[0] = pwPopupFrame.document.getElementById('api_pwdtext_newpwd');
          pupCodeElem[1] = pwPopupFrame.document.getElementById('api_pwdtext_confpwd');
        } else if (config.reqCode === 128) {
          // Save textbox element
          pupRstrElem = pwPopupFrame.document.getElementById('api_pwdtext_crnt');
        } else if (config.reqCode !== 0) {
          // Save textbox element (removed element is null)
          pupItemElem[0] = pwPopupFrame.document.getElementById('api_pwdtext_base');
          pupItemElem[1] = pwPopupFrame.document.getElementById('api_pwdtext_posspd');
          pupItemElem[2] = pwPopupFrame.document.getElementById('api_pwdtext_iocnct');
        }

        // set ok button click event
        passOkButton.setCallback(function (id, operation) { // eslint-disable-line no-unused-vars
          blurTextboxFocus();
          passOkButton.blur();
          // Pickup pass number from input box
          var passNumber = '' + passTextBox.value; // toString
          var codeList = {master: '', base: '', posspd: '', iocnct: '', new: '', conf: '', prev: ''};
          if ((!config.doApply) && (config.codeKind >= 0)) { // Code setting
            codeList.master = passNumber;
            codeList.new = pupCodeElem[0].value;
            codeList.conf = pupCodeElem[1].value;
          } else { // Apply or clear code
            codeList.master = passNumber;
            if (config.reqCode == 128) { // Image/Backup restore
              codeList.master = pupRstrElem.value; // Current Master (=after restore)
              codeList.prev = passNumber; // Previous Master (=before restore)
            } else if (config.reqCode > 0) { // master or input each pasword
              for (var pi = 0; pi < 3; pi++) {
                if (pupItemElem[pi] !== null) {
                  codeList[pupItemId[pi]] = '' + pupItemElem[pi].value;
                }
              }
            }
          }
          top.IHMIComponents.cf.removeFrame(iframe);
          if (!config.doApply) { // Input code only, no apply.
            irpapi.inner.callDCSCallback(pupCallback, top.IO_SUCCESS, CONFAPPLY_INPUTONLY, cbArgs, codeList);
            return;
          }
          // Apply password.
          irpapi.dispOnWaiting();
          top.getApplyPassword(codeList, config.reqApply, resultApplyPassword, iframe); // irprog_io
        });

        // set close button click event
        pwPopupFrame.document.getElementById('closeBtn').setCallback(function () {
          blurTextboxFocus();
          top.IHMIComponents.cf.removeFrame(iframe);
          // Close password input.
          irpapi.inner.callDCSCallback(pupCallback, CONFAPPLY_SUCCESS, CONFAPPLY_PINCANCEL, cbArgs);
        });
        // set cancel button event
        pwPopupFrame.document.getElementById('buttonCancel').setCallback(function () {
          blurTextboxFocus();
          top.IHMIComponents.cf.removeFrame(iframe);
          // Cancel password input.
          irpapi.inner.callDCSCallback(pupCallback, CONFAPPLY_SUCCESS, CONFAPPLY_PINCANCEL, cbArgs);
        });

        // Blur from textbox focus
        function blurTextboxFocus() {
          passTextBox.blur();
          for (var pieIdx = 0; pieIdx < pupItemElem.length; pieIdx++) {
            if (pupItemElem[pieIdx] != undefined) {
              pupItemElem[pieIdx].blur();
            }
          }
          for (var pceIdx = 0; pceIdx < pupCodeElem.length; pceIdx++) {
            if (pupCodeElem[pceIdx] != undefined) {
              pupCodeElem[pceIdx].blur();
            }
          }
        }

        // callback of result for check PIN (password number).
        function resultApplyPassword(status, result, iframe) {
          if (status != top.IO_SUCCESS) {
            irpapi.dispOffWaiting();
            top.IHMIComponents.cf.removeFrame(iframe);
            irpapi.inner.callDCSCallback(pupCallback, status, CONFAPPLY_UNEXPECTED, cbArgs);
            return;
          }
          if (result == top.IO_DCSRESULT_OK) { // PIN is valid.
            top.IHMIComponents.cf.removeFrame(iframe);
            irpapi.inner.callDCSCallback(pupCallback, status, CONFAPPLY_SUCCESS, cbArgs); // Next is Check apply flag.
          } else { // PIN is invalid.
            irpapi.dispOffWaiting();
            irpapi.openAlertMessage(pupWin, 'applydcs_passerror', '', function() {
              irpapi.inner.callDCSCallback(pupCallback, status, CONFAPPLY_PINILLEGAL, cbArgs);
            });
          }
        }
      };

      var pupIframe = top.IHMIComponents.cf.appendFrame(pupWin, iframeSrc, "popup-frame orgstm", initPasswordPopup);
      pupIframe.allowTransparency = true;
    },
    /* Process of popup message */
    openAlertMessage: function(pupWin, msgId, hdrId, pupCallback) {
