function putIFFailLog(xhr, text, thrown) {
    var nowDate = new Date();
    var isoStr = (Date.prototype.toISOString) ? nowDate.toISOString(): nowDate.toTimeString();
    var failMsg = 'Status:' + text + ' ' + xhr.status + ' ' + thrown + ';';
    console.log('[' + isoStr + '] FATAL: Communication failed.\n        ' + failMsg);
}

