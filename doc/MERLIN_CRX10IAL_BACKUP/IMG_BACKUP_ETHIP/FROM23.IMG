        if (rpcStatus != IO_SUCCESS) {
          top.rpcmc_rprintf('Error: DCS_VRFY_REQ status(' + rpcStatus + ')');
          handle_error(rpcStatus);
        }
        if (callback != undefined) { 
          callback(rpcStatus, callbackarg, callbackarg2); // return verify result.
        }
      } else {
        top.rpcmc_rprintf('[rpcmc] unexpected reply ' + JSON.stringify(json));
        callback(top.IO_COMFAILURE, callbackarg, callbackarg2);
      }
    }).fail(function (xhr, textStatus, errorThrown) {
      if (callbackarg == undefined) {
        callbackarg = {};
      }
      callbackarg.failText = textStatus;
      callback(top.IO_COMFAILURE, callbackarg, callbackarg2);
    }
  );
}

/* 19.4
 * Request confirmation of apply.
 * (IN)  callback/cbarg: Callback function and arguments.
 * (OUT) callback : 
 *       status = 0:Normal, != 0:Error.
 *       result = 0:Apply OK, 1:no need to apply, 2:Invalid item, 3:DCS over time
 *       request = .code: Request item (0x01:Base, 0x02:Pos.Speed, 0x04: I/O connect, 0x80: prev/current master)
 *                 .apply: Items that need to be applied (Same as above) Use at DCS_CHECK_CODE
 */
function checkApplyEnable(callback, cbArgs) {
  $.getJSON('/COMET/rpc?func=DCS_CHECK_APPLY',
    function (json) {
      var checkApplyStatus = '';
      var checkApplyResult = '';
      var checkApplyRequest = {code: 0, apply: 0};
      if (json.FANUC.RPC != undefined) {
        checkApplyStatus = json.FANUC.RPC[0].status;
        checkApplyResult = json.FANUC.RPC[0].result;
        checkApplyRequest.code = json.FANUC.RPC[0].Req_code;
        checkApplyRequest.apply = json.FANUC.RPC[0].Req_apply;
        if (checkApplyStatus != top.IO_SUCCESS) {
          top.rpcmc_rprintf('Error: DCS_CHECK_APPLY status(' + checkApplyStatus + ')');
          handle_error(checkApplyStatus);
        }
      } else {
        checkApplyStatus = top.IO_COMFAILURE;
        top.rpcmc_rprintf('[rpcmc] unexpected reply ' + JSON.stringify(json));
      }
      callback(checkApplyStatus, checkApplyResult, checkApplyRequest, cbArgs);
    }).fail(function (xhr, textStatus, errorThrown) {
      if (cbArgs == undefined) {
        cbArgs = {};
      }
      cbArgs.failText = textStatus;
      callback(top.IO_COMFAILURE, null, {}, cbArgs);
    }
  );
}

