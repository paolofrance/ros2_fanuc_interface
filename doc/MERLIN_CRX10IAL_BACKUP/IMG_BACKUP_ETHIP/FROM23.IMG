        callback(rpcStatus, json.FANUC.RPC[0].result, cbarg, cbarg2);
      } else {
        top.rpcmc_rprintf("[rpcmc] unexpected reply " + JSON.stringify(json));
        callback(top.IO_COMFAILURE, null, cbarg, cbarg2);
      }
    }).fail(function (xhr, textStatus, errorThrown) {
      if (cbarg == undefined) {
        cbarg = {};
      }
      cbarg.failText = textStatus;
      callback(top.IO_COMFAILURE, null, cbarg, cbarg2);
    }
  );
}

