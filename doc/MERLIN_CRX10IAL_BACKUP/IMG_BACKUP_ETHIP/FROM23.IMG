// Construct text for exchange position data.
// ex) "gnum: n rep: n axes: n utool: n uframe: n Config: 'xxxxx' X: fff.fff ... "
//     "gnum: n rep: 9 axes: n utool: n uframe: n Config:  J1 = fff.fff unit ... "
//     when exist extend axes ... "EXTn: fff.fff unit"
// return text.
function constructExchangePosText(exchgInfo) {
  var recWork = $.extend({}, exchgInfo.srcRec); // Copy record data.
  // Group number
  var posText = 'gnum: ' + exchgInfo.grp;
  // Rep mode. Cartesian=2, Cartesian+Extend=6, Joint=9
  var workRep = (recWork.rep != undefined) ? recWork.rep :
    (recWork.frame == COORDINATECART) ? ((exchgInfo.ext > 0) ? POS_REP_BOTH : POS_REP_CART) : POS_REP_JOINT;
  posText += ' rep: ' + workRep;
  // Axes count.
  var axesCount = (workRep == POS_REP_JOINT) ? exchgInfo.robot + exchgInfo.ext :
    (workRep == POS_REP_BOTH) ? exchgInfo.ext : 0;
  posText += ' axes: ' + axesCount;
  // UT/UF
  posText += ' utool: ' + getUTUFDataText(recWork.UT, exchgInfo.UTVal);
  posText += ' uframe: ' + getUTUFDataText(recWork.UF, exchgInfo.UFVal);
  // Config
  posText += ' Config: ' + ((recWork.frame == COORDINATECART) ? recWork.Config : '');
  posText += ' ';	// add separator
  // position data
  if (recWork.frame == COORDINATECART) {	// Cartesian
    posText += CODETAB + 'X: ' + Number(recWork.X).toFixed(6);
    posText += CODETAB + 'Y: ' + Number(recWork.Y).toFixed(6);
    posText += CODETAB + 'Z: ' + Number(recWork.Z).toFixed(6);
    posText += CODETAB + 'W: ' + Number(recWork.W).toFixed(6);
    posText += CODETAB + 'P: ' + Number(recWork.P).toFixed(6);
    posText += CODETAB + 'R: ' + Number(recWork.R).toFixed(6);
  } else {	// Joint
    for (var i = 0; i < exchgInfo.robot; i++) {
      var axesName = 'J' + (i + 1);
      posText += CODETAB + axesName + ' = ' + Number(recWork[axesName]).toFixed(6);
      posText += ' ' + exchgInfo.unit[i];
    }
  }
  if (exchgInfo.ext > 0) {
    for (var ext = 1; ext <= exchgInfo.ext; ext++) {
      posText += CODETAB + 'EXT' + ext + ': ' + Number(recWork['EXT' + ext]).toFixed(6);
      posText += ' ' + exchgInfo.unit[ext + 5];
    }
  }
  return posText;
}
