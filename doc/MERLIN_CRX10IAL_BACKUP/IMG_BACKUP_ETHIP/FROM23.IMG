      top.getXHR('PMCDev.cvr', file_cvr, extractPMCDev, callback);
    
      // Extract data where $PMC_DEV[x].$OCCPY is not 0.
      function extractPMCDev(status, getText, callback) {
        var xml_obj = null;
        var dom_parser = new DOMParser();
        try {
          xml_obj = dom_parser.parseFromString(getText, 'text/xml');
        } catch (e) {
          irpapi.inner.apiErrorLog(e);
