ELF);
  orgCopyData.pop();
  for (var i = 0; i < orgCopyData.length; i++) { 
    copyData[i] = (orgCopyData[i].replace(/^,|,$/g, '')).split(',');
  }
  if (axislimitsGrid.getSelectType() == SELGRID_ROW) {
    for (var i = 0; i < copyData.length; i++) {
      var makeCopyData = [];
      makeCopyData[0] = copyData[i][COLUMN_LOWER];
      makeCopyData[1] = copyData[i][COLUMN_UPPER];
      copyData[i] = [];
      copyData[i] = makeCopyData;
    }
  }
  copyEventText.text = copyData;
}

function changeEvent(chgData) {
  var chgRow = (axislimitsGrid.getRecord(chgData.row)).record;
  var chgCol = AXIS_CLM_OBJ[chgData.fieldId];
  var nextVal = Number(chgData.val);

  var recAxis = chgRow[COLUMN_AXIS].recVal;
  var group = chgRow[COLUMN_GROUP].recVal;
  var grpAxis = chgRow[COLUMN_GRP_AXIS].recVal;
  var index = axislimitsGrid.getSelectRange().start.row;
  var colLowerDF = Number(chgRow[COLUMN_LOWER_DF].recVal);
  var colUpperDF = Number(chgRow[COLUMN_UPPER_DF].recVal);

  // Block string pasting and Limit the range of Value
  // Is it a correct number?
  if (nextVal !== "" && nextVal >= AXIS_VAL_MIN && nextVal <= AXIS_VAL_MAX) {
    if (grpAxis <= g_num_axis_dlg_enb[group - 1] && !g_is_independ_axis[group - 1]) {
      // Whether the value is within the limits
      if (nextVal >= colLowerDF && nextVal <= colUpperDF) {
        // set change
        setAxisLimitsVar(chgCol, recAxis, group, grpAxis, nextVal, getAxisLimitsVarCallback);
        // post alarm when lower changed
        if (chgCol == COLUMN_LOWER) {
          postAlarmLimitsChg(group, grpAxis, "Lowr");
        } else {
          // post alarm when upper changed
          postAlarmLimitsChg(group, grpAxis, "Uper");
        }
      } else {
        var lowerStr = colLowerDF.toFixed(2);
        var upperStr = colUpperDF.toFixed(2);
        var dlgMsg = resources['ERRStrLim'] + lowerStr + '  ~   ' + upperStr;
        top.openCmpAlert(window, 'errStrLimDlg', dlgMsg);
        axislimitsGrid.setCellValue(index, AXIS_CLM_ARR[chgCol], chgData.prev);
      }
    } else {
      setAxisLimitsVar(chgCol, recAxis, group, grpAxis, nextVal, getAxisLimitsVarCallback);
      // post alarm when lower changed
      if (chgCol == COLUMN_LOWER) {
        postAlarmLimitsChg(group, grpAxis, "Lowr");
      } else {
        // post alarm when upper changed
        postAlarmLimitsChg(group, grpAxis, "Uper");
      }
    }
  } else {
    top.openCmpAlert(window, 'errInvRealDlg', resources['ERRInvReal']);
    axislimitsGrid.setCellValue(index, AXIS_CLM_ARR[chgCol], chgData.prev);
  }
}

/*
 * Initialize Axis Limits Page
 */
function initAxisLimitsPage() {
  var linebuff = [];
  if (axislimitsGrid.getRecord(1) != null) {
    axislimitsGrid.clearGrid();
  }
  for (var i = g_current_item_no_start; i <= g_current_item_no_end; i++) {
    linebuff.push(createRecord(i));
  }
  if (linebuff.length != 0) {
    axislimitsGrid.drawRecord(linebuff);
  }
  
  var axisData = [];
  for (var j = 0; j < linebuff.length; j++) {
    var updRec = axislimitsGrid.getRecord(j + 1).record;
    if (updRec[COLUMN_GROUP].recVal > 0) {
      axisData[j] = {
        index: (j + 1),
        group:updRec[COLUMN_GROUP].recVal,
        grpAxis:updRec[COLUMN_GRP_AXIS].recVal,
      };
    }
  }
  getAxisLimitsXVR(axisData, getAxisLimitsXVRCallback);

  // Function key and TP key had be hidden, readjust the screen size.
  $(window).trigger('resize');
}
/*
 * Check paste Data
 */
function checkPasteData() {
  var pasteCol = (axislimitsGrid.getSelectType() == SELGRID_ROW) ? COLUMN_LOWER : axislimitsGrid.getSelectRange().start.col;
  var pasteRow = (axislimitsGrid.getSelectType() == SELGRID_ROW) ? axislimitsGrid.getSelectRow().start : axislimitsGrid.getSelectRange().start.row;
  var copydata = copyEventText.text;
  var dataList = [];
  var checkNum = /^[-]?([1-9]\d*|0)(\.\d+)?$/;
  for (var i = 0; i < copydata.length; i++) {
    // If the maximum number of records in the grid is exceeded, the process ends.
    if (pasteRow + i > g_current_item_no_end - g_current_item_no_start + 1) {
      break;
    }
    var recordData = copydata[i];
    var pasteRecord = axislimitsGrid.getRecord(pasteRow + i).record;
    if (pasteRecord == undefined) {
      return false; // If it try to paste into uneditable lines, skip the process.
    }
    for (var j = 0; j < recordData.length; j++) {
      if (pasteRecord[COLUMN_LOWER].recRead || pasteRecord[COLUMN_UPPER].recRead) {
        return false;
      }
      if (checkNum.test(recordData[j].replace(/\"/g,""))) {
        var nextVal = Number(recordData[j].replace(/\"/g,""));
      } else {
        return false; // If it try to paste something other than numbers, skip the process.
      }

      var nextCol = pasteCol + j;
      // Other than upper column and lower column can not be changed.
      if ((nextCol == COLUMN_LOWER) || (nextCol == COLUMN_UPPER)) {
        // It can paste only numbers in the range of upper and lower limits.
        if ((nextVal !== "") && (nextVal >= AXIS_VAL_MIN) && (nextVal <= AXIS_VAL_MAX)) {
          if (pasteRecord[COLUMN_GRP_AXIS].recVal <= g_num_axis_dlg_enb[pasteRecord[COLUMN_GROUP].recVal - 1] && !g_is_independ_axis[pasteRecord[COLUMN_GROUP].recVal - 1]) {
            //Whether the value is within the unique limits
            if ((nextVal < Number(pasteRecord[COLUMN_LOWER_DF].recVal)) ||
              (nextVal > Number(pasteRecord[COLUMN_UPPER_DF].recVal))) {
              return false;
            }
          }
          // push change data to list.
          var axis = pasteRecord[COLUMN_AXIS].recVal;
          var group = pasteRecord[COLUMN_GROUP].recVal;
          var groupAxis = pasteRecord[COLUMN_GRP_AXIS].recVal;
          dataList.push(createValArr(nextCol, axis, group, groupAxis, nextVal, pasteRow + i, getAxisLimitsVarCallback));
        } else {
          return false;
        }
      } else {
        return false;
      }
    }
  }
  return dataList;
}

/*
 * Set value after paste
 */
function pasteLimitData(dataList) {
  for (var i = 0; i < dataList.length; i++) {
    setAxisLimitsVar(dataList[i].col, dataList[i].id, dataList[i].group, dataList[i].grp_axis, dataList[i].next_val, dataList[i].callback);
    if (dataList[i].col == COLUMN_LOWER) {
      postAlarmLimitsChg(dataList[i].group, dataList[i].grp_axis, "Lowr");
    } else {
      postAlarmLimitsChg(dataList[i].group, dataList[i].grp_axis, "Uper");
    }
  }
}

/*
 * Grid Record Create
 */
function createRecord(axisNum) {
  var empty = {
    record: []
  };
  var groupNum = 0;
  var grpAxis = 0;
  var readOnly = true;
  if (axisNum == undefined) {
    axisNum = 1;
  } else if (axisNum <= g_group_arr.length) {
    var arr_id = g_group_arr.indexOf(g_group_arr[axisNum - 1]);
    groupNum = g_group_arr[axisNum - 1];
    grpAxis = axisNum - arr_id;
    readOnly = false;
  }

  empty.record[COLUMN_AXIS] = {recVal : axisNum};
  empty.record[COLUMN_GROUP] = {recVal : groupNum};
  empty.record[COLUMN_LOWER] = {recVal : 0.00, recRead: readOnly};
  empty.record[COLUMN_UPPER] = {recVal : 0.00, recRead: readOnly};
  empty.record[COLUMN_UNIT] = {recVal : resources['UnitMM']};
  empty.record[COLUMN_LOWER_DF] = {recVal : ""};
  empty.record[COLUMN_UPPER_DF] = {recVal : ""};
  empty.record[COLUMN_GRP_AXIS] = {recVal : grpAxis};

  return empty;
}

function createValArr(nextCol, id, group, grp_axis, next_val, recid, callback) {
  var setArr = {};
  setArr.col = nextCol;
  setArr.id = id;
  setArr.group = group;
  setArr.grp_axis = grp_axis;
  setArr.next_val = next_val;
  setArr.recid = recid;
  setArr.callback = callback;
  return setArr;
}

/*
 * Get num of groups callback
 */
function getNumGroupCallback(prog, sysvar, type, grp) {
  g_num_groups = Number(grp);
  getJointAxisXVR(g_num_groups, getJointAxisXVRCallback);
}

function getJointAxisXVRCallback(status, getText) {
  if (status != top.IO_SUCCESS) {
    console.log("getXHR error");
  } else {
    var xmlObj = null;
    var domParser = new DOMParser();
    try {
      xmlObj = domParser.parseFromString(getText, 'text/xml');
    } catch (e) {
      top.irprogapi.inner.apiErrorLog(e);
    }
    if ((xmlObj == null) || (xmlObj.getElementsByTagName('parsererror').length)) {
      // failure Get XML Data.
      console.log("xml parse error.");
      top.irprogapi.inner.apiErrorLog('set axislimits : xml error.');
    } else {
      // Success Get XML Data.
      var xmlVarElem = xmlObj.getElementsByTagName("XMLVAR");
      var varArr = xmlVarElem[0].getElementsByTagName("VAR");

      var numRobAxsArr = [0, 0, 0, 0, 0, 0, 0, 0];
      var numRedAxsArr = [0, 0, 0, 0, 0, 0, 0, 0];
      var rlibsw02Arr = [0, 0, 0, 0, 0, 0, 0, 0];
      var numAxesArr = [0, 0, 0, 0, 0, 0, 0, 0];
      for (var i = 0; i < varArr.length; i++) {
        var sysvar = varArr[i].getAttributeNode("name").value;
        var group = sysvar.match(/[0-9]/)[0]; 
        var value = varArr[i].textContent;

        if (sysvar.indexOf('NUM_ROB_AXS') > 0) {
          numRobAxsArr[group - 1] = Number(value);
        } else if(sysvar.indexOf('NUM_RED_AXS') > 0) {
          numRedAxsArr[group - 1] = Number(value);
        } else if(sysvar.indexOf('RLIBSW02') > 0) {
          rlibsw02Arr[group - 1] = Number(value);
        } else if (sysvar.indexOf('NUM_AXES') > 0) {
          numAxesArr[group - 1] = Number(value);
        } else if (sysvar.indexOf('ARM_TYPE') > 0) {
          g_is_independ_axis[group - 1] = (value == '27' ? true : false);
        }
      }

      for (var i = 0; i < g_num_groups; i++) {
        // Get Num of Axis with Dialog Enabled.
        if (rlibsw02Arr[i] == 128) {
          g_num_axis_dlg_enb[i] = numRobAxsArr[i] + 1;
        } else {
          g_num_axis_dlg_enb[i] = numRobAxsArr[i] + numRedAxsArr[i];
        }
        // Get num of groups
        for(var j = 0; j < numAxesArr[i]; j++) {
          g_group_arr.push(i + 1);
        }
      }
      if (g_group_arr.length > 0) {
        g_group_arr.sort(function (a, b) {
          if (a < b) return -1;
          if (a > b) return 1;
          return 0;
        });
        initPageArea(g_num_axis_limits);
        initAxisLimitsPage();
      }
    }
  }
}

/*
 * Get Axis Limits Value callback for XVR
 */
function getAxisLimitsXVRCallback(status, getText, axisData) {
  if (status != top.IO_SUCCESS) {
    console.log("getXHR error");
  } else {
    var strXml = getText;
    var xmlObj = null;
    var domParser = new DOMParser();
    try {
      xmlObj = domParser.parseFromString(strXml, 'text/xml');
    } catch (e) {
      top.irprogapi.inner.apiErrorLog(e);
    }
    if ((xmlObj == null) || (xmlObj.getElementsByTagName('parsererror').length)) {
      // failure Get XML Data.
      console.log("xml parse error.");
      top.irprogapi.inner.apiErrorLog('set axislimits : xml error.');
    } else {
      // Success Get XML Data.
      var xmlVarElem = xmlObj.getElementsByTagName("XMLVAR");
      var varArr = xmlVarElem[0].getElementsByTagName("ARRAY");
      for(var i = 0; i < varArr.length; i++) {
        var sysVarName = (varArr[i].getAttributeNode("name").value).split('.');
        var axis = 0;
        var grpNum = sysVarName[0].match(/[0-9]/)[0]; 
        var grpAxis = sysVarName[1].match(/[0-9]/)[0];

        for (var j = 0; j < axisData.length; j++) {
          if (axisData[j].group == Number(grpNum) && axisData[j].grpAxis == Number(grpAxis)) {
            axis = axisData[j].index;
            break;
          }
        }
        var value = 0;
        var column = AXIS_CLM_ARR[0];
        if (sysVarName[1].indexOf('LOWERLIMSDF') > 0 || sysVarName[1].indexOf('UPPERLIMSDF') > 0 ||
          sysVarName[1].indexOf('LOWERLIMS') > 0 || sysVarName[1].indexOf('UPPERLIMS') > 0) {
          // Set column id.
          if (sysVarName[1].indexOf('DF') > 0) {
            column = AXIS_CLM_ARR[(sysVarName[1].indexOf('LOWERLIMSDF') > 0 ? COLUMN_LOWER_DF : COLUMN_UPPER_DF)];
          } else {
            column = AXIS_CLM_ARR[(sysVarName[1].indexOf('LOWERLIMS') > 0 ? COLUMN_LOWER : COLUMN_UPPER)];
          }
          // Round off the values.
          if (Number(varArr[i].textContent) < 0.001 && Number(varArr[i].textContent) > -0.001 && Number(varArr[i].textContent) != 0) {
            value = Number(varArr[i].textContent).toExponential(5);
          } else {
            value = String(Math.round(Number(varArr[i].textContent) * 100000) / 100000);
            if (Number(value) < 0) {
              value = (Number(value.substr(0,8)).toPrecision(6)).substr(0,8);
            } else {
              value = (Number(value.substr(0,7)).toPrecision(6)).substr(0,7);
            }
          }
        } else if (sysVarName[1].indexOf('ROTARY_AXS') > 0) {
          column = AXIS_CLM_ARR[COLUMN_UNIT];
          value = varArr[i].textContent == 'TRUE' ? resources['UnitDG'] : resources['UnitMM'];
        }
        axislimitsGrid.setCellValue(axis, column, value);
      }
    }
  }
}
function getAxisLimitsVarCallback(prog, sysvar, type, val, clm, index) {
  if (index < g_current_item_no_start
    || index > g_current_item_no_end) {
    return;
  }
  var axisColData = axislimitsGrid.getColumn(AXIS_CLM_ARR[COLUMN_AXIS]);
  var updRowNum;
  for (i = 0; i < axisColData.length; i++) {
    if (Number(axisColData[i]) == index) {
      updRowNum = i + 1;
      break;
    }
  }
  switch (clm) {
    case COLUMN_LOWER:
    case COLUMN_UPPER:
      var axisValue = '0.00000';
      if (val.indexOf('e') >= 0) {
        axisValue = Number(val).toExponential(5);
      } else {
        if (Number(val) < 0) {
          axisValue = (Number(val).toPrecision(6)).substr(0,8);
        } else {
          axisValue = (Number(val).toPrecision(6)).substr(0,7);
        }
      }
      axislimitsGrid.setCellValue(updRowNum, AXIS_CLM_ARR[clm], axisValue);
      break;
    case COLUMN_UNIT:
      if (val == 'TRUE') {
        axislimitsGrid.setCellValue(updRowNum, AXIS_CLM_ARR[clm], resources['UnitDG']);
      } else if (val == 'FALSE') {
        axislimitsGrid.setCellValue(updRowNum, AXIS_CLM_ARR[clm], resources['UnitMM']);
      }
      break;
    case COLUMN_LOWER_DF:
    case COLUMN_UPPER_DF:
      axislimitsGrid.setCellValue(updRowNum, AXIS_CLM_ARR[clm], Number(val));
      break;
  }
}

