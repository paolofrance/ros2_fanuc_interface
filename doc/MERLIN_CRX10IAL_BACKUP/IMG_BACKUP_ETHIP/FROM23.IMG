.w2ui-reset input:not([type=button]):not([type=submit]):not([type=checkbox]):not([type=radio]) {
  background-color:#BEE1FF !important;
}

.w2ui-ss .w2ui-grid-body .w2ui-selection,
.w2ui-grid .w2ui-edit-box {
  border: 2px solid #0E6EB8;
