/* ##### For get error detail information. ##### */
/*
 * Request processing for error additional information.
 * (IN) status Error status code.
 *      Get DPEWRITE_STR information.
 */
function handle_error(status) {
  var nowDate = new Date();
  if (status != IO_SUCCESS) {
    // Try to get more information for user
    $.getJSON("/COMET/rpc?func=DPEWRITE_STR&ercode=" + status, function (json) {
      var errorStr = '';
      if (json.FANUC.RPC != undefined) {
        json.FANUC.RPC[0].status = json.FANUC.RPC[0].status.toLowerCase();
        if (json.FANUC.RPC[0].status != IO_SUCCESS) {
            errorStr = 'Failed to get additional information. (' + status + ')';
        } else {
            errorStr = json.FANUC.RPC[0].value;
        }
      } else {
        errorStr = 'Failed to get additional information. (' + status + ')';
      }
      top.rpcmc_rprintf("[rpcmc] handle_error:" + errorStr);
      console.log('[' + nowDate.toISOString() + '] ERROR: handle_error:' + errorStr);
    }).fail(function (xhr, textStatus, errorThrown) {
      console.log('[' + nowDate.toISOString() + '] ERROR: handle_error:' + errorThrown);
    });
  }
}

/*
 * Request processing for error additional information.
 * (IN) status : Error status code.
 *      Get DPEWRITE_STR information.
 * (IN) callback : A callback function to receive error message.
 * (IN) arg1 : Arguments to pass to the callback function. (ex: Dialog ID)
 */
function handleErrorString(status, callback, callbackarg1, callbackarg2) {
  var nowDate = new Date();
  if (status != IO_SUCCESS) {
    // Try to get more information for user
    $.getJSON("/COMET/rpc?func=DPEWRITE_STR&ercode=" + status, function (json) {
      var errorStr = '';
      if (json.FANUC.RPC != undefined) {
        json.FANUC.RPC[0].status = json.FANUC.RPC[0].status.toLowerCase();
        if (json.FANUC.RPC[0].status != IO_SUCCESS) {
            errorStr = 'Failed to get additional information. (' + status + ')';
        } else {
            errorStr = json.FANUC.RPC[0].value;
        }
      } else {
        errorStr = 'Failed to get additional information. (' + status + ')';
      }
      top.rpcmc_rprintf("[rpcmc] handle_error:" + errorStr);
      console.log('[' + nowDate.toISOString() + '] ERROR: handleErrorString:' + errorStr);
      if ((callback != undefined) && (json.FANUC.RPC[0].status == IO_SUCCESS)) {
        callback(json.FANUC.RPC[0].status, errorStr, callbackarg1, callbackarg2);
      }
    }).fail(function (xhr, textStatus, errorThrown) {
      console.log('[' + nowDate.toISOString() + '] ERROR: handleErrorString:' + errorThrown);
      callback(IO_COMFAILURE, errorThrown, callbackarg1, callbackarg2);
    });
  }
}

