 * (IN) callback   : callback function when data received
 *          arg1   : argument for callback function
 *          arg2   : argument for callback function
 */
function isGetVar(
    prog_name,
    var_name,
    callback,
    callbackarg1,
    callbackarg2) {
    $.getJSON("/COMET/rpc?func=VMIP_READVA" +
        "&prog_name=" + encodeASCII(prog_name) +
        "&var_name=" + var_name, function (json) {
            try {
                if (undefined != json.FANUC.RPC) {
                    if (json.FANUC.RPC[0].status != 0) {
                        callback(false, prog_name, var_name, null, 0, callbackarg1, callbackarg2);
                        rpcmc_rprintf("[rpcmc] vmip_readva [" + prog_name + "]" + var_name + " failed, status " +
                            json.FANUC.RPC[0].status);
                    }
                    else {
                        var type_code = json.FANUC.RPC[0].type_code;
                        var str = json.FANUC.RPC[0].value;
                        callback(true, prog_name, var_name, type_code, str, callbackarg1, callbackarg2);
                    }
                }
            } catch (err) { /* nop */ };
        });

}

/**
 * Send file. 
 * (IN) file_spec : File name. (String)
 * (IN) file_data : Send File Data.
 * (IN) callback : Callback function.
 * (IN) callbackarg : Callback argument.
 */
function sendFile(file_spec, file_data, callback, callbackarg) {
    // Ready to xhr.
    $.ajax({
        url: "/md?file=" + file_spec,
        type: "POST",
        data: file_data,
        processData: false,
        contentType: false,
        success: function (res) {
            //console.log('file uploaded' + res);
            callback(IO_SUCCESS, callbackarg);
        },
        error: function (xmlhttprequest, textstatus, errorthrown) {
            console.log(xmlhttprequest);
            console.log(textstatus);
            console.log(errorthrown);
            callback(-1, callbackarg);
        }
    });
}

function postXHR(file_spec, file_data, callback, callbackarg) {
  // Ready to xhr.
  var xhr = new XMLHttpRequest();
  // var parts = location.host.split(':');
  var sent_file = true;
  var postUrl = 'http://' + location.host + '/SOFTPART/XMLJSON?func=SETVAR';
  xhr.open('POST', postUrl, true);
  xhr.onload = function () {
    if (this.status == 200) {
      if (sent_file) {
        sent_file = false;
        // Send url request
        xhr.abort();
        postUrl = 'http://' + location.host + '/SOFTPART/BOGUS?file=' + file_spec;
        xhr.open("GET", postUrl, true);
        xhr.send();
      } else {
        callback(IO_SUCCESS, callbackarg);
      }
    } else {
      console.log('file upload error: ', this.responseText);
      callback(-1, callbackarg);
    }
  }

  // Send file
  xhr.send(file_data);
}

function getXHR(file_spec, file_data, callback, callbackarg) {
  // Prepare XHR
  var xhr = new XMLHttpRequest();
