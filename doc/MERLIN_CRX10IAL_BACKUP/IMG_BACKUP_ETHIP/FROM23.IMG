    /*
     * Set Position Register Value
     * (IN) posregData : write position register data list object.
     *      callback : callback function to get position.
    */
    setPosRegMltGrpValue: function(posregData, callback) {
      // Create Write PosReg Data
      var posreg_xml;
      // Header
      posreg_xml =
        '<?xml version="1.0" encoding="' + irpapi.global.langEncoding[irpapi.global.currentLang] + '"?>\r' +
        '<XMLVAR>\r' +
        ' <PROG name="*POSREG*">\r';
      var index = posregData.index;
      // set comment
      var setComment = posregData.comment;
      irpapi.setPosRegComment(index, setComment);

      // set position value
      for (var i = 0; i < posregData.writeDataNum; i++) {
        var rep = Number(posregData[i].rep);
        var gnum = posregData[i].grpNum;

        var tagPos = '[' + gnum + ',' + index + ']';

        if(posregData[i].force) {
          posreg_xml = posreg_xml +
          '  <ARRAY name = "$POSREG' + tagPos + '" prot = "RW" force ="TRUE">\r' +
          '  gnum: ' + gnum + ' rep: ' + rep + ' axes: ' + posregData[i].axes + ' utool: 255 uframe: 255 Config: ' + posregData[i].config + '\r';
        } else {
          posreg_xml = posreg_xml +
          '  <ARRAY name = "$POSREG' + tagPos + '" prot = "RW">\r' +
          '  gnum: ' + gnum + ' rep: ' + rep + ' axes: ' + posregData[i].axes + ' utool: 255 uframe: 255 Config: ' + posregData[i].config + '\r';
        }

        // Position data
        var format_axis = '';
        var joint = '';
        switch (rep) {
          case POS_REP_CART:
            posreg_xml = posreg_xml +
                '  X:   ' + parseFloat(posregData[i].X).toFixed(6) + '   Y:     ' + parseFloat(posregData[i].Y).toFixed(6) + '   Z:   ' + parseFloat(posregData[i].Z).toFixed(6) + '\r' +
                '  W:   ' + parseFloat(posregData[i].W).toFixed(6) + '   P:     ' + parseFloat(posregData[i].P).toFixed(6) + '   R:   ' + parseFloat(posregData[i].R).toFixed(6) + '\r';
            break;

          case POS_REP_BOTH:
            posreg_xml = posreg_xml +
            '  X:   ' + parseFloat(posregData[i].X).toFixed(6) + '   Y:     ' + parseFloat(posregData[i].Y).toFixed(6) + '   Z:   ' + parseFloat(posregData[i].Z).toFixed(6) + '\r' +
            '  W:   ' + parseFloat(posregData[i].W).toFixed(6) + '   P:     ' + parseFloat(posregData[i].P).toFixed(6) + '   R:   ' + parseFloat(posregData[i].R).toFixed(6) + '\r';
            for (var j = 0; j < posregData[i].axes; j++) {
              joint = 'EXT' + (j + 1);
              format_axis = format_axis + joint + ': ' + ('         ' + parseFloat(posregData[i][joint].data).toFixed(6)).slice(-12) + ' ' + posregData[i][joint].unit + ' ';
            }
            if (format_axis != '') {
              posreg_xml = posreg_xml + '  ' + format_axis + '\r';
            }
            break;

            case POS_REP_JOINT:
              for (var k = 0; k < posregData[i].axes; k++) {
                if (k < posregData[i].rob_axes) {
                  //Jxx
                  joint = 'J' + (k + 1);
                  format_axis = format_axis + joint + ' = ' + ('         ' + parseFloat(posregData[i][joint].data).toFixed(6)).slice(-12) + ' ' + posregData[i][joint].unit + ' ';
                } else {
                  //EXTxx
                  joint = 'EXT' + (k - posregData[i].rob_axes + 1);
                  format_axis = format_axis + joint + ': ' + ('         ' + parseFloat(posregData[i][joint].data).toFixed(6)).slice(-12) + ' ' + posregData[i][joint].unit + ' ';
                }
                if (((k + 1) % 3) == 0) {
                  posreg_xml = posreg_xml + '  ' + format_axis + '\r';
                  format_axis = '';
                }
              }
              if (format_axis != '') {
                posreg_xml = posreg_xml + '  ' + format_axis + '\r';
              }
              break;
        }
        // Footer
        posreg_xml = posreg_xml + '  </ARRAY>\r';
      }
      // Footer
      posreg_xml = posreg_xml +
        ' </PROG>\r' +
        '</XMLVAR>';
      // Create File PosRegCVR
      var file_xvr = irpapi.inner.createBlob(posreg_xml);
      postXHR(irpapi.global.xvrFileName, file_xvr, callback); // io
      var reader = new FileReader();
      reader.addEventListener('loadend', function () {});
      reader.readAsArrayBuffer(file_xvr);
    },
