/**
 * Set IO Maximum Number
 * DIO,Flag...System variable
 * AIO,GIO...Fix value
 * RIO...check robot io value step 8
 * (IN) callback
 */
function setIOMaxNum(callback) {
  ioMaxNum = [
    { iotype: new RegExp("^" + IO_TYPE_DIN + "$|^" + IO_TYPE_DOUT + "$"), maxvalue: 0, complete: false },
    { iotype: new RegExp("^" + IO_TYPE_AIN + "$|^" + IO_TYPE_AOUT + "$"), maxvalue: MAX_PORT_NUM_AIO, complete: true },
    { iotype: new RegExp("^" + IO_TYPE_RIN + "$|^" + IO_TYPE_ROUT + "$"), maxvalue: 0, complete: false },
    { iotype: new RegExp("^" + IO_TYPE_GIN + "$|^" + IO_TYPE_GOUT + "$"), maxvalue: MAX_PORT_NUM_GIO, complete: true },
    { iotype: new RegExp("^" + IO_TYPE_FLAG + "$"), maxvalue: 0, complete: false }
  ];
  checkIOMaxNum(callback);
}

/**
 * Get IO Maximum Number
 * (IN) iotype
 * (OUT)Return Maximum Number of iotype
 */
function getIOMaxNum(iotype) {
  if (ioMaxNum != null) {
    var index = getIOindex(iotype);
    return ioMaxNum[index].maxvalue;
  } else {
    return null;
  }
}

/**
 * Check Set IO Maximum Number
 * (IN) callback
 */
function checkIOMaxNum(callback) {
  for (i = 0; i < ioMaxNum.length; i++) {
    if (ioMaxNum[i].complete == false) {
      switch (true) {
        case ioMaxNum[i].iotype.test(IO_TYPE_DIN):
          top.rpcmc_getVar(top.SYSNAME_C, '$MAX_DIG_PRT', ioMaxNumCallback, callback, IO_TYPE_DIN);
          break;
        case ioMaxNum[i].iotype.test(IO_TYPE_RIN):
          top.get_ioall(IO_TYPE_RIN, 1, 64, rioMaxNumCallback, callback);
          break;
        case ioMaxNum[i].iotype.test(IO_TYPE_FLAG):
          top.rpcmc_getVar(top.SYSNAME_C, '$MIX_LOGIC.$NUM_FLG', ioMaxNumCallback, callback, IO_TYPE_FLAG);
          break;
      }
      break;
    }
  }
  if (i >= ioMaxNum.length) {
    console.log("complete get IO Port Num");
    callback();
  }
}

/**
 * IO Max Num Callback
 * (IN) progname...SYSNAME_C,NUMREG_C
 *      varname...System Variable Name
 *      typecode...Type Code(Not Used)
 *      value...Return Value
 *      callback...CallBack(callbackarg1)
 *      io_type...IO_TYPE
 */
function ioMaxNumCallback(progname, varname, typecode, value, callback, iotype) {
  var index = getIOindex(iotype);
  ioMaxNum[index].maxvalue = parseInt(value.slice(0), 10);
  ioMaxNum[index].complete = true;
  checkIOMaxNum(callback);
}

/**
 * RIO Max Num Callback
 * (IN) status...I/F Status
 *      iotype...IO Type
 *      ioindex...IO Index
 *      count... Get I/O Number
 *      value...IO Value
 *      callback...CallBack(callbackarg1)
 *      callbackarg2...not used
 */
function rioMaxNumCallback(status, iotype, ioindex, count, value, callback, callbackarg2) {
  var rio_max_value = 64;
  var index = getIOindex(iotype);
  if (status == IO_SUCCESS) {
    for (var rio_index = 0; rio_index < count; rio_index = rio_index + PORT_INDENT_RIO) {
      if ((value[rio_index].val != "0") && (value[rio_index].val != "1")) {
        rio_max_value = rio_index;
        break;
      }
    }
  } else {
    top.rpcmc_rprintf("Error: Failed to get RI / RO data.");
    rio_max_value = 0;
  }
  ioMaxNum[index].maxvalue = rio_max_value;
  ioMaxNum[index].complete = true;
  checkIOMaxNum(callback);
}

/**
 * Serch IO Index
 * (IN) iotype...IO Type
 * (OUT) Return index of iotype
 */
function getIOindex(iotype) {
  for (i = 0; i < ioMaxNum.length; i++) {
    if (ioMaxNum[i].iotype.test(iotype)) {
      break;
    }
  }
  return i;
}

