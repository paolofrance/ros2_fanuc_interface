    /*
    * Get System Value by xvr (support only *SYSTEM* and *POSREG* and *NUMREG*)
    * (IN) sysValObj: system values.
    *          .system : Array of System Value
    *          .posreg : Array of Position Register Value.
    *      callback : callback function to get info.
    *      cvrNameStr : A unique cvr name (if none, use irpapi.global.cvrFileName)
    */
    getSystemValByXvr: function(sysValObj, callback, cvrNameStr) {
      var arg = {};
      var sysValArray = [];
      var posRegArray = [];
      var numRegArray = [];
      arg.reqSysVals = sysValObj;
      arg.callback = callback;
      
      var sysnum_xml = '<?xml version="1.0" encoding="UTF-8"?>\r';
      sysnum_xml += '<XMLCFG>';
      if (sysValObj.system && (sysValObj.system.length > 0)) {
        sysValArray = sysValObj.system;
        sysnum_xml += '\r<PROG name="*SYSTEM*">\r';
        for (var i =0; i < sysValArray.length; i++) {
          sysnum_xml += ('<VAR name="' + sysValArray[i] + '" />\r');
        }
        sysnum_xml += '</PROG>\r';
      }
      if (sysValObj.posreg && (sysValObj.posreg.length > 0)) {
        posRegArray = sysValObj.posreg;
        sysnum_xml += '\r<PROG name="*POSREG*">\r';
        for (var r = 0; r < posRegArray.length; r++) {
          sysnum_xml += ('<VAR name="' + posRegArray[r] + '" />\r');
        }
        sysnum_xml += '</PROG>\r';
      }
      if (sysValObj.numreg && (sysValObj.numreg.length > 0)) {
        numRegArray = sysValObj.numreg;
        sysnum_xml += '\r<PROG name="*NUMREG*">\r';
        for (var n =0; n < numRegArray.length; n++) {
          sysnum_xml += ('<VAR name="' + numRegArray[n] + '" />\r');
        }
        sysnum_xml += '</PROG>\r';
      }
      sysnum_xml += '</XMLCFG>';

      // Create File PosRegCVR
      var file_cvr = new Blob([sysnum_xml], { type: 'text/plain' });
      getXHR((cvrNameStr || irpapi.global.cvrFileName), file_cvr, irpapi.inner.convertXVRCallBackByList, arg);
    },
    /*
    * Get All Position Register Value
    * (IN) grp_num  : group number or group number array [1,2,3]
    *      index    : index
    *      num      : (ex: index=1, num=3 -> get posreg index {1,2,3})
    *      callback : callback function to get position.
    *      cvrNameStr : A unique cvr name (if none, use irpapi.global.cvrFileName)
    */
    getPosRegValue: function(grp_num, index, num, callback, cvrNameStr) {
      var arg = {};
      var groups = [];
      arg.callback = callback;
      arg.type = REG_TYPE_POS;
      arg.cmt_flag = false;
      // Create PosReg Data
      var posreg_data =
        '<?xml version="1.0" encoding="' + irpapi.global.langEncoding[irpapi.global.currentLang] + '"?>\r' +
        '<XMLCFG>\r<PROG name="*POSREG*">\r';
      if (Array.isArray(grp_num)) {
        groups = grp_num; //multi group
      } else {
        groups.push(grp_num); //1group
      }
      for (var g = 0; g < groups.length; g++) {
        for (var i = 0; i < num; i++) {
          posreg_data = posreg_data + '<VAR name="$POSREG[' + groups[g] + ',' + (i + index) + ']" />\r';
        }
      }
      posreg_data = posreg_data + '</PROG>\r</XMLCFG>';

      // Create File PosRegCVR
      var file_cvr = new Blob([posreg_data], { type: 'text/plain' });
      getXHR((cvrNameStr || irpapi.global.cvrFileName), file_cvr, irpapi.inner.convertXVRCallBack, arg); // io
    },
