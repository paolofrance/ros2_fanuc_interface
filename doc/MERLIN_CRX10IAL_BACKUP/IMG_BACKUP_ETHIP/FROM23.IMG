/**
 * Analyze position data after convert XVR CallBack
 * (IN) callback : callback function to get position.
 */
function convertXVRCallBack(status, getText, callbackarg) {

  var str_xml = "";
  var filelines = getText.split(CODELF);
  var dom_parser = new DOMParser();
  for (var i = 0; i < filelines.length; i++) {
    str_xml = str_xml + filelines[i] + "\n";
  }
  var xml_obj = null;
  xmlErrorLog.flg = false;
  try {
    xml_obj = dom_parser.parseFromString(str_xml, "text/xml");
  } catch (e) {
    console.log(e);
  }
  if ((xml_obj == null) || (xml_obj.getElementsByTagName("parsererror").length)) {
    str_xml = delErrData(str_xml, filelines);
    xml_obj = dom_parser.parseFromString(str_xml, "text/xml");
    xmlErrorLog.flg = true;
    console.log("xml error");
  } else if ((navigator.userAgent.toLowerCase().indexOf('msie') != -1) ||
    (navigator.userAgent.toLowerCase().indexOf('trident') != -1)) { // IE
    var str_xml_IE = delIEGrbl(str_xml, filelines);
    if (str_xml_IE != str_xml) { // It is a process for when the garbled error which does not become an error in IE has occurred
      xml_obj = dom_parser.parseFromString(str_xml_IE, "text/xml");
    }
  }
  // Success Get XML Data.
  if (xml_obj.getElementsByTagName("ARRAY").length != 0) {
    var pos_data = {};
    var pos_elem = xml_obj.getElementsByTagName("ARRAY");
    // For position data
    //console.log(pos_elem);
    switch (callbackarg.type) {
      case REG_TYPE_POS:
        for (var i = 0; i < pos_elem.length; i++) {
          if (pos_elem[i].getAttributeNode("name").value.match(/\$POSREG/)) {
            pos_data[pos_elem[i].getAttributeNode("name").value.replace("$POSREG[", "").replace("]", "")] = analyzePosRegData(pos_elem[i].childNodes[0].data);
          }
        }
        break;
      case FRAME_TYPE_UTOOL:
        for (var i = 0; i < pos_elem.length; i++) {
          if (pos_elem[i].getAttributeNode("name").value.match(/\$MNUTOOL/)) {
            pos_data[pos_elem[i].getAttributeNode("name").value.replace("$MNUTOOL[", "").replace("]", "")] = analyzePosRegData(pos_elem[i].childNodes[0].data);
          }
        }
        break;
      case FRAME_TYPE_UFRAME:
        for (var i = 0; i < pos_elem.length; i++) {
          if (pos_elem[i].getAttributeNode("name").value.match(/\$MNUFRAME/)) {
            pos_data[pos_elem[i].getAttributeNode("name").value.replace("$MNUFRAME[", "").replace("]", "")] = analyzePosRegData(pos_elem[i].childNodes[0].data);
          }
        }
        break;
      default:
        return;
    }
    callbackarg.callback(pos_data);
    // For comment data
  } else if (xml_obj.getElementsByTagName("VAR").length != 0) {
    var cmt_data = {};
    var cmt_elem = xml_obj.getElementsByTagName("VAR");
    //console.log(cmt_elem);
    switch (callbackarg.type) {
      case FRAME_TYPE_UTOOL:
      case FRAME_TYPE_UFRAME:
        for (var i = 0; i < cmt_elem.length; i++) {
          if (cmt_elem[i].getAttributeNode("name").value.match(/SETUP_DATA/)) {
            cmt_data[cmt_elem[i].getAttributeNode("name").value.replace("SETUP_DATA[", "").replace("].$COMMENT", "")] = cmt_elem[i].textContent;
          }
        }
        break;
      default:
        return;
    }
    callbackarg.callback(cmt_data);
  }
}

/**
 * Analyze refposition data after convert XVR CallBack
 * (IN) callback : callback function to get position.
 */
function convertRefXVRCallBack(status, getText, callbackarg) {
  var str_xml = getText;
  var filelines = getText.split(CODELF);
  var xml_obj = null;
  var dom_parser = new DOMParser();
  xmlErrorLog.flg = false;
  try {
    xml_obj = dom_parser.parseFromString(str_xml, "text/xml");
  } catch (e) {
    console.log(e);
  }
  if ((xml_obj == null) || (xml_obj.getElementsByTagName("parsererror").length)) {
    str_xml = delErrData(str_xml, filelines);
    xml_obj = dom_parser.parseFromString(str_xml, "text/xml");
    xmlErrorLog.flg = true;
    console.log("xml error");
  }
  // Success Get XML Data.
  if ((xml_obj.getElementsByTagName("ARRAY").length != 0) &&
    (xml_obj.getElementsByTagName("FIELD").length != 0)) {

    var perch_elem = xml_obj.getElementsByTagName("ARRAY");
    var str_elem = xml_obj.getElementsByTagName("FIELD");
    var var_elem = xml_obj.getElementsByTagName("VAR");
    var grp_num = var_elem[0].getAttributeNode("name").value.replace(/[^0-9]/g, "");
    var data_max_num = perch_elem[perch_elem.length - 1].getAttributeNode("name").value.match(/\[.+?\]/);
    data_max_num = data_max_num[0].slice(1, -1);
    var PERCH_VAR_ARR = ['J1', 'J2', 'J3', 'J4', 'J5', 'J6', 'J7', 'J8', 'J9', 'J1', 'J2', 'J3', 'J4', 'J5', 'J6', 'J7', 'J8', 'J9'];
    var row_num = 0;
    var col_num = 0;
    var all_ref_data = [];
    var all_ref_arr = {};

    for (var k = 0; k < Number(data_max_num); k++) {
      all_ref_data.push({
        '$ATPERCH': "",
        '$ENABLED': "",
        '$COMMENT': "",
        '$HOMEPOS': "",
        '$DOUT_TYPE': "",
        '$DOUT_INDX': "",
        '$PERCHPOS': { "J1": 0, "J2": 0, "J3": 0, "J4": 0, "J5": 0, "J6": 0, "J7": 0, "J8": 0, "J9": 0 },
        '$PERCHTOL': { "J1": 0, "J2": 0, "J3": 0, "J4": 0, "J5": 0, "J6": 0, "J7": 0, "J8": 0, "J9": 0 }
      });
    }

    for (var i = 0; i < perch_elem.length; i++) {
      if (perch_elem[i].getAttributeNode("name").value.indexOf("$PERCHPOS[") > 0) {
        all_ref_data[row_num]["$PERCHPOS"][PERCH_VAR_ARR[col_num]] = perch_elem[i].childNodes[0].data;
        col_num++;
      } else if (perch_elem[i].getAttributeNode("name").value.indexOf("$PERCHTOL[") > 0) {
        all_ref_data[row_num]["$PERCHTOL"][PERCH_VAR_ARR[col_num]] = perch_elem[i].childNodes[0].data;
        col_num++;
      }
      if (col_num == PERCH_VAR_ARR.length) {
        row_num++;
        col_num = 0;
      }
    }

    row_num = 0;
    col_num = 0;
    for (var i = 0; i < str_elem.length; i++) {
      var str_val = str_elem[i].getAttributeNode("name").value;
      switch (str_val) {
        case REF_POS_VAR[COLUMN_AXIS1 - 1]:
        case REF_POS_VAR[COLUMN_RANGE1 - 1]:
          break;
        case REF_POS_VAR[COLUMN_INRANGE - 1]:
        case REF_POS_VAR[COLUMN_ENABLE - 1]:
        case REF_POS_VAR[COLUMN_COMMENT - 1]:
        case REF_POS_VAR[COLUMN_HOME - 1]:
        case REF_POS_VAR[COLUMN_SIGNAL_IO - 1]:
        case REF_POS_VAR[COLUMN_SIGNAL_IO_NUM - 1]:
          var next_val = ((typeof str_elem[i].childNodes[0] === "undefined") ? "" : str_elem[i].childNodes[0].data);
          all_ref_data[row_num][str_val] = next_val;
          col_num++;
          break;
        default:
          break;
      }
      if (col_num == REF_POS_VAR.length - 2) {
        all_ref_arr[grp_num + "," + (row_num + 1)] = all_ref_data[row_num];
        row_num++;
        col_num = 0;
      }
    }
    callbackarg.callback(Number(data_max_num), all_ref_arr);
  }
}

// If the position data have error, delete comment data.
function delErrData(str_xml, filelines) {
  var repStr = ""; // Data after error deletion
  var matchStr; // For loop processing
  var repBefStr = { before: "", after: "" }; // before: String before replacement, after: String after replacement

  if (str_xml.indexOf('$COMMENT" prot ="RW">') !== -1) { // for uframe, utool, refpositions
    repBefStr = {
      before: /\$COMMENT\"\sprot\s=\"RW\"><\/VAR>\r\n/,
      after: "$COMMENT\" prot =\"RW\">********</VAR>\r\n"
    };
    if (str_xml.indexOf('$REFPOS') !== -1) {
      repBefStr.after = "$COMMENT\" prot =\"RW\">********</FIELD>\r\n";
    }
    repStr = delGrblChar(filelines, false); // Processing for when filelines are garbled.
  } else if (str_xml.indexOf('$POSREG') !== -1) { // for posregister
    repBefStr = {
      before: /prot\s=\"RW\">\s*\'.+?\'+/,
      after: " prot =\"RW\">  ''"
    };
    repStr = delGrblChar(filelines, true); // Processing for when filelines are garbled.
  }

  if (repBefStr.before !== "") { // Process only the line that contains comment
    matchStr = repStr.replace(repBefStr.before, repBefStr.after);
    while (matchStr !== repStr) {
      matchStr = matchStr.replace(repBefStr.before, repBefStr.after);
      repStr = repStr.replace(repBefStr.before, repBefStr.after);
    }
  }

  return repStr;
}
function delGrblChar(filelines, posreg) {
  var repStr = ""; // Data after error deletion

  if (posreg) {
    for (var i = 0; i < filelines.length; i++) { // Processing for when single quotes are garbled.
      if (filelines[i].indexOf("'") !== -1) { // Process only the line that contains single quotes from the original.
        filelines[i] = filelines[i].replace("\r", "' \r"); // Add single quote
        filelines[i] = filelines[i].replace("</ARRAY>'", "'</ARRAY>"); // Add single quote
        filelines[i] = filelines[i].replace(" ********'</ARRAY>", "' ********</ARRAY>"); // Delete unnecessary single quote
      }
      repStr = repStr + filelines[i] + "\n";
    }
  } else {
    for (var i = 0; i < filelines.length; i++) { // Processing for when "<" was garbled.
      if (filelines[i].indexOf("COMMENT") !== -1) { // Process only the line that contains comment from the original.
        var headStr = filelines[i].substr(0, filelines[i].indexOf('$COMMENT" prot ="RW">') + 21);
        filelines[i] = headStr + "</VAR>\r";
      }
    repStr = repStr + filelines[i] + "\n";
    }
  }

  return repStr;
}
function delIEGrbl(str_xml, filelines) {
  var repStr = str_xml; // Data after error deletion
  var repBefStr = {
    before: /[^\']\s+\*{8}<\/ARRAY>/,
    after: "' ********</ARRAY>"
  };

  if (repBefStr['before'].test(str_xml)) {
    repStr = "";
    for (var i = 0; i < filelines.length; i++) { // Processing for when single quotes are garbled.
      filelines[i] = filelines[i].replace(repBefStr.before, repBefStr.after); // Add single quote
      repStr = repStr + filelines[i] + "\n";
    }
  }

  return repStr;
}
