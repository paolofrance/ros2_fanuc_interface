    /*
    * Count the number of characters that contain double-byte half-width characters.
    * (IN) str : Target string.
    */
    countChar: function(str) {
      var len = 0;
      str = escape(str);
      for (var i = 0; i < str.length; i++ , len++) {
        if (str.charAt(i) == '%') {
          if (str.charAt(++i) == 'u') {
            i += 3;
            len++;
          }
          i++;
        }
      }
      return len;
    },
    /*
    * Slice characters that contain double-byte half-width characters
    * (IN/OUT) str : Target string.
    * (IN) str_limit : limit of length.
    */
    multByteStringSlice: function(str, str_limit) {
      while (irpapi.countChar(str) > str_limit) {
        str = str.slice(0, str.length - 1);
      }
      return str;
    },
