function getRegValueXVR(index, num, callback) {
  var reg_cvr =
    '<?xml version="1.0" encoding="' + langEncoding[currentLang] + '"?>\r' +
    '<XMLCFG>\r<PROG name="*NUMREG*">\r';
  for (var i = 0; i < num; i++) {
    reg_cvr = reg_cvr + '<VAR name="$NUMREG[' + (i + index) + ']" />\r';
  }
  reg_cvr = reg_cvr + '</PROG>\r</XMLCFG>';

  // Create numreg.cvr
  var file_cvr = new Blob([reg_cvr], { type: "text/plain" });
  getXHR(cvrFileName, file_cvr, convertRegXVRCallBack, callback);

  function convertRegXVRCallBack(status, reg_xvr, callback) {
    var xml_obj = null;
    var dom_parser = new DOMParser();
    try {
      xml_obj = dom_parser.parseFromString(reg_xvr, 'text/xml');
    } catch (e) {
      irpapi.inner.apiErrorLog(e);
    }
    if ((xml_obj == null) || (xml_obj.getElementsByTagName('parsererror').length)) {
      irpapi.inner.apiErrorLog('convertNumRegXVRCallBack: xml error.');
    }
    // Analyze XML Data.
    if (xml_obj.getElementsByTagName("ARRAY").length != 0) {
      var reg_data = {};
      var array_elem = xml_obj.getElementsByTagName('ARRAY');
      for (var j = 0; j < array_elem.length; j++) {
        reg_data[array_elem[j].getAttributeNode("name").value.replace("$NUMREG[", "").replace("]", "")] = analyzeNumRegData(array_elem[j].childNodes[0].data);
      }
      callback(reg_data);
    }
  }
}

