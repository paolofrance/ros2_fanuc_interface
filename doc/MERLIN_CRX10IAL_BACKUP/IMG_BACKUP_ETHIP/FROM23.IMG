/* 3.9
 * Get specified path File List.
 * (IN) pathName: Path name.
 * (IN) callback : A callback function to get file list.
 *                 arg1: status
 *                 arg2: path name
 *                 arg3: file list (Array of String)
 *                 arg4: cbArg
 * (IN) cbArg : Callback argument object.
 */
function getFileList(pathName, callback, cbArg) {
  if (pathName.length == 0) { // Path is not specified.
    if (callback != undefined) {
      callback(IO_SUCCESS, pathName, [], cbArg);
    }
    return;
  }

  $.getJSON("/COMET/rpc?func=GTFILIST&path_name=" + pathName, function(json) {
    if (json.FANUC.RPC != undefined) {
      var status = json.FANUC.RPC[0].status.toLowerCase();
      var list = (status == IO_SUCCESS) ? json.FANUC.RPC[0].value.split(',') : [];
      if (callback != undefined) {
        callback(status, pathName, list, cbArg);
      }
    } else {
      rpcmc_rprintf("[rpcmc] unexpected reply " + JSON.stringify(json));
    }
  }).fail(function (xhr, textStatus, errorThrown) {
    putIFFailLog(xhr, textStatus, errorThrown);
    if (callback != undefined) {
      callback(IO_COMFAILURE, pathName, [], cbArg);
    }
  });
}

