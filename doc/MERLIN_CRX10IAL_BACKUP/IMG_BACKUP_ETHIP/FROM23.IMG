function resultLoadFile(result, decodedCount, blobData, callback, callbackarg) {
  decodedData[decodedCount] = result;
  decodedCount++;
  if (decodedCount >= blobData.length) {
    callback(decodedData, callbackarg);
  } else {
    var reader = new FileReader();
    reader.addEventListener("loadend", function () {
      resultLoadFile(reader.result, decodedCount, blobData, callback, callbackarg);
    });
    reader.readAsText(blobData[decodedCount], langEncoding[currentLang]);
  }
}

/**
 * convert Text Data
 * (IN) textData
 *      lineCount
 */
function convertTextData(textData, lineCount, callback, callbackarg) {
  var bytesArray = [];
  var text = "";
  var blobData = [];
  decodedData = [];
  decodedCount = 0;

  if (lineCount > 0) { // If online data read, need to decode.
    for (var txtIdx = 0; txtIdx < textData.length; txtIdx++) {
      text = window.atob(textData[txtIdx].buf);
      bytesArray = [];
      for (var convIdx = 0; convIdx < text.length; convIdx++) {
        var tempCharCode = text.charCodeAt(convIdx);
        if (tempCharCode > 0x00) { // Not null character
          bytesArray[convIdx] = tempCharCode;
        }
      }
      blobData[txtIdx] = new Blob([new Uint8Array(bytesArray)]);
    }

    var reader = new FileReader();
    decodedCount = 0;
    reader.addEventListener("loadend", function () {
      resultLoadFile(reader.result, decodedCount, blobData, callback, callbackarg);
    });
    reader.readAsText(blobData[decodedCount], langEncoding[currentLang]);

  } else { // For offline reading No need to decode.
    decodedData = textData;
    callback(decodedData, callbackarg);
  }
  // old code
  /*    if (lineCount > 0) { // If online data read, need to decode.
  /        for (var txtIndex = 0; txtIndex < textData.length; txtIndex++) {
  /            decodedData[txtIndex] = convertSjistoUcd(window.atob(textData[txtIndex].buf));
  /        }
  /    } else { // For offline reading No need to decode.
  /        decodedData = textData;
  /    }
  /    return decodedData;
  */
}

/**
 * convert Text Data
 * (IN) textData
 *      lineCount
 */
function convertLSData(textData, lineCount, callback, callbackarg) {
  var bytesArray = [];
  var text = "";
  var blobData = [];
  var sendDataArray = []; // Send all data at once.
  var CODECRLF = '\r\n';
  
  if (lineCount > 0) { // If online data read, need to decode.
    for (var txtIdx = 0; txtIdx < textData.length; txtIdx++) {
      text = window.atob(textData[txtIdx].buf) + CODECRLF;
      bytesArray = [];
      for (var convIdx = 0; convIdx < text.length; convIdx++) {
        var tempCharCode = text.charCodeAt(convIdx);
        if (tempCharCode > 0x00) { // Not null character
          bytesArray.push(tempCharCode);
        }
      }
      Array.prototype.push.apply(sendDataArray, bytesArray);
    }
    blobData[0] = new Blob([new Uint8Array(sendDataArray)]);
    var reader = new FileReader();
    reader.addEventListener("loadend", function () {
      var filelines = reader.result.split(CODECRLF);
      callback(filelines, callbackarg);
    });
    reader.readAsText(blobData[0], langEncoding[currentLang]);

  } else { // For offline reading No need to decode.
    callback(textData, callbackarg);
  }
}

