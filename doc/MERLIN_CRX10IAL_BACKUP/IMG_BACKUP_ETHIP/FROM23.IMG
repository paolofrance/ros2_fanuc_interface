    },
    /* Process of apply. */
    openApplyResultPopupProc: function(dcsPopupWin, dcsCallback, dcsCbArgs) {
      var pupWin = (dcsPopupWin != null) ? dcsPopupWin : parent;

      // Check apply flag.
      getDcsApplyFlg(getDcsApplyFlgCallback, dcsCbArgs);

      // Check Apply flag (get system value).
      function getDcsApplyFlg(callback, cbArg1) {
        top.rpcmc_getVar(top.SYSNAME_C, '$TABUI_DCS.$TABUI_APLY', callback, cbArg1);
      }

      var waitDcsApply = 0;
      // Repeat until the flag is turned on.
      function getDcsApplyFlgCallback(progname, varname, typecode, value, cbArg1) {
        // Check the flag every 2 seconds and wait 30 seconds.
        if (waitDcsApply > 14) { // over 30s. -> time out.
          irpapi.dispOffWaiting();
          irpapi.openAlertMessage(pupWin, 'applydcs_timeout', '', function() {
            irpapi.inner.callDCSCallback(dcsCallback, CONFAPPLY_UNEXPECTED, CONFAPPLY_TIMEOUT, cbArg1);
          });
          return;
        }
        if (value != '0') {
          setTimeout(function () {
            getDcsApplyFlg(getDcsApplyFlgCallback, cbArg1);
          }, 2000);
          waitDcsApply++;
          return;
        }
        waitDcsApply = 0; // clear timer
        irpapi.dispOffWaiting();
        // Display confirm popup.
        var pupApndFrame = top.IHMIComponents.cf.appendFrame(pupWin, '/frh/jcgtp/dcs_confirm_apply.stm', 'popup-frame', initializeApplyPopup);
        pupApndFrame.allowTransparency = true;
        pupApndFrame.style.width = '100%';
        pupApndFrame.style.height = '100%';
        pupApndFrame.style.top = 0;
        pupApndFrame.style.left = 0;
      }

      /* Initialize for confirm_apply.stm. */
      function initializeApplyPopup(iframe) {
        $(iframe).css('position', 'relative');
        // initialize event handler.
        // [OK] button event
        $(iframe).contents().find('#api_buttonConfirmYes').on('click', function() {
          irpapi.dispOnWaiting();
          top.sendVrfyRequest(true, displayRestartPopup);
        });
        // [Cancel] button event
        $(iframe).contents().find('#api_buttonConfirmNo').on('click', function() {
          top.sendVrfyRequest(false, cancelApplyCallback);
        });
        // [Close] button event
        $(iframe).contents().find('#api_buttonConfirmClose').on('click', function() {
          top.sendVrfyRequest(false, cancelApplyCallback);
        });
        // [ALL] tab event
        $(iframe).contents().find('#api_confirmTabAll').on('click', function() {
          setApplyData(iframe, applyDcsAll);
        });
        // [Difference] tabl event
        $(iframe).contents().find('#api_confirmTabDiff').on('click', function() {
          setApplyData(iframe, applyDcsDiff);
        });

        var applyActiveElem = document.activeElement;
        if (applyActiveElem != undefined) {
          applyActiveElem.blur(); // If there is an active element, defocus it.
        }
        $(iframe).contents().find('#api_buttonConfirmYes').focus(); // focus on [OK]

        // Get Apply data from file.
        getDCSDiffData(iframe);

        // making popup prompting to restart
        function displayRestartPopup(status) {
          iframe.contentWindow.setAlreadyVefified();
          top.IHMIComponents.cf.removeFrame(iframe);
          irpapi.dispOffWaiting();
          if (status != top.IO_SUCCESS) {
            irpapi.inner.callDCSCallback(dcsCallback, status, CONFAPPLY_APPLYFAILURE, dcsCbArgs);
            return;
          }
          irpapi.openAlertMessage(pupWin, 'applydcs_restartbody', '', function() {
            irpapi.inner.callDCSCallback(dcsCallback, status, CONFAPPLY_SUCCESS, dcsCbArgs);
          });
        }
        // back to dcs screen
        function cancelApplyCallback(status) {
          iframe.contentWindow.setAlreadyVefified();
          top.IHMIComponents.cf.removeFrame(iframe);
          var cancelApplyCode = (status != top.IO_SUCCESS) ? CONFAPPLY_APPLYFAILURE : CONFAPPLY_CONFCANCEL;
          irpapi.inner.callDCSCallback(dcsCallback, status, cancelApplyCode, dcsCbArgs);
        }
        // setup apply text data.
        function setApplyData(iframe, applydata) {
          var confirmArea = $(iframe).contents().find('#api_confirmArea');
          $(confirmArea).empty();
          var contentApply = $('<div>');
          var applyHtml = '<pre>' + applydata.textContent + '</pre>';
          contentApply.html(applyHtml);
          $(confirmArea).append(contentApply);
          var blankArea = $(iframe).contents().find('#blankArea');
          blankArea.css('padding-top', confirmArea.outerHeight(true));
        }
        /* Initialize Tab selection to Diff. */
        function initSelectTab(iframe) {
          var tabArgObj = {showTabObj: {
            'dcs_confirm_all' : true,
            'dcs_confirm_diff' : true
          }};
          var tabAreaElem = $(iframe).contents().find('#api-confirm-tabarea')[0];
          tabAreaElem.refresh('dcs_confirm_all', 2, 'dcs_confirm_diff', false, tabArgObj);
          // for iOS behavior
          if (irpapi.global.isiPad) {
            var $tabAll = $(iframe).contents().find('#api_confirmTabAll');
            var $tabDiff = $(iframe).contents().find('#api_confirmTabDiff');
            $tabAll.blur(); // hide software keyboard
            $tabDiff.blur(); // hide software keyboard
          }
        }
        // File read error
        function fileReadError() {
          // call callback
          irpapi.inner.callDCSCallback(dcsCallback, CONFAPPLY_UNEXPECTED, CONFAPPLY_READERROR, dcsCbArgs);
        }

        // Get Apply data from server file.
        var applyDcsDiff = '';
        var applyDcsAll = '';
        function getDCSDiffData(iframe) {
          $.ajax({
            url: '/md:\\DCSCHG01.DG',
            type: 'GET',
            dataType: 'text',
            timeout: 10000,
            success: function (res) {
              applyDcsDiff = extractApplyText(res);
              getDCSAllData(iframe);
            },
            error: function (xmlhttprequest, textstatus, errorthrown) {
              fileReadError();
              irpapi.inner.apiDebugLog(textstatus + ':' + errorthrown);
            }
          });
        }
        function getDCSAllData(iframe) {
          $.ajax({
            url: '/md:\\DCSVRFY.DG',
            type: 'GET',
            dataType: 'text',
            timeout: 30000,
            success: function (res) {
              applyDcsAll = extractApplyText(res);
              setApplyData(iframe, applyDcsDiff);
              initSelectTab(iframe);
            },
            error: function (xmlhttprequest, textstatus, errorthrown) {
              fileReadError();
              irpapi.inner.apiDebugLog(textstatus + ':' + errorthrown);
            }
          });
        }
        // Parse and extract apply text from file(HTML).
        function extractApplyText(htmlText) {
          var domPsr = new DOMParser();
          var applyDoc = domPsr.parseFromString(htmlText, 'text/html');
          var targetElem = applyDoc.querySelector('PRE');
          return targetElem;
        }
      }
    },
    /* Process of Confirm local code set.
       codeKind : 1=base, 2=position/speed check, 4=I/O connect.
       codeStr : Code string.
       callback : Function (status, isSet, cbArg)
       isSet Boolean true=is set, false=Not set.
       cbArg Object callback arguments
    */
