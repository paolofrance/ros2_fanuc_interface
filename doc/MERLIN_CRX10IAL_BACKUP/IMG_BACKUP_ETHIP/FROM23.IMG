    // Prevent the "x" button on the monitor and the number display from overlapping
    if (window.frameElement.id !== "prim" && window.frameElement.id !== "dual" && window.frameElement.id !== "third") {
      document.getElementById('page_right_area').style.paddingRight = "55px";
    }
}

/* 
 * Correct Page Layout(safari or eg)
 */
function correctPageLayout() {
    var lang = getCurrentLanguage();
    if (lang == 'jp'
        || lang == 'ch') {
        var userAgent = window.navigator.userAgent.toLowerCase();
        if (userAgent.indexOf('chrome') == -1
            && userAgent.indexOf('safari') != -1) {
            $('#page_unit').css('margin-top', '1px');
            $('#page_item_unit').css('margin-top', '-3px');
            $('#page_item_max_unit').css('margin-top', '-3px');
        }
    } else {
        $('#page_slash').css('margin-top', '1px');
        $('#page_max').css('margin-top', '6px');
        $('#page_unit').css('margin-top', '6px');
    }
}
/*
 * Initialize Page Area Data
 */
function initPageArea(item_max, page_cur_no) {
  // set variable and css
  g_current_item_max = item_max;

  if (page_cur_no == undefined) {
    page_cur_no = 1;
  }

  if (Math.ceil(g_current_item_max / GRID_ROW_MAX_COUNT) < page_cur_no){
    g_current_item_no_start = 1;
    g_current_page_no = 1;
  } else {
    g_current_item_no_start = (page_cur_no - 1) * GRID_ROW_MAX_COUNT + 1;
    g_current_page_no = page_cur_no;
  }

  g_current_page_max = Math.ceil(g_current_item_max / GRID_ROW_MAX_COUNT);
  if (g_current_page_max == 0) {
    // just in case, if the number of displayed data is 0, it is displayed as "1/1 page".
    g_current_page_max = 1;
  }
  if (g_current_page_max == g_current_page_no) {
    g_current_item_no_end = g_current_item_max;
  } else {
    g_current_item_no_end = g_current_item_no_start + (GRID_ROW_MAX_COUNT - 1);
  }

  updatePageButton();
  // Set a time interval to correctly set the page number for IE
  setTimeout("OntimerSetPageHtml()", 10);
}

/*
 * Set Page Area html
 */
function OntimerSetPageHtml() {
    var sel_btn = $('#page_btn');
    $('#page_num').text(g_current_page_no);
    $('#page_num').append(sel_btn);
    $('#page_max').html(g_current_page_max);
    if (g_current_item_max == 0) {
      // just in case, if the number of displayed data is 0, it is displayed as "0 pt / 0 pt".
      $('#page_item_num').html(g_current_item_max);
    } else {
      $('#page_item_num').html(g_current_item_no_start + "-" + g_current_item_no_end);
    }
    $('#page_item_max').html(g_current_item_max);
}

/*
 * Set page area event
 */
function setPageAreaEvent(entryFunc) {
    $("#page_prevbtn").on('click touchend', function (event) {
        if(event.type.indexOf('touch') >= 0){
          event.preventDefault();
        }
        if (g_current_page_no <= 1) {
            return;
        }
        var sel_btn = $('#page_btn');
        g_current_page_no = g_current_page_no - 1;
        $('#page_num').text(g_current_page_no);
        $('#page_num').append(sel_btn);

        updatePageButton();
        calcItemNum();
        setTimeout(function () {
            entryFunc(g_current_item_no_start, g_current_item_no_end);
        }, 0);
    });
    $("#page_nextbtn").on('click touchend', function (event) {
        if(event.type.indexOf('touch') >= 0){
          event.preventDefault();
        }        
        if (g_current_page_no >= g_current_page_max) {
            return;
        }
        var sel_btn = $('#page_btn');
        g_current_page_no = g_current_page_no + 1;
        $('#page_num').text(g_current_page_no);
        $('#page_num').append(sel_btn);

        updatePageButton();
        calcItemNum();
        setTimeout(function () {
            entryFunc(g_current_item_no_start, g_current_item_no_end);
        }, 0);
    });
    $("#page_num").on('click', function (event) {
        if (g_current_page_max == 1) {
            return;
        }

        var sel_menu = $('.page_frame');

        if (sel_menu.length) {
            var target = $(event.target);
            $('.page_frame').remove();
            $('#pageimg').attr('src', '../common/images/page_select_icon.png');

            if (target.hasClass('page_sel')) {
                var sel_btn = $('#page_btn');
                g_current_page_no = Number(target.text());
                $('#page_num').text(g_current_page_no);
                $('#page_num').append(sel_btn);
                updatePageButton();
                calcItemNum();
                setTimeout(function () {
                    entryFunc(g_current_item_no_start, g_current_item_no_end);
                }, 0);
            }
            return;
        } else {
            $('#pageimg').attr('src', '../common/images/page_select_icon_open.png');

            var menu_frame = $('<div></div>', {
                addClass: "page_frame"
            });

            for (var i = 1; i <= g_current_page_max; i++) {
                var menu_sel = $('<div></div>', {
                    addClass: "page_sel"
                });

                menu_sel.text(i);

                if (i == g_current_page_no) {
                    menu_sel.addClass('page_select');
                }
                if (i == g_current_page_max) {
                    menu_sel.addClass('page_end');
                }

                menu_frame.append(menu_sel);
            }

            $(this).append(menu_frame);
            return;
        }
    });
    $("#page_num").on('blur', function (event) {
        var sel_menu = $('.page_frame');
        if (sel_menu.length) {
            $('#pageimg').attr('src', '../common/images/page_select_icon.png');
            $('.page_frame').remove();
        }
        return;
    });
}

/*
 * Set color change in page button
 */
function updatePageButton() {
    if (g_current_page_no > 1) {
        if (g_current_page_max == 1) { // just in case
            $('#page_prevbtn').removeAttr("tabindex");
            $('#page_prevbtn').css('background-image', 'url(../common/images/page_prev_disable.png)');
        } else {
            $('#page_prevbtn').attr('tabindex', '1');
            $('#page_prevbtn').css('background-image', 'url(../common/images/page_prev.png)');
        }
    } else {
        $('#page_prevbtn').removeAttr("tabindex");
        $('#page_prevbtn').css('background-image', 'url(../common/images/page_prev_disable.png)');
    }

    if (g_current_page_no == g_current_page_max) {
        $('#page_nextbtn').removeAttr("tabindex");
        $('#page_nextbtn').css('background-image', 'url(../common/images/page_next_disable.png)');
    } else {
        $('#page_nextbtn').attr('tabindex', '3');
        $('#page_nextbtn').css('background-image', 'url(../common/images/page_next.png)');
    }

    if(g_current_page_max > 1) {
      $('#pageimg').attr('src', '../common/images/page_select_icon.png');
    } else {
      $('#pageimg').attr('src', '../common/images/page_select_icon_disable.png');
    }
}

/*
 * Calculate Display Item
 */
function calcItemNum() {
    g_current_item_no_start = GRID_ROW_MAX_COUNT * (g_current_page_no - 1) + 1;
    if (g_current_item_no_start + (GRID_ROW_MAX_COUNT - 1) <= g_current_item_max) {
        g_current_item_no_end = g_current_page_no * GRID_ROW_MAX_COUNT;
    } else {
        g_current_item_no_end = g_current_item_max;
    }
    if (g_current_item_max == 0) {
      // just in case, if the number of displayed data is 0, it is displayed as "0 pt / 0 pt".
      $('#page_item_num').html(g_current_item_max);
    } else {
      $('#page_item_num').html(g_current_item_no_start + "-" + g_current_item_no_end);
    }
}

