/**
 * Callback(Get Position Register Value)
 * (IN) status   : return status.
 *      grp_num  : group number.
 *      index    : register number (1, 2, ...).
 *      str      : position register value.
 *      callback : callback function to get position.
 */
function getPosRegCallback(status, grp_num, index, str, callback) {
  var posdata = {};
  var postype = null;
  var comment = "";
  if (status == IO_SUCCESS) {
    comment = str.substr(0, str.indexOf('\n'));
    comment = comment.slice(1, -2);
    var temp = str.substr(str.indexOf('\n'));
    if (temp.indexOf('Config:') != -1) {
      postype = COORDINATECART;
      posdata = getPosRegData(str);
    } else if (temp.indexOf('J1 =') != -1) {
      postype = COORDINATEJOINT;
      posdata = getPosRegJointData(str);
    } else {
      postype = COORDINATEUNINIT;
    }
  }
  callback(status, index, postype, posdata, comment);
}

