    },
    /* 
      * Analyze of position data.(multi group)
      * (IN) str     : Position string.
      *                 ex) ['Group: 1 Config: N U T, 0, 0, 0 X: 1807.000 '+
      *                     'Y: 0.000 Z: 1300.000 W: -180.000 P: -90.000 R: 0.000', ~]
      *                 ex) ['Group: 1 J1 = 0.000 deg J2 = 0.000 deg J3 = 0.000 deg '+
      *                     'J4 = 0.000 deg J5 = 0.000 deg J6 = 0.000 deg', ~]
      *                 ex) ['Group: 2 Uninitialized] // If uninitialized.
    */
   analyzeMultiPositionStr: function(str) {
      var posAnlArray = [];
      var searchStr;
      var frame;

      if (Array.isArray(str)) { // Multi group
        for (var i = 0; i < str.length; i++) {
          searchStr = str[i];
          frame = getFrame(searchStr);
          posAnlArray.push(irpapi.analyzePositionStr(frame, searchStr, {}));
        }
      } else { // Single group
        frame = getFrame(searchStr);
        posAnlArray.push(irpapi.analyzePositionStr(frame, str, {}));
      }

      function getFrame(searchStr) {
        if (searchStr.indexOf('Uninitialized') >= 0) {
          return top.COORDINATEUNINIT;
        }
        var matchFrame = (searchStr.match(/X:\s*([0-9.-]+)\s*/) !== null) ?
          top.COORDINATECART : top.COORDINATEJOINT;
        return matchFrame;
      }

      return posAnlArray;
    },
    /* 
     * Analyze of position data.
     * (IN) frame   : Frame type (Cartesian/Joint)
     *      str     : Position string.
     *                 ex) 'Group: 1 Config: N U T, 0, 0, 0 X: 1807.000 '+
     *                     'Y: 0.000 Z: 1300.000 W: -180.000 P: -90.000 R: 0.000'
     *                 ex) 'Group: 1 J1 = 0.000 deg J2 = 0.000 deg J3 = 0.000 deg '+
     *                     'J4 = 0.000 deg J5 = 0.000 deg J6 = 0.000 deg'
     *      posData : Position record.
    */
    analyzePositionStr: function(frame, str, posData) {
      var anlPos = {};
      var innerFunc = irpapi.inner.func;
      if (frame == top.COORDINATECART) {
        anlPos = cartesian(str, posData);
      } else if (frame == top.COORDINATEJOINT) {
        anlPos = joint(str, posData);
      }
      return anlPos;

      // Analyze cartesian data.
      function cartesian(str, posData) {
        // initialize rep
        var cartRep = top.POS_REP_CART;
        // Position data analize.
        var targetWord = ''; // Target word
        // Repeat for the number of characters
        var chIndex = innerFunc.skipToSpecChar(0, str, 'G');
        for (; chIndex < str.length; chIndex++) {
          chIndex = innerFunc.skipWhileSpecChar(chIndex, str, ' '); // Skip ' '
          // Extract up to ':'
          var strInfo = {};
          var ch = str.charAt(chIndex);
          if (ch == ':') {
            chIndex++;
            if (targetWord == 'Group') {
              chIndex = innerFunc.skipWhileSpecChar(chIndex, str, ' '); // Skip ' '
              strInfo = innerFunc.extractChars(chIndex, str); // Extract characters
              posData.GP = parseInt(strInfo.word); // Set group number
              chIndex = strInfo.chIndex; // Update index
            } else if (targetWord == 'Config') {
              // Retrieve up to the next ':' and return from ':' to ' '.
              var chStart = chIndex;
              chIndex = innerFunc.skipToSpecChar(chIndex, str, ':');
              chIndex = innerFunc.backToSpecChar(chIndex - 1, str, ' ');
              // Make the extracted string to string of 'Config'
              posData.Config = str.substring(chStart, chIndex).trim();
            } else if (targetWord.startsWith('EXT')) {  // Extend axes data exist.
              cartRep = top.POS_REP_BOTH;
              chIndex = innerFunc.skipWhileSpecChar(chIndex, str, ' '); // Skip ' '
              strInfo = innerFunc.extractChars(chIndex, str); // Extract characters
              if (targetWord.indexOf(':') < 0) {
                targetWord = targetWord + ':';
              }
              var targetKey = irpapi.convertExtElem(targetWord);
              posData[targetKey] = parseFloat(strInfo.word);
              chIndex = strInfo.chIndex + 1;
              chIndex = innerFunc.skipToSpecChar(chIndex, str, ' ') + 1;  // Skip unit
            } else { // X to R and UF/UT
              // The following is due to the addition of functions in recPos_multi
              targetWord = targetWord.trim(); // 'UF:' measures.
              chIndex = innerFunc.skipWhileSpecChar(chIndex, str, ' '); // Skip ' '
              strInfo = innerFunc.extractChars(chIndex, str); // Extract characters
              posData[targetWord] = parseFloat(strInfo.word);
              chIndex = strInfo.chIndex;
            }
            targetWord = '';
          } else {
            targetWord += ch;
          }
        }
        posData.rep = cartRep;
        return posData;
      }
      // Analyze joint data.
      function joint(str, posData) {
        // Except for the group,
        // trim () the character string extracted up to '=' as each axis key.
        // Skip whitespace and extract up to the next blanks to make each axis value.
        // Skip whitespace and skip to the next blank as a unit.
        // Its iteration ...

        // Position data analyze.
        // Repeat for the number of characters
        // Repeat for the number of characters
        var chIndex = innerFunc.skipToSpecChar(0, str, 'G');
        chIndex = innerFunc.skipToSpecChar(chIndex, str, ' ') + 1;
        var strInfo = {};
        strInfo = innerFunc.extractChars(chIndex, str); // Extract characters
        posData.GP = parseInt(strInfo.word);  // Set group number
        chIndex = strInfo.chIndex + 1;
        for (; chIndex < str.length; chIndex++) {
          chIndex = innerFunc.skipToSpecChar(chIndex, str, ' ') + 1;  // Skip to '='
          chIndex = innerFunc.skipWhileSpecChar(chIndex, str, ' ');   // Skip to value
          strInfo = innerFunc.extractChars(chIndex, str); // Extract characters
          var targetKey = strInfo.word.trim();
          if (targetKey.length == 0) {
            continue; // Illegal keyword
          }
          // Keyword translate when analize UF:/UT:
          var clnidx = targetKey.indexOf(':');
          if (clnidx >= 0) {
            targetKey = targetKey.slice(0, clnidx);
          }
          chIndex = strInfo.chIndex;
          // UF / UT has different expressions
          if ((targetKey == 'UF') || (targetKey == 'UT')) {
            chIndex = innerFunc.skipWhileSpecChar(chIndex, str, ' '); // Skip to value
            strInfo = innerFunc.extractChars(chIndex, str);
            chIndex = strInfo.chIndex - 1; // (UF: 0 UT: 1) nextIndex back to 0.
          } else {
            chIndex = innerFunc.skipWhileSpecChar(chIndex, str, ' '); // Skip to value
            chIndex = innerFunc.skipWhileSpecChar(chIndex, str, '='); // Skip to value
            chIndex = innerFunc.skipWhileSpecChar(chIndex, str, ' '); // Skip to value
            strInfo = innerFunc.extractChars(chIndex, str);
            targetKey = irpapi.convertExtElem(targetKey);
            chIndex = strInfo.chIndex + 1;
            chIndex = innerFunc.skipToSpecChar(chIndex, str, ' ') + 1;  // Skip unit
          }
          posData[targetKey] = parseFloat(strInfo.word);
        }
        // [P19: recPos_multi]
        // The code below may not be needed, but I'll comment it out and leave it.
        //if (typeof posData['UF:'] !== 'undefined') { // analyze error
        //  posData['UF'] = str.slice(str.indexOf('UF:') + 3, str.indexOf('UT:')).trim();
        //  posData['UT'] = str.slice(str.indexOf('UT:') + 3, str.length).trim();
        //  if ((!isNaN(posData['UF'])) && (!isNaN(posData['UT']))) {
        //    posData['UF'] = Number(posData['UF']);
        //    posData['UT'] = Number(posData['UT']);
        //  }
        //}
        posData.rep = top.POS_REP_JOINT;
        return posData;
      }
    },
    /* 
     * Merges the two data passed to it and executes editPos.
     * (IN) progName : Program name
     * (IN) meargeArgs : Merge data object
     *          .baseData  : Position object
     *          .mergeData : Position object (Overwriters)
     *           ex) {
     *                 0: {axes: 0, cart: {X: 000 ...}}
     *                 1: {axes: 0, joint: {J1: 000 ...}}
     *               }
     *          .armType   : 27=Independent axis, 0=Other
    */
    editMergePositionData: function (progName, mergeArgs, callback) {
      var baseData = mergeArgs.baseData;
      var mergeData = mergeArgs.mergeData;
      var armType = mergeArgs.armType;
      var sortPosData = {};
      var sortPosCnt  = [].concat(Object.keys(baseData)).concat(Object.keys(mergeData));
      var cmtList = {};

      // Sort the base data while overwriting it with merge data
      for (var i = 0; i < sortPosCnt.length; i++) {
        var trgtPosData = ((i >= Object.keys(baseData).length) ? mergeData[sortPosCnt[i]] : baseData[sortPosCnt[i]]);
        sortPosData[trgtPosData.posNumber] = sortPosData[trgtPosData.posNumber] || {};
        sortPosData[trgtPosData.posNumber][trgtPosData.group] = trgtPosData;
        cmtList[trgtPosData.posNumber] = trgtPosData.comment;
      }

      var mergeInfo = {uf: mergeData[0].uf, ut: mergeData[0].ut, conf: mergeData[0].conf,
        frame: mergeData[0].frame, rep: mergeData[0].rep};
      // Analyze the generated object into a string that can be sent to editPos
      top.editPosMulti(progName, makePositionDataStr(sortPosData, cmtList, mergeInfo), callback); // io

      function makePositionDataStr(posObj, cmtdata, mergeInfo) {
        var posLangRes = irpapi.global.posCmdTable[(irpapi.global.currentLang == 'jp') ? 'jp' : 'eg'];
        var refAxisVar = irpapi.global.axisData;
        var posText = ''; // return string

        for (var key in posObj) {
          // Gen. pos header
          var posObjIdx = Object.keys(posObj[key]);
          posText = posLangRes.pos + '[' + key;
          posText += ((cmtdata[key] != '') ? (':"' + cmtdata[key] + '"') : '') + ']{' + CODECRLF;

          for (var j = 0; j < posObjIdx.length; j++) {
            var trgtPos = posObj[key][posObjIdx[j]];
            var isJoint = (trgtPos.rep == POS_REP_JOINT);
            var trgtArm = armType[trgtPos.group];
            if ((trgtPos.rep == POS_REP_NONE) && (trgtArm != 27)) { // uninitialized position.
              trgtPos.uf = mergeInfo.uf;
              trgtPos.ut = mergeInfo.ut;
              trgtPos.conf = mergeInfo.conf;
              trgtPos.frame = mergeInfo.frame;
            }
            var validArr = refAxisVar.valid[trgtPos.group][(isJoint ? 'joint' : 'cart')];

            // Group, UT, UF
            // When uninitialized, UF/UT set 'F'.
            posText += '   GP' + trgtPos.group + ':' + CODECRLF;
            posText += CODETAB + 'UF : ' + (((trgtPos.uf !== undefined) && (!trgtPos.empty)) ? trgtPos.uf : 'F') + ', ';
            posText += 'UT : ' + (((trgtPos.ut !== undefined) && (!trgtPos.empty)) ? trgtPos.ut : 'F') + ', ';

            // Config data
            var confStr = (!trgtPos.empty) ? trgtPos.conf : '';
            posText += (isJoint ? '' :
                        CODETAB + CODETAB + posLangRes.conf + ':\'' + confStr + '\',') + CODECRLF;

            // position data
            // Robot axis
            var axisCount = ((!isJoint) || (trgtPos.empty) && (trgtArm == 27)) ? 6 : refAxisVar.robot[trgtPos.group];
            // Identify the key where the real value is located. except ext axis.
            var coordKey = isJoint ? 'joint' : 'cart';
            for (var k = 0; k < axisCount; k++){
              // Maybe no need below 3.
              //if (!trgtPos[coordKey]) {
              //  continue; // If we can get the number successfully, we will add it to the string
              //}
              var trgtKey = validArr.axis[k]; // X - R or Jx
              var mainPosData = trgtPos[coordKey][trgtKey]; // position value
              if (trgtPos.empty) { // uninitialized
                mainPosData = null; // set to empty string
              }
              if (k > 0) {
                posText += ','; // data continue
                if ((k % 3) == 0) {
                  posText += CODECRLF; // Line feed
                }
              }
              posText += CODETAB + trgtKey + '= ' + convFtoA(mainPosData) + ' ' + validArr.unit[k]; // Axis name + Position value
            }
            // Extend axis
            var extendAxes = refAxisVar.ext[trgtPos.group];
            if (extendAxes > 0) {
              posText += ',' + CODECRLF;
              for (var ext = 0; ext < extendAxes; ext++) {
                if (ext > 0) {
                  posText += ',';
                }
                var trgtExtKey = validArr.axis[ext + axisCount]; // EXTn
                var trgtExtName = trgtExtKey.replace('EXT', 'E');
                var extPosData = trgtPos['ext'][trgtExtName]; // position value
                // Remedies when the axis name is not as expected.
                if (extPosData === undefined) {
                  extPosData = trgtPos['ext'][trgtExtKey]; // Maybe it's handled by EXT.
                }
                if (trgtPos.empty) { // uninitialized
                  extPosData = null; // set to empty string
                }
                posText += CODETAB + 'E' + (ext + 1) + '= ' + convFtoA(extPosData);
                // The unit of the extend axis is specified from the continuation of the robot axis.
                posText += ' ' + validArr.unit[ext + axisCount];
              }
            }
            posText += CODECRLF;
          }
          // footer
          posText += '};' + CODECRLF;
        }

        return posText;
      }
      function convFtoA(fVal) {
        if (fVal === null) {
          return ' ********'; // uninitialzed string for I/F
        }
        // 'toFixed' is only Number, so convert if it is a string.
        if (typeof fVal === 'string') {
          fVal = parseFloat(fVal);
        }
        var fStr = ' ' + fVal.toFixed(6); // Rounded to 6 decimal places
        return fStr;
      }
    },
    /*
    * Get UF,UT value from system-variable.
    * (IN) grp : Group number
    *      callback : A callback function to get UF/UT value
    *          arg1 : uf/ut value... arg1.grp is group num, arg1.UF is UF value, arg1.UT is UT value.
    */
    getUFUTValue: function(grp) {
      irpapi.global.getCompUfUtValue = false; // Initialize 'complete flag'.
      var ufutVal = {};
      ufutVal.grp = grp;
      top.rpcmc_getVar(top.SYSNAME_C, '$MNUFRAMENUM[' + grp + ']', getUFValueCallback, ufutVal, getUfUtValueCallback);

      function getUFValueCallback(prog_name, var_name, type_code, ufStr, ufutVal, callback) {
        ufutVal.UF = parseInt(ufStr);
        top.rpcmc_getVar(top.SYSNAME_C, '$MNUTOOLNUM[' + ufutVal.grp + ']', getUTValueCallback, ufutVal, callback);
      }
      function getUTValueCallback(prog_name, var_name, type_code, utStr, ufutVal, callback) {
        ufutVal.UT = parseInt(utStr);
        callback(ufutVal);  // getUfUtValueCallback()
      }
      /* Management of UF/UT value from system variable. */
      function getUfUtValueCallback(ufutVal) {
        irpapi.global.ufutSystemValues[ufutVal.grp - 1] = ufutVal;
        if (ufutVal.grp < irpapi.getGroupCount()) {
          irpapi.getUFUTValue(ufutVal.grp + 1);	// see above
          return;
        }
        irpapi.global.getCompUfUtValue = true;
        irpapi.global.retryInitUfUtCount = 0;
      }
    },
    /*
    * Queries whether the UF / UT value has been obtained.
    * return : ture = Completed. false = Not yet.
    */
    isGetUfUtValue: function() {
      return irpapi.global.getCompUfUtValue;
    },
    /*
    * Watch get uf/uf value. Retry until complete.
    * (IN) callback : Callback function when get UF/UT value is completed.
    * (IN) cbArg : Argument object to be passed to the callback function.
    */
    watchGetUfUtVal: function(callback, cbArg) {
      if (!irpapi.isGetUfUtValue()) { // Acquired UF/UT values?
        //console.log('watchGetUfUtVal: ' + retryInitUfUtCount + 'time(s)');
        setTimeout(function () {
          irpapi.global.retryInitUfUtCount++;
          irpapi.watchGetUfUtVal(callback, cbArg);
        }, 100);
        return;
      }
      irpapi.global.retryInitUfUtCount = 0;
      callback(cbArg);
    },
    /*
    * get UF/UT value.
    * (IN) groupNum : group number
    * return : UF/UT System value of target group.
    */
    getUfUtSystemValue: function(groupNum) {
      return irpapi.global.ufutSystemValues[groupNum - 1];
    },
    /*
    * Update UF/UT System value of global.
    * (IN) grp : group number.
    *      frameIndex : User frame or User tool.
    *      frameNum : Frame number value.
    */
    updateUfUtValue: function(grp, frameIndex, frameNum) {
      var ufutVal = irpapi.global.ufutSystemValues[grp - 1];
      var ufutFrm = (typeof frameNum === 'string') ? parseInt(frameNum, 10): frameNum;
      if (ufutVal === undefined) {
        ufutVal = {};
      }
      if (frameIndex == FRMVAL_USER) {
        ufutVal.UF = ufutFrm;
      } else if (frameIndex == FRMVAL_TOOL) {
        ufutVal.UT = ufutFrm;
      }
      irpapi.global.ufutSystemValues[grp - 1] = ufutVal;
    },
    /*
    * Create new position record.
    * return : Position record element.
    */
    createNewPosElement: function() {
      var newPosElem = {
        group: 0, frame: -1, uf: 0, ut: 0, conf: '',
        cart: {X: 0.0, Y: 0.0, Z: 0.0, W: 0.0, P: 0.0, R: 0.0},
        joint: {J1: 0.0, J2: 0.0, J3: 0.0, J4: 0.0, J5: 0.0, J6: 0.0},
        ext: {E1: 0.0, E2: 0.0, E3: 0.0}};
      return newPosElem;
    },
    /*
    * Exchange Position Data / Position Register.
    * Cartesian to Joint, Joint to Cartesian.
    * Usage : At first, set data to exchgInfo,
    *         2nd, call exchangePosFrame(),
    *         3rd, set converted data to table. (by callback function)
    * (IN) exchgInfo : Exchange Record Data Information. (!!Set in advance!!) (See execExchangePosFrame())
    *                  .srcRec : Source Record data.  = w2ui[tbl].get(recid);
    *                  .newRec : Target new record. (Set the default value in advance.)
    *                  .exchgTo : Convert to COORDINATECART/COORDINATEJOINT?
    *                  .cb : Callback function to receive the converted record data.
    *                  .tbl : Position Table Kind. 0=Position Data, 1=Position Register.
    *                  .grp : Group number.
    *                  .robot : Robot Axes count.
    *                  .ext : Extend Axes count.
    *                  .unit : Current group Axes unit array. [J1, J2, J3, J4, J5, J6, EXT1, EXT2, EXT3]
    *                  .UFVal : Target UF value. ex) PosReg = 'F'
    *                  .UTVal : Target UT value. ex) same as above.
    *                  .orgData :exchgInfo.orgData is for REPRE of position register.
    */
    // After get the exchange position and converting it, call the callback function.
    exchangePosFrame: function(exchgInfo) {
      // Construct Exchange Input Position Data
      var posStr = irpapi.constructExchangePosText(exchgInfo);
      // Call interface.
      top.rpcmc_mnchgrep(posStr, exchangePosCallback, exchgInfo); // rpcmc

      function exchangePosCallback(status, posDataStr, exchgInfo) {
        // Analyze get string of exchanged data.
        var exchgRec = exchgInfo.newRec;
        if ((status == IO_SUCCESS) && (posDataStr != undefined) && (posDataStr.length > 0)) {
          if (exchgInfo.exchgTo == COORDINATECART) {
            var anlArg = {};
            anlArg.rec = exchgRec;
            anlArg.tblKind = exchgInfo.tbl;
            exchgRec = irpapi.analyzeCartesianPosition(posDataStr, anlArg);
          } else if (exchgInfo.exchgTo == COORDINATEJOINT) {
            exchgRec = irpapi.analyzeJointPosition(posDataStr, exchgRec);
            exchgRec.rep = POS_REP_JOINT;
            exchgRec.force = true;
          } else {
            exchgRec = null;
          }
          if (exchgRec != null) {
            // Set UF / UT value according to table kind and current value.
            if (exchgInfo.tbl == POSTBLKIND_DATA) {
              // If the UF / UT value has already been set, it use the set value.
              // Otherwise, the default value is used.
              exchgRec.UF = (exchgRec.UF !== '') ? exchgRec.UF : irpapi.getUfUtSystemValue(exchgRec.GP).UF;
              exchgRec.UT = (exchgRec.UT !== '') ? exchgRec.UT : irpapi.getUfUtSystemValue(exchgRec.GP).UT;
            } else {
              exchgRec.UF = POS_REG_UF_VAL;
              exchgRec.UT = POS_REG_UT_VAL;
            }
          }
        } else {
          exchgRec = null;
        }
        // Call Original callback function
        exchgInfo.cb(status, exchgRec, exchgInfo.newRec.tbl, exchgInfo.orgData);
      }
    },
    /* 
    * Construct text for exchange position data.
    * ex) 'gnum: n rep: n axes: n utool: n uframe: n Config: 'xxxxx' X: fff.fff ... '
    *     'gnum: n rep: 9 axes: n utool: n uframe: n Config:  J1 = fff.fff unit ... '
    *     when exist extend axes ... 'EXTn: fff.fff unit'
    * (IN) exchgInfo : Exchange information object. Please refer exchangePosFrame().
    * return : Exchange text.
    */
    constructExchangePosText: function(exchgInfo) {
      var recWork = JSON.parse(JSON.stringify(exchgInfo.srcRec)); // Copy record data.
      // Group number
      var posText = 'gnum: ' + exchgInfo.grp;
      // Rep mode. Cartesian=2, Cartesian+Extend=6, Joint=9
      var workRep = (recWork.rep != undefined) ? recWork.rep :
        (recWork.frame == COORDINATECART) ? ((exchgInfo.ext > 0) ? POS_REP_BOTH : POS_REP_CART) : POS_REP_JOINT;
      posText += ' rep: ' + workRep;
      // Axes count.
      var axesCount = (workRep == POS_REP_JOINT) ? exchgInfo.robot + exchgInfo.ext :
        (workRep == POS_REP_BOTH) ? exchgInfo.ext : 0;
      posText += ' axes: ' + axesCount;
      // UT/UF
      posText += ' utool: ' + getUTUFDataText(recWork.UT, exchgInfo.UTVal);
      posText += ' uframe: ' + getUTUFDataText(recWork.UF, exchgInfo.UFVal);
      // Config
      posText += ' Config: ' + ((recWork.frame == COORDINATECART) ? recWork.Config : '');
      posText += ' ';	// add separator
      // position data
      if (recWork.frame == COORDINATECART) {	// Cartesian
        posText += CODETAB + 'X: ' + Number(recWork.X).toFixed(6);
        posText += CODETAB + 'Y: ' + Number(recWork.Y).toFixed(6);
        posText += CODETAB + 'Z: ' + Number(recWork.Z).toFixed(6);
        posText += CODETAB + 'W: ' + Number(recWork.W).toFixed(6);
        posText += CODETAB + 'P: ' + Number(recWork.P).toFixed(6);
        posText += CODETAB + 'R: ' + Number(recWork.R).toFixed(6);
      } else {	// Joint
        for (var i = 0; i < exchgInfo.robot; i++) {
          var axesName = 'J' + (i + 1);
          posText += CODETAB + axesName + ' = ' + Number(recWork[axesName]).toFixed(6);
          posText += ' ' + exchgInfo.unit[i];
        }
      }
      if (exchgInfo.ext > 0) {
        for (var ext = 1; ext <= exchgInfo.ext; ext++) {
          posText += CODETAB + 'EXT' + ext + ': ' + Number(recWork['EXT' + ext]).toFixed(6);
          posText += ' ' + exchgInfo.unit[ext + 5];
        }
      }
      return posText;

      function getUTUFDataText(uData, uDefault) {
        var uVal = 0;
        if (uData === POSREG_USERVAL) {
          uVal = 255;
        } else {
          uVal = (uData !== '') ? uData : uDefault;
        }
        return uVal;
      }
    },
    /*
    * Parse Program Text Data
    * (IN) fileLines : File line array.
    * return : Parsed object.
    *          progData = {attrArray: [], mnArray: [], posArray: []};
    */
    parseProgramText: function(fileLines) {
      var lsConst = {
        /* === Tag group define for Analyze LS file === */
        TAGPROG : 0,
        TAGATTR : 1,
        TAGAPPL : 2,
        TAGMN : 3,
        TAGPOS : 4,
        TAGEND : 5
      };
      var lsTable = {
        tagNameList : [
          { name: 'PROG', group: lsConst.TAGPROG },
          { name: 'ATTR', group: lsConst.TAGATTR },
          { name: 'APPL', group: lsConst.TAGAPPL },
          { name: 'MN', group: lsConst.TAGMN },
          { name: 'POS', group: lsConst.TAGPOS },
          { name: 'END', group: lsConst.TAGEND }
        ],
        /* === Application data group name list === */
        applNameList: [
          { name: "AUTO_SINGULARITY_HEADER;", group: "singularity" },
          { name: 'LINE_TRACK;', group: 'track' },
          { name: 'ï¾Šï¾Ÿï¾šï¾€ï½²ï½¼ï¾ï¾ï½¸ï¾[', group: 'pallet' },
          { name: 'Palletizing[', group: 'pallet' }
        ],
      };
      var tagGroup = 0;
      var applGroup = -1;
      var posStatus = {stat: 0, group: 0};
      var attrArray = [];
      var applArray = [];
      var mnArray = [];
      var posArray = [];

      for (var i = 0; i < fileLines.length; i++) {
        // Get 1 line data
        var line = fileLines[i];
        // Start at slash?
        if (line.startsWith('/')) { // Header
          var tags = line.split(' '); // Program name is exist after tag name
          var tag = tags[0].substr(1).trim(); // Cut out tag name
          // tag name check
          tagGroup = -1;
          for (var j = 0; j < lsTable.tagNameList.length; j++) {
            if (tag == lsTable.tagNameList[j].name) {
              tagGroup = lsTable.tagNameList[j].group;
            }
          }
          continue;
        }
        // Body data.
        switch (tagGroup) {
          case lsConst.TAGATTR: // Attributes
            var attr = line.split('='); // extract attr-key and attr-val.
            var attrPair = {};
            attrPair.key = attr[0].trim();
            attrPair.val = attr[1].trim();
            attrArray[attrArray.length] = attrPair;
            break;
          case lsConst.TAGAPPL: // Application data
            applArray[applArray.length] = line.trim(); // Store as is.
            break;
          case lsConst.TAGMN: // Line is mnemonic
            var colonPos = line.indexOf(':'); // Skip go colon
            var mnText = line.substr(colonPos + 1);
            var setIndex = mnArray.length;
            var lineNum = parseInt(line.trim());
            // If there is no line number, concatenate it to the previous line.
            if (isNaN(lineNum)) {
              setIndex--;
              mnText = mnArray[setIndex].trim() + ' ' + mnText.trim();
            }
            mnText = (mnText[mnText.length - 1] == ';') ? mnText.substr(0, mnText.length - 1) : mnText;
            mnArray[setIndex] = removeSemicolonEoL(mnText);
            break;
          case lsConst.TAGPOS: // Line is Position data
            posStatus = analizePositionText(line, posStatus, posArray);
            break;
          default:
            // especially none.
            irpapi.inner.apiErrorLog('parseProgramText: May be illegal Tag grp.(' + tagGroup + ')');
            break;
        }
      }
      var progData = {attrArray: attrArray, applArray: applArray, mnArray: mnArray, posArray: posArray};
      return progData;

      /* Remove semicolon at end of line */
      function removeSemicolonEoL(lineStr) {
        var i = lineStr.length - 1;
        for (; i >= 0; i--) {
          if (lineStr[i] != ' ') {
            break;
          }
        }
        lineStr = lineStr.substr(0, i + 1);
        return lineStr;
      }

      /* (inner)
      * Analyze Position data string
      * (IN) line : Line text
      * (IN) status : Analyze target status
      * (IN/OUT) posArray : Position data object array
      * 
      * Detail:
      *     posArray = [{posNumber: 1, comment: '', 
      *                  group: [{group: 1, frame: -1, uf: 0, ut: 1, conf: '', 
      *                  cart: {X: 0.0, Y: 0.0, Z: 0.0, W: 0.0, P: 0.0, R: 0.0}, 
      *                  joint: {J1: 0.0, J2: 0.0, J3: 0.0, J4: 0.0, J5: 0.0, J6: 0.0}, 
      *                  ext: {E1: 0.0, E2: 0.0, E3: 0.0}}]
      *                 }]
      */
      function analizePositionText(line, status, posArray) {
        var posConf = {
              /* === Position data status for Analyze LS file === */
              POSCAPTION : 0,
              POSGROUP : 1,
              POSCOORDINATE : 2,
              POSPOSITION : 3
        };
        // When the position data is '***', there is a bug just before the expansion axis.
        // (The comma at the end of the line is missing.)
        // Normally it is analysis of group, but analysis of extend axis is necessary.
        if ((status.stat == posConf.POSGROUP) && (line.indexOf('GP') < 0)) {
          if (line.indexOf(';') < 0) { // No terminate position data.
            status.stat = posConf.POSPOSITION;
          }
        }
        // Get target position array index.
        // Add element when parsing caption, and update recent elements otherwise.
        var targetIndex = (status.stat == posConf.POSCAPTION) ? posArray.length : posArray.length - 1;
        switch (status.stat) {
          case posConf.POSCAPTION: // Position number
            // pickup number of [n]
            var sqBraLf = line.indexOf('[');
            var sqBraRi = line.indexOf(']', sqBraLf);
            if ((sqBraLf == -1) || (sqBraRi == -1)) {
              return -1;  // Illegal format
            }
            var numStr = line.substring(sqBraLf + 1, sqBraRi);
            var cmntStr = '';
            var cmntPos = numStr.indexOf(':');
            if (cmntPos >= 0) { // comment is exist
              cmntStr = numStr.substr(cmntPos + 1).slice(1, -1); // Remove quotes around it
              numStr = numStr.substring(0, cmntPos);
            }
            var posNum = parseInt(numStr);
            var posData = {};
            posData.posNumber = posNum;
            posData.comment = cmntStr;
            posArray[targetIndex] = posData;  // Add position element
            status.stat = posConf.POSGROUP;
            break;
          case posConf.POSGROUP: // Group number
            // pickup number of GPn:
            var grpPos = line.indexOf('GP');
            var grpEnd = line.indexOf(':', grpPos);
            if ((grpPos == -1) || (grpEnd == -1)) {
              status.stat = posConf.POSCAPTION; // May be close position [};]. Back to position top.
              return status;
            }
            var grpStr = line.substring(grpPos + 'GP'.length, grpEnd);
            var grpNum = parseInt(grpStr);
            var grpData = irpapi.createNewPosElement();
            grpData.group = grpNum;
            var posData_g = posArray[targetIndex];
            if (posData_g.group == undefined) { // 1st group
              posData_g.group = [];
            }
            posData_g.group[(grpNum - 1)] = grpData;
            posArray[targetIndex] = posData_g;
            status.stat = posConf.POSCOORDINATE;
            status.group = grpNum;
            break;
          case posConf.POSCOORDINATE: // Coordinate data
            var posData_c = posArray[targetIndex].group[status.group - 1];
            // Analyze UF, UT, CONFIG...
            var crds = line.split(','); // Split for each comma
            for (var index in crds) {
              var crd = crds[index].split(':'); // Split by colon
              var crdKey = crd[0].trim();
              if (crdKey == 'UF') {
                var ufVal = parseInt(crd[1].trim());
                if (isNaN(ufVal)) {
                  ufVal = '';
                }
                posData_c.uf = ufVal;
              } else if (crdKey == 'UT') {
                var utVal = parseInt(crd[1].trim());
                if (isNaN(utVal)) {
                  utVal = '';
                }
                posData_c.ut = utVal;
              } else if (crdKey.length > 0) { // if any string is exist, set as ('CONFIG').
                var confPos = line.lastIndexOf(':');
                var confStr = line.substr(confPos + 1);
                var confSta = confStr.indexOf("'");
                var confEnd = confStr.lastIndexOf("'");
                confStr = confStr.substring(confSta + 1, confEnd);
                posData_c.conf = confStr;
                break; // Do not analyze any more...
              }
            }
            posArray[targetIndex].group[status.group - 1] = posData_c;
            status.stat = posConf.POSPOSITION;
            break;
          case posConf.POSPOSITION: // Position raw data
            var posData_p = posArray[targetIndex].group[status.group - 1];
            var posElem = line.split(','); // Split for each comma
            for (var index_p in posElem) {
              var elem = posElem[index_p]; // 1 item
              if (elem.length == 0) {
                continue;
              }
              var keys = elem.split('='); // Split by equal
              var keyStr = keys[0].trim();
              var keyVal = parseFloat(keys[1]);
              if (isNaN(keyVal)) {
                keyVal = '';
              }
              if (keyStr.startsWith('J')) {
                posData_p.joint[keyStr] = keyVal;
              } else if (keyStr.startsWith('E')) {
                posData_p.ext[keyStr] = keyVal;
              } else { // May be cartesian
                posData_p.cart[keyStr] = keyVal;
              }
              posArray[targetIndex].group[status.group - 1] = posData_p;
            }
            if (line.charAt(line.length - 1) != ',') { // line mode continue...
              status.stat = posConf.POSGROUP;
            }
            break;
          default:
            // especially none
            irpapi.inner.apiErrorLog('analizePositionText: Illegal status.stat.(' + tagGroup + ')');
            break;
        }
        return status;
      }
    },
    /* 
    * Display the overlay on all sides.
    * (IN) isWait : Display the icon of waiting.
    *      ovlZ   : Set the z-index of the overlay and the waitiing icon.
    */
    putWaitOvl: function (isWait, ovlZ) {
      if (document.getElementById("overlayRoot") == null) {
        var $overlayBox = $('<div>', { id: 'overlayRoot', class: 'screenoverlay' });
        if (isWait) {
          var $waitingBox = $('<div>', { id: 'waitingIconRoot' });
          var $waitingImg = $('<img>', { 'src': '/frh/irprog/pceditor/images/waitnotice.gif' });
          $waitingBox.append($waitingImg);
          $overlayBox.append($waitingBox);
        }
        parent.$('#root').append($overlayBox);
        parent.$('#overlayRoot').show();

        if (typeof ovlZ == "number") {
          document.getElementById("overlayRoot").style.zIndex = ovlZ;
          if (isWait) {
            document.getElementById("waitingIconRoot").style.zIndex = ovlZ;
          }
        }
      }
    },
    /* 
    * Hide the overlay and the icon of waiting.
    */
    rmvWaitOvl: function () {
      parent.$('#overlayRoot').remove();
    },
    /*
    * Get override limit when releasing the deadman at T2.
    * (IN) numGrp: Number of groups.
    *      callback : callback function to get frame.
    */
    getT2OvrLmt: function(numGrp, callback) {
      // Header
      var ovrXml = '<?xml version="1.0" encoding="' + irpapi.global.langEncoding[irpapi.global.currentLang] + '"?>\r';
      ovrXml = ovrXml + '<XMLCFG>\r<PROG name="*SYSTEM*">\r';
      for (var i = 1; i <= numGrp; i++){
        for (var j = 1; j <= 9; j++){
          ovrXml = ovrXml + '<VAR name="$SCR_GRP[' + i + '].$JOGLIM_JNT[' + j + ']" />\r';
        }
        ovrXml = ovrXml + '<VAR name="$MRR_GRP[' + i + '].$SPEEDLIM" />\r';
      }
      ovrXml = ovrXml + '</PROG>\r</XMLCFG>';

      // Create CVR.
      var file_cvr = new Blob([ovrXml], { type: 'text/plain' });
      getXHR('t2ovrlmt.cvr', file_cvr, convertT2OvrLmt, callback);

      /* (inner)
      * Analyze data after convert XVR CallBack.
      * (IN) callback : callback function to get data.
      */
      function convertT2OvrLmt(status, getText, callback) {
        var str_xml = getText;
        var filelines = getText.split(CODELF);
        var xml_obj = null;
        var dom_parser = new DOMParser();
        var xmlErrorFlg = false;
        try {
          xml_obj = dom_parser.parseFromString(str_xml, 'text/xml');
        } catch (e) {
          irpapi.inner.apiErrorLog(e);
        }
        if ((xml_obj == null) || (xml_obj.getElementsByTagName('parsererror').length)) {
          str_xml = irpapi.inner.delErrData(str_xml, filelines);
          xml_obj = dom_parser.parseFromString(str_xml, 'text/xml');
          xmlErrorFlg = true;
          irpapi.inner.apiErrorLog('convertT2OvrLim: xml error.');
        }
        irpapi.global.xmlErrorLog.flg = xmlErrorFlg;
        // Success Get XML Data.
        var arrElem = xml_obj.getElementsByTagName('ARRAY');
        var varElem = xml_obj.getElementsByTagName('VAR');
        var valList = [];
        var minVal = 100;
        for (var k = 0; k < arrElem.length; k++) {
          valList.push(Number(arrElem[k].childNodes[0].data));
        }
        for (var l = 0; l < varElem.length; l++) {
          var lmt = 250 / parseFloat(varElem[l].childNodes[0].data) * 100;
          if (lmt > 10) lmt = 10;
          valList.push(lmt);
        }
        valList.forEach(function(val){
          if (minVal > val) minVal = val;
        });
        callback(minVal, xmlErrorFlg);
      }
    },
