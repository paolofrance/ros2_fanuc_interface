    /* 
    * Function to set Fine and VFine for all group.
    * (IN)  .ovr_obj : Override Data(FINE, VFINE, OVR)
    *       .callback : Callback Function
    */
    setJogFineValue: function(ovr_obj, callback) {
      // Create Write Override Data
      var ovrride_xml;
      // Header
      ovrride_xml = '<?xml version="1.0" encoding="' + irpapi.global.langEncoding[irpapi.global.currentLang] + '"?>\r' + '<XMLVAR>\r';
      ovrride_xml += ' <PROG name="*SYSTEM*">\r';

      // Fine and VFine value setting
      ovrride_xml += '  <VAR name="$JCR_GRP">\r';
      for (var i = 1; i <= Number(irpapi.getGroupCount()); i ++){
        ovrride_xml += '    <ARRAY name="$JCR_GRP['+ i +']">\r';
