    /*
    * Converts the received element name to the usage name.
    * (IN) word : Extend axes word (with :)
    * return : Usage extend axes word (omit :)
    */
    convertExtElem: function(word) {
      return (word.startsWith('EXT')) ? word.slice(0, (word.indexOf(':') < 0) ? word.length : -1): word;
    },
    /*
    * Callback function of get cartesian position data.
    * (IN) str : target cartesian position string for analyze.
    *      cbArg : Position record to set. 
    *              (Passed by object for compatibility with past functionality.)
    */
    analyzeCartesianPosition: function(str, cbArg) {
      var posJson = cbArg.rec;
      return irpapi.analyzePositionStr(top.COORDINATECART, str, posJson);
    },
    /*
    * Callback function of get joint position data.
    * (IN) str : target cartesian position string for analyze.
