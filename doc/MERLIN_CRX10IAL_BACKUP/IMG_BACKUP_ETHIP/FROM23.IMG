/*
 * Check Current Language and Character bytes
 * (IN) str          : character string
 *      selectRegExp : 0 or 1
 * (OUT)regExpRes    : true or false
 */
function checkLangAndCharBytes(str, selectRegExp) {
  var RegExp = (selectRegExp == 0) ? /[^\x00-\x7E]+/g : /[^\x01-\x7E]/;
  if ((getCurrentLanguage() != LANG_JP && getCurrentLanguage() != LANG_CH) && str.match(RegExp)) {
    return true;
  }
  return false;
}
