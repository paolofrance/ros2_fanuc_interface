    if (typeof callback === 'function') callback(IO_COMFAILURE, cbArg);
  });
} // delIOAssign

/**
 * Process where common_io.js calls setIOAssign.
 */
function callSetIOAssign(setIOInf, oldStartNum, callback, cbArg) {
  setTimeout(function() {setIOAssign(setIOInf, oldStartNum, callback, cbArg)}, 0);
} // callSetIOAssign

/**
 * Set I/O assign. 
 * (IN) setIOInf : Information for I/O assign. 
 * (IN) oldStartNum : A I/O start Number for removing old I/O information.
 * (IN) callback : Callback function.
 * (IN) cbarg : Callback argument.
 */
function setIOAssign(setIOInf, oldStartNum, callback, cbArg) {
  delIOAssign(setIOInf.logPortType, oldStartNum, function () {
    $.getJSON("/COMET/rpc?func=IOASGLOG" +
              "&log_port_type=" + setIOInf.logPortType + // I/O type.
              "&fst_log_port=" + setIOInf.fstLogPort +   // I/O start number.
              "&n_log_ports=" + setIOInf.nLogPorts +     // DIO:Number of bits. AIO:1. GIO:Number points.
              "&rack_no=" + setIOInf.rackNo +            // Rack number.
              "&slot_no=" + setIOInf.slotNo +            // Slot number.
              "&phy_port_type=" + setIOInf.phyPortType + // Value according to set value.
              "&fst_phy_port=" + setIOInf.fstPhyPort,    // DIO/GIO:Start point. AIO:Channel.
      function (json) {
        if (json.FANUC.RPC != undefined) {
          json.FANUC.RPC[0].status = json.FANUC.RPC[0].status.toLowerCase();
          if (json.FANUC.RPC[0].status != 0) {
            top.rpcmc_rprintf("[rpcmc] IOASGLOG set failed, status " + json.FANUC.RPC[0].status);
          }
          if (typeof callback === 'function') callback(json.FANUC.RPC[0].status, setIOInf, cbArg);
        } else {
          top.rpcmc_rprintf("[rpcmc] unexpected reply " + JSON.stringify(json));
        }
      }
    ).fail(function (xhr, textStatus, errorThrown) {
      putIFFailLog(xhr, textStatus, errorThrown);
      if (typeof callback === 'function') callback(IO_COMFAILURE, setIOInf, cbArg);
    });
  });
} // setIOAssign

function setPayloadDataIF(grp_no, index, mode, callback, setPldInf, cbArg) {
  var strPGPLDUTL = ("/COMET/rpc?func=PGPLDUTL" +
                    "&grp_num=" + grp_no +
                    "&sch_num=" + index +
                    "&mode=" + mode );
  if (setPldInf != null) {
    for (var i = 0; i < Object.keys(setPldInf).length; i++) {
      switch (Object.keys(setPldInf)[i]) {
        case 'payload':
          strPGPLDUTL += "&pld_weight=" + setPldInf.payload;
          break;
        case 'ix':
          strPGPLDUTL += "&pld_ix=" + setPldInf.ix;
          break;
        case 'iy':
          strPGPLDUTL += "&pld_iy=" + setPldInf.iy;
          break;
        case 'iz':
          strPGPLDUTL += "&pld_iz=" + setPldInf.iz;
          break;
        case 'x':
          strPGPLDUTL += "&pld_x=" + setPldInf.x;
          break;
        case 'y':
          strPGPLDUTL += "&pld_y=" + setPldInf.y;
          break;
        case 'z':
          strPGPLDUTL += "&pld_z=" + setPldInf.z;
          break;
        default:
          break;
      }
    }
  }
  $.getJSON(strPGPLDUTL,
    function (json) {
      if (json.FANUC.RPC != undefined) {
        json.FANUC.RPC[0].status = json.FANUC.RPC[0].status.toLowerCase();
        if (json.FANUC.RPC[0].status != IO_SUCCESS) {
          top.rpcmc_rprintf("[rpcmc] PGPLDUTL set failed, status " + json.FANUC.RPC[0].status);
        }
        if (typeof callback === 'function') callback(json.FANUC.RPC[0].status, json.FANUC.RPC[0].lim_err, json.FANUC.RPC[0].pld_stat, mode, setPldInf, cbArg);
      } else {
        top.rpcmc_rprintf("[rpcmc] unexpected reply " + JSON.stringify(json));
      }
    }
  ).fail(function (xhr, textStatus, errorThrown) {
    putIFFailLog(xhr, textStatus, errorThrown);
    if (typeof callback === 'function') callback(IO_COMFAILURE, null, null, setPldInf, cbArg);
  });
} // setPayloadData

/*
 * Setting jog mode.
 * (IN) grpNum : Target group number.
 * (IN) reqMode : Applicability to control equipment. 0 = not apply / 1 = apply
 * (IN) setConfObj : Setting data object.
 * (IN) callback : A callback function to receive error message.
 * (IN) cbArg : Arguments to pass to the callback function.
 */
function sendJogmodeConf(grpNum, reqMode, setConfObj, callback, cbArg) {
  var strJOGMODE_CONF = "";
  var keyArr = ['req_frm', 'req_wrst', 'req_rtcp','req_mltjog','req_cd_ornt','req_cd_flw','req_trk_mirr','req_trk_flw','req_cd_ldr'];

  if (setConfObj != null) {
    for (var i = 0; i < Object.keys(setConfObj).length; i++) {
      for (var j = 0; j < keyArr.length; j++) {
        if (keyArr[j] == Object.keys(setConfObj)[i]) {
          strJOGMODE_CONF += '&' + keyArr[j] + '=' + setConfObj[keyArr[j]];
          break;
        }
      }
    }
  }

  if (reqMode == 0) {
    strJOGMODE_CONF += '&req_update=0';
  } else {
    strJOGMODE_CONF += '&req_update=1';
  }
  $.getJSON("/COMET/rpc?func=JOGMODE_CONF&grp_num=" + grpNum + "&mode=1" + strJOGMODE_CONF,
    function (json) {
      if (json.FANUC.RPC != undefined) {
        json.FANUC.RPC[0].status = json.FANUC.RPC[0].status.toLowerCase();
        if (json.FANUC.RPC[0].status != IO_SUCCESS) {
          top.rpcmc_rprintf("[rpcmc] JOGMODE_CONF set failed, status " + json.FANUC.RPC[0].status);
        }
        if (typeof callback === 'function') callback(json.FANUC.RPC[0].status, json.FANUC.RPC[0], cbArg);
      } else {
        top.rpcmc_rprintf("[rpcmc] unexpected reply " + JSON.stringify(json));
      }
    }
  ).fail(function (xhr, textStatus, errorThrown) {
    putIFFailLog(xhr, textStatus, errorThrown);
    if (typeof callback === 'function') callback(IO_COMFAILURE, setConfObj, cbArg);
  });
}

