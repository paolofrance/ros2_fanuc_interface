/*
 * Count the number of characters that contain double-byte half-width characters
 */
function countChar(str) {
  len = 0;
  str = escape(str);
  for (i = 0; i < str.length; i++ , len++) {
    if (str.charAt(i) == "%") {
      if (str.charAt(++i) == "u") {
        i += 3;
        len++;
      }
      i++;
    }
  }
  return len;
}

/*
 * Slice characters that contain double-byte half-width characters
 */
function multByteStringSlice(str, str_limit) {
  while (countChar(str) > str_limit) {
    str = str.slice(0, str.length - 1);
  }
  return str;
}

