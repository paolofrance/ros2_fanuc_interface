/* 19.1
 * for DCS_CHECK_CODE (Code check only.)
 * pinCode: Password string.
 * callback/cbarg/cbarg2: Callback function and arguments.
 * callback : result = 1:invalid, 0:valid.
 */
function getDcsCheckCode(pinCode, callback, callbackarg, callbackarg2) {
  var pinList = {master: pinCode, prev: ''};
  getApplyPassword(pinList, 0, dcsCheckCodeCallback, callbackarg, callbackarg2);

  // Bypass to absorb the difference in arguments.
  // status is not used.
  function dcsCheckCodeCallback(status, result, cbarg, cbarg2) {
    callback(result, cbarg, cbarg2);
  }
}

/* 19.2
 * for DCS_CHECK_CODE with apply.
 * If the password is correct, apply processing is performed.
 * codeList: Password list object.
 *     {master: master code, base: base code, posspd: pos/spd code, iocnct: I/O connect code}
 *     [[[Caution!]]]
 *        If you modify this argument, check the 19.1 call location! 
 *     [[[Caution!]]]
 * applyflg: 0=Just check the validity of PIN., Not 0=Specify a return value for DCS_CHECK_APPLY.
 * callback/cbarg/cbarg2: Callback function and arguments.
 * callback : status = 0:success, not 0:error, result = 1:invalid, 0:valid, 2:exist undef.
 */
function getApplyPassword(codeList, applyflg, callback, cbarg, cbarg2) {
  var codeSpecifiedStr = '';
  if (codeList.master.length > 0) { // Specified master
    codeSpecifiedStr = 'code_master=' + codeList.master;
    if (codeList.prev.length > 0) {
      codeSpecifiedStr += '&prev_code_master=' + codeList.prev;
    }
  } else {
    if (codeList.base.length > 0) {
      codeSpecifiedStr += 'code1=' + codeList.base;
    }
    if (codeList.posspd.length > 0) {
      if (codeSpecifiedStr.length > 0) {
        codeSpecifiedStr += '&';
      }
      codeSpecifiedStr += 'code2=' + codeList.posspd;
    }
    if (codeList.iocnct.length > 0) {
      if (codeSpecifiedStr.length > 0) {
        codeSpecifiedStr += '&';
      }
      codeSpecifiedStr += 'code3=' + codeList.iocnct;
    }
  }
  $.getJSON("/COMET/rpc?func=DCS_CHECK_CODE&" + codeSpecifiedStr + "&req_apply=" + applyflg, function (json) {
    if (json.FANUC.RPC != undefined) {
      var rpcStatus = json.FANUC.RPC[0].status;
      if (rpcStatus != IO_SUCCESS) {
        top.rpcmc_rprintf('Error: DCS_CHECK_CODE status(' + rpcStatus + ')');
        handle_error(rpcStatus);
      }
      callback(rpcStatus, json.FANUC.RPC[0].result, cbarg, cbarg2);
    } else {
      top.rpcmc_rprintf("[rpcmc] unexpected reply " + JSON.stringify(json));
      callback(top.IO_COMFAILURE, null, cbarg, cbarg2);
    }
  }).fail(function (xhr, textStatus, errorThrown) {
    if (cbarg == undefined) {
      cbarg = {};
    }
    cbarg.failText = textStatus;
    callback(top.IO_COMFAILURE, null, cbarg, cbarg2);
  });
}

/* 19.3
 * Verify the check for changed data.
 * Notify the completion of confirmation and complete the apply process.
 * (IN)  vrfyFlg: TRUE=Apply OK, FALSE= Apply cancel.
 * (IN)  callback/cbarg/cbarg2: Callback function and arguments.
 * (OUT) callback : status = 0:Normal, != 0:Error.
 */
function sendVrfyRequest(vrfyFlg, callback, callbackarg, callbackarg2) {
  var vrfyStr = (vrfyFlg) ? 1 : 0;
  $.getJSON('/COMET/rpc?func=DCS_VRFY_REQ&vrfy_flag=' + vrfyStr, // Server function
    function (json) {
      if (json.FANUC.RPC != undefined) {
