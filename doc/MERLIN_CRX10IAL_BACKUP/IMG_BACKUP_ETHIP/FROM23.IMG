 **/
// Skip to the specified character
function skipToSpecChar(index, str, to_char) {
    for (; index < str.length; index++) {
        if (str.charAt(index) == to_char) {
            break;
        }
    }
    return index;
}
// Skip while the specified character appears
function skipWhileSpecChar(index, str, to_char) {
    for (; index < str.length; index++) {
        if (str.charAt(index) != to_char) {
            break;
        }
    }
    return index;
}
// Extract character strings up to blank
function extractCharsUt(index, str) {
    var ext_word = '';
    for (; index < str.length; index++) {
        var target_char = str.charAt(index);
        if (target_char == ' ') {
            break;
        }
        ext_word += target_char;
    }
    var str_info = {};
    str_info.index = index;
    str_info.word = ext_word;
    return str_info;
}
// Back to the specified character
function backToSpecChar(index, str, to_char) {
    for (; index > 0; index--) {
        if (str.charAt(index) == to_char) {
            break;
        }
    }
    return index;
}

function getPosRegData(str) {
    var posdata = {};
    posdata[POS_GROUP] = "";
    posdata[POS_CONFIG] = "";
    posdata[POS_X] = "";
    posdata[POS_Y] = "";
    posdata[POS_Z] = "";
    posdata[POS_W] = "";
    posdata[POS_P] = "";
    posdata[POS_R] = "";
    // Position data analize.
    var target_word = ''; // Target Word
    // Repeat for the number of characters
    var index = str.indexOf(POS_GROUP + ":");
    for (; index < str.length; index++) {
        index = skipWhileSpecChar(index, str, ' '); // Skip ' '
        // Extract up to ':'
        var str_info = {};
        var ch = str.charAt(index);
        if (ch == ':') {
            index++;
            if (target_word == POS_GROUP) {
                index = skipWhileSpecChar(index, str, ' '); // Skip ' '
                str_info = extractCharsUt(index, str); // Extract characters
                posdata[target_word] = parseInt(str_info.word); // Set group number
                index = str_info.index; // Update index
            } else if (target_word == POS_CONFIG) {
                // Retrieve up to the next ':' and return from ':' to ' '.
                var idx_start = index;
                index = skipToSpecChar(index, str, ':');
                index = backToSpecChar(index - 1, str, ' ');
                // Make the extracted string to string of 'Config'
                posdata[target_word] = str.substring(idx_start, index).trim();
            } else { // X to R
                index = skipWhileSpecChar(index, str, ' '); // Skip ' '
                str_info = extractCharsUt(index, str); // Extract characters
                posdata[target_word] = parseFloat(str_info.word);
                index = str_info.index;
            }
            target_word = '';
        } else {
            target_word += ch;
        }
    }
    return posdata;
}

function getPosRegJointData(str) {
    var posdata = {};
    posdata[POS_GROUP] = "";
    posdata[POS_J1] = "";
    posdata[POS_J2] = "";
    posdata[POS_J3] = "";
    posdata[POS_J4] = "";
    posdata[POS_J5] = "";
    posdata[POS_J6] = "";
    posdata[POS_EXT1] = "";
    posdata[POS_EXT2] = "";
    posdata[POS_EXT3] = "";
    // Position data analize.
    // Repeat for the number of characters
    var index = str.indexOf(POS_GROUP + ":");
    index = skipToSpecChar(index, str, ' ') + 1;
    var str_info = {};
    str_info = extractCharsUt(index, str); // Extract characters
    posdata[POS_GROUP] = parseInt(str_info.word); // Set group number
    index = str_info.index + 1;
    for (; index < str.length; index++) {
        index = skipToSpecChar(index, str, ' ') + 1; // Skip to '='
        index = skipWhileSpecChar(index, str, ' '); // Skip to value
        str_info = extractCharsUt(index, str); // Extract characters
        if (str_info.word.indexOf(':') != -1) {
            str_info.word = str_info.word.replace(/:/g, "");
        }
        var target_key = str_info.word.trim();
        if (target_key.length == 0) {
            continue; // Illegal keyword
        }
        index = str_info.index;
        index = skipWhileSpecChar(index, str, ' '); // Skip to value
        index = skipWhileSpecChar(index, str, '='); // Skip to value
        index = skipWhileSpecChar(index, str, ' '); // Skip to value
        str_info = extractCharsUt(index, str);
        posdata[target_key] = parseFloat(str_info.word);
        index = str_info.index + 1;
        index = skipToSpecChar(index, str, ' ') + 1; // Skip unit
    }
    return posdata;
}

function analyzeNumRegData(data) {
  var rtn_value = {};

  // NumReg data analyze.
  var target_key = '';    // Target Key
  var target_value = '';  // Target Value
  var value_temp = '';    // Value Temp(Next Key)
  for (var index = 0; index < data.length; index++) {
      switch (data[index]) {
          case '\'':  // Comment
              if ("comment" in rtn_value) {
                  rtn_value[target_key] = target_value;
                  target_key = "";
                  target_value = "";
              } else {
                  target_value = "";
                  target_key = "comment";
                  rtn_value[target_key] = "";
              }
              break;
          case ':':   // Delimiter
              if (target_key == "comment") {
                  target_value = target_value + data[index];
              } else {
                  if (target_key != "") {
                      rtn_value[target_key] = target_value.trim();
                  }
                  target_key = value_temp.trim();
                  target_value = "";
                  value_temp = "";
              }
              break;
          default:    // Character and Space
              if (target_key == "comment") {
                  target_value = target_value + data[index];
              } else {
                  if ((data[index] != ' ') && (value_temp.slice(-1) == ' ')) {
                      target_value = target_value + value_temp;
                      value_temp = "";
                  }
                  value_temp = value_temp + data[index];
              }
              break;
      }
  }
  if (target_key != "") {
      rtn_value[target_key] = (target_value + value_temp).trim();
  }
  return rtn_value;
}

function analyzePosRegData(data) {
    var rtn_value = {};

    // PosReg data analyze.
    var target_key = '';    // Target Key
    var target_value = '';  // Target Value
    var value_temp = '';    // Value Temp(Next Key)
    for (var index = 0; index < data.length; index++) {
        switch (data[index]) {
            case '\'':  // Comment
                if ("comment" in rtn_value) {
                    rtn_value[target_key] = target_value;
                    target_key = "";
                    target_value = "";
                } else {
                    target_value = "";
                    target_key = "comment";
                    rtn_value[target_key] = "";
                }
                break;
            case ':':   // Delimiter
            case '=':   // Delimiter(Axis)
                if (target_key == "comment") {
                    target_value = target_value + data[index];
                } else {
                    if (target_key != "") {
                        rtn_value[target_key] = target_value.trim();
                    }
                    target_key = value_temp.trim();
                    target_value = "";
                    value_temp = "";
                }
                break;
            case '\n':  // Enter
                if (target_key != "") {
                    rtn_value[target_key] = (target_value + value_temp).trim();
                    target_key = "";
                    target_value = "";
                    value_temp = "";
                }
                break;
            default:    // Character and Space
                if (target_key == "comment") {
                    target_value = target_value + data[index];
                } else {
                    if ((data[index] != ' ') && (value_temp.slice(-1) == ' ')) {
                        target_value = target_value + value_temp;
                        value_temp = "";
                    }
                    value_temp = value_temp + data[index];
                }
                break;
        }
    }
    if (target_key != "") {
        rtn_value[target_key] = (target_value + value_temp).trim();
    }
    // Axis Data => {data:000.000, unit:"deg"}
    for (var key in rtn_value) {
        if (key.match(/J\d/)
            || key.match(/EXT\d/)) {
            var value = rtn_value[key].split(' ');
            rtn_value[key] = { data: value[0], unit: value[1] };
        }
    }

    return rtn_value;
