        document.getElementById("freque").refresh(parent.ls_ini[2], parent.ls_ini[9], parent.ls_ini[10], false);
        document.getElementById("r_duty").refresh(parent.ls_ini[3], parent.ls_ini[11], parent.ls_ini[12], false);
        document.getElementById("out_number").refresh("0", "0", "32767", false);
        if(parent.ls_ini[15] != 3){
          document.getElementById("sp_unit_i").style.display = "block";
          document.getElementById("sp_unit_r").style.display = "none";
          document.getElementById("r_tlrance").refresh(parent.ls_ini[4], parent.ls_ini[13], parent.ls_ini[14], false);
        }else{
          document.getElementById("sp_unit_i").style.display = "none";
          document.getElementById("sp_unit_r").style.display = "block";
          var c_value_def = parseInt(parent.ls_ini[4]);
          var c_value_min = parseInt(parent.ls_ini[13]);
          var c_value_max = parseInt(parent.ls_ini[14]);
          c_value_def = c_value_def / 10;
          c_value_min = c_value_min / 10;
          c_value_max = c_value_max / 10;
          c_value_def = c_value_def.toString();
          c_value_min = c_value_min.toString();
          c_value_max = c_value_max.toString();
          document.getElementById("r_tlrance_r").refresh(c_value_def , c_value_min, c_value_max, false);
        }
      }else{
        document.getElementById("ls_data_area").style.display = "none";
        document.getElementById("ls_sch_dat_area").style.display = "";
        document.getElementById("ls_title").style.display = "none";
        document.getElementById("ls_sch_title").style.display = "inline-block";
        document.getElementById("sch_pos").refresh([],1,null,false);
        document.getElementById("sch_val").refresh(parent.ls_ini[16], parent.ls_ini[17], parent.ls_ini[18], false);
      }
      document.getElementById("slope_up").refresh(false, false);
      document.getElementById("slope_down").refresh(false, false);
      parent.detailFix("sch_val", "changed", "1");
    }
