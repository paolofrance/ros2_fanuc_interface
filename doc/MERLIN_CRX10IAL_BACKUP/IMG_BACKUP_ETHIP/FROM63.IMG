      document.getElementById("sch_pos").setCallback(input_val_type);
      document.getElementById("ls_command").setCallback(select_command);
      document.getElementById("p_power").setCallback(textboxCallback);
      document.getElementById("b_power").setCallback(textboxCallback);
      document.getElementById("freque").setCallback(textboxCallback);
      document.getElementById("r_duty").setCallback(textboxCallback);
      document.getElementById("r_tlrance").setCallback(textboxCallback);
      document.getElementById("r_tlrance_r").setCallback(textboxCallback);
      document.getElementById("sch_val").setCallback(textboxCallback);
      document.getElementById("out_number").setCallback(textboxCallback);
      if(parent.ls_ini[15] == '1'){
         document.getElementById("tlsp_label_m").style.display = "";
         document.getElementById("tlsp_label_c").style.display = "none";
         document.getElementById("tlsp_label_i").style.display = "none";
      }else if(parent.ls_ini[15] == '2'){
         document.getElementById("tlsp_label_m").style.display = "none";
         document.getElementById("tlsp_label_c").style.display = "";
         document.getElementById("tlsp_label_i").style.display = "none";
      }else{
         document.getElementById("tlsp_label_m").style.display = "none";
         document.getElementById("tlsp_label_c").style.display = "none";
         document.getElementById("tlsp_label_i").style.display = "";
      }
      select_tab(1);
    };
    function pushBtnHelpIcon(val) {
      var help_title = "";
      switch (val) {
        case 1:
          help_title = "PeakPower";
          break;
        case 2:
          help_title = "BottomPower";
          break;
        case 3:
          help_title = "Frequency";
          break;
        case 4:
          help_title = "duty";
          break;
        default:
          break;
      }
      var customizeObj = {
        callerWindowObj: window,
        width: "620px",
        height: "420px",
        designpattern: "mouse",
        appendArg: { showHelpTitle: help_title },
      }
      openHelp(customizeObj);
    }
    function pushBtnHelpIcon_v(val) {
      var help_title = "";
      switch (val) {
        case 1:
          help_title = "mm/sec";
          break;
        case 2:
          help_title = "cm/min";
          break;
        case 3:
          help_title = "inch/min";
          break;
        default:
          break;
      }
      var screen_height;
      if (top.irprogapi.getCurrentLanguage() == 'jp') {
        screen_height = "200px";
      }else{
        screen_height = "250px";
      }
      var customizeObj = {
        callerWindowObj: window,
        width: "620px",
        height: screen_height,
        designpattern: "mouse",
        appendArg: { showHelpTitle: help_title },
      }
      openHelp_v(customizeObj);
    }
    function pushBtnHelpIcon_sn() {
      var help_title = "sch_ls";
      var help_width = "";
      var lang = top.irprogapi.getCurrentLanguage();
      if(lang == 'jp'){
        help_width = "510px";
      }else{
        help_width = "640px";
      }
      var customizeObj = {
        callerWindowObj: window,
        width: help_width,
        height: "200px",
        designpattern: "mouse",
        appendArg: { showHelpTitle: help_title },
      }
      openHelp_l(customizeObj);
    }
    function pushBtnHelpIcon_sl(val) {
      var help_title = "";
      switch (val) {
        case 1:
          help_title = "slop_up";
          break;
        case 2:
          help_title = "slop_down";
          break;
        default:
          break;
      }
      var help_width = "";
      var customizeObj = {
        callerWindowObj: window,
        width: "620px",
        height: "420px",
        designpattern: "mouse",
        appendArg: { showHelpTitle: help_title },
      }
      openHelp_s(customizeObj);
    }
    function openHelp(customizeObj) {
      top.IHMIComponents.cf.appendPopupFrm(URL_HELP_POPUP, null, window.parent, customizeObj);
    }
    function openHelp_v(customizeObj) {
      top.IHMIComponents.cf.appendPopupFrm(URL_HELP_POPUP_V, null, window.parent, customizeObj);
    }
    function openHelp_l(customizeObj) {
      top.IHMIComponents.cf.appendPopupFrm(URL_HELP_POPUP_L, null, window.parent, customizeObj);
    }
    function openHelp_s(customizeObj) {
      top.IHMIComponents.cf.appendPopupFrm(URL_HELP_POPUP_S, null, window.parent, customizeObj);
    }
    </script>
