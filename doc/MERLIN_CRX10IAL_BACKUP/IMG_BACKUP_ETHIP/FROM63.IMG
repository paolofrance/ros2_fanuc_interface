            parent.detailFix("laser_off", "changed", "1");
          }else{
            document.getElementById("lof_chk").style.display = "none";
            document.getElementById("lof_reg").style.display = "block";
            document.getElementById("laser_off_reg").refresh(1, "1", "16383", false);
