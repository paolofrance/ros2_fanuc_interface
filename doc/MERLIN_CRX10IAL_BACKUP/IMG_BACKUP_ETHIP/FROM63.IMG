        document.getElementById("option_formlist").refresh(optionFormList, null, parent.DETAIL_ADD_MOTION_MAX, "680px");
        document.getElementById("addmotion_select_1").className = "select touch input-form-list-select";
      } else {
        document.getElementById("option_formlist").refresh(optionFormList, null, parent.DETAIL_ADD_MOTION_MAX, "560px");
        document.getElementById("addmotion_select_1").className = "select touch input-form-list-select";
      }
      document.getElementById("ch_val").setCallback(textboxCallback);
      document.getElementById("sel_pos").setCallback(input_val_type);
      select_tab(1);
    };
    function pushBtnHelpIcon() {
      var help_title = "";
      var help_width = "";
      var lang = top.irprogapi.getCurrentLanguage();
      if(parent.pre_ls_ini[0] == 14){
        help_title = "Beam switch channel";
        help_width = "580px";
      }else{
        help_title = "Laser control request";
        if(lang == 'jp'){
          help_width = "480px";
        }else{
          help_width = "580px";
        }
      }
      var customizeObj = {
        callerWindowObj: window,
        width: help_width,
        height: "200px",
        designpattern: "mouse",
        appendArg: { showHelpTitle: help_title },
      }
      openHelp(customizeObj);
    }
    function openHelp(customizeObj) {
      top.IHMIComponents.cf.appendPopupFrm(URL_HELP_POPUP, null, window.parent, customizeObj);
    }
