      [ "inch/min", "3" ],
      [ "deg/sec", "4" ],
      [ "sec", "5" ],
      [ "msec", "6" ]
    ];
    var optionFormList = [
      {
        type: "label",
        id: "addmotion_label_1"
      },
      {
        type: "textboxString",
        id: "addmotion_1"
      },
      {
        type: "select",
        id: "addmotion_select_1"
      }
    ];
