      [ 'FINE', "0" ],
      [ 'CNT', "1" ],
      [ 'CNT R', "2" ],
    //   [ 'CR', "3" ]
    ];
    var optionFormList = [
      {
        type: "label",
        id: "addmotion_label_1"
      },
      {
        type: "textboxString",
        id: "addmotion_1"
      },
      {
        type: "select",
