    function request_lof_status(callback, argObj){
      var lang = top.irprogapi.getCurrentLanguage();
      var frameXml = "";
      frameXml += '<?xml version="1.0" encoding="' + top.irprogapi.global.langEncoding[lang] + '"?>\r';
      frameXml += '<XMLCFG>\r<PROG name="*SYSTEM*">\r';
      frameXml += '<VAR name="$LIFSET.$HD[1].$MAKER" />\r'; // add system value name
      frameXml += '<VAR name="$LIFSET.$HD[1].$MODEL" />\r'; // add system value name
      frameXml += '</PROG>\r</XMLCFG>';
      // Create Blob frame CVR
      var fileCvr = new Blob([frameXml], { type: "text/plain" });
      var cbArgs = {};
      cbArgs.sysIndex = 1;
      cbArgs.cbFunc = callback;
      cbArgs.cbObj = argObj;
      top.getXHR('crxeditorinit.cvr', fileCvr, set_lof_state, cbArgs);
    }
    function set_lof_state(status, getText, cbArgs){
      if(status != 0){ return;}
      var XmlObj = getSysValXmlData(getText, 'VAR');
      var help_title = "";
      var help_width = "";
      var help_height = "200px";
      var lang = top.irprogapi.getCurrentLanguage();
      maker_val = parseInt(XmlObj.getElementsByTagName("VAR")[0].textContent, 10);
      model_val = parseInt(XmlObj.getElementsByTagName("VAR")[1].textContent, 10);
      if((maker_val == 3) && (model_val == 1)){
        help_title = "Laser ON output";
        if(lang == 'jp'){
          help_width = "580px";
        }else{
          help_width = "560px";
        }
      }else if((maker_val == 3) && (model_val == 2)){
        help_title = "Interlock Ch output";
        if(lang == 'jp'){
          help_width = "560px";
        }else{
          help_width = "640px";
        }
      }else{
        help_title = "Not Suppoted";
        help_width = "300px";
        help_height = "100px";
      }
      var customizeObj = {
        callerWindowObj: window,
        width: help_width,
        height: help_height,
        designpattern: "mouse",
        appendArg: { showHelpTitle: help_title },
      }
      openHelp(customizeObj);
    }
