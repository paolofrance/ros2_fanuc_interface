    function pushBtnHelpIcon(val) {
      var help_title = "";
      switch (val) {
        case 1:
          help_title = "PeakPower";
          break;
        case 2:
          help_title = "BottomPower";
          break;
        case 3:
          help_title = "Frequency";
          break;
        case 4:
          help_title = "duty";
          break;
        default:
          break;
      }
