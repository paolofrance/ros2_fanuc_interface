    function input_val_type(id, operation, arg) {
      var select_type
      select_type = document.getElementById(id).getValue();
      switch(id){
        case 's_pos':
          if(select_type == 1){
            document.getElementById("p_power").refresh("0", parent.ls_ini[5], parent.ls_ini[6], false);
          }else{
            document.getElementById("p_power").refresh("1", "1", "16383", false);
          }
          parent.detailFix("p_power", "changed", "1");
          break;
        case 'l_pos':
          if(select_type == 1){
            document.getElementById("b_power").refresh("0", parent.ls_ini[7], parent.ls_ini[8], false);
          }else{
            document.getElementById("b_power").refresh("1", "1", "16383", false);
          }
          parent.detailFix("b_power", "changed", "1");
          break;
        case 'p_pos':
          if(select_type == 1){
            document.getElementById("freque").refresh(0, parent.ls_ini[9], parent.ls_ini[10], false);
          }else{
            document.getElementById("freque").refresh("1", "1", "16383", false);
          }
          parent.detailFix("freque", "changed", "1");
          break;
        case 'q_pos':
          if(select_type == 1){
            document.getElementById("r_duty").refresh(0, parent.ls_ini[11], parent.ls_ini[12], false);
          }else{
            document.getElementById("r_duty").refresh("1", "1", "16383", false);
          }
          parent.detailFix("r_duty", "changed", "1");
          break;
        case 'v_pos':
          if(select_type == 1){
            if(parent.ls_ini[15] != 3){
              document.getElementById("r_tlrance").refresh(0, parent.ls_ini[13], parent.ls_ini[14], false);
            }else{
              document.getElementById("sp_unit_i").style.display = "none";
              document.getElementById("sp_unit_r").style.display = "block";
              var c_value_min = parseFloat(parent.ls_ini[13]) / 10;
              var c_value_max = parseFloat(parent.ls_ini[14]) / 10;
              c_value_min = c_value_min.toString();
              c_value_max = c_value_max.toString();
              document.getElementById("r_tlrance_r").refresh("0.0", c_value_min, c_value_max, false);
            }
          }else{
            document.getElementById("sp_unit_i").style.display = "block";
            document.getElementById("sp_unit_r").style.display = "none";
            document.getElementById("r_tlrance").refresh("1", "1", "16383", false);
          }
          parent.detailFix("r_tlrance", "changed", "1");
          break;
        case 'sch_pos':
          if(select_type == 1){
            document.getElementById("sch_val").refresh(parent.ls_ini[16], parent.ls_ini[17], parent.ls_ini[18], false);
          }else{
            document.getElementById("sch_val").refresh("1", "1", "16383", false);
