    function select_command(id, operation, arg) {
      var select_type
      select_type = document.getElementById(id).getValue();
      if(select_type == 0){
        document.getElementById("ls_data_area").style.display = "";
        document.getElementById("ls_sch_dat_area").style.display = "none";
        document.getElementById("ls_title").style.display = "inline-block";
        document.getElementById("ls_sch_title").style.display = "none";
        document.getElementById("s_pos").refresh([],1,null,false);
        document.getElementById("l_pos").refresh([],1,null,false);
        document.getElementById("p_pos").refresh([],1,null,false);
        document.getElementById("q_pos").refresh([],1,null,false);
        document.getElementById("v_pos").refresh([],1,null,false);
        document.getElementById("o_pos").refresh([],1,null,false);
        document.getElementById("p_power").refresh(parent.ls_ini[0], parent.ls_ini[5], parent.ls_ini[6], false);
