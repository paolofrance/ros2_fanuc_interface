      document.getElementById("q_pos").setCallback(input_val_type);
      document.getElementById("v_pos").setCallback(input_val_type);
      document.getElementById("o_pos").setCallback(input_val_type);
      document.getElementById("sch_pos").setCallback(input_val_type);
